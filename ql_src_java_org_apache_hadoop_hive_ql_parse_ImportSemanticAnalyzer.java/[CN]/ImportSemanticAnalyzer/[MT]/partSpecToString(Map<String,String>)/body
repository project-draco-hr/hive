{
  StringBuilder sb=new StringBuilder();
  boolean firstTime=true;
  for (  Map.Entry<String,String> entry : partSpec.entrySet()) {
    if (!firstTime) {
      sb.append(',');
    }
    firstTime=false;
    sb.append(entry.getKey());
    sb.append('=');
    sb.append(entry.getValue());
  }
  return sb.toString();
}
