{
  execContext.resetRow();
  try {
    mo.process((Writable)value);
    if (isLogInfoEnabled) {
      logMemoryInfo();
    }
  }
 catch (  Throwable e) {
    abort=true;
    if (e instanceof OutOfMemoryError) {
      throw (OutOfMemoryError)e;
    }
 else {
      String msg="Error processing row: " + e;
      LOG.fatal(msg,e);
      throw new RuntimeException(msg,e);
    }
  }
}
