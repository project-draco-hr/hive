{
  if (fieldIsNull[fieldID])   return null;
  if (!inited[fieldID]) {
    BytesRefWritable passedInField=rawBytesField[fieldID];
    try {
      cachedByteArrayRef[fieldID].setData(passedInField.getData());
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
    fields[fieldID].init(cachedByteArrayRef[fieldID],passedInField.getStart(),passedInField.getLength());
    inited[fieldID]=true;
  }
  byte[] data=cachedByteArrayRef[fieldID].getData();
  int fieldLen=data.length;
  if (fieldLen == nullSequence.getLength() && LazyUtils.compare(data,0,fieldLen,nullSequence.getBytes(),0,nullSequence.getLength()) == 0) {
    return null;
  }
  return fields[fieldID].getObject();
}
