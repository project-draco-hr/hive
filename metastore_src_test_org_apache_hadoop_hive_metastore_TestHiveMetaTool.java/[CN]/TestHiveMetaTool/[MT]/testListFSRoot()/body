{
  redirectOutputStream();
  String[] args=new String[1];
  args[0]=new String("-listFSRoot");
  try {
    HiveMetaTool.main(args);
    String out=os.toString();
    boolean b=out.contains(locationUri);
    assertTrue(b);
  }
  finally {
    restoreOutputStream();
    System.out.println("Completed testListFSRoot");
  }
}
