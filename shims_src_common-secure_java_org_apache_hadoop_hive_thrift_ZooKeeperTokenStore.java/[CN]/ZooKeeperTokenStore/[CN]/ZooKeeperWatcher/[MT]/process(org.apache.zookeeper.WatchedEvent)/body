{
  LOGGER.info(event.toString());
  if (event.getState() == Watcher.Event.KeeperState.Expired) {
    LOGGER.warn("ZooKeeper session expired, discarding connection");
    try {
      zkSession.close();
    }
 catch (    Throwable e) {
      LOGGER.warn("Failed to close connection on expired session",e);
    }
  }
}
