{
  if (zkSession == null || zkSession.getState() == States.CLOSED) {
synchronized (this) {
      if (zkSession == null || zkSession.getState() == States.CLOSED) {
        try {
          zkSession=createConnectedClient(this.zkConnectString,this.zkSessionTimeout,this.connectTimeoutMillis,new ZooKeeperWatcher());
        }
 catch (        IOException ex) {
          throw new TokenStoreException("Token store error.",ex);
        }
      }
    }
  }
  return zkSession;
}
