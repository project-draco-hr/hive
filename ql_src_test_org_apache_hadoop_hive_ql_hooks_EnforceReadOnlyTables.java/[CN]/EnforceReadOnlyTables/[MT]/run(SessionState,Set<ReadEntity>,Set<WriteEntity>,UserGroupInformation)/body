{
  for (  WriteEntity w : outputs) {
    if ((w.getTyp() == WriteEntity.Type.TABLE) || (w.getTyp() == WriteEntity.Type.PARTITION)) {
      Table t=w.getTable();
      if (DEFAULT_DATABASE_NAME.equalsIgnoreCase(t.getDbName()) && QTestUtil.srcTables.contains(t.getTableName())) {
        throw new RuntimeException("Cannot overwrite read-only table: " + t.getTableName());
      }
    }
  }
}
