{
switch (fieldID) {
case VALUES:
    return getValues();
case DBNAME:
  return getDbName();
case TABLENAME:
return getTableName();
case CREATETIME:
return new Integer(getCreateTime());
case LASTACCESSTIME:
return new Integer(getLastAccessTime());
case SD:
return getSd();
case PARAMETERS:
return getParameters();
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
