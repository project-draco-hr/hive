{
  if (blackListedConfEntries == null) {
    blackListedConfEntries=new HashSet<String>();
    if (conf != null) {
      String bl=conf.get(HiveConf.ConfVars.HIVESCRIPT_ENV_BLACKLIST.toString(),HiveConf.ConfVars.HIVESCRIPT_ENV_BLACKLIST.getDefaultValue());
      if (bl != null && !bl.isEmpty()) {
        String[] bls=bl.split(",");
        Collections.addAll(blackListedConfEntries,bls);
      }
    }
  }
  return blackListedConfEntries.contains(name);
}
