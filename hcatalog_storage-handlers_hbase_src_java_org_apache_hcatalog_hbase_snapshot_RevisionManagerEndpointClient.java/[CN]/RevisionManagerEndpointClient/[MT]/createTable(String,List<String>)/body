{
  call(new Batch.Call<RevisionManagerEndpointService,Void>(){
    @Override public Void call(    RevisionManagerEndpointService service) throws IOException {
      ServerRpcController controller=new ServerRpcController();
      BlockingRpcCallback<CreateTableResponse> done=new BlockingRpcCallback<CreateTableResponse>();
      CreateTableRequest request=CreateTableRequest.newBuilder().setTableName(table).addAllColumnFamilies(columnFamilies).build();
      service.createTable(controller,request,done);
      blockOnResponse(done,controller);
      return null;
    }
  }
);
}
