{
  if (!this.pGraphContext.getJoinOps().contains(joinOperator)) {
    return false;
  }
  String[] srcs=joinOperator.getConf().getBaseSrc();
  List<Order> sortColumnsFirstTable=new ArrayList<Order>();
  for (int pos=0; pos < srcs.length; pos++) {
    if (!isEligibleForBucketSortMergeJoin(smbJoinContext,smbJoinContext.getKeyExprMap().get((byte)pos),joinOperator.getConf().getAliasToOpInfo(),srcs,pos,sortColumnsFirstTable)) {
      return false;
    }
  }
  smbJoinContext.setSrcs(srcs);
  return true;
}
