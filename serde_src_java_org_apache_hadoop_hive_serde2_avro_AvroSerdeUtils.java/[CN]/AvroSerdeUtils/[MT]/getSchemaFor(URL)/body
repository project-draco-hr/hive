{
  InputStream in=null;
  try {
    in=url.openStream();
    return getSchemaFor(in);
  }
 catch (  Exception e) {
    throw new RuntimeException("Failed to parse Avro schema",e);
  }
 finally {
    if (in != null) {
      try {
        in.close();
      }
 catch (      IOException e) {
      }
    }
  }
}
