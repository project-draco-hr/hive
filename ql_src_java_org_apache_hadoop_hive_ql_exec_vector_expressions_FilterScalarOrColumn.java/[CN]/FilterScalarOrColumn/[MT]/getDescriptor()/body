{
  return (new VectorExpressionDescriptor.Builder()).setMode(VectorExpressionDescriptor.Mode.FILTER).setNumArguments(2).setArgumentTypes(VectorExpressionDescriptor.ArgumentType.LONG,VectorExpressionDescriptor.ArgumentType.LONG).setInputExpressionTypes(VectorExpressionDescriptor.InputExpressionType.SCALAR,VectorExpressionDescriptor.InputExpressionType.COLUMN).build();
}
