{
switch (inputTypes[0]) {
case DATE:
    date.setTime(DateWritable.daysToMillis((int)vector[i]));
  break;
case TIMESTAMP:
date.setTime(vector[i] / 1000000);
break;
}
try {
byte[] bytes=formatter.format(date).getBytes("UTF-8");
outV.setRef(i,bytes,0,bytes.length);
}
 catch (UnsupportedEncodingException e) {
outV.vector[i]=null;
outV.isNull[i]=true;
}
}
