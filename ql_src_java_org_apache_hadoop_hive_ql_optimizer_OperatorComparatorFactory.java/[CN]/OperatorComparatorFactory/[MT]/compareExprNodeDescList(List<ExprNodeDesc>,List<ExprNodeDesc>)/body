{
  if (first == null && second == null) {
    return true;
  }
  if ((first == null && second != null) || (first != null && second == null)) {
    return false;
  }
  if (first.size() != second.size()) {
    return false;
  }
 else {
    for (int i=0; i < first.size(); i++) {
      if (!first.get(i).isSame(second.get(i))) {
        return false;
      }
    }
  }
  return true;
}
