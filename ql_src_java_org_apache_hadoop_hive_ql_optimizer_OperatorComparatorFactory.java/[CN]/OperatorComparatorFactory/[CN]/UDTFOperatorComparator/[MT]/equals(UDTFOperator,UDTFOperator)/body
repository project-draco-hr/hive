{
  Preconditions.checkNotNull(op1);
  Preconditions.checkNotNull(op2);
  UDTFDesc desc1=op1.getConf();
  UDTFDesc desc2=op2.getConf();
  if (compareString(desc1.getUDTFName(),desc2.getUDTFName()) && compareString(desc1.isOuterLateralView(),desc2.isOuterLateralView())) {
    return true;
  }
 else {
    return false;
  }
}
