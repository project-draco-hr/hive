{
  Preconditions.checkNotNull(op1);
  Preconditions.checkNotNull(op2);
  TableScanDesc op1Conf=op1.getConf();
  TableScanDesc op2Conf=op2.getConf();
  if (compareString(op1Conf.getAlias(),op2Conf.getAlias()) && compareExprNodeDesc(op1Conf.getFilterExpr(),op2Conf.getFilterExpr()) && op1Conf.getRowLimit() == op2Conf.getRowLimit() && op1Conf.isGatherStats() == op2Conf.isGatherStats()) {
    return true;
  }
 else {
    return false;
  }
}
