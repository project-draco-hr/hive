{
  ColumnStreamData cb;
  cb=SB_POOL.take();
  cb.init(sctx.kind.getNumber());
  cb.incRef();
  if (isDebugTracingEnabled) {
    LOG.info("Getting data for column " + colIx + " "+ (isLastRg ? "last " : "")+ "RG "+ rgIx+ " stream "+ sctx.kind+ " at "+ sctx.offset+ ", "+ sctx.length+ " index position "+ sctx.streamIndexOffset+ ": "+ (isCompressed ? "" : "un")+ "compressed ["+ cOffset+ ", "+ endCOffset+ ")");
  }
  return cb;
}
