{
  HTableInterface htab=conn.getHBaseTable(table);
  Scan s=new Scan();
  if (keyStart != null) {
    s.setStartRow(keyStart);
  }
  if (keyEnd != null) {
    s.setStopRow(keyEnd);
  }
  s.addColumn(colFam,colName);
  if (filter != null) {
    s.setFilter(filter);
  }
  ResultScanner scanner=htab.getScanner(s);
  return scanner.iterator();
}
