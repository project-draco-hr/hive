{
  try {
    CuratorFramework zk=getSession();
    String node=zk.create().creatingParentsIfNeeded().withMode(CreateMode.PERSISTENT).withACL(acl).forPath(path);
    LOGGER.info("Created path: {} ",node);
  }
 catch (  KeeperException.NodeExistsException e) {
  }
catch (  Exception e) {
    throw new TokenStoreException("Error creating path " + path,e);
  }
}
