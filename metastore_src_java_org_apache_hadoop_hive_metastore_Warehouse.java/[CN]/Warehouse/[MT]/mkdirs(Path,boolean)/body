{
  boolean inheritPerms=HiveConf.getBoolVar(conf,HiveConf.ConfVars.HIVE_WAREHOUSE_SUBDIR_INHERIT_PERMS) && inheritPermCandidate;
  FileSystem fs=null;
  try {
    fs=getFs(f);
    return FileUtils.mkdir(fs,f,inheritPerms,conf);
  }
 catch (  IOException e) {
    closeFs(fs);
    MetaStoreUtils.logAndThrowMetaException(e);
  }
  return false;
}
