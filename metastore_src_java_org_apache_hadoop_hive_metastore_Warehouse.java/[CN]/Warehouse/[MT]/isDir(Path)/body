{
  FileSystem fs=null;
  try {
    fs=getFs(f);
    FileStatus fstatus=fs.getFileStatus(f);
    if (!fstatus.isDir()) {
      return false;
    }
  }
 catch (  FileNotFoundException e) {
    return false;
  }
catch (  IOException e) {
    closeFs(fs);
    MetaStoreUtils.logAndThrowMetaException(e);
  }
  return true;
}
