{
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < path.length(); i++) {
    char c=path.charAt(i);
    if (c == '%' && i + 2 < path.length()) {
      int code=-1;
      try {
        code=Integer.valueOf(path.substring(i + 1,i + 3),16);
      }
 catch (      Exception e) {
        code=-1;
      }
      if (code >= 0) {
        sb.append((char)code);
        i+=2;
        continue;
      }
    }
    sb.append(c);
  }
  return sb.toString();
}
