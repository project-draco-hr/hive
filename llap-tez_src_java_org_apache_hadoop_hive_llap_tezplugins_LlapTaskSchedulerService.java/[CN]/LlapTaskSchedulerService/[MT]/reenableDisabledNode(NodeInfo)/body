{
  writeLock.lock();
  try {
    LOG.info("Attempting to re-enable node: " + nodeInfo.getServiceInstance());
    if (nodeInfo.getServiceInstance().isAlive()) {
      nodeInfo.enableNode();
    }
 else {
      if (LOG.isInfoEnabled()) {
        LOG.info("Removing dead node " + nodeInfo);
      }
    }
  }
  finally {
    writeLock.unlock();
  }
}
