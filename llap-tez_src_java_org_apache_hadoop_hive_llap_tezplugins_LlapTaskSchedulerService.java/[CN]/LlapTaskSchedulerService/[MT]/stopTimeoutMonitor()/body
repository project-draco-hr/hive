{
  timeoutLock.lock();
  try {
    if (timeoutFuture != null && activeInstances.size() != 0 && timeoutFuture.cancel(false)) {
      LOG.info("Stopped timeout monitor task");
    }
 else {
      LOG.info("Timeout monitor task not stopped. Timeout future state: {}, #instances: {}",timeoutFuture == null ? "null" : timeoutFuture.isDone(),activeInstances.size());
    }
    timeoutFuture=null;
  }
  finally {
    timeoutLock.unlock();
  }
}
