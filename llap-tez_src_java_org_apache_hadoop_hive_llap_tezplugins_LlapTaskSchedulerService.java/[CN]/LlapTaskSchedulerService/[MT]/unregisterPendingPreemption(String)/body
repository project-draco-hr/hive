{
  writeLock.lock();
  try {
    pendingPreemptions.decrementAndGet();
    if (metrics != null) {
      metrics.decrPendingPreemptionTasksCount();
    }
    MutableInt val=pendingPreemptionsPerHost.get(host);
    Preconditions.checkNotNull(val);
    val.decrement();
  }
  finally {
    writeLock.unlock();
  }
}
