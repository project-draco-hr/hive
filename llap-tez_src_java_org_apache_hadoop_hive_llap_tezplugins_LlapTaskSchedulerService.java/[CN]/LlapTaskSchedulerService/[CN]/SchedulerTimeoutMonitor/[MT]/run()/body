{
  LOG.info("Reporting SERVICE_UNAVAILABLE error as no instances are running");
  try {
    getContext().reportError(ServicePluginErrorDefaults.SERVICE_UNAVAILABLE,"No LLAP Daemons are running",getContext().getCurrentDagInfo());
  }
 catch (  Exception e) {
    LOG.error("Exception when reporting SERVICE_UNAVAILABLE error for dag: {}",getContext().getCurrentDagInfo().getName(),e);
  }
}
