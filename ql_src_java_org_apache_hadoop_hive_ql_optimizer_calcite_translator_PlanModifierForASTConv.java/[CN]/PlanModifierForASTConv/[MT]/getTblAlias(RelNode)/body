{
  if (null == rel) {
    return null;
  }
  if (rel instanceof HiveTableScan) {
    return ((RelOptHiveTable)((HiveTableScan)rel).getTable()).getTableAlias();
  }
  if (rel instanceof Project) {
    return null;
  }
  if (rel.getInputs().size() == 1) {
    return getTblAlias(rel.getInput(0));
  }
  return null;
}
