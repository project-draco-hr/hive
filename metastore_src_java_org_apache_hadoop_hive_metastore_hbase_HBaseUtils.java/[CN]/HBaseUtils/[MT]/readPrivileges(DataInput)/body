{
  if (in.readBoolean()) {
    PrincipalPrivilegeSet pps=new PrincipalPrivilegeSet();
    pps.setUserPrivileges(readPrivilege(in));
    pps.setGroupPrivileges(readPrivilege(in));
    pps.setRolePrivileges(readPrivilege(in));
    return pps;
  }
 else {
    return new PrincipalPrivilegeSet();
  }
}
