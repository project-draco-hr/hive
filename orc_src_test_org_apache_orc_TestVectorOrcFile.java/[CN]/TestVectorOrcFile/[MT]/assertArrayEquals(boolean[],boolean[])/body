{
  assertEquals(expected.length,actual.length);
  boolean diff=false;
  for (int i=0; i < expected.length; ++i) {
    if (expected[i] != actual[i]) {
      System.out.println("Difference at " + i + " expected: "+ expected[i]+ " actual: "+ actual[i]);
      diff=true;
    }
  }
  assertEquals(false,diff);
}
