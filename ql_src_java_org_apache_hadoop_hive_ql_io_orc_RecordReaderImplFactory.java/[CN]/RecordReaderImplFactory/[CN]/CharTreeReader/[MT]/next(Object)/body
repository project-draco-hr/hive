{
  HiveCharWritable result=null;
  if (previous == null) {
    result=new HiveCharWritable();
  }
 else {
    result=(HiveCharWritable)previous;
  }
  Object textVal=super.next(result.getTextValue());
  if (textVal == null) {
    return null;
  }
  result.enforceMaxLength(maxLength);
  return result;
}
