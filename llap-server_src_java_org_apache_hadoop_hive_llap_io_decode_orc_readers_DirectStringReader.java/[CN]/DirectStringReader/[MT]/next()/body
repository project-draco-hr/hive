{
  int len=(int)lengths.next();
  int offset=0;
  byte[] bytes=new byte[len];
  while (len > 0) {
    int written=data.read(bytes,offset,len);
    if (written < 0) {
      throw new EOFException("Can't finish byte read from " + data);
    }
    len-=written;
    offset+=written;
  }
  return new Text(bytes);
}
