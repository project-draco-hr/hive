{
switch (op) {
case LESS_THAN:
    return lt(doubleColumn(columnName),((Number)constant).doubleValue());
case IS_NULL:
case EQUALS:
case NULL_SAFE_EQUALS:
  return eq(doubleColumn(columnName),(constant == null) ? null : ((Number)constant).doubleValue());
case LESS_THAN_EQUALS:
return ltEq(FilterApi.doubleColumn(columnName),((Number)constant).doubleValue());
default :
throw new RuntimeException("Unknown PredicateLeaf Operator type: " + op);
}
}
