{
  ParseContext tempParseContext=getParseContext(pCtx,rootTasks);
  Deque<Operator<?>> deque=new LinkedList<Operator<?>>();
  deque.addAll(pCtx.getTopOps().values());
  GenTezProcContext procCtx=new GenTezProcContext(conf,tempParseContext,mvTask,rootTasks,inputs,outputs,deque);
  Map<Rule,NodeProcessor> opRules=new LinkedHashMap<Rule,NodeProcessor>();
  opRules.put(new RuleRegExp(new String("Split Work - ReduceSink"),ReduceSinkOperator.getOperatorName() + "%"),new GenTezWork());
  opRules.put(new RuleRegExp(new String("Split Work - FileSink"),FileSinkOperator.getOperatorName() + "%"),new GenTezWork());
  Dispatcher disp=new DefaultRuleDispatcher(null,opRules,procCtx);
  List<Node> topNodes=new ArrayList<Node>();
  topNodes.addAll(pCtx.getTopOps().values());
  GraphWalker ogw=new GenTezTaskWalker(disp);
  ogw.startWalking(topNodes,null);
}
