{
  Integer sparkJobId=jobHandle.getSparkJobIds().size() == 1 ? jobHandle.getSparkJobIds().get(0) : null;
  if (sparkJobId == null) {
    return null;
  }
  JobHandle<HiveSparkJobInfo> getJobInfo=sparkClient.submit(new GetJobInfoJob(jobHandle.getClientJobId(),sparkJobId));
  try {
    return getJobInfo.get();
  }
 catch (  Throwable t) {
    LOG.warn("Error getting job info",t);
    return null;
  }
}
