{
switch (fieldId) {
case 1:
    return STATUS;
case 2:
  return OPERATION_STATE;
case 3:
return SQL_STATE;
case 4:
return ERROR_CODE;
case 5:
return ERROR_MESSAGE;
case 6:
return TASK_STATUS;
case 7:
return OPERATION_STARTED;
case 8:
return OPERATION_COMPLETED;
case 9:
return HAS_RESULT_SET;
default :
return null;
}
}
