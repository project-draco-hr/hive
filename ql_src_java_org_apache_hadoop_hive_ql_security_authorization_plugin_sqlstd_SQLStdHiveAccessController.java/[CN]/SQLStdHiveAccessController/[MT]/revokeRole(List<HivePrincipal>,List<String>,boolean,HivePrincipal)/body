{
  if (grantOption) {
    throw new HiveAuthorizationPluginException("Revoking only the admin privileges on " + "role is not currently supported");
  }
  for (  HivePrincipal hivePrincipal : hivePrincipals) {
    for (    String roleName : roleNames) {
      try {
        metastoreClientFactory.getHiveMetastoreClient().revoke_role(roleName,hivePrincipal.getName(),AuthorizationUtils.getThriftPrincipalType(hivePrincipal.getType()));
      }
 catch (      Exception e) {
        String msg="Error revoking roles for " + hivePrincipal.getName() + " to role "+ roleName+ hivePrincipal.getName();
        throw new HiveAuthorizationPluginException(msg,e);
      }
    }
  }
}
