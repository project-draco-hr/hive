{
  int oldSize=hashAggregations.size();
  Iterator iter=hashAggregations.entrySet().iterator();
  int numDel=0;
  while (iter.hasNext()) {
    Map.Entry<ArrayList<Object>,UDAF[]> m=(Map.Entry)iter.next();
    forward(m.getKey(),m.getValue());
    iter.remove();
    numDel++;
    if (numDel * 10 >= oldSize)     return;
  }
}
