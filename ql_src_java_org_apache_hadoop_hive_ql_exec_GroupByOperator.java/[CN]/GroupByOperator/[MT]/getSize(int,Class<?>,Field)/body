{
  if (c.isPrimitive() || c.isInstance(Boolean.valueOf(true)) || c.isInstance(Byte.valueOf((byte)0))|| c.isInstance(Short.valueOf((short)0))|| c.isInstance(Integer.valueOf(0))|| c.isInstance(Long.valueOf(0))|| c.isInstance(new Float(0))|| c.isInstance(new Double(0))) {
    return javaSizePrimitiveType;
  }
  if (c.isInstance(new String())) {
    int idx=0;
    varLenFields v=null;
    for (idx=0; idx < aggrPositions.size(); idx++) {
      v=aggrPositions.get(idx);
      if (v.getAggrPos() == pos) {
        break;
      }
    }
    if (idx == aggrPositions.size()) {
      v=new varLenFields(pos,new ArrayList<Field>());
      aggrPositions.add(v);
    }
    v.getFields().add(f);
    return javaObjectOverHead;
  }
  return javaSizeUnknownType;
}
