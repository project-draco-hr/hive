{
  if (hashAggr) {
    newKeys.setHashKey();
    processHashAggr(row,rowInspector,newKeys);
  }
 else {
    processAggr(row,rowInspector,newKeys);
  }
  if (countAfterReport != 0 && (countAfterReport % heartbeatInterval) == 0 && (reporter != null)) {
    reporter.progress();
    countAfterReport=0;
  }
}
