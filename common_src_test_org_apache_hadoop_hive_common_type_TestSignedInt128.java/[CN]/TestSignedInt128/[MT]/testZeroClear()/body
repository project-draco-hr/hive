{
  assertFalse(one.isZero());
  assertFalse(two.isZero());
  assertTrue(0L != one.longValue());
  assertTrue(0L != two.longValue());
  two.zeroClear();
  assertTrue(0L != one.longValue());
  assertEquals(0L,two.longValue());
  assertFalse(one.isZero());
  assertTrue(two.isZero());
  one.zeroClear();
  assertEquals(0L,one.longValue());
  assertEquals(0L,two.longValue());
  assertTrue(one.isZero());
  assertTrue(two.isZero());
}
