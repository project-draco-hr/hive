{
  String byteData=null;
  try {
    byteData=Text.decode(bytes.getData(),start,length);
    data.set(new BigDecimal(byteData));
    isNull=false;
  }
 catch (  NumberFormatException e) {
    isNull=true;
    LOG.debug("Data not in the BigDecimal data type range so converted to null. Given data is :" + byteData,e);
  }
catch (  CharacterCodingException e) {
    isNull=true;
    LOG.debug("Data not in the BigDecimal data type range so converted to null.",e);
  }
}
