{
  windowExpressions=windowExpressions == null ? new ArrayList<WindowExpressionSpec>() : windowExpressions;
  aliasToWdwExpr=aliasToWdwExpr == null ? new HashMap<String,WindowExpressionSpec>() : aliasToWdwExpr;
  WindowExpressionSpec wExprSpec=new WindowExpressionSpec();
  wExprSpec.setAlias(alias);
  wExprSpec.setExpression(expr);
  windowExpressions.add(wExprSpec);
  aliasToWdwExpr.put(alias,wExprSpec);
}
