{
  if (!(writable instanceof AccumuloHiveRow)) {
    throw new SerDeException(getClass().getName() + " : " + "Expected AccumuloHiveRow. Got "+ writable.getClass().getName());
  }
  cachedRow.init((AccumuloHiveRow)writable,accumuloSerDeParameters.getColumnMappings(),accumuloSerDeParameters.getRowIdFactory());
  return cachedRow;
}
