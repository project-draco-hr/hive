{
  Operator<? extends OperatorDesc> currentOp=op;
  while (currentOp.getParentOperators().size() > 0) {
    currentOp=currentOp.getParentOperators().get(0);
    if (currentOp.getType().equals(OperatorType.GROUPBY)) {
      if (!opsDone.contains(op)) {
        opsDone.add(op);
      }
      return true;
    }
  }
  return false;
}
