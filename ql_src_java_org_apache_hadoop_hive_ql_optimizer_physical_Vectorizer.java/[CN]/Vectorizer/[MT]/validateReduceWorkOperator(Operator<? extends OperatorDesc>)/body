{
  boolean ret=false;
switch (op.getType()) {
case EXTRACT:
    ret=validateExtractOperator((ExtractOperator)op);
  break;
case FILTER:
ret=validateFilterOperator((FilterOperator)op);
break;
case SELECT:
ret=validateSelectOperator((SelectOperator)op);
break;
case REDUCESINK:
ret=validateReduceSinkOperator((ReduceSinkOperator)op);
break;
case FILESINK:
case LIMIT:
ret=true;
break;
default :
ret=false;
break;
}
return ret;
}
