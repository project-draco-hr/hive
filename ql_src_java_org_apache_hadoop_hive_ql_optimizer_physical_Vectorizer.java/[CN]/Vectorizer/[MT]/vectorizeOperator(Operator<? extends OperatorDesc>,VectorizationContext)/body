{
  Operator<? extends OperatorDesc> vectorOp=null;
switch (op.getType()) {
case MAPJOIN:
    HiveConf.setBoolVar(physicalContext.getConf(),HiveConf.ConfVars.HIVEUSEHYBRIDGRACEHASHJOIN,false);
case GROUPBY:
case FILTER:
case SELECT:
case FILESINK:
case REDUCESINK:
case LIMIT:
case EXTRACT:
case EVENT:
  vectorOp=OperatorFactory.getVectorOperator(op.getConf(),vContext);
break;
default :
vectorOp=op;
break;
}
if (vectorOp != op) {
fixupParentChildOperators(op,vectorOp);
((AbstractOperatorDesc)vectorOp.getConf()).setVectorMode(true);
}
return vectorOp;
}
