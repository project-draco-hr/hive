{
  String colNames="";
  String val="";
  int numCols=HiveProfilerStats.COLUMN_NAMES.length;
  for (int i=0; i < numCols; i++) {
    colNames+=HiveProfilerStats.COLUMN_NAMES[i];
    val+="?";
    if (i < numCols - 1) {
      colNames+=",";
      val+=",";
    }
  }
  return "INSERT INTO " + tableName + " ("+ colNames+ ") VALUES ("+ val+ ")";
}
