{
  HiveToken ht=null;
switch (optiqType.getSqlTypeName()) {
case CHAR:
{
      ht=new HiveToken(HiveParser.TOK_CHAR,"TOK_CHAR",String.valueOf(optiqType.getPrecision()));
    }
  break;
case VARCHAR:
{
  if (optiqType.getPrecision() == Integer.MAX_VALUE)   ht=new HiveToken(HiveParser.TOK_STRING,"TOK_STRING",String.valueOf(optiqType.getPrecision()));
 else   ht=new HiveToken(HiveParser.TOK_VARCHAR,"TOK_VARCHAR",String.valueOf(optiqType.getPrecision()));
}
break;
case DECIMAL:
{
ht=new HiveToken(HiveParser.TOK_DECIMAL,"TOK_DECIMAL",String.valueOf(optiqType.getPrecision()),String.valueOf(optiqType.getScale()));
}
break;
default :
ht=optiqToHiveTypeNameMap.get(optiqType.getSqlTypeName().getName());
}
return ht;
}
