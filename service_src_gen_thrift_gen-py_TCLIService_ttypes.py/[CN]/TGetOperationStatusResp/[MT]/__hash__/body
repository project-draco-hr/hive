def __hash__(self):
    value = 17
    value = ((value * 31) ^ hash(self.status))
    value = ((value * 31) ^ hash(self.operationState))
    value = ((value * 31) ^ hash(self.sqlState))
    value = ((value * 31) ^ hash(self.errorCode))
    value = ((value * 31) ^ hash(self.errorMessage))
    return value
