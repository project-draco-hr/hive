{
  if (getPasswordMethod == null) {
    return conf.get(name);
  }
 else {
    try {
      char[] pw=(char[])getPasswordMethod.invoke(conf,name);
      if (pw == null) {
        return null;
      }
      return new String(pw);
    }
 catch (    Exception err) {
      throw new IOException(err.getMessage(),err);
    }
  }
}
