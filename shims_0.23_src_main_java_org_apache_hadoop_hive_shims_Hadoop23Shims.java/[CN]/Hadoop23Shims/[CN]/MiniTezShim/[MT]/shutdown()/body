{
  mr.stop();
  if (miniLlapKlass != null && miniLlapCluster != null) {
    try {
      Method stop=miniLlapKlass.getMethod("stop",new Class[]{});
      stop.invoke(miniLlapCluster);
    }
 catch (    Exception e) {
      LOG.error("Unable to stop llap daemon. Exception: " + e.getMessage());
    }
  }
}
