{
  super.start(env);
  try {
    Configuration conf=env.getConfiguration();
    String className=conf.get(REVISION_MGR_ENDPOINT_IMPL_CLASS,ZKBasedRevisionManager.class.getName());
    rmImpl=RevisionManagerFactory.getOpenedRevisionManager(className,conf);
  }
 catch (  IOException e) {
    LOGGER.error("Failed to initialize revision manager",e);
  }
}
