{
  FileSystem fs=opts.fileSystemValue == null ? path.getFileSystem(opts.configuration) : opts.fileSystemValue;
  return new WriterImpl(fs,path,opts.configuration,opts.inspectorValue,opts.stripeSizeValue,opts.compressValue,opts.bufferSizeValue,opts.rowIndexStrideValue,opts.memoryManagerValue,opts.blockPaddingValue,opts.versionValue,opts.callback,opts.encodingStrategy,opts.compressionStrategy,opts.paddingTolerance,opts.blockSizeValue);
}
