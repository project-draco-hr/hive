{
  configuration=conf;
  memoryManagerValue=getMemoryManager(conf);
  stripeSizeValue=conf.getLong(HiveConf.ConfVars.HIVE_ORC_DEFAULT_STRIPE_SIZE.varname,DEFAULT_STRIPE_SIZE);
  rowIndexStrideValue=conf.getInt(HiveConf.ConfVars.HIVE_ORC_DEFAULT_ROW_INDEX_STRIDE.varname,DEFAULT_ROW_INDEX_STRIDE);
  bufferSizeValue=conf.getInt(HiveConf.ConfVars.HIVE_ORC_DEFAULT_BUFFER_SIZE.varname,DEFAULT_ROW_INDEX_STRIDE);
  blockPaddingValue=conf.getBoolean(HiveConf.ConfVars.HIVE_ORC_DEFAULT_BLOCK_PADDING.varname,DEFAULT_BLOCK_PADDING);
  compressValue=CompressionKind.valueOf(conf.get(HiveConf.ConfVars.HIVE_ORC_DEFAULT_COMPRESS.varname,DEFAULT_COMPRESSION_KIND.toString()));
  String versionName=conf.get(HiveConf.ConfVars.HIVE_ORC_WRITE_FORMAT.varname);
  if (versionName == null) {
    versionValue=Version.CURRENT;
  }
 else {
    versionValue=Version.byName(versionName);
  }
}
