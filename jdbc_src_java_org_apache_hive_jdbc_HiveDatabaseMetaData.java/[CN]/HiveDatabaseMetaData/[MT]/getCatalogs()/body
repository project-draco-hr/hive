{
  TGetCatalogsResp catalogResp;
  try {
    catalogResp=client.GetCatalogs(new TGetCatalogsReq(sessHandle));
  }
 catch (  TException e) {
    throw new SQLException(e.getMessage(),"08S01",e);
  }
  Utils.verifySuccess(catalogResp.getStatus());
  return new HiveQueryResultSet.Builder().setClient(client).setSessionHandle(sessHandle).setStmtHandle(catalogResp.getOperationHandle()).build();
}
