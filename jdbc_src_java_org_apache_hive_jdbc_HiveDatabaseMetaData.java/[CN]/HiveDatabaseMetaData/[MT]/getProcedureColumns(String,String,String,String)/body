{
  return new HiveQueryResultSet.Builder(connection).setClient(client).setEmptyResultSet(true).setSchema(Arrays.asList("PROCEDURE_CAT","PROCEDURE_SCHEM","PROCEDURE_NAME","COLUMN_NAME","COLUMN_TYPE","DATA_TYPE","TYPE_NAME","PRECISION","LENGTH","SCALE","RADIX","NULLABLE","REMARKS","COLUMN_DEF","SQL_DATA_TYPE","SQL_DATETIME_SUB","CHAR_OCTET_LENGTH","ORDINAL_POSITION","IS_NULLABLE","SPECIFIC_NAME"),Arrays.asList("STRING","STRING","STRING","STRING","SMALLINT","INT","STRING","INT","INT","SMALLINT","SMALLINT","SMALLINT","STRING","STRING","INT","INT","INT","INT","STRING","STRING")).build();
