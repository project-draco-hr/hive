{
  return new HiveQueryResultSet.Builder(connection).setClient(client).setEmptyResultSet(true).setSchema(Arrays.asList("PKTABLE_CAT","PKTABLE_SCHEM","PKTABLE_NAME","PKCOLUMN_NAME","FKTABLE_CAT","FKTABLE_SCHEM","FKTABLE_NAME","FKCOLUMN_NAME","KEY_SEQ","UPDATE_RULE","DELETE_RULE","FK_NAME","PK_NAME","DEFERRABILITY"),Arrays.asList("STRING","STRING","STRING","STRING","STRING","STRING","STRING","STRING","SMALLINT","SMALLINT","SMALLINT","STRING","STRING","STRING")).build();
}
