{
  TGetTableTypesResp tableTypeResp;
  try {
    tableTypeResp=client.GetTableTypes(new TGetTableTypesReq(sessHandle));
  }
 catch (  TException e) {
    throw new SQLException(e.getMessage(),"08S01",e);
  }
  Utils.verifySuccess(tableTypeResp.getStatus());
  return new HiveQueryResultSet.Builder(null).setClient(client).setSessionHandle(sessHandle).setStmtHandle(tableTypeResp.getOperationHandle()).build();
}
