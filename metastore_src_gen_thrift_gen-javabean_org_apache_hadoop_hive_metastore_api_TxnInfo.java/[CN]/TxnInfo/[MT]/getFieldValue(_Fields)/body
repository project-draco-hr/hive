{
switch (field) {
case ID:
    return getId();
case STATE:
  return getState();
case USER:
return getUser();
case HOSTNAME:
return getHostname();
case AGENT_INFO:
return getAgentInfo();
case HEARTBEAT_COUNT:
return getHeartbeatCount();
case META_INFO:
return getMetaInfo();
}
throw new IllegalStateException();
}
