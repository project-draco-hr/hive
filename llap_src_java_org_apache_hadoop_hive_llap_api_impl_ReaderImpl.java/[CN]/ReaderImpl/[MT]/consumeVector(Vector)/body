{
  if (DebugUtils.isTraceEnabled()) {
    Llap.LOG.info("consumeVector called; current=" + current + ", closed "+ isClosed+ ", done "+ isDone+ ", err "+ pendingError+ ", pending "+ pendingData.size());
  }
synchronized (pendingData) {
    if (isClosed) {
      feedback.returnCompleteVector(vector);
      return;
    }
    pendingData.add(vector);
    pendingData.notifyAll();
  }
}
