{
  String vectorDesc="0x" + Integer.toHexString(System.identityHashCode(vector)) + " with buffers [";
  VectorImpl currentImpl=(VectorImpl)vector;
  boolean isFirst=true;
  for (  WeakBuffer wb : currentImpl.getCacheBuffers()) {
    if (isFirst) {
      vectorDesc+=", ";
      isFirst=false;
    }
    vectorDesc+=wb.toString();
  }
  return vectorDesc + "]";
}
