{
  LinkedList<Vector> unused=new LinkedList<Vector>();
  if (current != null) {
    unused.add(current);
    current=null;
  }
synchronized (pendingData) {
    isClosed=true;
    unused.addAll(pendingData);
    pendingData.clear();
  }
  for (  Vector v : unused) {
    feedback.returnCompleteVector(v);
  }
}
