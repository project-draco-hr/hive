{
  AverageSize averageSize=getAverageSize(inpFs,dirPath);
  if (averageSize.getTotalSize() <= 0) {
    return -1;
  }
  if (averageSize.getNumFiles() <= 1) {
    return -1;
  }
  if (averageSize.getTotalSize() / averageSize.getNumFiles() < avgSize) {
    return averageSize.getTotalSize();
  }
  return -1;
}
