{
  ExprProcCtx pprCtx=new ExprProcCtx(tabAlias);
  Map<Node,Object> outputMap=PrunerUtils.walkExprTree(pred,pprCtx,getColumnProcessor(),getFieldProcessor(),getGenericFuncProcessor(),getDefaultExprProcessor());
  hasNonPartCols=pprCtx.getHasNonPartCols();
  return (ExprNodeDesc)outputMap.get(pred);
}
