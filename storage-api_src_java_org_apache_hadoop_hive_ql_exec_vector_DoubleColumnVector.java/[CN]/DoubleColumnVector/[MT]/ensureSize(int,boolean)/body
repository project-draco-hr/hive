{
  if (size > vector.length) {
    super.ensureSize(size,preserveData);
    double[] oldArray=vector;
    vector=new double[size];
    if (preserveData) {
      if (isRepeating) {
        vector[0]=oldArray[0];
      }
 else {
        System.arraycopy(oldArray,0,vector,0,oldArray.length);
      }
    }
  }
}
