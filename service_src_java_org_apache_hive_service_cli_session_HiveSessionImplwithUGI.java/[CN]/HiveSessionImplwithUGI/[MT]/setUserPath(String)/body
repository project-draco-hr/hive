{
  for (  HiveConf.ConfVars var : HiveConf.userVars) {
    String userVar=getHiveConf().getVar(var);
    if (userVar != null) {
      if (userVar.endsWith(File.separator)) {
        userVar=userVar.substring(0,userVar.length() - 2);
      }
      getHiveConf().setVar(var,userVar + "-" + userName);
    }
  }
}
