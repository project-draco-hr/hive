{
  Set<Operator<?>> returnSet=new LinkedHashSet<Operator<?>>();
  Stack<Operator<?>> opStack=new Stack<Operator<?>>();
  opStack.addAll(opSet);
  while (!opStack.empty()) {
    Operator<?> op=opStack.pop();
    returnSet.add(op);
    if (op.getChildOperators() != null) {
      opStack.addAll(op.getChildOperators());
    }
  }
  return returnSet;
}
