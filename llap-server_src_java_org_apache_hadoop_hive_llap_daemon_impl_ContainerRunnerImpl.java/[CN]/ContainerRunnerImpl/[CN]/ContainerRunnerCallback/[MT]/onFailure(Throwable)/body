{
  LOG.error("TezChild execution failed for : " + request.getApplicationIdString() + ", containerId="+ request.getContainerIdString());
  TezChild tezChild=containerRunnerCallable.getTezChild();
  if (tezChild != null) {
    tezChild.shutdown();
  }
  HistoryLogger.logFragmentEnd(request.getApplicationIdString(),request.getContainerIdString(),localAddress.get().getHostName(),null,null,-1,-1,containerRunnerCallable.startTime,false);
  metrics.decrExecutorNumQueuedRequests();
}
