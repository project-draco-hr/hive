{
  while (true) {
    try {
      doOneCleanupRound();
    }
 catch (    InterruptedException ex) {
      Llap.LOG.warn("Cleanup thread has been interrupted");
      Thread.currentThread().interrupt();
      break;
    }
catch (    Throwable t) {
      Llap.LOG.error("Cleanup has failed; the thread will now exit",t);
      break;
    }
  }
}
