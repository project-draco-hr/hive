{
  PrimitiveCategory pc=((PrimitiveObjectInspector)oi).getPrimitiveCategory();
switch (pc) {
case FLOAT:
case DOUBLE:
case DECIMAL:
    break;
default :
  throw new UDFArgumentTypeException(1,"Only a floating point or decimal, or " + "floating point or decimal array argument is accepted as parameter 2, but " + pc + " was passed instead.");
}
}
