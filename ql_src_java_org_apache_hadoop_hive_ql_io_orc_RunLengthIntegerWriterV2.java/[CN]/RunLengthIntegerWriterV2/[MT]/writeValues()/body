{
  if (numLiterals != 0) {
    if (encoding.equals(EncodingType.SHORT_REPEAT)) {
      writeShortRepeatValues();
    }
 else     if (encoding.equals(EncodingType.DIRECT)) {
      writeDirectValues();
    }
 else     if (encoding.equals(EncodingType.PATCHED_BASE)) {
      writePatchedBaseValues();
    }
 else {
      writeDeltaValues();
    }
    clear();
  }
}
