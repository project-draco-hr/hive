{
  BufferedReader br=null;
  try {
    InputStreamReader isr=new InputStreamReader(is);
    br=new BufferedReader(isr);
    String line=null;
    if (type != null) {
      while ((line=br.readLine()) != null) {
        os.println(type + ">" + line);
      }
    }
 else {
      while ((line=br.readLine()) != null) {
        os.println(line);
      }
    }
    br.close();
    br=null;
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
 finally {
    IOUtils.closeStream(br);
  }
}
