{
  assert(!inner);
  TList list=new TList();
  if (columns[index] == null || columns[index].equals(nullString)) {
    index++;
    if (returnNulls) {
      return null;
    }
    list.size=0;
  }
 else   if (columns[index].isEmpty()) {
    list.size=0;
    index++;
  }
 else {
    fields=secondaryPattern.split(columns[index++]);
    list.size=fields.length;
  }
  innerIndex=0;
  inner=true;
  return list;
}
