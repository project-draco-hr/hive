{
  primarySeparator=getByteValue(tbl.getProperty(serdeConstants.FIELD_DELIM),primarySeparator);
  secondarySeparator=getByteValue(tbl.getProperty(serdeConstants.COLLECTION_DELIM),secondarySeparator);
  rowSeparator=getByteValue(tbl.getProperty(serdeConstants.LINE_DELIM),rowSeparator);
  mapSeparator=getByteValue(tbl.getProperty(serdeConstants.MAPKEY_DELIM),mapSeparator);
  returnNulls=Boolean.parseBoolean(tbl.getProperty(ReturnNullsKey,String.valueOf(returnNulls)));
  bufferSize=Integer.parseInt(tbl.getProperty(BufferSizeKey,String.valueOf(bufferSize)));
  nullString=tbl.getProperty(serdeConstants.SERIALIZATION_NULL_FORMAT,"\\N");
  quote=tbl.getProperty(serdeConstants.QUOTE_CHAR,null);
  internalInitialize();
}
