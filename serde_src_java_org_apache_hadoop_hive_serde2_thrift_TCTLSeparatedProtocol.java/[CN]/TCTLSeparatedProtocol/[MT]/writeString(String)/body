{
  if (inner) {
    if (!firstInnerField) {
      if (isMap && elemIndex++ % 2 == 0) {
        writeByte(mapSeparatorByte);
      }
 else {
        writeByte(secondarySeparatorByte);
      }
    }
 else {
      firstInnerField=false;
    }
  }
  final byte buf[]=str.getBytes();
  trans_.write(buf,0,buf.length);
}
