{
  String dbName="grdbp_db";
  try {
    Database db=new Database(dbName,"no description","file:///tmp",emptyParameters);
    store.createDatabase(db);
    doGrantRevoke(HiveObjectType.DATABASE,dbName,null,new String[]{"grdbp_role1","grdbp_role2"},new String[]{"fred","barney","wilma","betty"});
  }
  finally {
    store.dropDatabase(dbName);
  }
}
