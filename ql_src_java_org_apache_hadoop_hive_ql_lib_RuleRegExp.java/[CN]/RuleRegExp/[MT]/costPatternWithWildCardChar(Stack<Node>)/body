{
  int numElems=(stack != null ? stack.size() : 0);
  String name="";
  Matcher m=patternWithWildCardChar.matcher("");
  for (int pos=numElems - 1; pos >= 0; pos--) {
    name=stack.get(pos).getName() + "%" + name;
    m.reset(name);
    if (m.matches()) {
      return name.length();
    }
  }
  return -1;
}
