{
switch (columnType) {
case Types.BOOLEAN:
case Types.CHAR:
case Types.VARCHAR:
case Types.TINYINT:
case Types.SMALLINT:
case Types.INTEGER:
case Types.BIGINT:
case Types.DATE:
    return 0;
case Types.FLOAT:
  return 7;
case Types.DOUBLE:
return 15;
case Types.TIMESTAMP:
return 9;
case Types.DECIMAL:
return columnAttributes.scale;
default :
throw new SQLException("Invalid column type: " + columnType);
}
}
