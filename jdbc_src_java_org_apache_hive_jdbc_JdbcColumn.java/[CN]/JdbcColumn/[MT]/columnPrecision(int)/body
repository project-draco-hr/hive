{
switch (columnType) {
case Types.BOOLEAN:
    return 1;
case Types.VARCHAR:
  return Integer.MAX_VALUE;
case Types.TINYINT:
return 3;
case Types.SMALLINT:
return 5;
case Types.INTEGER:
return 10;
case Types.BIGINT:
return 19;
case Types.FLOAT:
return 7;
case Types.DOUBLE:
return 15;
case Types.TIMESTAMP:
return 29;
case Types.DECIMAL:
return Integer.MAX_VALUE;
default :
throw new SQLException("Invalid column type: " + columnType);
}
}
