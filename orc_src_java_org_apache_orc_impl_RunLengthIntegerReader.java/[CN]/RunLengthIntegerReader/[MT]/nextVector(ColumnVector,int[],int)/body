{
  if (vector.noNulls) {
    for (int r=0; r < data.length && r < size; ++r) {
      data[r]=(int)next();
    }
  }
 else   if (!(vector.isRepeating && vector.isNull[0])) {
    for (int r=0; r < data.length && r < size; ++r) {
      if (!vector.isNull[r]) {
        data[r]=(int)next();
      }
 else {
        data[r]=1;
      }
    }
  }
}
