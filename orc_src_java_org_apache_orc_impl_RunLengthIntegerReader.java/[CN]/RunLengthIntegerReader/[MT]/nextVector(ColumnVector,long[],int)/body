{
  previous.isRepeating=true;
  for (int i=0; i < previousLen; i++) {
    if (!previous.isNull[i]) {
      data[i]=next();
    }
 else {
      data[i]=1;
    }
    if (previous.isRepeating && i > 0 && (data[0] != data[i] || previous.isNull[0] != previous.isNull[i])) {
      previous.isRepeating=false;
    }
  }
}
