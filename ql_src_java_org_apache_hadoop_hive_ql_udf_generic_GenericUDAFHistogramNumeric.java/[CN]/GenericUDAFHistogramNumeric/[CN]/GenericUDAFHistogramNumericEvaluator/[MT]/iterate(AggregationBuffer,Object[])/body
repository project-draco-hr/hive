{
  assert(parameters.length == 2);
  if (parameters[0] == null || parameters[1] == null) {
    return;
  }
  StdAgg myagg=(StdAgg)agg;
  if (myagg.nbins == 0) {
    try {
      myagg.nbins=PrimitiveObjectInspectorUtils.getInt(parameters[1],nbinsOI);
    }
 catch (    NumberFormatException e) {
      throw new HiveException(getClass().getSimpleName() + " " + StringUtils.stringifyException(e));
    }
    if (myagg.nbins < 2) {
      throw new HiveException(getClass().getSimpleName() + " needs nbins to be at least 2," + " but you supplied "+ myagg.nbins+ ".");
    }
    myagg.hist=new StdAgg.Coord[myagg.nbins + 1];
    for (int i=0; i < myagg.nbins + 1; i++) {
      myagg.hist[i]=new StdAgg.Coord();
    }
    myagg.nusedbins=0;
  }
  Object p=parameters[0];
  if (p != null) {
    double v=PrimitiveObjectInspectorUtils.getDouble(p,inputOI);
    insert(myagg,v);
  }
}
