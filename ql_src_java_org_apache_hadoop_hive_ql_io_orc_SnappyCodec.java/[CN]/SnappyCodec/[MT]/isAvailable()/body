{
  if (direct == null) {
    try {
      if (ShimLoader.getHadoopShims().getDirectDecompressor(DirectCompressionType.SNAPPY) != null) {
        direct=Boolean.valueOf(true);
      }
 else {
        direct=Boolean.valueOf(false);
      }
    }
 catch (    UnsatisfiedLinkError ule) {
      direct=Boolean.valueOf(false);
    }
  }
  return direct.booleanValue();
}
