{
  PrimitiveObjectInspector poi=PrimitiveObjectInspectorFactory.getPrimitiveJavaObjectInspector(PrimitiveCategory.VARCHAR);
  assertEquals(poi,PrimitiveObjectInspectorFactory.javaHiveVarcharObjectInspector);
  poi=PrimitiveObjectInspectorFactory.getPrimitiveJavaObjectInspector(PrimitiveCategory.CHAR);
  assertEquals(poi,PrimitiveObjectInspectorFactory.javaHiveCharObjectInspector);
}
