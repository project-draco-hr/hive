{
  for (int i=0; i < fields.length && (vectorColumnCount == -1 || i < vectorColumnCount); ++i) {
    ColumnVector colVector=batch.cols[i];
    if (colVector != null) {
      colVector.reset();
      colVector.ensureSize((int)batchSize,false);
      fields[i].nextVector(colVector,null,batchSize);
    }
  }
}
