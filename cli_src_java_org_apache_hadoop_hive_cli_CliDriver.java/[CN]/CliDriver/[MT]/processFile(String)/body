{
  FileReader fileReader=null;
  BufferedReader bufferReader=null;
  int rc=0;
  try {
    fileReader=new FileReader(fileName);
    bufferReader=new BufferedReader(fileReader);
    rc=processReader(bufferReader);
    bufferReader.close();
    bufferReader=null;
  }
  finally {
    IOUtils.closeStream(bufferReader);
  }
  return rc;
}
