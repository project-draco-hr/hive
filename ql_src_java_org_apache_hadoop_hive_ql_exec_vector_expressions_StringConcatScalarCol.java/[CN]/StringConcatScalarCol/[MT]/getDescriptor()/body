{
  return (new VectorExpressionDescriptor.Builder()).setMode(VectorExpressionDescriptor.Mode.PROJECTION).setNumArguments(2).setArgumentTypes(VectorExpressionDescriptor.ArgumentType.STRING,VectorExpressionDescriptor.ArgumentType.STRING).setInputExpressionTypes(VectorExpressionDescriptor.InputExpressionType.SCALAR,VectorExpressionDescriptor.InputExpressionType.COLUMN).build();
}
