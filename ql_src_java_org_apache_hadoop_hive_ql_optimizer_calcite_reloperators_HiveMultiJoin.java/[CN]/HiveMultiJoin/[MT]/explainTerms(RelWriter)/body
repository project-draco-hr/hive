{
  List<String> joinsString=new ArrayList<String>();
  for (int i=0; i < joinInputs.size(); i++) {
    final StringBuilder sb=new StringBuilder();
    sb.append(joinInputs.get(i).left).append(" - ").append(joinInputs.get(i).right).append(" : ").append(joinTypes.get(i).name());
    joinsString.add(sb.toString());
  }
  super.explainTerms(pw);
  for (  Ord<RelNode> ord : Ord.zip(inputs)) {
    pw.input("input#" + ord.i,ord.e);
  }
  return pw.item("condition",condition).item("joinsDescription",joinsString);
}
