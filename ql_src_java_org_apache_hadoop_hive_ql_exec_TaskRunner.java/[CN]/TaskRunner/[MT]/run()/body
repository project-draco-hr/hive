{
  runner=Thread.currentThread();
  try {
    OperationLog.setCurrentOperationLog(operationLog);
    SessionState.start(ss);
    runSequential();
  }
  finally {
    try {
      Hive.closeCurrent();
    }
 catch (    Exception e) {
      LOG.warn("Exception closing Metastore connection:" + e.getMessage());
    }
    runner=null;
    result.setRunning(false);
  }
}
