{
  int exitVal=-101;
  try {
    exitVal=tsk.executeTask();
  }
 catch (  Throwable t) {
    if (tsk.getException() == null) {
      tsk.setException(t);
    }
    LOG.error("Error in executeTask",t);
  }
  result.setExitVal(exitVal);
  if (tsk.getException() != null) {
    result.setTaskError(tsk.getException());
  }
}
