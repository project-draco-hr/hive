{
  ArrayList<Operator<?>> opList=new ArrayList<Operator<?>>();
  Map<String,ArrayList<String>> pa=getPathToAliases();
  if (pa != null) {
    for (    List<String> ls : pa.values()) {
      for (      String a : ls) {
        Operator<?> op=getAliasToWork().get(a);
        if (op != null) {
          opList.add(op);
        }
      }
    }
  }
  return opList;
}
