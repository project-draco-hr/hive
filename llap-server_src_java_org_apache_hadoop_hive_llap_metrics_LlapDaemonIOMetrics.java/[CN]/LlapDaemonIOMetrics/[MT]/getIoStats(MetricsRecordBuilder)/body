{
  rb.addGauge(EncodedColumnBatchPoolSize,encodedColumnBatchPoolSize.value()).addGauge(ColumnStreamDataPoolSize,columnStreamDataPoolSize.value()).addGauge(ColumnVectorBatchPoolSize,columnVectorBatchPool.value()).addGauge(IoThreadPoolSize,ioThreadPoolSize.value()).addGauge(MaxDecodingTime,maxDecodingTime.value());
  rateOfDecoding.snapshot(rb,true);
  for (  MutableQuantiles q : decodingTimes) {
    q.snapshot(rb,true);
  }
}
