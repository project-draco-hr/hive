{
  try {
    Deadline.registerIfNot(socketTimeout);
    boolean isTimerStarted=Deadline.startTimer(method.getName());
    try {
      return method.invoke(base,args);
    }
  finally {
      if (isTimerStarted) {
        Deadline.stopTimer();
      }
    }
  }
 catch (  UndeclaredThrowableException e) {
    throw e.getCause();
  }
catch (  InvocationTargetException e) {
    throw e.getCause();
  }
}
