{
  char[] sqlArray=sbLine.toCharArray();
  List<String> initSqlList=new ArrayList();
  int index=0;
  int beginIndex=0;
  for (; index < sqlArray.length; index++) {
    if (sqlArray[index] == ';') {
      String sql=sbLine.substring(beginIndex,index).trim();
      initSqlList.add(sql);
      beginIndex=index + 1;
    }
  }
  return initSqlList;
}
