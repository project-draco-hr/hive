{
  transport=isHttpTransportMode() ? createHttpTransport() : createBinaryTransport();
  try {
    if (!transport.isOpen()) {
      transport.open();
    }
  }
 catch (  TTransportException e) {
    throw new SQLException("Could not open connection to " + jdbcURI + ": "+ e.getMessage()," 08S01",e);
  }
}
