{
  if ((fieldIndex % 8) == 0) {
    if (fieldIndex > 0) {
      output.writeByte(nullOffset,nullByte);
      nullByte=0;
      nullOffset=output.getLength();
    }
    output.reserve(1);
  }
  nullByte|=1 << (fieldIndex % 8);
  LazyBinaryUtils.writeVInt(output,dateAsDays);
  fieldIndex++;
  if (fieldIndex == fieldCount) {
    output.writeByte(nullOffset,nullByte);
  }
}
