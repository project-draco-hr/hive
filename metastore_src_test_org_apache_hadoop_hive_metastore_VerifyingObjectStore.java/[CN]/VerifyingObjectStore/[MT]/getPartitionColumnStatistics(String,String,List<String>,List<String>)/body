{
  List<ColumnStatistics> sqlResult=getPartitionColumnStatisticsInternal(dbName,tableName,partNames,colNames,true,false);
  List<ColumnStatistics> jdoResult=getPartitionColumnStatisticsInternal(dbName,tableName,partNames,colNames,false,true);
  if (sqlResult.size() != jdoResult.size()) {
    String msg="Lists are not the same size: SQL " + sqlResult.size() + ", ORM "+ jdoResult.size();
    LOG.error(msg);
    throw new MetaException(msg);
  }
  for (int i=0; i < jdoResult.size(); i++) {
    Collections.sort(sqlResult.get(i).getStatsObj(),new ColumnStatsComparator());
    Collections.sort(jdoResult.get(i).getStatsObj(),new ColumnStatsComparator());
    verifyObjects(sqlResult.get(i),jdoResult.get(i),ColumnStatistics.class);
  }
  return sqlResult;
}
