{
  VectorizedRowBatch batch=new VectorizedRowBatch(2);
  final int size=VectorizedRowBatch.DEFAULT_SIZE;
  LongColumnVector lcv=new LongColumnVector();
  for (int i=0; i < size; i++) {
    lcv.vector[i]=(i + 1) * 37;
  }
  batch.cols[0]=lcv;
  batch.cols[1]=new LongColumnVector();
  batch.size=size;
  return batch;
}
