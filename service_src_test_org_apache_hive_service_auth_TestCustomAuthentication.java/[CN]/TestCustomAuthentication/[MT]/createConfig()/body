{
  Configuration conf=new Configuration(false);
  conf.set("hive.server2.authentication","CUSTOM");
  conf.set("hive.server2.custom.authentication.class","org.apache.hive.service.auth.TestCustomAuthentication$SimpleAuthenticationProviderImpl");
  configFile=new File("../build/service/test/resources","hive-site.xml");
  FileOutputStream out=new FileOutputStream(configFile);
  conf.writeXml(out);
}
