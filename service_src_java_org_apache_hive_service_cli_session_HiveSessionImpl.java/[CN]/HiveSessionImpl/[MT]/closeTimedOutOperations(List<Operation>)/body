{
  acquire(false);
  try {
    for (    Operation operation : operations) {
      opHandleSet.remove(operation.getHandle());
      try {
        operation.close();
      }
 catch (      Exception e) {
        LOG.warn("Exception is thrown closing timed-out operation " + operation.getHandle(),e);
      }
    }
  }
  finally {
    release(false);
  }
}
