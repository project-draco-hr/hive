{
  acquire();
  try {
    MetadataOperation operation=getOperationManager().newGetTablesOperation(getSession(),catalogName,schemaName,tableName,tableTypes);
    operation.run();
    OperationHandle opHandle=operation.getHandle();
    opHandleSet.add(opHandle);
    return opHandle;
  }
  finally {
    release();
  }
}
