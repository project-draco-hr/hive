{
  acquire();
  try {
    if (fetchType == FetchType.QUERY_OUTPUT) {
      return sessionManager.getOperationManager().getOperationNextRowSet(opHandle,orientation,maxRows);
    }
 else {
      return sessionManager.getOperationManager().getOperationLogRowSet(opHandle,orientation,maxRows);
    }
  }
  finally {
    release();
  }
}
