{
  PTFPersistence.lock(lock.readLock());
  try {
    ByteBasedList list=memList.get();
    if (list == null) {
      try {
        list=new ByteBasedList(file);
        load(list,file);
        memList=new SoftReference<ByteBasedList>(list);
      }
 catch (      Exception ie) {
        throw new RuntimeException(ie);
      }
    }
    return list;
  }
  finally {
    lock.readLock().unlock();
  }
}
