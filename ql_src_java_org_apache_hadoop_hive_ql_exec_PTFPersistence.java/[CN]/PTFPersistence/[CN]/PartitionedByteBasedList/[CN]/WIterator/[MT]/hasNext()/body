{
  if (pIter.hasNext()) {
    return true;
  }
  if (checkTime != lastModified) {
    throw new ConcurrentModificationException();
  }
  try {
    if (i < partitions.size()) {
      pIter=partitions.get(i++).iterator(wObj);
      return hasNext();
    }
    return false;
  }
 catch (  HiveException e) {
    throw new RuntimeException(e);
  }
}
