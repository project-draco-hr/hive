{
  List<TypeInfo> columnTypes=hSerdeParams.getColumnTypes();
  ArrayList<ObjectInspector> columnObjectInspectors=new ArrayList<ObjectInspector>(columnTypes.size());
  for (int i=0; i < columnTypes.size(); i++) {
    if (i == hSerdeParams.getKeyIndex()) {
      columnObjectInspectors.add(hSerdeParams.getKeyFactory().createKeyObjectInspector(columnTypes.get(i)));
    }
 else {
      columnObjectInspectors.add(hSerdeParams.getValueFactories().get(i).createValueObjectInspector(columnTypes.get(i)));
    }
  }
  List<String> structFieldComments=tbl.getProperty("columns.comments") == null ? new ArrayList<String>(Collections.nCopies(columnTypes.size(),"")) : Arrays.asList(tbl.getProperty("columns.comments").split("\0",columnTypes.size()));
  return LazyObjectInspectorFactory.getLazySimpleStructObjectInspector(hSerdeParams.getColumnNames(),columnObjectInspectors,structFieldComments,hSerdeParams.getSerdeParams().getSeparators()[0],hSerdeParams.getSerdeParams(),ObjectInspectorOptions.JAVA);
}
