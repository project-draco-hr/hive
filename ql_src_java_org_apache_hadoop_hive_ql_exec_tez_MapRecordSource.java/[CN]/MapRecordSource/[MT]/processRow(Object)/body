{
  try {
    if (mapOp.getDone()) {
      return false;
    }
 else {
      mapOp.process((Writable)value);
    }
  }
 catch (  Throwable e) {
    if (e instanceof OutOfMemoryError) {
      throw (OutOfMemoryError)e;
    }
 else {
      LOG.fatal(StringUtils.stringifyException(e));
      closeReader();
      throw new RuntimeException(e);
    }
  }
  return true;
}
