{
  execContext.resetRow();
  try {
    if (reader.next()) {
      Object value;
      try {
        value=reader.getCurrentValue();
      }
 catch (      IOException e) {
        throw new HiveException(e);
      }
      return processRow(value);
    }
  }
 catch (  IOException e) {
    throw new HiveException(e);
  }
  return false;
}
