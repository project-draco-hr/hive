{
  Query query=new Query(sql);
  try {
    Connection conn=getConnection(connName);
    runPreSql(connName,conn);
    Statement stmt=conn.createStatement();
    ResultSet rs=null;
    if (stmt.execute(sql)) {
      rs=stmt.getResultSet();
    }
    query.set(conn,stmt,rs);
  }
 catch (  Exception e) {
    query.setError(e);
  }
  return query;
}
