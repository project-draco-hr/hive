{
  String row_str="";
  try {
    row_str=(String)client.fetchOne();
    if (!row_str.equals("")) {
      Object o=ds.deserialize(new BytesWritable(row_str.getBytes()));
      row=(ArrayList<?>)o;
    }
  }
 catch (  Exception ex) {
    throw new SQLException("Error retrieving next row");
  }
  return !row_str.equals("");
}
