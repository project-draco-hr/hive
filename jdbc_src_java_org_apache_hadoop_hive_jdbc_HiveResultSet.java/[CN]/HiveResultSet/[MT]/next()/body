{
  if (maxRows > 0 && rowsFetched >= maxRows) {
    return false;
  }
  String rowStr="";
  try {
    rowStr=(String)client.fetchOne();
    rowsFetched++;
    if (!rowStr.equals("")) {
      Object o=ds.deserialize(new BytesWritable(rowStr.getBytes()));
      row=(ArrayList<?>)o;
    }
  }
 catch (  Exception ex) {
    ex.printStackTrace();
    throw new SQLException("Error retrieving next row");
  }
  return !rowStr.equals("");
}
