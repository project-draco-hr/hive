{
  StringBuilder sb=new StringBuilder();
  sb.append(Constants.UNION_TYPE_NAME + "<");
  for (int i=0; i < allUnionObjectTypeInfos.size(); i++) {
    if (i > 0) {
      sb.append(",");
    }
    sb.append(allUnionObjectTypeInfos.get(i).getTypeName());
  }
  sb.append(">");
  return sb.toString();
}
