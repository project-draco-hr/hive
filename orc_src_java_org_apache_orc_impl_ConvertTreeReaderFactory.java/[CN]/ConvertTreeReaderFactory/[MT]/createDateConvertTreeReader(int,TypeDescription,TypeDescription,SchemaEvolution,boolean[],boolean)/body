{
switch (readerType.getCategory()) {
case STRING:
case CHAR:
case VARCHAR:
    return new StringGroupFromDateTreeReader(columnId,readerType,skipCorrupt);
case TIMESTAMP:
  return new TimestampFromDateTreeReader(columnId,readerType,skipCorrupt);
case DATE:
throw new IllegalArgumentException("No conversion of type " + readerType.getCategory() + " to self needed");
case BOOLEAN:
case BYTE:
case FLOAT:
case SHORT:
case INT:
case LONG:
case DOUBLE:
case BINARY:
case DECIMAL:
case STRUCT:
case LIST:
case MAP:
case UNION:
default :
throw new IllegalArgumentException("Unsupported type " + readerType.getCategory());
}
}
