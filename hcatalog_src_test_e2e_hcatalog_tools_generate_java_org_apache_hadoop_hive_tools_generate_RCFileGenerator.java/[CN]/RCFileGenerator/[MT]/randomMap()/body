{
  int len=rand.nextInt(5) + 1;
  StringBuffer buf=new StringBuffer();
  for (int i=0; i < len; i++) {
    if (i != 0)     buf.append('\u0002');
    buf.append(firstName[rand.nextInt(26)]);
    buf.append('\u0003');
    buf.append(lastName[rand.nextInt(26)]);
  }
  return buf.toString().getBytes("UTF-8");
}
