{
  final HiveEndPoint ep=new HiveEndPoint(metaStoreURI,dbName,tblName,partitionVals);
  WriterThd t1=new WriterThd(ep,"1,Matrix");
  WriterThd t2=new WriterThd(ep,"2,Gandhi");
  WriterThd t3=new WriterThd(ep,"3,Silence");
  t1.start();
  t2.start();
  t3.start();
  t1.join();
  t2.join();
  t3.join();
}
