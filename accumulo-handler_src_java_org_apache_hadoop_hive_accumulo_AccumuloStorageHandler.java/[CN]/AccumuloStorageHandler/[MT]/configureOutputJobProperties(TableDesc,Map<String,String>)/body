{
  Properties props=tableDesc.getProperties();
  jobProperties.put(AccumuloSerDeParameters.COLUMN_MAPPINGS,props.getProperty(AccumuloSerDeParameters.COLUMN_MAPPINGS));
  String tableName=props.getProperty(AccumuloSerDeParameters.TABLE_NAME);
  if (null == tableName) {
    tableName=getTableName(tableDesc);
  }
  jobProperties.put(AccumuloSerDeParameters.TABLE_NAME,tableName);
  if (props.containsKey(AccumuloSerDeParameters.DEFAULT_STORAGE_TYPE)) {
    jobProperties.put(AccumuloSerDeParameters.DEFAULT_STORAGE_TYPE,props.getProperty(AccumuloSerDeParameters.DEFAULT_STORAGE_TYPE));
  }
  if (props.containsKey(AccumuloSerDeParameters.VISIBILITY_LABEL_KEY)) {
    jobProperties.put(AccumuloSerDeParameters.VISIBILITY_LABEL_KEY,props.getProperty(AccumuloSerDeParameters.VISIBILITY_LABEL_KEY));
  }
}
