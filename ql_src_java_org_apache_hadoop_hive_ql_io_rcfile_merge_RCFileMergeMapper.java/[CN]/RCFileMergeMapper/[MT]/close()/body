{
  if (outWriter == null) {
    return;
  }
  outWriter.close();
  outWriter=null;
  if (!exception) {
    FileStatus fss=fs.getFileStatus(outPath);
    System.out.println("renamed path " + outPath + " to "+ finalPath+ " . File size is "+ fss.getLen());
    if (!fs.rename(outPath,finalPath)) {
      throw new IOException("Unable to rename output to " + finalPath);
    }
  }
 else {
    if (!autoDelete) {
      fs.delete(outPath,true);
    }
  }
}
