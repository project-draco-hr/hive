{
  commandLine=new GnuParser().parse(options,args);
  if (commandLine.hasOption(OptionConstants.HELP.getShortOpt()) || false == commandLine.hasOption(OptionConstants.NAME.getLongOpt())) {
    printUsage();
    return null;
  }
  String name=commandLine.getOptionValue(OptionConstants.NAME.getLongOpt());
  return new LlapStatusOptions(name);
}
