{
  for (  Operator<? extends OperatorDesc> op : work.getMapWork().getAliasToWork().values()) {
    if (op.checkFatalErrors(ctrs,errMsg)) {
      return true;
    }
  }
  if (work.getReduceWork() != null) {
    if (work.getReduceWork().getReducer().checkFatalErrors(ctrs,errMsg)) {
      return true;
    }
  }
  return false;
}
