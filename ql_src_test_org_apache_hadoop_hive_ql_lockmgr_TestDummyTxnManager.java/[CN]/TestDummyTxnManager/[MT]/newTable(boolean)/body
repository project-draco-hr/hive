{
  Table t=new Table("default","table" + Integer.toString(nextInput++));
  if (isPartitioned) {
    FieldSchema fs=new FieldSchema();
    fs.setName("version");
    fs.setType("String");
    List<FieldSchema> partCols=new ArrayList<FieldSchema>(1);
    partCols.add(fs);
    t.setPartCols(partCols);
  }
  return t;
}
