{
  if (!reader.hasNext()) {
    return false;
  }
  try {
    if (addPartitionCols) {
      rbCtx.addPartitionColsToBatch(value);
      addPartitionCols=false;
    }
    reader.nextBatch(value);
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  progress=reader.getProgress();
  return true;
}
