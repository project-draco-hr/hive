{
  Pattern pattern=Pattern.compile(patternAsString);
  Matcher matcher;
  String line;
  List<String> jobs=new ArrayList<String>();
  while ((line=in.readLine()) != null) {
    matcher=pattern.matcher(line);
    if (matcher.find()) {
      String jobid=matcher.group(1);
      jobs.add(jobid);
    }
  }
  return jobs;
}
