{
  if (n == null) {
    return null;
  }
  int len=n.get();
  if (len < 0) {
    len=0;
  }
  if (result.getBytes().length >= len) {
    result.set(result.getBytes(),0,len);
  }
 else {
    byte[] spaces=new byte[len];
    Arrays.fill(spaces,(byte)' ');
    result.set(spaces);
  }
  return result;
}
