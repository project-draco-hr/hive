{
  Matcher m=bpattern.matcher(p.getName());
  if (m.find()) {
    String bnum_str=m.group(1);
    try {
      return (Integer.parseInt(bnum_str));
    }
 catch (    NumberFormatException e) {
      throw new RuntimeException("Unexpected error parsing: " + p.getName() + ","+ bnum_str);
    }
  }
  return 0;
}
