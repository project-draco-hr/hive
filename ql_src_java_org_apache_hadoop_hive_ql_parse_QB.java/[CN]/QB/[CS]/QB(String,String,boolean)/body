{
  aliasToTabs=new HashMap<String,String>();
  aliasToSubq=new HashMap<String,QBExpr>();
  aliases=new ArrayList<String>();
  if (alias != null) {
    alias=alias.toLowerCase();
  }
  qbp=new QBParseInfo(alias,isSubQ);
