{
  Set<String> usersToRemap;
switch (type) {
case USER:
    usersToRemap=new HashSet<String>();
  usersToRemap.add(principalName);
break;
case ROLE:
usersToRemap=getHBase().findAllUsersInRole(role.getRoleName());
break;
default :
throw new RuntimeException("Unknown principal type " + type);
}
return usersToRemap;
}
