{
  boolean commit=false;
  openTransaction();
  try {
    getHBase().putFunction(newFunction);
    commit=true;
  }
 catch (  IOException e) {
    LOG.error("Unable to alter function ",e);
    throw new MetaException("Unable to read from or write to hbase " + e.getMessage());
  }
 finally {
    commitOrRoleBack(commit);
  }
}
