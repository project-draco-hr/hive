{
  boolean commit=false;
  openTransaction();
  try {
    Index index=getHBase().getIndex(HiveStringUtils.normalizeIdentifier(dbName),HiveStringUtils.normalizeIdentifier(origTableName),HiveStringUtils.normalizeIdentifier(indexName));
    if (index == null) {
      LOG.debug("Unable to find index " + indexNameForErrorMsg(dbName,origTableName,indexName));
    }
    commit=true;
    return index;
  }
 catch (  IOException e) {
    LOG.error("Unable to get index",e);
    throw new MetaException("Error reading index " + e.getMessage());
  }
 finally {
    commitOrRoleBack(commit);
  }
}
