{
  boolean commit=false;
  openTransaction();
  try {
    List<String> tableNames=getTableNamesInTx(dbName,pattern);
    commit=true;
    return tableNames;
  }
 catch (  IOException e) {
    LOG.error("Unable to get tables ",e);
    throw new MetaException("Unable to get tables, " + e.getMessage());
  }
 finally {
    commitOrRoleBack(commit);
  }
}
