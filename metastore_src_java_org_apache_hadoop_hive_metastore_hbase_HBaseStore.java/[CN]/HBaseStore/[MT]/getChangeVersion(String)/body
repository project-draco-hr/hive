{
  openTransaction();
  boolean commit=true;
  try {
    return getHBase().getChangeVersion(topic);
  }
 catch (  IOException e) {
    commit=false;
    LOG.error("Unable to get change version",e);
    throw new MetaException("Unable to get change version " + e.getMessage());
  }
 finally {
    commitOrRoleBack(commit);
  }
}
