{
  boolean commit=false;
  openTransaction();
  try {
    Table table=getHBase().getTable(HiveStringUtils.normalizeIdentifier(dbName),HiveStringUtils.normalizeIdentifier(tableName));
    if (table == null) {
      LOG.debug("Unable to find table " + tableNameForErrorMsg(dbName,tableName));
    }
    commit=true;
    return table;
  }
 catch (  IOException e) {
    LOG.error("Unable to get table",e);
    throw new MetaException("Error reading table " + e.getMessage());
  }
 finally {
    commitOrRoleBack(commit);
  }
}
