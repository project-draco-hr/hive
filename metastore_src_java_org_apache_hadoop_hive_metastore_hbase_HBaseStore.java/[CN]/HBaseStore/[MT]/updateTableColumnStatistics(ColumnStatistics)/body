{
  boolean commit=false;
  openTransaction();
  try {
    getHBase().updateStatistics(colStats.getStatsDesc().getDbName(),colStats.getStatsDesc().getTableName(),null,null,colStats);
    commit=true;
    return true;
  }
 catch (  IOException e) {
    LOG.error("Unable to update column statistics",e);
    throw new MetaException("Failed to update column statistics, " + e.getMessage());
  }
 finally {
    commitOrRoleBack(commit);
  }
}
