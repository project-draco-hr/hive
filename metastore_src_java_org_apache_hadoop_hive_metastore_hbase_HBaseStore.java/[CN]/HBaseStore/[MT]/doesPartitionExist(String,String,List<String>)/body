{
  boolean commit=false;
  openTransaction();
  try {
    boolean exists=getHBase().getPartition(HiveStringUtils.normalizeIdentifier(dbName),HiveStringUtils.normalizeIdentifier(tableName),part_vals) != null;
    commit=true;
    return exists;
  }
 catch (  IOException e) {
    LOG.error("Unable to get partition",e);
    throw new MetaException("Error reading partition " + e.getMessage());
  }
 finally {
    commitOrRoleBack(commit);
  }
}
