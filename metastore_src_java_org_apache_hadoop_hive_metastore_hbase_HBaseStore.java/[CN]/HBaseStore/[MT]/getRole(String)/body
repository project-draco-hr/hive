{
  try {
    Role role=getHBase().getRole(roleName);
    if (role == null) {
      throw new NoSuchObjectException("Unable to find role " + roleName);
    }
    return role;
  }
 catch (  IOException e) {
    LOG.error("Unable to get role",e);
    throw new NoSuchObjectException("Error reading table " + e.getMessage());
  }
}
