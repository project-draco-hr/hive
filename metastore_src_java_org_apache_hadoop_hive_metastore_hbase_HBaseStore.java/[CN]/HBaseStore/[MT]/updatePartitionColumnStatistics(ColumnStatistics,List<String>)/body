{
  try {
    getHBase().updateStatistics(statsObj.getStatsDesc().getDbName(),statsObj.getStatsDesc().getTableName(),statsObj.getStatsDesc().getPartName(),partVals,statsObj);
    return true;
  }
 catch (  IOException e) {
    LOG.error("Unable to update column statistics",e);
    throw new MetaException("Failed to update column statistics, " + e.getMessage());
  }
}
