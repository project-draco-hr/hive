{
  String dir=System.getProperty("beeline.rcfile");
  if (dir != null && dir.length() > 0) {
    return new File(dir);
  }
  File f=new File(System.getProperty("user.home"),(System.getProperty("os.name").toLowerCase().indexOf("windows") != -1 ? "" : ".") + "beeline").getAbsoluteFile();
  try {
    f.mkdirs();
  }
 catch (  Exception e) {
  }
  return f;
}
