{
  this.driver=driver;
  this.queryFilesProperty=queryFilesProperty;
  this.tests=tests;
  String name=Joiner.on("-").join(driver,Joiner.on("-").join(Iterators.toArray(Iterators.limit(tests.iterator(),3),String.class)));
  if (tests.size() > 3) {
    name=Joiner.on("-").join(name,"and",(tests.size() - 3),"more");
  }
  this.name=name;
  this.isParallel=isParallel;
}
