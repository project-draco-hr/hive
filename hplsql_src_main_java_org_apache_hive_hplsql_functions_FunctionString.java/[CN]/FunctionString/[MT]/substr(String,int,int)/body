{
  if (str == null) {
    evalNull();
    return;
  }
 else   if (str.isEmpty()) {
    evalString(str);
    return;
  }
  if (start == 0) {
    start=1;
  }
  if (len == -1) {
    if (start > 0) {
      evalString(str.substring(start - 1));
    }
  }
 else {
    evalString(str.substring(start - 1,start - 1 + len));
  }
}
