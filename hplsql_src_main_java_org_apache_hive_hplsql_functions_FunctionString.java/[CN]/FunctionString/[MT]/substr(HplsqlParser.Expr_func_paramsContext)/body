{
  int cnt=ctx.expr().size();
  if (cnt < 2) {
    evalNull();
    return;
  }
  String str=evalPop(ctx.expr(0)).toString();
  int start=evalPop(ctx.expr(1)).intValue();
  int len=-1;
  if (start == 0) {
    start=1;
  }
  if (cnt > 2) {
    len=evalPop(ctx.expr(2)).intValue();
  }
  substr(str,start,len);
}
