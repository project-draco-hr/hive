{
  int cnt=getParamCount(ctx);
  if (cnt < 2) {
    evalNull();
    return;
  }
  String str=evalPop(ctx.func_param(0).expr()).toString();
  int start=evalPop(ctx.func_param(1).expr()).intValue();
  int len=-1;
  if (start == 0) {
    start=1;
  }
  if (cnt > 2) {
    len=evalPop(ctx.func_param(2).expr()).intValue();
  }
  substr(str,start,len);
}
