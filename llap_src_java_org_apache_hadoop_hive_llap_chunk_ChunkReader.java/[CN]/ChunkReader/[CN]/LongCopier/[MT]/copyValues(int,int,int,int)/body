{
  dataBuffer.position((valuesOffsetBytes >>> 3) + valuesToSkip);
  dataBuffer.get(dest,destOffset,valuesToCopy);
  if (DebugUtils.isTraceDataEnabled()) {
    Llap.LOG.info("After copying " + valuesToCopy + " from "+ valuesOffsetBytes+ " (skip "+ valuesToSkip+ ", long offset "+ ((valuesOffsetBytes >>> 3) + valuesToSkip)+ ") to "+ destOffset+ ", values are "+ DebugUtils.toString(dest,destOffset,valuesToCopy)+ " and dest is "+ DebugUtils.toString(dest,0,dest.length));
  }
}
