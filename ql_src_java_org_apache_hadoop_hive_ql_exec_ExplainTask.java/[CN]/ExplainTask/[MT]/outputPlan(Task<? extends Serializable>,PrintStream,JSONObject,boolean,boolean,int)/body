{
  if (out != null) {
    out.print(indentString(indent));
    out.printf("Stage: %s\n",task.getId());
  }
  JSONObject jsonOutputPlan=outputPlan(task.getWork(),out,extended,jsonOutput,jsonOutput ? 0 : indent + 2);
  if (out != null) {
    out.println();
  }
  if (jsonOutput) {
    parentJSON.put(task.getId(),jsonOutputPlan);
  }
  return null;
}
