{
  if (out != null) {
    out.print(indentString(indent));
    out.println("STAGE DEPENDENCIES:");
  }
  JSONObject json=jsonOutput ? new JSONObject() : null;
  Set<Task<? extends Serializable>> dependenciesTaskSet=new HashSet<Task<? extends Serializable>>();
  for (  Task<? extends Serializable> rootTask : rootTasks) {
    JSONObject jsonOut=outputDependencies(rootTask,dependenciesTaskSet,out,json,jsonOutput,jsonOutput ? 0 : indent + 2,true);
    if (jsonOutput && (jsonOut != null)) {
      json.put(rootTask.getId(),jsonOut);
    }
  }
  return jsonOutput ? json : null;
}
