{
  this.inpJobInfo=inputJobInfo;
  String snapshotString=inpJobInfo.getProperties().getProperty(HBaseConstants.PROPERTY_TABLE_SNAPSHOT_KEY);
  HCatTableSnapshot hcatSnapshot=(HCatTableSnapshot)HCatUtil.deserialize(snapshotString);
  this.snapshot=HBaseInputStorageDriver.convertSnapshot(hcatSnapshot,inpJobInfo.getTableInfo());
  this.maxRevisions=1;
}
