{
  if (other == null || !(other instanceof Key))   return false;
  Key that=(Key)other;
  if (partName == null) {
    return that.partName == null && dbName.equals(that.dbName) && tableName.equals(that.tableName) && colName.equals(that.colName);
  }
 else {
    return dbName.equals(that.dbName) && tableName.equals(that.tableName) && partName.equals(that.partName)&& colName.equals(that.colName);
  }
}
