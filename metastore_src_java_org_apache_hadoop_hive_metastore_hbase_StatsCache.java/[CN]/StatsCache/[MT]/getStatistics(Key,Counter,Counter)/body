{
  StatsInfo s=null;
  lock.readLock().lock();
  try {
    s=cache.get(key);
  }
  finally {
    lock.readLock().unlock();
  }
  if (s == null) {
    misses.incr();
    return null;
  }
 else   if (tooLate(s)) {
    remove(key);
    misses.incr();
    return null;
  }
 else {
    s.lastTouched=System.currentTimeMillis();
    hits.incr();
    return s;
  }
}
