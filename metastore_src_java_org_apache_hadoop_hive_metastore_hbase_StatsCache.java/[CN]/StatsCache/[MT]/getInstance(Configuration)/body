{
  if (self == null) {
    int totalObjectsToCache=HiveConf.getIntVar(conf,HiveConf.ConfVars.METASTORE_HBASE_CACHE_SIZE);
    long timeToLive=HiveConf.getTimeVar(conf,HiveConf.ConfVars.METASTORE_HBASE_CACHE_TIME_TO_LIVE,TimeUnit.SECONDS);
    self=new StatsCache(totalObjectsToCache / 2,timeToLive);
  }
  return self;
}
