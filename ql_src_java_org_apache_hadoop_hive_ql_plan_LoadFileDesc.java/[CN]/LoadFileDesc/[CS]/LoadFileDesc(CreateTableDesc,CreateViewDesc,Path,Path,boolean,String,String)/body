{
  this(sourcePath,targetDir,isDfsDir,columns,columnTypes);
  if (createTableDesc != null && createTableDesc.getDatabaseName() != null && createTableDesc.getTableName() != null) {
    destinationCreateTable=(createTableDesc.getTableName().contains(".") ? "" : createTableDesc.getDatabaseName() + ".") + createTableDesc.getTableName();
  }
 else   if (createViewDesc != null) {
    destinationCreateTable=createViewDesc.getViewName();
  }
}
