{
  byte value=0;
  try {
    in=new ByteStream.Input(bytes.getData(),start,length);
    din=new DataInputStream(in);
    value=din.readByte();
    data.set(value);
    isNull=false;
  }
 catch (  Exception e) {
    isNull=true;
  }
 finally {
    try {
      din.close();
    }
 catch (    IOException e) {
    }
    try {
      in.close();
    }
 catch (    IOException e) {
    }
  }
}
