{
  SearchArgument sarg=SearchArgumentFactory.newBuilder().startNot().startOr().isNull("x",PredicateLeaf.Type.INTEGER).between("y",PredicateLeaf.Type.INTEGER,10,20).in("z",PredicateLeaf.Type.INTEGER,1,2,3).nullSafeEquals("a",PredicateLeaf.Type.STRING,"stinger").end().end().build();
  FilterPredicate p=ParquetFilterPredicateConverter.toFilterPredicate(sarg);
  String expected="and(and(and(not(eq(x, null)), not(and(lt(y, 20), not(lteq(y, 10))))), not(or(or(eq(z, 1), " + "eq(z, 2)), eq(z, 3)))), not(eq(a, Binary{\"stinger\"})))";
  assertEquals(expected,p.toString());
}
