{
  super.ensureSize(size,preserveData);
  if (tags.length < size) {
    if (preserveData) {
      int[] oldTags=tags;
      tags=new int[size];
      System.arraycopy(oldTags,0,tags,0,oldTags.length);
    }
 else {
      tags=new int[size];
    }
    for (int i=0; i < fields.length; ++i) {
      fields[i].ensureSize(size,preserveData);
    }
  }
}
