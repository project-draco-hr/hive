{
  RelOptHiveTable hTbl=(RelOptHiveTable)scan.getTable();
  ASTBuilder b=ASTBuilder.construct(HiveParser.TOK_TABREF,"TOK_TABREF").add(ASTBuilder.construct(HiveParser.TOK_TABNAME,"TOK_TABNAME").add(HiveParser.Identifier,hTbl.getHiveTableMD().getDbName()).add(HiveParser.Identifier,hTbl.getHiveTableMD().getTableName())).add(HiveParser.Identifier,hTbl.getName());
  return b.node();
}
