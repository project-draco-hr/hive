{
  if (signum == 0) {
    return 0L;
  }
  long ret;
  UnsignedInt128 tmp;
  if (scale == 0) {
    ret=this.unscaledValue.getV1();
    ret<<=32L;
    ret|=SqlMathUtil.LONG_MASK & this.unscaledValue.getV0();
  }
 else {
    tmp=new UnsignedInt128(this.unscaledValue);
    tmp.scaleDownTenDestructive(scale);
    ret=tmp.getV1();
    ret<<=32L;
    ret|=SqlMathUtil.LONG_MASK & tmp.getV0();
  }
  if (signum >= 0) {
    return ret;
  }
 else {
    return -ret;
  }
}
