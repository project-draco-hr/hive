{
  if (signum == 0) {
    return 0L;
  }
  if (scale == 0) {
    long ret;
    ret=this.unscaledValue.getV1();
    ret<<=32L;
    ret|=SqlMathUtil.LONG_MASK & this.unscaledValue.getV0();
    if (signum >= 0) {
      return ret;
    }
 else {
      return -ret;
    }
  }
 else {
    HiveDecimal hd=HiveDecimal.create(this.toBigDecimal());
    return hd.longValue();
  }
}
