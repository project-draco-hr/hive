{
  String user=null;
  try {
    user=conf.getUser();
  }
 catch (  IOException e) {
    formatter.consoleError(console,"Unable to get current user: " + e.getMessage(),stringifyException(e),formatter.ERROR);
    return false;
  }
  params.put("last_modified_by",user);
  params.put("last_modified_time",Long.toString(System.currentTimeMillis() / 1000));
  return true;
}
