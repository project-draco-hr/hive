{
  String user=null;
  try {
    user=conf.getUser();
  }
 catch (  IOException e) {
    throw new HiveException(e,ErrorMsg.GENERIC_ERROR,"Unable to get current user");
  }
  params.put("last_modified_by",user);
  params.put("last_modified_time",Long.toString(System.currentTimeMillis() / 1000));
  return true;
}
