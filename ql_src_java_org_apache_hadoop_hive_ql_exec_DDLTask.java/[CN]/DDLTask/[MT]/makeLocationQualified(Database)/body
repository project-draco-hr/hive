{
  if (database.isSetLocationUri()) {
    database.setLocationUri(Utilities.getQualifiedPath(conf,new Path(database.getLocationUri())));
  }
 else {
    database.setLocationUri(Utilities.getQualifiedPath(conf,new Path(HiveConf.getVar(conf,HiveConf.ConfVars.METASTOREWAREHOUSE),database.getName().toLowerCase() + DATABASE_PATH_SUFFIX)));
  }
}
