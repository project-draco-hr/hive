{
  try {
    Deserializer d=ReflectionUtils.newInstance(conf.getClassByName(serdeName).asSubclass(Deserializer.class),conf);
    if (d != null) {
      LOG.debug("Found class for " + serdeName);
    }
  }
 catch (  Exception e) {
    throw new HiveException("Cannot validate serde: " + serdeName,e);
  }
}
