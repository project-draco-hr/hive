{
  if (obj instanceof SparkStageProgress) {
    SparkStageProgress other=(SparkStageProgress)obj;
    return getTotalTaskCount() == other.getTotalTaskCount() && getSucceededTaskCount() == other.getSucceededTaskCount() && getRunningTaskCount() == other.getRunningTaskCount() && getFailedTaskCount() == other.getFailedTaskCount();
  }
  return false;
}
