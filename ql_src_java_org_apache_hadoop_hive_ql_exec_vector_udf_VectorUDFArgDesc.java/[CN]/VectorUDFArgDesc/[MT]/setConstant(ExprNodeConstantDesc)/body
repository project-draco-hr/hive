{
  isConstant=true;
  if (expr != null) {
    if (expr.getTypeInfo().getCategory() == Category.PRIMITIVE) {
      PrimitiveCategory primitiveCategory=((PrimitiveTypeInfo)expr.getTypeInfo()).getPrimitiveCategory();
      if (primitiveCategory == PrimitiveCategory.VOID) {
        expr=null;
      }
    }
  }
  constExpr=expr;
}
