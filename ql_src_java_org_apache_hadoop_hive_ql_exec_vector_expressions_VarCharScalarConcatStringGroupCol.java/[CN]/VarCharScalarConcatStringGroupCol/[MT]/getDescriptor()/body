{
  return (new VectorExpressionDescriptor.Builder()).setMode(VectorExpressionDescriptor.Mode.PROJECTION).setNumArguments(2).setArgumentTypes(VectorExpressionDescriptor.ArgumentType.VARCHAR,VectorExpressionDescriptor.ArgumentType.STRING_FAMILY).setInputExpressionTypes(VectorExpressionDescriptor.InputExpressionType.SCALAR,VectorExpressionDescriptor.InputExpressionType.COLUMN).build();
}
