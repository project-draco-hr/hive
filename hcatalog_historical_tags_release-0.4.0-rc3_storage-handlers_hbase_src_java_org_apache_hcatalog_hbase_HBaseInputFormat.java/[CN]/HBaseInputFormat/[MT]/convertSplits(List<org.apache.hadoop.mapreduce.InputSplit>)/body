{
  InputSplit[] converted=new InputSplit[splits.size()];
  for (int i=0; i < splits.size(); i++) {
    org.apache.hadoop.hbase.mapreduce.TableSplit tableSplit=(org.apache.hadoop.hbase.mapreduce.TableSplit)splits.get(i);
    TableSplit newTableSplit=new TableSplit(tableSplit.getTableName(),tableSplit.getStartRow(),tableSplit.getEndRow(),tableSplit.getRegionLocation());
    converted[i]=newTableSplit;
  }
  return converted;
}
