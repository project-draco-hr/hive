{
  if (s == null) {
    return null;
  }
  String str;
  if (s.getLength() % 2 == 1) {
    str="0" + s.toString();
  }
 else {
    str=s.toString();
  }
  byte[] result=new byte[str.length() / 2];
  for (int i=0; i < str.length(); i+=2) {
    try {
      result[i / 2]=((byte)Integer.parseInt(str.substring(i,i + 2),16));
    }
 catch (    NumberFormatException e) {
      return null;
    }
  }
  return new Text(result);
}
