  for (int i=0; i < TxnHandler.TIMED_OUT_TXN_ABORT_BATCH_SIZE + 17; i++) {
    ((DbTxnManager)txnMgr).acquireLocks(qp,ctx,"PeterI" + i,true);
  }
  expireLocks(txnMgr,TxnHandler.TIMED_OUT_TXN_ABORT_BATCH_SIZE + 17);
}
