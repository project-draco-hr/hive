{
  addPartitionInput(newTable(true));
  QueryPlan qp=new MockQueryPlan(this);
  expireLocks(txnMgr,0);
  for (int i=0; i < 5; i++) {
    ((DbTxnManager)txnMgr).acquireLocks(qp,ctx,"PeterI" + i,true);
  }
