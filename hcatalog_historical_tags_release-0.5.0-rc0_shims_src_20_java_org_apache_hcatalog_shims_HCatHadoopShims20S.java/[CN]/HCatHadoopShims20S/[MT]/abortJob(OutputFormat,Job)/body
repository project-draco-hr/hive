{
  if (job.getConfiguration().get("mapred.job.tracker","").equalsIgnoreCase("local")) {
    try {
      outputFormat.getOutputCommitter(HCatHadoopShims.Instance.get().createTaskAttemptContext(job.getConfiguration(),new TaskAttemptID())).abortJob(job,State.FAILED);
    }
 catch (    IOException e) {
      throw new IOException("Failed to abort job",e);
    }
catch (    InterruptedException e) {
      throw new IOException("Failed to abort job",e);
    }
  }
}
