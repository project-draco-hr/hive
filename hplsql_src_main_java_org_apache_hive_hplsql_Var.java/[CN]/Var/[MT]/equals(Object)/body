{
  if (this == obj) {
    return true;
  }
 else   if (obj == null || this.value == null) {
    return false;
  }
 else   if (getClass() != obj.getClass()) {
    return false;
  }
  Var var=(Var)obj;
  if (type == Type.BIGINT && var.type == Type.BIGINT && ((Long)value).longValue() == ((Long)var.value).longValue()) {
    return true;
  }
 else   if (type == Type.STRING && var.type == Type.STRING && ((String)value).equals((String)var.value)) {
    return true;
  }
  return false;
}
