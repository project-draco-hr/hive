{
  if (key == null || key.length != fieldObjs.length) {
    key=new Object[fieldObjs.length];
  }
  for (int keyIndex=0; keyIndex < fieldObjs.length; ++keyIndex) {
    key[keyIndex]=(ObjectInspectorUtils.copyToStandardObject(fieldObjs[keyIndex],keyFieldsOI.get(keyIndex),ObjectInspectorCopyOption.WRITABLE));
  }
}
