{
  if (order1 == null || order1.trim().equals("")) {
    if (order2 == null || order2.trim().equals("")) {
      return true;
    }
    return false;
  }
  if (order2 == null || order2.trim().equals("")) {
    return false;
  }
  order1=order1.trim();
  order2=order2.trim();
  if (!order1.equals(order2)) {
    return false;
  }
  return true;
}
