{
  if (k1.size() != k2.size()) {
    return false;
  }
  for (int i=0; i < k1.size(); i++) {
    ExprNodeDesc expr1=k1.get(i);
    ExprNodeDesc expr2=k2.get(i);
    if (expr1 == null) {
      if (expr2 == null) {
        continue;
      }
 else {
        return false;
      }
    }
 else {
      if (!expr1.isSame(expr2)) {
        return false;
      }
    }
  }
  return true;
}
