{
  CacheEntry entry;
  Long id;
  checkIfClosed();
  id=new Long(recid);
  try {
    entry=(CacheEntry)_cache.get(id);
    if (entry != null) {
      entry._obj=obj;
      entry._serializer=serializer;
      entry._isDirty=true;
    }
 else {
      _cache.put(id,new CacheEntry(recid,obj,serializer,true));
    }
  }
 catch (  CacheEvictionException except) {
    throw new IOException(except.getMessage());
  }
}
