{
  StringBuilder sb=new StringBuilder();
  BufferedReader reader=new BufferedReader(new StringReader(s));
  String line=null;
  while ((line=reader.readLine()) != null) {
    if (!line.startsWith("log4j:") && !line.contains("INFO Log4j")) {
      sb.append(line);
      sb.append("\n");
    }
  }
  return sb.toString();
}
