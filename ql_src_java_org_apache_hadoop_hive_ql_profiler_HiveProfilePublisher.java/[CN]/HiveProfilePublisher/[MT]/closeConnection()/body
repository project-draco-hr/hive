{
  if (info == null || info.getConnection() == null) {
    return true;
  }
  try {
    if (info.getInsertStatement() != null) {
      info.closeInsertStatement();
    }
    info.getConnection().close();
    return true;
  }
 catch (  SQLException e) {
    LOG.error("Error during JDBC termination. ",e);
    return false;
  }
}
