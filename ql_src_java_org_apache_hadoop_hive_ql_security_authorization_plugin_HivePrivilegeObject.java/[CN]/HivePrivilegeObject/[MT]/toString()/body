{
  String name=null;
switch (type) {
case DATABASE:
    name=dbname;
  break;
case TABLE_OR_VIEW:
case PARTITION:
name=getDbObjectName(dbname,objectName);
if (partKeys != null) {
name+=partKeys.toString();
}
break;
case FUNCTION:
name=getDbObjectName(dbname,objectName);
break;
case COLUMN:
case LOCAL_URI:
case DFS_URI:
name=objectName;
break;
case COMMAND_PARAMS:
name=commandParams.toString();
break;
}
String actionTypeStr="";
if (actionType != null) {
switch (actionType) {
case INSERT:
case INSERT_OVERWRITE:
actionTypeStr=", action=" + actionType;
default :
}
}
return "Object [type=" + type + ", name="+ name+ actionTypeStr+ "]";
}
