{
  String name=null;
switch (type) {
case DATABASE:
    name=dbname;
  break;
case TABLE_OR_VIEW:
name=(dbname == null ? "" : dbname + ".") + tableviewname;
break;
case LOCAL_URI:
case DFS_URI:
name=tableviewname;
break;
case COMMAND_PARAMS:
name=commandParams.toString();
break;
case PARTITION:
break;
}
return "Object [type=" + type + ", name="+ name+ "]";
}
