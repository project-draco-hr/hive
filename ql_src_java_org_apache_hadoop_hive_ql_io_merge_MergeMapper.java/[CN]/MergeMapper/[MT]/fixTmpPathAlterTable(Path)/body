{
  if (isListBucketingAlterTableConcatenate) {
    if (this.tmpPathFixedConcatenate) {
      checkPartitionsMatch(path);
    }
 else {
      fixTmpPathConcatenate(path);
      tmpPathFixedConcatenate=true;
    }
  }
 else {
    if (hasDynamicPartitions || (listBucketingDepth > 0)) {
      if (tmpPathFixed) {
        checkPartitionsMatch(path);
      }
 else {
        fixTmpPath(path);
        tmpPathFixed=true;
      }
    }
  }
}
