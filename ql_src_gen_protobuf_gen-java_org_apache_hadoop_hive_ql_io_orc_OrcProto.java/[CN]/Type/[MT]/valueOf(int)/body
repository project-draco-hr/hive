{
switch (value) {
case 0:
    return BOOLEAN;
case 1:
  return BYTE;
case 2:
return SHORT;
case 3:
return INT;
case 4:
return LONG;
case 5:
return FLOAT;
case 6:
return DOUBLE;
case 7:
return STRING;
case 8:
return BINARY;
case 9:
return TIMESTAMP;
case 10:
return LIST;
case 11:
return MAP;
case 12:
return STRUCT;
case 13:
return UNION;
case 14:
return DECIMAL;
case 15:
return DATE;
case 16:
return VARCHAR;
case 17:
return CHAR;
default :
return null;
}
}
