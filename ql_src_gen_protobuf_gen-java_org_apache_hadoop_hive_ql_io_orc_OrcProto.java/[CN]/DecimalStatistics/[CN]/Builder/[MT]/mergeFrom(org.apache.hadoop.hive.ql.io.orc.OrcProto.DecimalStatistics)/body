{
  if (other == org.apache.hadoop.hive.ql.io.orc.OrcProto.DecimalStatistics.getDefaultInstance())   return this;
  if (other.hasMinimum()) {
    setMinimum(other.getMinimum());
  }
  if (other.hasMaximum()) {
    setMaximum(other.getMaximum());
  }
  if (other.hasSum()) {
    setSum(other.getSum());
  }
  this.mergeUnknownFields(other.getUnknownFields());
  return this;
}
