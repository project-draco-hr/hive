{
  if (arguments.length != 2) {
    throw new UDFArgumentLengthException("The function " + name + "(b1, b2) takes exactly 2 arguments");
  }
  if (arguments[0].getCategory().equals(Category.LIST)) {
    b1OI=(ListObjectInspector)arguments[0];
  }
 else {
    throw new UDFArgumentTypeException(0,"\"" + Category.LIST.toString().toLowerCase() + "\" is expected at function "+ name+ ", but \""+ arguments[0].getTypeName()+ "\" is found");
  }
  if (!arguments[1].getCategory().equals(Category.LIST)) {
    throw new UDFArgumentTypeException(1,"\"" + Category.LIST.toString().toLowerCase() + "\" is expected at function "+ name+ ", but \""+ arguments[1].getTypeName()+ "\" is found");
  }
  return ObjectInspectorFactory.getStandardListObjectInspector(PrimitiveObjectInspectorFactory.writableLongObjectInspector);
}
