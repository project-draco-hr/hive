{
  testAggregateDecimal("Decimal","max",2,Arrays.asList(new Object[]{HiveDecimal.create(1),HiveDecimal.create(2),HiveDecimal.create(3)}),HiveDecimal.create(3));
  testAggregateDecimal("Decimal","max",2,Arrays.asList(new Object[]{HiveDecimal.create(3),HiveDecimal.create(2),HiveDecimal.create(1)}),HiveDecimal.create(3));
  testAggregateDecimal("Decimal","max",2,Arrays.asList(new Object[]{HiveDecimal.create(2),HiveDecimal.create(3),HiveDecimal.create(1)}),HiveDecimal.create(3));
}
