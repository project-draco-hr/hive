{
  testAggregateLongAggregate("avg",2,Arrays.asList(new Long[]{null}),null);
  testAggregateLongAggregate("avg",2,Arrays.asList(new Long[]{null,null,null}),null);
  testAggregateLongAggregate("avg",2,Arrays.asList(new Long[]{null,5L,7L,19L}),(double)(5L + 7L + 19L) / (double)3L);
  testAggregateLongAggregate("avg",2,Arrays.asList(new Long[]{13L,null,7L,19L}),(double)(13L + +7L + 19L) / (double)3L);
}
