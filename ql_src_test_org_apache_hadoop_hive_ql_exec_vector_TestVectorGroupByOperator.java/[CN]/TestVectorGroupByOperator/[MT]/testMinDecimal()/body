{
  testAggregateDecimal("min",2,Arrays.asList(new Object[]{new Decimal128(1),new Decimal128(2),new Decimal128(3)}),new Decimal128(1));
  testAggregateDecimal("min",2,Arrays.asList(new Object[]{new Decimal128(3),new Decimal128(2),new Decimal128(1)}),new Decimal128(1));
  testAggregateDecimal("min",2,Arrays.asList(new Object[]{new Decimal128(2),new Decimal128(1),new Decimal128(3)}),new Decimal128(1));
}
