{
  if (arguments[i].getCategory() != ObjectInspector.Category.PRIMITIVE) {
    throw new UDFArgumentTypeException(i,"Only primitive type arguments are accepted but " + arguments[i].getTypeName() + " is passed.");
  }
  Converter converter=ObjectInspectorConverters.getConverter((PrimitiveObjectInspector)arguments[i],PrimitiveObjectInspectorFactory.writableStringObjectInspector);
  return converter;
}
