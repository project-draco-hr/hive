{
  StringBuffer buf=new StringBuffer();
  buf.append(getOpenTxns().getTxn_high_water_mark());
  Set<Long> openTxns=getOpenTxns().getOpen_txns();
  if (openTxns != null && openTxns.size() > 0) {
    for (    long txn : openTxns) {
      buf.append(':');
      buf.append(txn);
    }
  }
 else {
    buf.append(':');
  }
  return buf.toString();
}
