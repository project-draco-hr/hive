{
  String expected=DummyJdoConnectionUrlHook.newUrl;
  String actual=arg0.get(HiveConf.ConfVars.METASTORECONNECTURLKEY.varname);
  Assert.assertEquals("The expected URL used by JDO to connect to the metastore: " + expected + " did not match the actual value when the Raw Store was initialized: "+ actual,expected,actual);
}
