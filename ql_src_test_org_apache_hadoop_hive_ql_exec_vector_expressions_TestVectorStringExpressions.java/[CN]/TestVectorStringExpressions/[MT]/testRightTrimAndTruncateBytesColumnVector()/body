  StringExpr.rightTrimAndTruncate(outV,i,blanksBoth,0,blanksBoth.length,6);
  expectedResultLen=5;
  Assert.assertTrue(vectorEqual(outV,i,blanksBoth,0,expectedResultLen));
  Assert.assertTrue(vectorCharacterCount(outV,i) == 5);
  i++;
