  StringExpr.rightTrimAndTruncate(outV,i,sentenceOne,0,sentenceOneLen - 3,6);
  expectedResultLen=13;
  Assert.assertTrue(vectorEqual(outV,i,sentenceOne,0,expectedResultLen));
  i++;
  sentenceTwo=new byte[100];
