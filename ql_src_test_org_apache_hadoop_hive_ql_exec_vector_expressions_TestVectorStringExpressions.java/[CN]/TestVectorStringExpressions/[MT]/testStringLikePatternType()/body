{
  FilterStringColLikeStringScalar expr;
  expr=new FilterStringColLikeStringScalar(0,new Text("abc%"));
  Assert.assertEquals(FilterStringColLikeStringScalar.PatternType.BEGIN,expr.getType());
  expr=new FilterStringColLikeStringScalar(0,new Text("%abc"));
  Assert.assertEquals(FilterStringColLikeStringScalar.PatternType.END,expr.getType());
  expr=new FilterStringColLikeStringScalar(0,new Text("%abc%"));
  Assert.assertEquals(FilterStringColLikeStringScalar.PatternType.MIDDLE,expr.getType());
  expr=new FilterStringColLikeStringScalar(0,new Text("%abc%de"));
  Assert.assertEquals(FilterStringColLikeStringScalar.PatternType.COMPLEX,expr.getType());
  expr=new FilterStringColLikeStringScalar(0,new Text("abc"));
  Assert.assertEquals(FilterStringColLikeStringScalar.PatternType.NONE,expr.getType());
}
