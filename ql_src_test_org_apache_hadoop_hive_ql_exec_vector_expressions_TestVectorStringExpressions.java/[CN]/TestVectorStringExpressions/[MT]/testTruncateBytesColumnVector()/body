  i++;
  int start;
  byte[] sentenceOnePaddedLeft=new byte[100];
  start=addPads(sentenceOnePaddedLeft,0,3);
  int sentenceOnePaddedLeftLen=addMultiByteCharSentenceOne(sentenceOnePaddedLeft,start);
