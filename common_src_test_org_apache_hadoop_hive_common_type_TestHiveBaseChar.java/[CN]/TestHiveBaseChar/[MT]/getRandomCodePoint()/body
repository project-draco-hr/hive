{
  int codePoint;
  if (rnd.nextDouble() < 0.50) {
    codePoint=32 + rnd.nextInt(90);
  }
 else {
    codePoint=getRandomSupplementaryChar();
  }
  if (!Character.isValidCodePoint(codePoint)) {
    System.out.println(Integer.toHexString(codePoint) + " is not a valid code point");
  }
  return codePoint;
}
