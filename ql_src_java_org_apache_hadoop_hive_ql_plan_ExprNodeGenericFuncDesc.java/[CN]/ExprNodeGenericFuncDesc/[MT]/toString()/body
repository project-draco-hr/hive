{
  StringBuilder sb=new StringBuilder();
  sb.append(genericUDF.getClass().getSimpleName());
  sb.append("(");
  if (chidren != null) {
    for (int i=0; i < chidren.size(); i++) {
      if (i > 0) {
        sb.append(", ");
      }
      sb.append(chidren.get(i));
    }
  }
  sb.append(")");
  return sb.toString();
}
