{
  StringBuilder sb=new StringBuilder();
  sb.append(genericUDF.getClass().toString());
  sb.append("(");
  for (int i=0; i < chidren.size(); i++) {
    if (i > 0) {
      sb.append(", ");
    }
    sb.append(chidren.get(i).toString());
  }
  sb.append("(");
  sb.append(")");
  return sb.toString();
}
