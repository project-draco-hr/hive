{
  boolean found=false;
  for (  OOR_VALUE_OPT_VALUES v : OOR_VALUE_OPT_VALUES.values()) {
    if (v.name().equalsIgnoreCase(optVal)) {
      found=true;
      break;
    }
  }
  if (!found) {
    throw new FrontendException("Unexpected value for '" + ON_OOR_VALUE_OPT + "' found: "+ optVal);
  }
}
