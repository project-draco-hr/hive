{
  if ((a == null) || (b == null)) {
    return null;
  }
  HiveDecimal av=a.getHiveDecimal();
  HiveDecimal bv=b.getHiveDecimal();
  if (bv.compareTo(HiveDecimal.ZERO) == 0) {
    return null;
  }
  HiveDecimal dec=av.remainder(bv);
  if (dec == null) {
    return null;
  }
  decimalWritable.set(dec);
  return decimalWritable;
}
