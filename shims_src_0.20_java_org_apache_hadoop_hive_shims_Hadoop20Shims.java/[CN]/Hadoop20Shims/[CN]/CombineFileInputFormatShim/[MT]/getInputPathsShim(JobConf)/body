{
  Path[] paths;
  try {
    paths=FileInputFormat.getInputPaths(conf);
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  Path[] newPaths=new Path[paths.length];
  for (int pos=0; pos < paths.length; pos++) {
    newPaths[pos]=new Path(paths[pos].toUri().getPath());
  }
  return newPaths;
}
