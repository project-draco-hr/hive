{
  try {
    Statement stmt=conn.createStatement();
    String delete="DELETE FROM " + JDBCStatsSetupConstants.PART_STAT_TABLE_NAME + " WHERE "+ JDBCStatsSetupConstants.PART_STAT_ID_COLUMN_NAME+ " LIKE '"+ rowID+ "%'";
    stmt.executeUpdate(delete);
    stmt.close();
    return closeConnection();
  }
 catch (  SQLException e) {
    LOG.error("Error during publishing aggregation. " + e);
    return false;
  }
}
