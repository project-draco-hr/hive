{
  LlapNodeId llapNodeId=getNodeIdForContainer(containerId);
  if (llapNodeId != null) {
    BiMap<ContainerId,TezTaskAttemptID> bMap=nodeMap.get(llapNodeId);
    if (bMap != null) {
synchronized (bMap) {
        return bMap.get(containerId);
      }
    }
 else {
      return null;
    }
  }
 else {
    return null;
  }
}
