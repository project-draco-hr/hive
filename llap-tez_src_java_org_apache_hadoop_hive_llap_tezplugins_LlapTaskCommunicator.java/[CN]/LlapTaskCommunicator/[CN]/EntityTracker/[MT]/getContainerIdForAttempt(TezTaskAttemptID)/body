{
  LlapNodeId llapNodeId=getNodeIdForTaskAttempt(taskAttemptId);
  if (llapNodeId != null) {
    BiMap<TezTaskAttemptID,ContainerId> bMap=nodeMap.get(llapNodeId).inverse();
    if (bMap != null) {
synchronized (bMap) {
        return bMap.get(taskAttemptId);
      }
    }
 else {
      return null;
    }
  }
 else {
    return null;
  }
}
