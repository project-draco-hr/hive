{
  if (!isActive())   return;
  String createPath=null;
  try {
    createPath=getSlotPath(currentSlot);
    LOG.info("Attempting to create " + createPath);
    client.create().withMode(CreateMode.EPHEMERAL).inBackground(backgroundCallback).forPath(createPath,data);
  }
 catch (  Exception e) {
    LOG.error("Creating node. Path: " + createPath,e);
    throw new RuntimeException(e);
  }
}
