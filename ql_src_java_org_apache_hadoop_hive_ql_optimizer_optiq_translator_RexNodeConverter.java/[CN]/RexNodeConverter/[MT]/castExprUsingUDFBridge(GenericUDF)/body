{
  boolean castExpr=false;
  if (gUDF != null && gUDF instanceof GenericUDFBridge) {
    String udfClassName=((GenericUDFBridge)gUDF).getUdfClassName();
    if (udfClassName != null) {
      int sp=udfClassName.lastIndexOf('.');
      if (sp >= 0 & (sp + 1) < udfClassName.length()) {
        udfClassName=udfClassName.substring(sp + 1);
        if (udfClassName.equals("UDFToBoolean") || udfClassName.equals("UDFToByte") || udfClassName.equals("UDFToDouble")|| udfClassName.equals("UDFToInteger")|| udfClassName.equals("UDFToLong")|| udfClassName.equals("UDFToShort")|| udfClassName.equals("UDFToFloat")|| udfClassName.equals("UDFToString"))         castExpr=true;
      }
    }
  }
  return castExpr;
}
