{
  Preconditions.checkArgument(primaryKeyOffset >= 0,"A valid offset to the mapping for the Accumulo RowID is required, received " + primaryKeyOffset);
  this.rowIdOffset=primaryKeyOffset;
  this.output=new ByteStream.Output();
  this.serDeParams=serDeParams;
  this.mappings=mappings;
  this.visibility=visibility;
  this.rowIdFactory=rowIdFactory;
}
