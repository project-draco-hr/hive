{
switch (fieldID) {
case VERSION:
    if (value == null) {
      unsetVersion();
    }
 else {
      setVersion((String)value);
    }
  break;
case COMMENTS:
if (value == null) {
  unsetComments();
}
 else {
  setComments((String)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
