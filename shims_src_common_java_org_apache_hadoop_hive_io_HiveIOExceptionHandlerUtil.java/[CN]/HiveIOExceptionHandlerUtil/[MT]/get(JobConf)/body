{
  HiveIOExceptionHandlerChain cache=HiveIOExceptionHandlerUtil.handlerChainInstance.get();
  if (cache == null) {
    HiveIOExceptionHandlerChain toSet=HiveIOExceptionHandlerChain.getHiveIOExceptionHandlerChain(job);
    handlerChainInstance.set(toSet);
    cache=HiveIOExceptionHandlerUtil.handlerChainInstance.get();
  }
  return cache;
}
