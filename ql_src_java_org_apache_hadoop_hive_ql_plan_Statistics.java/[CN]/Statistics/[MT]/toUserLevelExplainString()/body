{
  StringBuilder sb=new StringBuilder();
  sb.append("rows=");
  sb.append(numRows);
  if (runTimeNumRows >= 0) {
    sb.append("/" + runTimeNumRows);
  }
  sb.append(" width=");
  if (numRows != 0) {
    sb.append(dataSize / numRows);
  }
 else {
    sb.append("-1");
  }
  return sb.toString();
}
