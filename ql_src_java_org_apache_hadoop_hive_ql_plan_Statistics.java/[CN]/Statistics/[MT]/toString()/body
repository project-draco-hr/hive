{
  StringBuilder sb=new StringBuilder();
  sb.append("Num rows: ");
  sb.append(numRows);
  if (runTimeNumRows >= 0) {
    sb.append("/" + runTimeNumRows);
  }
  sb.append(" Data size: ");
  sb.append(dataSize);
  sb.append(" Basic stats: ");
  sb.append(basicStatsState);
  sb.append(" Column stats: ");
  sb.append(columnStatsState);
  return sb.toString();
}
