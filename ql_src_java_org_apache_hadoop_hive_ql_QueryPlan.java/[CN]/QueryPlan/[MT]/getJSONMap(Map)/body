{
  if (map == null) {
    return "null";
  }
  StringBuilder sb=new StringBuilder();
  sb.append("{");
  for (  Object entry : map.entrySet()) {
    Map.Entry e=(Map.Entry)entry;
    sb.append(getJSONKeyValue(e.getKey(),e.getValue()));
  }
  sb.deleteCharAt(sb.length() - 1);
  sb.append("}");
  return sb.toString();
}
