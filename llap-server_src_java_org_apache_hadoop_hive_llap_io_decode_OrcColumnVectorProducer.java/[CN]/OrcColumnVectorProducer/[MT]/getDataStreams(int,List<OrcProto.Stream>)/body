{
  List<OrcProto.Stream> result=Lists.newArrayList();
  for (  OrcProto.Stream stream : streams) {
    if (stream.getColumn() == colIx) {
switch (stream.getKind()) {
case PRESENT:
case DATA:
case LENGTH:
case DICTIONARY_DATA:
case SECONDARY:
        result.add(stream);
default :
    }
  }
}
return result;
}
