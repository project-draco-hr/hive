{
  if (present != null) {
    if (_isFileCompressed) {
      index.getNext();
    }
    present.seek(index);
  }
  if (_dataStream.available() > 0) {
    if (_isFileCompressed) {
      index.getNext();
    }
    stream.seek(index);
  }
  if (lengths != null && _lengthsStream.available() > 0) {
    if (_isFileCompressed) {
      index.getNext();
    }
    lengths.seek(index);
  }
}
