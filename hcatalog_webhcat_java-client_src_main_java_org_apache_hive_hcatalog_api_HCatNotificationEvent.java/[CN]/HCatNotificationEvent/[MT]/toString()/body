{
  StringBuilder buf=new StringBuilder();
  buf.append("eventId:");
  buf.append(eventId);
  buf.append(" eventTime:");
  buf.append(eventTime);
  buf.append(" eventType:<");
  buf.append(eventType);
  buf.append("> dbName:<");
  buf.append(dbName);
  buf.append("> tableName:<");
  buf.append(tableName);
  buf.append("> message:<");
  buf.append(message);
  buf.append(">");
  return buf.toString();
}
