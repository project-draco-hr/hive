{
  int failedThreshold=conf.getIntVar(HiveConf.ConfVars.COMPACTOR_INITIATOR_FAILED_THRESHOLD);
  int failedRetention=conf.getIntVar(HiveConf.ConfVars.COMPACTOR_HISTORY_RETENTION_FAILED);
  if (failedRetention < failedThreshold) {
    LOG.warn("Invalid configuration " + HiveConf.ConfVars.COMPACTOR_INITIATOR_FAILED_THRESHOLD.varname + "="+ failedRetention+ " < "+ HiveConf.ConfVars.COMPACTOR_HISTORY_RETENTION_FAILED+ "="+ failedRetention+ ".  Will use "+ HiveConf.ConfVars.COMPACTOR_INITIATOR_FAILED_THRESHOLD.varname+ "="+ failedRetention);
    failedRetention=failedThreshold;
  }
  return failedRetention;
}
