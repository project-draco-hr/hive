{
switch (ci.state) {
case ATTEMPTED_STATE:
    if (--rc.attemptedRetention < 0) {
      deleteSet.add(ci.id);
    }
  break;
case FAILED_STATE:
if (--rc.failedRetention < 0) {
  deleteSet.add(ci.id);
}
break;
case SUCCEEDED_STATE:
if (--rc.succeededRetention < 0) {
deleteSet.add(ci.id);
}
break;
default :
}
}
