{
  URL hive_l4j=ExecDriver.class.getClassLoader().getResource(SessionState.HIVE_EXEC_L4J);
  if (hive_l4j == null)   hive_l4j=ExecDriver.class.getClassLoader().getResource(SessionState.HIVE_L4J);
  if (hive_l4j != null) {
    System.setProperty(HiveConf.ConfVars.HIVEQUERYID.toString(),HiveConf.getVar(conf,HiveConf.ConfVars.HIVEQUERYID));
    LogManager.resetConfiguration();
    PropertyConfigurator.configure(hive_l4j);
  }
}
