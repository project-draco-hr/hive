{
  long r=0;
  FileSystem fs=FileSystem.get(job);
  for (  String path : work.getPathToAliases().keySet()) {
    try {
      ContentSummary cs=fs.getContentSummary(new Path(path));
      r+=cs.getLength();
    }
 catch (    IOException e) {
      LOG.info("Cannot get size of " + path + ". Safely ignored.");
    }
  }
  return r;
}
