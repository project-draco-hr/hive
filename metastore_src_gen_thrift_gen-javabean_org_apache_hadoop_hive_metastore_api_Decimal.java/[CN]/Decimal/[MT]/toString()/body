{
  StringBuilder sb=new StringBuilder("Decimal(");
  boolean first=true;
  sb.append("unscaled:");
  if (this.unscaled == null) {
    sb.append("null");
  }
 else {
    org.apache.thrift.TBaseHelper.toString(this.unscaled,sb);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("scale:");
  sb.append(this.scale);
  first=false;
  sb.append(")");
  return sb.toString();
}
