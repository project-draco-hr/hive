{
  Queue<ASTNode> queue=new ArrayDeque<ASTNode>();
  queue.add(root);
  while (!queue.isEmpty()) {
    ASTNode current=queue.remove();
    if (predicate.apply(current)) {
      return true;
    }
 else {
      if (current.getChildCount() > 0) {
        for (        Node child : current.getChildren()) {
          queue.add((ASTNode)child);
        }
      }
    }
  }
  return false;
}
