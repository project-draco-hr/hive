{
  byte[] data;
  long recid;
  Location physRowId;
  checkIfClosed();
  data=serializer.serialize(obj);
  physRowId=_physMgr.insert(data,0,data.length);
  recid=_logMgr.insert(physRowId).toLong();
  if (DEBUG) {
    System.out.println("BaseRecordManager.insert() recid " + recid + " length "+ data.length);
  }
  return recid;
}
