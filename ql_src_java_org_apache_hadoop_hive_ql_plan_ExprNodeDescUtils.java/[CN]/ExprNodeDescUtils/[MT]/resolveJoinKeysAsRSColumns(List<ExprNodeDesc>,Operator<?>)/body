{
  ArrayList<ExprNodeDesc> result=new ArrayList<ExprNodeDesc>(sourceList.size());
  for (  ExprNodeDesc source : sourceList) {
    ExprNodeDesc newExpr=resolveJoinKeysAsRSColumns(source,reduceSinkOp);
    if (newExpr == null) {
      return null;
    }
    result.add(newExpr);
  }
  return result;
}
