{
  if (requiredPrivs == null) {
    return null;
  }
  List<Privilege> privList=new ArrayList<Privilege>();
  for (  Privilege priv : requiredPrivs) {
    if (priv.equals(Privilege.DROP)) {
      hasDropPrivilege=true;
    }
 else {
      privList.add(priv);
    }
  }
  return privList.toArray(new Privilege[0]);
}
