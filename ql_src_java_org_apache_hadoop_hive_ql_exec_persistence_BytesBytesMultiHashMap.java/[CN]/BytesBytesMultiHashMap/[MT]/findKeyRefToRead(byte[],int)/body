{
  final int bucketMask=(refs.length - 1);
  int hashCode=writeBuffers.hashCode(key,0,length);
  int slot=hashCode & bucketMask;
  long probeSlot=slot;
  int i=0;
  while (true) {
    long ref=refs[slot];
    if (ref == 0) {
      return 0;
    }
    if (isSameKey(key,length,ref,hashCode)) {
      return ref;
    }
    probeSlot+=(++i);
    if (i > largestNumberOfSteps) {
      return 0;
    }
    slot=(int)(probeSlot & bucketMask);
  }
}
