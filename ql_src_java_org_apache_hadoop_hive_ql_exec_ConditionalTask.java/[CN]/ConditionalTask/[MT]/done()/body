{
  boolean ret=true;
  List<Task<? extends Serializable>> parentTasks=this.getParentTasks();
  if (parentTasks != null) {
    for (    Task<? extends Serializable> par : parentTasks)     ret=ret && par.done();
  }
  List<Task<? extends Serializable>> retTasks;
  if (resolved)   retTasks=this.resTasks;
 else   retTasks=getListTasks();
  if (ret && retTasks != null) {
    for (    Task<? extends Serializable> tsk : retTasks)     ret=ret && tsk.done();
  }
  return ret;
}
