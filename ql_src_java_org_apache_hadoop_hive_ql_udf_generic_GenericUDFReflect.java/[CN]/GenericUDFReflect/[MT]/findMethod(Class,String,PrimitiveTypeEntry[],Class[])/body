{
  for (  Method method : clazz.getMethods()) {
    if (!method.getName().equals(name) || method.getReturnType() != String.class || method.getParameterTypes().length != parameterTypes.length) {
      continue;
    }
    boolean match=true;
    Class<?>[] types=method.getParameterTypes();
    for (int i=0; i < parameterTypes.length; i++) {
      if (types[i] != parameterTypes[i].primitiveJavaType && types[i] != parameterTypes[i].primitiveJavaClass) {
        match=false;
        break;
      }
    }
    if (match) {
      return method;
    }
  }
  return clazz.getMethod(name,parameterClasses);
}
