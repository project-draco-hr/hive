{
  StringBuffer buff=new StringBuffer(100);
  if (items.length == 0)   return "".getBytes();
  int i=0;
  for (; i < items.length - 1; ++i) {
    if (items[i] != null) {
      buff.append(items[i]);
    }
    buff.append(separator);
  }
  if (items[i] != null) {
    buff.append(items[i]);
  }
  return buff.toString().getBytes();
}
