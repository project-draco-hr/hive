{
  super.write(obj);
  if (obj != null) {
    ListObjectInspector insp=(ListObjectInspector)inspector;
    int len=insp.getListLength(obj);
    lengths.write(len);
    if (createBloomFilter) {
      bloomFilter.addLong(len);
    }
    for (int i=0; i < len; ++i) {
      childrenWriters[0].write(insp.getListElement(obj,i));
    }
  }
}
