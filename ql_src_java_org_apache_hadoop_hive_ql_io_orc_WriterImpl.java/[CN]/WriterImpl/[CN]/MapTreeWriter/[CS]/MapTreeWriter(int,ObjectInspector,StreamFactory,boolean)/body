{
  super(columnId,inspector,writer,nullable);
  MapObjectInspector insp=(MapObjectInspector)inspector;
  childrenWriters=new TreeWriter[2];
  childrenWriters[0]=createTreeWriter(insp.getMapKeyObjectInspector(),writer,true);
  childrenWriters[1]=createTreeWriter(insp.getMapValueObjectInspector(),writer,true);
  lengths=new RunLengthIntegerWriter(writer.createStream(columnId,OrcProto.Stream.Kind.LENGTH),false);
  recordPosition(rowIndexPosition);
}
