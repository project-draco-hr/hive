{
  super.write(obj);
  if (obj != null) {
    StructObjectInspector insp=(StructObjectInspector)inspector;
    for (int i=0; i < fields.size(); ++i) {
      StructField field=fields.get(i);
      TreeWriter writer=childrenWriters[i];
      writer.write(insp.getStructFieldData(obj,field));
    }
  }
}
