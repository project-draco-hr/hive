{
  super.write(obj);
  if (obj != null) {
    long val;
    if (intInspector != null) {
      val=intInspector.get(obj);
    }
 else     if (longInspector != null) {
      val=longInspector.get(obj);
    }
 else {
      val=shortInspector.get(obj);
    }
    indexStatistics.updateInteger(val);
    if (createBloomFilter) {
      bloomFilter.addLong(val);
    }
    writer.write(val);
  }
}
