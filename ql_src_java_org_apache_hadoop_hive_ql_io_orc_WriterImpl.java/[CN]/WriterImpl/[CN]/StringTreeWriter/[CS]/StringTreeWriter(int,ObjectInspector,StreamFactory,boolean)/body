{
  super(columnId,inspector,writer,nullable);
  stringOutput=writer.createStream(id,OrcProto.Stream.Kind.DICTIONARY_DATA);
  lengthOutput=new RunLengthIntegerWriter(writer.createStream(id,OrcProto.Stream.Kind.LENGTH),false);
  rowOutput=new RunLengthIntegerWriter(writer.createStream(id,OrcProto.Stream.Kind.DATA),false);
  recordPosition(rowIndexPosition);
  rowIndexValueCount.add(0L);
  buildIndex=writer.buildIndex();
  directStreamOutput=writer.createStream(id,OrcProto.Stream.Kind.DATA);
  directLengthOutput=new RunLengthIntegerWriter(writer.createStream(id,OrcProto.Stream.Kind.LENGTH),false);
  dictionaryKeySizeThreshold=writer.getConfiguration().getFloat(HiveConf.ConfVars.HIVE_ORC_DICTIONARY_KEY_SIZE_THRESHOLD.varname,HiveConf.ConfVars.HIVE_ORC_DICTIONARY_KEY_SIZE_THRESHOLD.defaultFloatVal);
}
