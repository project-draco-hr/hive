{
  String writeFormat=writer.getConfiguration().get(HiveConf.ConfVars.HIVE_ORC_WRITE_FORMAT.varname);
  if (writeFormat == null) {
    LOG.warn("ORC write format not defined. Using 0.12 ORC write format.");
    return true;
  }
  if (writeFormat.equals(HiveConf.ConfVars.HIVE_ORC_WRITE_FORMAT.defaultVal)) {
    LOG.info("Using 0.11 ORC write format.");
    return false;
  }
  LOG.info("Using 0.12 ORC write format.");
  return true;
}
