{
  long total=0;
switch (oi.getCategory()) {
case PRIMITIVE:
    total+=getRawDataSizeFromPrimitives(child,oi);
  break;
case LIST:
case MAP:
case UNION:
case STRUCT:
for (TreeWriter tw : child.childrenWriters) {
  total+=getRawDataSizeFromInspectors(tw,tw.inspector);
}
break;
default :
LOG.debug("Unknown object inspector category.");
break;
}
return total;
}
