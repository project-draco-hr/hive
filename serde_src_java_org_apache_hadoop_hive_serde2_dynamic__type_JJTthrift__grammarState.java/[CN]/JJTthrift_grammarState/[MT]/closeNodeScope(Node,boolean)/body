{
  if (condition) {
    int a=nodeArity();
    mk=((Integer)marks.pop()).intValue();
    while (a-- > 0) {
      Node c=popNode();
      c.jjtSetParent(n);
      n.jjtAddChild(c,a);
    }
    n.jjtClose();
    pushNode(n);
    node_created=true;
  }
 else {
    mk=((Integer)marks.pop()).intValue();
    node_created=false;
  }
}
