{
  super(stats);
  OrcProto.IntegerStatistics intStat=stats.getIntStatistics();
  if (intStat.hasMinimum()) {
    hasMinimum=true;
    minimum=intStat.getMinimum();
  }
  if (intStat.hasMaximum()) {
    maximum=intStat.getMaximum();
  }
  if (intStat.hasSum()) {
    sum=intStat.getSum();
  }
 else {
    overflow=true;
  }
}
