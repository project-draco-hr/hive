{
  if (other instanceof BinaryColumnStatistics) {
    BinaryStatisticsImpl bin=(BinaryStatisticsImpl)other;
    sum+=bin.sum;
  }
 else {
    if (isStatsExists() && sum != 0) {
      throw new IllegalArgumentException("Incompatible merging of binary column statistics");
    }
  }
  super.merge(other);
}
