{
  try {
    hmsClient.dropTable(checkDB(dbName),tableName,true,ifExists);
  }
 catch (  NoSuchObjectException e) {
    if (!ifExists) {
      throw new ObjectNotFoundException("NoSuchObjectException while dropping table.",e);
    }
  }
catch (  MetaException e) {
    throw new HCatException("MetaException while dropping table.",e);
  }
catch (  TException e) {
    throw new ConnectionFailureException("TException while dropping table.",e);
  }
}
