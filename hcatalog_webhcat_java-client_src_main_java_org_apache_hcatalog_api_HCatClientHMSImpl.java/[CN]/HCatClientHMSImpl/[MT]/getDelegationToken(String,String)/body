{
  String token=null;
  try {
    token=hmsClient.getDelegationToken(owner,renewerKerberosPrincipalName);
  }
 catch (  MetaException e) {
    throw new HCatException("MetaException while getting delegation token.",e);
  }
catch (  TException e) {
    throw new ConnectionFailureException("TException while getting delegation token.",e);
  }
  return token;
}
