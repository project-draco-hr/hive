{
  try {
    DB.createDB("foo1",conf_);
    DB.createDB("foo2",conf_);
    assertTrue(MetaStore.dbExists("foo1",conf_) == true);
    assertTrue(MetaStore.dbExists("foo2",conf_) == true);
    assertTrue(MetaStore.dbExists("foo3",conf_) == false);
    cleanup();
  }
 catch (  MetaException e) {
    e.printStackTrace();
    throw e;
  }
}
