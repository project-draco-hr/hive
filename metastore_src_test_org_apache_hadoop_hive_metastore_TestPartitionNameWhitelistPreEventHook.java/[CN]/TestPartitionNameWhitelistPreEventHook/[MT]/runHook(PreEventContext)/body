{
  Configuration config=new Configuration();
  config.set(HiveConf.ConfVars.METASTORE_PARTITION_NAME_WHITELIST_PATTERN.varname,"[\\x20-\\x7E&&[^,]]*");
  PartitionNameWhitelistPreEventListener hook=new PartitionNameWhitelistPreEventListener(config);
  try {
    hook.onEvent(event);
  }
 catch (  Exception e) {
    return false;
  }
  return true;
}
