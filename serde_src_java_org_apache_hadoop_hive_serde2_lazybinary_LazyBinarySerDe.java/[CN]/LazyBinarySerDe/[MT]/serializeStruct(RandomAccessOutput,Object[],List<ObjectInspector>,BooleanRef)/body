{
  int lasti=0;
  byte nullByte=0;
  int size=fieldData.length;
  for (int i=0; i < size; i++) {
    if (null != fieldData[i]) {
      nullByte|=1 << (i % 8);
    }
    if (7 == i % 8 || i == size - 1) {
      byteStream.write(nullByte);
      for (int j=lasti; j <= i; j++) {
        serialize(byteStream,fieldData[j],fieldOis.get(j),false,warnedOnceNullMapKey);
      }
      lasti=i + 1;
      nullByte=0;
    }
  }
}
