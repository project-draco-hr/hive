{
  if (node != null) {
    if (isApplicable(node)) {
      init(node);
      return;
    }
 else {
      for (int i=1; i < node.getChildCount(); ++i) {
        ASTNode child=(ASTNode)node.getChild(i);
        if (isApplicable(child)) {
          init(child);
          return;
        }
      }
    }
  }
}
