{
switch (key) {
case REPL_SCOPE:
switch (getScope()) {
case MD_ONLY:
      return "metadata";
case REPL:
    return "all";
case NO_REPL:
  return "none";
}
case EVENT_ID:
return getReplicationState();
case CURR_STATE_ID:
return getCurrentReplicationState();
case NOOP:
return String.valueOf(isNoop());
}
return null;
}
