{
  int numFns=tabDef.getWindowFunctions().size();
  rollingPart.reset();
  for (int i=0; i < fnOutputs.length; i++) {
    fnOutputs[i].clear();
  }
  for (int i=0; i < numFns; i++) {
    WindowFunctionDef wFn=tabDef.getWindowFunctions().get(i);
    aggBuffers[i]=wFn.getWFnEval().getNewAggregationBuffer();
  }
}
