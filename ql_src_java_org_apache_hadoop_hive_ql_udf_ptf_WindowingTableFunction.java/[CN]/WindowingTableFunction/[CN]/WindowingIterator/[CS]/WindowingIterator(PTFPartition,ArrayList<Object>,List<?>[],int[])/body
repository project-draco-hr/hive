{
  this.iPart=iPart;
  this.output=output;
  this.outputFromPivotFunctions=outputFromPivotFunctions;
  this.wFnsToProcess=wFnsToProcess;
  this.currIdx=0;
  wTFnDef=(WindowTableFunctionDef)getTableDef();
