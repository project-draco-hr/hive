{
switch (timeunit) {
case DAYS:
    return "day";
case HOURS:
  return "hour";
case MINUTES:
return "min";
case SECONDS:
return "sec";
case MILLISECONDS:
return "msec";
case MICROSECONDS:
return "usec";
case NANOSECONDS:
return "nsec";
}
throw new IllegalArgumentException("Invalid timeunit " + timeunit);
}
