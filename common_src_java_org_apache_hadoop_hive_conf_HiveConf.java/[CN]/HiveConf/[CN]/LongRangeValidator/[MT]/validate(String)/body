{
  try {
    if (value == null) {
      return "Value cannot be null";
    }
    value=value.trim();
    long lvalue=Long.parseLong(value);
    if (lvalue < lower || lvalue > upper) {
      return "Invalid value  " + value + ", which should be in between "+ lower+ " and "+ upper;
    }
  }
 catch (  NumberFormatException e) {
    return e.toString();
  }
  return null;
}
