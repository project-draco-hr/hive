{
  Random r=new Random(5678);
  int c=0;
  testVectorSerializeRow(c++,r,SerializationType.BINARY_SORTABLE);
  testVectorSerializeRow(c++,r,SerializationType.LAZY_BINARY);
  testVectorSerializeRow(c++,r,SerializationType.LAZY_SIMPLE);
  testVectorDeserializeRow(c++,r,SerializationType.BINARY_SORTABLE,false,false,false);
  testVectorDeserializeRow(c++,r,SerializationType.BINARY_SORTABLE,true,false,false);
  testVectorDeserializeRow(c++,r,SerializationType.BINARY_SORTABLE,false,false,true);
  testVectorDeserializeRow(c++,r,SerializationType.BINARY_SORTABLE,true,false,true);
  testVectorDeserializeRow(c++,r,SerializationType.BINARY_SORTABLE,false,true,false);
  testVectorDeserializeRow(c++,r,SerializationType.BINARY_SORTABLE,true,true,false);
  testVectorDeserializeRow(c++,r,SerializationType.BINARY_SORTABLE,false,true,true);
  testVectorDeserializeRow(c++,r,SerializationType.BINARY_SORTABLE,true,true,true);
  testVectorDeserializeRow(c++,r,SerializationType.LAZY_BINARY,false,false,false);
  testVectorDeserializeRow(c++,r,SerializationType.LAZY_BINARY,false,false,true);
  testVectorDeserializeRow(c++,r,SerializationType.LAZY_SIMPLE,false,false,false);
  testVectorDeserializeRow(c++,r,SerializationType.LAZY_SIMPLE,false,false,true);
  testVectorDeserializeRow(c++,r,SerializationType.LAZY_SIMPLE,true,false,false);
  testVectorDeserializeRow(c++,r,SerializationType.LAZY_SIMPLE,true,false,true);
}
