{
  ArrayList<Object> values=StartFinishHook.extractValues(sess,inputs,outputs,ugi);
  String query="INSERT INTO query_time_log SET " + "query = ?, " + "query_type = ?, "+ "inputs = ?, "+ "outputs = ?, "+ "query_id = ?, "+ "user_info = ?, "+ "start_time = now()";
  HiveConf conf=sess.getConf();
  HookUtils.runInsert(conf,urlFactory,query,values,5);
}
