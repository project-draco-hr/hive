{
  ArrayList<Object> values=StartFinishHook.extractValues(sess,inputs,outputs,ugi);
  values.addAll(values);
  String valueString="query = ?, " + "query_type = ?, " + "inputs = ?, "+ "outputs = ?, "+ "query_id = ?, "+ "user_info = ?, "+ "finish_time = now()";
  String query="INSERT INTO query_time_log SET " + valueString + " ON DUPLICATE KEY UPDATE "+ valueString;
  HiveConf conf=sess.getConf();
  HookUtils.runInsert(conf,urlFactory,query,values,HookUtils.getSqlNumRetry(conf));
}
