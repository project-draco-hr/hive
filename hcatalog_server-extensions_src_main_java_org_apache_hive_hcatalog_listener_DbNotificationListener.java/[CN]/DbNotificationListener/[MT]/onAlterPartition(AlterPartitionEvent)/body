{
  Partition before=partitionEvent.getOldPartition();
  Partition after=partitionEvent.getNewPartition();
  NotificationEvent event=new NotificationEvent(0,now(),HCatConstants.HCAT_ALTER_PARTITION_EVENT,msgFactory.buildAlterPartitionMessage(before,after).toString());
  event.setDbName(before.getDbName());
  event.setTableName(before.getTableName());
  enqueue(event);
}
