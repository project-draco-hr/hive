{
  StringBuilder sb=new StringBuilder();
  sb.append(Long.toHexString(relativeOffsetWord));
  sb.append(", ");
  long nextRelativeOffset=(relativeOffsetWord & NextRelativeValueOffset.bitMask) >> NextRelativeValueOffset.bitShift;
  sb.append("nextRelativeOffset ");
  sb.append(nextRelativeOffset);
  sb.append(" (");
  sb.append(Long.toHexString(nextRelativeOffset));
  sb.append("), ");
  boolean isNextLast=((relativeOffsetWord & IsNextValueLastFlag.flagOnMask) != 0);
  sb.append("isNextLast ");
  sb.append(isNextLast);
  sb.append(", ");
  boolean isNextValueLengthSmall=((relativeOffsetWord & IsNextValueLengthSmallFlag.flagOnMask) != 0);
  sb.append("isNextValueLengthSmall ");
  sb.append(isNextValueLengthSmall);
  return sb.toString();
}
