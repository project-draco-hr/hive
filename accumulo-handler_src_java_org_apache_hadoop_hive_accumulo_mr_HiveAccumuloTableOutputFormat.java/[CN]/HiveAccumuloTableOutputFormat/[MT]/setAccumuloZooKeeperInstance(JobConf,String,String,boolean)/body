{
  try {
    if (isSasl) {
      helper.setZooKeeperInstance(conf,AccumuloOutputFormat.class,zookeepers,instanceName,isSasl);
    }
 else {
      AccumuloOutputFormat.setZooKeeperInstance(conf,instanceName,zookeepers);
    }
  }
 catch (  IllegalStateException ise) {
    log.debug("Ignoring exception setting ZooKeeper instance of " + instanceName + " at "+ zookeepers,ise);
  }
}
