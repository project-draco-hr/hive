{
  T codec=null;
synchronized (pool) {
    if (pool.containsKey(codecClass)) {
      List<T> codecList=pool.get(codecClass);
      if (codecList != null) {
synchronized (codecList) {
          if (!codecList.isEmpty()) {
            codec=codecList.remove(codecList.size() - 1);
          }
        }
      }
    }
  }
  return codec;
}
