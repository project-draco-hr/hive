{
  r=new HiveObject[5];
  for (int i=0; i < 5; i++) {
    ArrayList<String> data=new ArrayList<String>();
    data.add("" + i);
    data.add("" + (i + 1));
    data.add("" + (i + 2));
    ColumnSet cs=new ColumnSet(data);
    try {
      r[i]=new TableHiveObject(cs,new columnsetSerDe());
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
  }
}
