{
  if (data == null) {
    return null;
  }
  boolean isArray=data.getClass().isArray();
  if (isArray) {
    Object[] list=(Object[])data;
    if (index < 0 || index >= list.length) {
      return null;
    }
    return list[index];
  }
 else {
    List<?> list=(List<?>)data;
    if (index < 0 || index >= list.size()) {
      return null;
    }
    return list.get(index);
  }
}
