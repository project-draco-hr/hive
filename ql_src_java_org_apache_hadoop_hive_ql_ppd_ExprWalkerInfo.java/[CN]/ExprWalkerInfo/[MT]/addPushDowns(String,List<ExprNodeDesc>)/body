{
  List<ExprNodeDesc> predicates=getPushdownPreds(alias);
  boolean isNew;
  for (  ExprNodeDesc newPred : pushDowns) {
    isNew=true;
    for (    ExprNodeDesc curPred : predicates) {
      if (curPred.isSame(newPred)) {
        isNew=false;
        break;
      }
    }
    if (isNew) {
      predicates.add(newPred);
    }
  }
}
