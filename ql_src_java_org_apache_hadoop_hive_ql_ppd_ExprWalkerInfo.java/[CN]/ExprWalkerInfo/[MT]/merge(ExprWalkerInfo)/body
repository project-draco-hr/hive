{
  if (ewi == null) {
    return;
  }
  for (  Entry<String,List<ExprNodeDesc>> e : ewi.getFinalCandidates().entrySet()) {
    List<ExprNodeDesc> predList=pushdownPreds.get(e.getKey());
    if (predList != null) {
      predList.addAll(e.getValue());
    }
 else {
      pushdownPreds.put(e.getKey(),e.getValue());
    }
  }
}
