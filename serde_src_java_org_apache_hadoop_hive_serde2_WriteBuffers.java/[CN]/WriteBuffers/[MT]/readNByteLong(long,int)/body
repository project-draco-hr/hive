{
  setReadPoint(offset);
  long v=0;
  if (isAllInOneReadBuffer(bytes)) {
    for (int i=0; i < bytes; ++i) {
      v=(v << 8) + (currentReadBuffer[currentReadOffset + i] & 0xff);
    }
    currentReadOffset+=bytes;
  }
 else {
    for (int i=0; i < bytes; ++i) {
      v=(v << 8) + (readNextByte() & 0xff);
    }
  }
  return v;
}
