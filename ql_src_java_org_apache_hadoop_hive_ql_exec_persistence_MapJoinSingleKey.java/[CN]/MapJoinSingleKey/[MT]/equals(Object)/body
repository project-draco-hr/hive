{
  if (o instanceof MapJoinSingleKey) {
    MapJoinSingleKey mObj=(MapJoinSingleKey)o;
    Object key=mObj.getObj();
    if ((obj == null) && (key == null)) {
      return true;
    }
    if ((obj != null) && (key != null)) {
      if (obj.equals(key)) {
        return true;
      }
    }
  }
  return false;
}
