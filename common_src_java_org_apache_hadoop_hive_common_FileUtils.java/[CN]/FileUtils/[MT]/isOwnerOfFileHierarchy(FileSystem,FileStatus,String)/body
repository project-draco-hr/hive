{
  if (!fileStatus.getOwner().equals(userName)) {
    return false;
  }
  if (!fileStatus.isDir()) {
    return true;
  }
  FileStatus[] childStatuses=fs.listStatus(fileStatus.getPath());
  for (  FileStatus childStatus : childStatuses) {
    if (!isOwnerOfFileHierarchy(fs,childStatus,userName)) {
      return false;
    }
  }
  return true;
}
