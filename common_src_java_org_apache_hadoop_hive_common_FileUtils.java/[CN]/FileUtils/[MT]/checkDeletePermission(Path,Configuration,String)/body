{
  if (path == null) {
    return;
  }
  final FileSystem fs=path.getFileSystem(conf);
  FileStatus stat=null;
  try {
    stat=fs.getFileStatus(path);
  }
 catch (  FileNotFoundException e) {
  }
  if (stat == null) {
    return;
  }
  checkDeletePermission(fs,Lists.newArrayList(stat),conf,user);
}
