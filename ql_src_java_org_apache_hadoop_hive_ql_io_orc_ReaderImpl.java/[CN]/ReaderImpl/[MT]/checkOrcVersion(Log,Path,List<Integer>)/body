{
  if (version.size() >= 1) {
    int major=version.get(0);
    int minor=0;
    if (version.size() >= 2) {
      minor=version.get(1);
    }
    if (major > OrcFile.MAJOR_VERSION || (major == OrcFile.MAJOR_VERSION && minor > OrcFile.MINOR_VERSION)) {
      log.warn("ORC file " + path + " was written by a future Hive version "+ versionString(version)+ ". This file may not be readable by "+ "this version of Hive.");
    }
  }
}
