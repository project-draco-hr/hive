{
  if (version.size() >= 1) {
    int major=version.get(0);
    int minor=0;
    if (version.size() >= 2) {
      minor=version.get(1);
    }
    if (major > OrcFile.Version.CURRENT.getMajor() || (major == OrcFile.Version.CURRENT.getMajor() && minor > OrcFile.Version.CURRENT.getMinor())) {
      log.warn(path + " was written by a future Hive version " + versionString(version)+ ". This file may not be readable by this version of Hive.");
    }
  }
}
