{
  LOG.info("Reading ORC rows from " + path + " with "+ options);
  boolean[] include=options.getInclude();
  if (include == null) {
    include=new boolean[footer.getTypesCount()];
    Arrays.fill(include,true);
    options.include(include);
  }
  return new RecordReaderImpl(this.getStripes(),fileSystem,path,options,footer.getTypesList(),codec,bufferSize,footer.getRowIndexStride(),conf);
}
