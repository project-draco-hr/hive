{
  this.fileSystem=fs;
  this.path=path;
  this.conf=conf;
  MetaInfoObjExtractor rInfo=new MetaInfoObjExtractor(fMetaInfo.compressionType,fMetaInfo.bufferSize,fMetaInfo.metadataSize,fMetaInfo.footerBuffer);
