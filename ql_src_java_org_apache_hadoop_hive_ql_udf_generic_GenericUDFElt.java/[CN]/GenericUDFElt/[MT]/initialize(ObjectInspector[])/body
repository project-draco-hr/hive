{
  if (arguments.length < 2) {
    throw new UDFArgumentLengthException("The function ELT(N,str1,str2,str3,...) needs at least two arguments.");
  }
  for (int i=0; i < arguments.length; i++) {
    Category category=arguments[i].getCategory();
    if (category != Category.PRIMITIVE) {
      throw new UDFArgumentTypeException(i,"The " + GenericUDFUtils.getOrdinal(i + 1) + " argument of function ELT is expected to a "+ Category.PRIMITIVE.toString().toLowerCase()+ " type, but "+ category.toString().toLowerCase()+ " is found");
    }
  }
  converters=new ObjectInspectorConverters.Converter[arguments.length];
  for (int i=0; i < arguments.length; i++) {
    if (i == 0) {
      converters[i]=ObjectInspectorConverters.getConverter(arguments[i],PrimitiveObjectInspectorFactory.writableIntObjectInspector);
    }
 else {
      converters[i]=ObjectInspectorConverters.getConverter(arguments[i],PrimitiveObjectInspectorFactory.writableStringObjectInspector);
    }
  }
  return PrimitiveObjectInspectorFactory.writableStringObjectInspector;
}
