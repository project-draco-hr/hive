{
synchronized (this) {
    LOG.info("Killing task with id {}, taskRunnerSetup={}",taskSpec.getTaskAttemptID(),(taskRunner != null));
    if (taskRunner != null) {
      killtimerWatch.start();
      LOG.info("Issuing kill to task {}" + taskSpec.getTaskAttemptID());
      taskRunner.killTask();
      shouldRunTask=false;
    }
  }
  reportTaskKilled();
}
