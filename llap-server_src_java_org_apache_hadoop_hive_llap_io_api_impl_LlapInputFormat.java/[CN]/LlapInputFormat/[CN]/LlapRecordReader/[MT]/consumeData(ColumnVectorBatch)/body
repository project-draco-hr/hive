{
  if (LlapIoImpl.LOG.isTraceEnabled()) {
    LlapIoImpl.LOG.trace("consume called; closed {}, done {}, err {}, pending {}",isClosed,isDone,pendingError,pendingData.size());
  }
synchronized (pendingData) {
    if (isClosed) {
      return;
    }
    pendingData.add(data);
    pendingData.notifyAll();
  }
}
