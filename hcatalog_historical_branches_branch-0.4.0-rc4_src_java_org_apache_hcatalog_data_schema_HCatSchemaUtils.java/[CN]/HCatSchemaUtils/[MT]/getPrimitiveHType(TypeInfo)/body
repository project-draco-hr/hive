{
switch (((PrimitiveTypeInfo)basePrimitiveTypeInfo).getPrimitiveCategory()) {
case BOOLEAN:
    return Type.BOOLEAN;
case BYTE:
  return Type.TINYINT;
case DOUBLE:
return Type.DOUBLE;
case FLOAT:
return Type.FLOAT;
case INT:
return Type.INT;
case LONG:
return Type.BIGINT;
case SHORT:
return Type.SMALLINT;
case STRING:
return Type.STRING;
case BINARY:
return Type.BINARY;
default :
throw new TypeNotPresentException(((PrimitiveTypeInfo)basePrimitiveTypeInfo).getTypeName(),null);
}
}
