{
  Object next=null;
  if (_iter != null && _iter.hasNext()) {
    next=_iter.next();
  }
 else {
    try {
      prepareNext();
    }
 catch (    IOException except) {
      throw new IterationException(except);
    }
    if (_iter != null && _iter.hasNext()) {
      return next();
    }
  }
  return next;
}
