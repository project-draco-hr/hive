{
  if (i == 0) {
    out.write('0');
    return;
  }
  boolean negative=i < 0;
  if (negative) {
    out.write('-');
  }
 else {
    i=-i;
  }
  long start=1000000000000000000L;
  while (i / start == 0) {
    start/=10;
  }
  while (start > 0) {
    out.write('0' - (int)((i / start) % 10));
    start/=10;
  }
}
