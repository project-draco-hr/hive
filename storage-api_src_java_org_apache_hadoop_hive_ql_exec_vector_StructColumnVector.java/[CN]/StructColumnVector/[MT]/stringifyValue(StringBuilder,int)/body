{
  if (isRepeating) {
    row=0;
  }
  if (noNulls || !isNull[row]) {
    buffer.append('[');
    for (int i=0; i < fields.length; ++i) {
      if (i != 0) {
        buffer.append(", ");
      }
      fields[i].stringifyValue(buffer,row);
    }
    buffer.append(']');
  }
 else {
    buffer.append("null");
  }
}
