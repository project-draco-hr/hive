{
  Descriptor other=(Descriptor)o;
  if (!mode.equals(other.mode) || (argCount != other.argCount)) {
    return false;
  }
  for (int i=0; i < argCount; i++) {
    if (!argTypes[i].equals(other.argTypes[i]) && (!argTypes[i].equals(ArgumentType.ANY) && !other.argTypes[i].equals(ArgumentType.ANY))) {
      return false;
    }
    if (!exprTypes[i].equals(other.exprTypes[i])) {
      return false;
    }
  }
  return true;
}
