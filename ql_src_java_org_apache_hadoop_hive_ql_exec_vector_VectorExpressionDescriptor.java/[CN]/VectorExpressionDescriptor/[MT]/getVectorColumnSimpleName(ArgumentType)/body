{
  if (argType == INT_FAMILY || argType == DATE || argType == INTERVAL_YEAR_MONTH) {
    return "Long";
  }
 else   if (argType == TIMESTAMP || argType == INTERVAL_DAY_TIME) {
    return "Timestamp";
  }
 else   if (argType == FLOAT_FAMILY) {
    return "Double";
  }
 else   if (argType == DECIMAL) {
    return "Decimal";
  }
 else   if (argType == STRING || argType == CHAR || argType == VARCHAR || argType == BINARY) {
    return "String";
  }
 else {
    return "None";
  }
}
