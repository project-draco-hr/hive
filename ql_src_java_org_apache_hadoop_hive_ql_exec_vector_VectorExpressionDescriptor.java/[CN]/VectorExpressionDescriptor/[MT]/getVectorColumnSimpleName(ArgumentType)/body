{
  if (argType == INT_FAMILY || argType == DATE || argType == TIMESTAMP) {
    return "Long";
  }
 else   if (argType == FLOAT_FAMILY) {
    return "Double";
  }
 else   if (argType == DECIMAL) {
    return "Decimal";
  }
 else   if (argType == STRING || argType == CHAR || argType == VARCHAR) {
    return "String";
  }
 else {
    return "None";
  }
}
