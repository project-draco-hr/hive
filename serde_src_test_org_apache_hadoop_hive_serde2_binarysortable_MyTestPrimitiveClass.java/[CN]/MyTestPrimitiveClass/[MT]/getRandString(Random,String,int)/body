{
  if (characters == null) {
    characters="ABCDEFGHIJKLMNOPQRSTUVWXYZ";
  }
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < length; i++) {
    if (characters == null) {
      sb.append((char)(r.nextInt(128)));
    }
