{
switch (readerType.getCategory()) {
case BOOLEAN:
    return input == 0 ? 0 : 1;
case BYTE:
  return (byte)input;
case SHORT:
return (short)input;
case INT:
return (int)input;
case LONG:
return input;
default :
throw new RuntimeException("Unexpected type kind " + readerType.getCategory().name());
}
}
