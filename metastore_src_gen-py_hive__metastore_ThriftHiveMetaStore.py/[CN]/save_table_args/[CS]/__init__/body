def __init__(self, d=None):
    self.table_name = None
    self.schema = None
    self.overwrite = None
    if isinstance(d, dict):
        if ('table_name' in d):
            self.table_name = d['table_name']
        if ('schema' in d):
            self.schema = d['schema']
        if ('overwrite' in d):
            self.overwrite = d['overwrite']
