def __init__(self, d=None):
    self.table_name = None
    self.columns = None
    self.partition_cols = None
    if isinstance(d, dict):
        if ('table_name' in d):
            self.table_name = d['table_name']
        if ('columns' in d):
            self.columns = d['columns']
        if ('partition_cols' in d):
            self.partition_cols = d['partition_cols']
