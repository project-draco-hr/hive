def process_drop(self, seqid, iprot, oprot):
    args = drop_args()
    args.read(iprot)
    iprot.readMessageEnd()
    result = drop_result()
    try:
        self._handler.drop(args.table_name, args.delete_data)
    except MetaException as ouch:
        result.ouch = ouch
    except UnknownTableException as ouch2:
        result.ouch2 = ouch2
    oprot.writeMessageBegin('drop', TMessageType.REPLY, seqid)
    result.write(oprot)
    oprot.writeMessageEnd()
    oprot.trans.flush()
