{
  StringBuilder sb=new StringBuilder();
  sb.append("struct<");
  for (int i=0; i < allStructFieldNames.size(); i++) {
    if (i > 0)     sb.append(",");
    sb.append(allStructFieldNames.get(i));
    sb.append(":");
    sb.append(allStructFieldTypeInfos.get(i).getTypeName());
  }
  sb.append(">");
  return sb.toString();
}
