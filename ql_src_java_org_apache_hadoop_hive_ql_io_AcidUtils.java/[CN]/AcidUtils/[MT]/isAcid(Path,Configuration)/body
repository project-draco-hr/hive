{
  FileSystem fs=directory.getFileSystem(conf);
  for (  FileStatus file : fs.listStatus(directory)) {
    String filename=file.getPath().getName();
    if (filename.startsWith(BASE_PREFIX) || filename.startsWith(DELTA_PREFIX)) {
      if (file.isDir()) {
        return true;
      }
    }
  }
  return false;
}
