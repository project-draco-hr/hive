{
  LazyBinaryUtils.readVInt(bytes,offset,vInt);
  scale=vInt.value;
  offset+=vInt.length;
  LazyBinaryUtils.readVInt(bytes,offset,vInt);
  offset+=vInt.length;
  if (internalStorage.length != vInt.value) {
    internalStorage=new byte[vInt.value];
  }
  System.arraycopy(bytes,offset,internalStorage,0,vInt.value);
}
