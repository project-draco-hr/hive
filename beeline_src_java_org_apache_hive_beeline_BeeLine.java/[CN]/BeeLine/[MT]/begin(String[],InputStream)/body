{
  try {
    getOpts().load();
  }
 catch (  Exception e) {
  }
  ConsoleReader reader=getConsoleReader(inputStream);
  if (!(initArgs(args))) {
    usage();
    return;
  }
  try {
    info(getApplicationTitle());
  }
 catch (  Exception e) {
  }
  while (!exit) {
    try {
      dispatch(reader.readLine(getPrompt()));
    }
 catch (    EOFException eof) {
      commands.quit(null);
    }
catch (    Throwable t) {
      handleException(t);
    }
  }
  commands.closeall(null);
}
