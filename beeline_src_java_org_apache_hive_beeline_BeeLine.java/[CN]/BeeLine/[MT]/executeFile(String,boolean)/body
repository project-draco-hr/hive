{
  FileInputStream initStream=null;
  try {
    initStream=new FileInputStream(fileName);
    return execute(getConsoleReader(initStream),true);
  }
 catch (  Throwable t) {
    handleException(t);
    return ERRNO_OTHER;
  }
 finally {
    IOUtils.closeStream(initStream);
    if (!isSourceCMD) {
      consoleReader=null;
      output("");
    }
  }
}
