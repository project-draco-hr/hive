{
switch (call.getKind()) {
case LESS_THAN:
    return (RexCall)rexBuilder.makeCall(SqlStdOperatorTable.GREATER_THAN,Lists.reverse(call.getOperands()));
case GREATER_THAN:
  return (RexCall)rexBuilder.makeCall(SqlStdOperatorTable.LESS_THAN,Lists.reverse(call.getOperands()));
case LESS_THAN_OR_EQUAL:
return (RexCall)rexBuilder.makeCall(SqlStdOperatorTable.GREATER_THAN_OR_EQUAL,Lists.reverse(call.getOperands()));
case GREATER_THAN_OR_EQUAL:
return (RexCall)rexBuilder.makeCall(SqlStdOperatorTable.LESS_THAN_OR_EQUAL,Lists.reverse(call.getOperands()));
}
return null;
}
