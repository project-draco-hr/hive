{
  try {
    this.currentCount=0;
    return toCnf2(rex);
  }
 catch (  OverflowError e) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Transformation to CNF not carried out as number of resulting nodes " + "in expression is greater than the max number of nodes allowed");
    }
    Util.swallow(e,null);
    return rex;
  }
}
