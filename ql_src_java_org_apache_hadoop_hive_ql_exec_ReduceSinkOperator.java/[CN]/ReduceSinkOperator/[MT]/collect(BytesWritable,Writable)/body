{
  if (null != out) {
    numRows++;
    runTimeNumRows++;
    if (isLogInfoEnabled) {
      if (numRows == cntr) {
        cntr=logEveryNRows == 0 ? cntr * 10 : numRows + logEveryNRows;
        if (cntr < 0 || numRows < 0) {
          cntr=0;
          numRows=1;
        }
        LOG.info(toString() + ": records written - " + numRows);
      }
    }
    out.collect(keyWritable,valueWritable);
  }
}
