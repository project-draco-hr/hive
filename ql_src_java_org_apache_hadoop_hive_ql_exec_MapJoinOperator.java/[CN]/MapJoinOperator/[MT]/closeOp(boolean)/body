{
  for (  MapJoinTableContainer tableContainer : mapJoinTables) {
    if (tableContainer != null) {
      tableContainer.dumpMetrics();
    }
  }
  if ((this.getExecContext() != null) && (this.getExecContext().getLocalWork() != null) && (this.getExecContext().getLocalWork().getInputFileChangeSensitive())&& mapJoinTables != null) {
    for (    MapJoinTableContainer tableContainer : mapJoinTables) {
      if (tableContainer != null) {
        tableContainer.clear();
      }
    }
  }
  super.closeOp(abort);
}
