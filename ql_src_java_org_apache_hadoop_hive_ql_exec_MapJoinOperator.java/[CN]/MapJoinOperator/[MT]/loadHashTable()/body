{
  if (this.getExecContext().getLocalWork() == null || !this.getExecContext().getLocalWork().getInputFileChangeSensitive()) {
    if (hashTblInitedOnce) {
      return;
    }
 else {
      hashTblInitedOnce=true;
    }
  }
  loader.load(this.getExecContext(),hconf,this.getConf(),posBigTable,mapJoinTables,mapJoinTableSerdes);
  cache.cache(tableKey,mapJoinTables);
  cache.cache(serdeKey,mapJoinTableSerdes);
}
