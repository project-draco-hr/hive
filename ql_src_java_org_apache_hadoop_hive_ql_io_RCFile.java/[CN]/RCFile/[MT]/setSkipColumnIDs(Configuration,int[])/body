{
  String id=null;
  if (ids != null) {
    for (int i=0; i < ids.length; i++) {
      if (i == 0) {
        id="" + ids[i];
      }
 else {
        id=id + StringUtils.COMMA_STR + ids[i];
      }
    }
  }
  if (id == null || id.length() <= 0) {
    conf.set(SKIP_COLUMN_IDS_CONF_STR,"");
    return;
  }
  conf.set(SKIP_COLUMN_IDS_CONF_STR,id);
}
