{
  if (columnRunLength[i] > 0) {
    --columnRunLength[i];
    return columnPrvLength[i];
  }
 else {
    int length=(int)WritableUtils.readVLong(colValLenBufferReadIn[i]);
    if (length < 0) {
      columnRunLength[i]=~length;
      columnRunLength[i]--;
      length=columnPrvLength[i];
    }
 else {
      columnPrvLength[i]=length;
      columnRunLength[i]=0;
    }
    return length;
  }
}
