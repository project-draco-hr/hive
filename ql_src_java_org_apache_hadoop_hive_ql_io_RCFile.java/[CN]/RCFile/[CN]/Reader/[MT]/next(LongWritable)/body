{
  if (hasRecordsInBuffer()) {
    readRows.set(passedRowsNum);
    readRowsIndexInBuffer++;
    passedRowsNum++;
    rowFetched=false;
    return true;
  }
 else {
    keyInit=false;
  }
  int ret=-1;
  if (tolerateCorruptions) {
    ret=nextKeyValueTolerateCorruptions();
  }
 else {
    try {
      ret=nextKeyBuffer();
    }
 catch (    EOFException eof) {
      eof.printStackTrace();
    }
  }
  return (ret > 0) && next(readRows);
}
