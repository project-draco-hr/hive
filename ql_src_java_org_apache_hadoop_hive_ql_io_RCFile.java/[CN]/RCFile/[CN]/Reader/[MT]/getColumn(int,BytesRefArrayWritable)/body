{
  if (skippedColIDs[columnID]) {
    return null;
  }
  if (rest == null) {
    rest=new BytesRefArrayWritable();
  }
  rest.resetValid(recordsNumInValBuffer);
  if (!currentValue.inited)   currentValueBuffer();
  int columnNextRowStart=0;
  fetchColumnTempBuf.reset(currentKey.allCellValLenBuffer[columnID].getData(),currentKey.allCellValLenBuffer[columnID].getLength());
  for (int i=0; i < recordsNumInValBuffer; i++) {
    int length=WritableUtils.readVInt(fetchColumnTempBuf);
    BytesRefWritable currentCell=rest.get(i);
    if (currentValue.decompressedFlag[columnID])     currentCell.set(currentValue.loadedColumnsValueBuffer[columnID].getData(),columnNextRowStart,length);
 else     currentCell.set(currentValue.lazyDecompressCallbackObjs[columnID],columnNextRowStart,length);
    columnNextRowStart=columnNextRowStart + length;
  }
  return rest;
}
