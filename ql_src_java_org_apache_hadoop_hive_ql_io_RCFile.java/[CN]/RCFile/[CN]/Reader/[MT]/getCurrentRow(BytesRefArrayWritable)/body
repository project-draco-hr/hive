{
  if (!keyInit || rowFetched)   return;
  if (!currentValue.inited) {
    currentValueBuffer();
    ret.resetValid(columnNumber);
  }
  for (int j=0; j < prjColIDs.length; ++j) {
    int i=prjColIDs[j];
    BytesRefWritable ref=ret.unCheckedGet(i);
    int columnCurrentRowStart=(int)columnRowReadIndex[i];
    int length=getColumnNextValueLength(i);
    columnRowReadIndex[i]=columnCurrentRowStart + length;
    if (currentValue.decompressedFlag[j])     ref.set(currentValue.loadedColumnsValueBuffer[j].getData(),columnCurrentRowStart,length);
 else     ref.set(currentValue.lazyDecompressCallbackObjs[j],columnCurrentRowStart,length);
  }
  rowFetched=true;
}
