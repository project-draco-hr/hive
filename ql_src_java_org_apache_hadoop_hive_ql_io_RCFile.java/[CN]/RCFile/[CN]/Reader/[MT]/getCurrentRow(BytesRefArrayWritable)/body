{
  if (!keyInit || rowFetched)   return;
  if (!currentValue.inited) {
    currentValueBuffer();
  }
  ret.resetValid(columnNumber);
  for (int i=0, readIndex=0; i < columnNumber; i++) {
    BytesRefWritable ref=ret.unCheckedGet(i);
    if (skippedColIDs[i]) {
      if (ref != BytesRefWritable.ZeroBytesRefWritable)       ret.set(i,BytesRefWritable.ZeroBytesRefWritable);
      continue;
    }
    int columnCurrentRowStart=(int)columnRowReadIndex[i];
    int length=(int)WritableUtils.readVLong(colValLenBufferReadIn[i]);
    columnRowReadIndex[i]=columnCurrentRowStart + length;
    ref.set(currentValue.loadedColumnsValueBuffer[readIndex].getData(),columnCurrentRowStart,length);
    readIndex++;
  }
  rowFetched=true;
}
