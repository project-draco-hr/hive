{
  long currentOffset=in.getPos();
  int ret=-1;
  try {
    ret=nextKeyBuffer();
    this.currentValueBuffer();
  }
 catch (  IOException ioe) {
    String msg=ioe.getMessage();
    if (msg != null && msg.startsWith(BLOCK_MISSING_MESSAGE)) {
      LOG.warn("Re-throwing block-missing exception" + ioe);
      throw ioe;
    }
    LOG.warn("Ignoring IOException in file " + file + " after offset "+ currentOffset,ioe);
    ret=-1;
  }
catch (  Throwable t) {
    LOG.warn("Ignoring unknown error in " + file + " after offset "+ currentOffset,t);
    ret=-1;
  }
  return ret;
}
