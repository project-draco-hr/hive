{
  if (codec != null) {
    for (    NonSyncDataOutputBuffer currentBuf : compressedColumnsValueBuffer) {
      out.write(currentBuf.getData(),0,currentBuf.getLength());
    }
  }
 else {
    for (    NonSyncDataOutputBuffer currentBuf : loadedColumnsValueBuffer) {
      out.write(currentBuf.getData(),0,currentBuf.getLength());
    }
  }
}
