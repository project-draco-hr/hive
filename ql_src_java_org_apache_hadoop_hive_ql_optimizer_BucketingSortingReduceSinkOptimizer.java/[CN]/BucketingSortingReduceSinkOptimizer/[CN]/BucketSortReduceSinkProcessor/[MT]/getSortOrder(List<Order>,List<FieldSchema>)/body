{
  List<Integer> sortOrders=new ArrayList<Integer>();
  for (  Order sortCol : tabSortCols) {
    int pos=0;
    for (    FieldSchema tabCol : tabCols) {
      if (sortCol.getCol().equals(tabCol.getName())) {
        sortOrders.add(sortCol.getOrder());
        break;
      }
      pos++;
    }
  }
  return sortOrders;
}
