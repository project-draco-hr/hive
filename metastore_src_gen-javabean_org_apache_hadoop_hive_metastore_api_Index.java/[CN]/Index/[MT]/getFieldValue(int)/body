{
switch (fieldID) {
case INDEXNAME:
    return getIndexName();
case INDEXTYPE:
  return new Integer(getIndexType());
case TABLENAME:
return getTableName();
case DBNAME:
return getDbName();
case COLNAMES:
return getColNames();
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
