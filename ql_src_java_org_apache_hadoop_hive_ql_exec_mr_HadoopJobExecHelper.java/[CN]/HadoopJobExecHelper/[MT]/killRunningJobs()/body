{
synchronized (runningJobs) {
    for (    RunningJob rj : runningJobs) {
      try {
        System.err.println("killing job with: " + rj.getID());
        rj.killJob();
      }
 catch (      Exception e) {
        LOG.warn(e);
        System.err.println("Failed to kill job: " + rj.getID());
      }
    }
  }
}
