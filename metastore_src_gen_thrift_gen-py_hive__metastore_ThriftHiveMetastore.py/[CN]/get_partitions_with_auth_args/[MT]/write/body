        return
    oprot.writeStructBegin('get_partitions_with_auth_args')
    if (self.db_name is not None):
        oprot.writeFieldBegin('db_name', TType.STRING, 1)
        oprot.writeString(self.db_name)
        oprot.writeFieldEnd()
