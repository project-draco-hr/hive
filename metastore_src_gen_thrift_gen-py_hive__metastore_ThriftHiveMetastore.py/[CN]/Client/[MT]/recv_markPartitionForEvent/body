def recv_markPartitionForEvent(self):
    (fname, mtype, rseqid) = self._iprot.readMessageBegin()
    if (mtype == TMessageType.EXCEPTION):
        x = TApplicationException()
        x.read(self._iprot)
        self._iprot.readMessageEnd()
        raise x
    result = markPartitionForEvent_result()
    result.read(self._iprot)
    self._iprot.readMessageEnd()
    if (result.o1 != None):
        raise result.o1
    if (result.o2 != None):
        raise result.o2
    if (result.o3 != None):
        raise result.o3
    if (result.o4 != None):
        raise result.o4
    if (result.o5 != None):
        raise result.o5
    if (result.o6 != None):
        raise result.o6
    return
