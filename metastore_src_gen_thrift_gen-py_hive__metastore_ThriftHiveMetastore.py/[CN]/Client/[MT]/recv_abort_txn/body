def recv_abort_txn(self):
    iprot = self._iprot
    (fname, mtype, rseqid) = iprot.readMessageBegin()
    if (mtype == TMessageType.EXCEPTION):
        x = TApplicationException()
        x.read(iprot)
        iprot.readMessageEnd()
        raise x
    result = abort_txn_result()
    result.read(iprot)
    iprot.readMessageEnd()
    if (result.o1 is not None):
        raise result.o1
    return
