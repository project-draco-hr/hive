{
  setMetadata(parent.getMetadata());
  this.parent=parent;
  this.keyConverter=getConverterFromDescription(keyValueType.getType(0),0,this,hiveTypeInfo == null ? null : ((MapTypeInfo)hiveTypeInfo).getMapKeyTypeInfo());
  this.valueConverter=getConverterFromDescription(keyValueType.getType(1),1,this,hiveTypeInfo == null ? null : ((MapTypeInfo)hiveTypeInfo).getMapValueTypeInfo());
}
