{
  int len=4;
  if (hasDecimalOrSecondVInt(bytes[offset])) {
    int firstVIntLen=WritableUtils.decodeVIntSize(bytes[offset + 4]);
    len+=firstVIntLen;
    if (hasSecondVInt(bytes[offset + 4])) {
      len+=WritableUtils.decodeVIntSize(bytes[offset + 4 + firstVIntLen]);
    }
  }
  return len;
}
