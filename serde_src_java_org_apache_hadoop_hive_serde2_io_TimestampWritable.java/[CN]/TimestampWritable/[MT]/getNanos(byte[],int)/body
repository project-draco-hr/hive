{
  VInt vInt=LazyBinaryUtils.threadLocalVInt.get();
  LazyBinaryUtils.readVInt(bytes,offset,vInt);
  int val=vInt.value;
  if (val < 0) {
    val=-val - 1;
  }
  int len=(int)Math.floor(Math.log10(val)) + 1;
  int tmp=0;
  while (val != 0) {
    tmp*=10;
    tmp+=val % 10;
    val/=10;
  }
  val=tmp;
  if (len < 9) {
    val*=Math.pow(10,9 - len);
  }
  return val;
}
