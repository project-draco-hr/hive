{
  int decimal=0;
  if (nanos != 0) {
    int counter=0;
    while (counter < 9) {
      decimal*=10;
      decimal+=nanos % 10;
      nanos/=10;
      counter++;
    }
  }
  if (hasSecondVInt || decimal != 0) {
    LazyBinaryUtils.writeVLongToByteArray(b,offset,hasSecondVInt ? (-decimal - 1) : decimal);
  }
  return decimal != 0;
}
