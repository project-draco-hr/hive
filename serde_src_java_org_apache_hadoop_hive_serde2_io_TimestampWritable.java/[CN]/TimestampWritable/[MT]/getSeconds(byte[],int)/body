{
  int lowest31BitsOfSecondsAndFlag=bytesToInt(bytes,offset);
  if (lowest31BitsOfSecondsAndFlag >= 0 || !hasSecondVInt(bytes[offset + 4])) {
    return lowest31BitsOfSecondsAndFlag & LOWEST_31_BITS_OF_SEC_MASK;
  }
  return ((long)(lowest31BitsOfSecondsAndFlag & LOWEST_31_BITS_OF_SEC_MASK)) | (LazyBinaryUtils.readVLongFromByteArray(bytes,offset + 4 + WritableUtils.decodeVIntSize(bytes[offset + 4])) << 31);
}
