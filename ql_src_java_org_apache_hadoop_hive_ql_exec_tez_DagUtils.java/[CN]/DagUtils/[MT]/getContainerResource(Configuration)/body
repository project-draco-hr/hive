{
  int memory=HiveConf.getIntVar(conf,HiveConf.ConfVars.HIVETEZCONTAINERSIZE) > 0 ? HiveConf.getIntVar(conf,HiveConf.ConfVars.HIVETEZCONTAINERSIZE) : conf.getInt(MRJobConfig.MAP_MEMORY_MB,MRJobConfig.DEFAULT_MAP_MEMORY_MB);
  int cpus=conf.getInt(MRJobConfig.MAP_CPU_VCORES,MRJobConfig.DEFAULT_MAP_CPU_VCORES);
  return Resource.newInstance(memory,cpus);
}
