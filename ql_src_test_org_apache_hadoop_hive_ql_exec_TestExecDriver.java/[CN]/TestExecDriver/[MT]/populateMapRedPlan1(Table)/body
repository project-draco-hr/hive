{
  mr.setNumReduceTasks(Integer.valueOf(1));
  Operator<reduceSinkDesc> op1=OperatorFactory.get(PlanUtils.getReduceSinkDesc(Utilities.makeList(new exprNodeColumnDesc(String.class,"key")),Utilities.makeList(new exprNodeColumnDesc(String.class,"value")),1));
  Utilities.addMapWork(mr,src,"a",op1);
  Operator<fileSinkDesc> op3=OperatorFactory.get(new fileSinkDesc(tmpdir + "mapredplan1.out",Utilities.defaultTd));
  Operator<extractDesc> op2=OperatorFactory.get(new extractDesc(new exprNodeColumnDesc(String.class,Utilities.ReduceField.VALUE.toString())),op3);
  mr.setReducer(op2);
}
