{
  List<String> nonExecMethods=Arrays.asList("commitTransaction","commitTransaction");
  RawStore rawStore=RetryingRawStore.getProxy(hiveConf,new Configuration(hiveConf),hiveConf.getVar(HiveConf.ConfVars.METASTORE_RAW_STORE_IMPL),1);
  for (  Method rawStoreMethod : RawStore.class.getMethods()) {
    if (nonExecMethods.contains(rawStoreMethod.getName())) {
      assertNotNull(rawStoreMethod.getAnnotation(RawStore.CanNotRetry.class));
    }
  }
}
