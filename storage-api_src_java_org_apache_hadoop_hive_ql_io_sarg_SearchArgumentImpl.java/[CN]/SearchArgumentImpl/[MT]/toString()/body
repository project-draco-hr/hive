{
  StringBuilder buffer=new StringBuilder();
  for (int i=0; i < leaves.size(); ++i) {
    buffer.append("leaf-");
    buffer.append(i);
    buffer.append(" = ");
    buffer.append(leaves.get(i).toString());
    buffer.append('\n');
  }
  buffer.append("expr = ");
  buffer.append(expression);
  return buffer.toString();
}
