{
  List<Operator<?>> returnList=new ArrayList<Operator<?>>();
  List<Operator<?>> opList=getAllRootOperators();
  while (!opList.isEmpty()) {
    Operator<?> op=opList.remove(0);
    if (op.getChildOperators() != null) {
      opList.addAll(op.getChildOperators());
    }
    returnList.add(op);
  }
  return returnList;
}
