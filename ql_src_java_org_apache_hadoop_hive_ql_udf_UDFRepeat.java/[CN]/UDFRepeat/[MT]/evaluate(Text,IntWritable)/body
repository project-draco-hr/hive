{
  if (n == null || s == null) {
    return null;
  }
  int len=n.get() * s.getLength();
  if (len < 0) {
    len=0;
  }
  byte[] data=result.getBytes();
  if (data.length < len) {
    data=new byte[len];
  }
  for (int i=0; i < len; i+=s.getLength()) {
    for (int j=0; j < s.getLength(); j++) {
      data[i + j]=s.getBytes()[j];
    }
  }
  result.set(data,0,len);
  return result;
}
