{
  super("QueryTracker");
  this.localDirsBase=localDirsBase;
  try {
    localFs=FileSystem.getLocal(conf);
  }
 catch (  IOException e) {
    throw new RuntimeException("Failed to setup local filesystem instance",e);
  }
  this.defaultDeleteDelaySeconds=conf.getLong(LlapConfiguration.LLAP_FILE_CLEANUP_DELAY_SECONDS,LlapConfiguration.LLAP_FILE_CLEANUP_DELAY_SECONDS_DEFAULT);
  queryFileCleaner=new QueryFileCleaner(conf,localFs);
  addService(queryFileCleaner);
}
