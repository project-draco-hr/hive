{
  Object o;
  try {
    o=registry.get(key);
    if (o == null) {
      o=fn.call();
      LOG.info("Caching key: " + key);
      registry.cacheForVertex(key,o);
    }
 else {
      LOG.info("Found " + key + " in cache with value: "+ o);
    }
  }
 catch (  Exception e) {
    throw new HiveException(e);
  }
  return o;
}
