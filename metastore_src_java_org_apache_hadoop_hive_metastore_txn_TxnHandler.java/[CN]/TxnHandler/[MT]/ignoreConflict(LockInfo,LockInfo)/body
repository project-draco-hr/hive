{
  return ((desiredLock.isDbLock() && desiredLock.type == LockType.SHARED_READ && existingLock.isTableLock() && existingLock.type == LockType.EXCLUSIVE) || (existingLock.isDbLock() && existingLock.type == LockType.SHARED_READ && desiredLock.isTableLock() && desiredLock.type == LockType.EXCLUSIVE)) || (desiredLock.txnId != 0 && desiredLock.txnId == existingLock.txnId) || (desiredLock.txnId == 0 && desiredLock.extLockId == existingLock.extLockId);
}
