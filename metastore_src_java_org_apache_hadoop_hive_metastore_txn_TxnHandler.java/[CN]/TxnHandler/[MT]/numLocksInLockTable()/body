{
  Connection dbConn=getDbConn();
  try {
    Statement stmt=dbConn.createStatement();
    String s="select count(*) from HIVE_LOCKS";
    LOG.debug("Going to execute query <" + s + ">");
    ResultSet rs=stmt.executeQuery(s);
    rs.next();
    int rc=rs.getInt(1);
    dbConn.rollback();
    return rc;
  }
  finally {
    closeDbConn(dbConn);
  }
}
