{
  long txnid=rqst.getTxnid();
  try {
    Connection dbConn=getDbConn(Connection.TRANSACTION_SERIALIZABLE);
    try {
      List<Long> txnids=new ArrayList<Long>(1);
      txnids.add(txnid);
      if (abortTxns(dbConn,txnids) != 1) {
        LOG.debug("Going to rollback");
        dbConn.rollback();
        throw new NoSuchTxnException("No such transaction: " + txnid);
      }
      LOG.debug("Going to commit");
      dbConn.commit();
    }
 catch (    SQLException e) {
      try {
        LOG.debug("Going to rollback");
        dbConn.rollback();
      }
 catch (      SQLException e1) {
      }
      detectDeadlock(e,"abortTxn");
      throw new MetaException("Unable to update transaction database " + StringUtils.stringifyException(e));
    }
 finally {
      closeDbConn(dbConn);
    }
  }
 catch (  DeadlockException e) {
    abortTxn(rqst);
  }
 finally {
    deadlockCnt=0;
  }
}
