{
  SettableUncompressedStream present=StreamUtils.createSettableUncompressedStream(OrcProto.Stream.Kind.PRESENT.name(),fileId,presentStream);
  SettableUncompressedStream data=StreamUtils.createSettableUncompressedStream(OrcProto.Stream.Kind.DATA.name(),fileId,dataStream);
  SettableUncompressedStream nanos=StreamUtils.createSettableUncompressedStream(OrcProto.Stream.Kind.SECONDARY.name(),fileId,nanosStream);
  boolean isFileCompressed=compressionCodec != null;
  return new TimestampStreamReader(columnIndex,present,data,nanos,isFileCompressed,columnEncoding,skipCorrupt);
}
