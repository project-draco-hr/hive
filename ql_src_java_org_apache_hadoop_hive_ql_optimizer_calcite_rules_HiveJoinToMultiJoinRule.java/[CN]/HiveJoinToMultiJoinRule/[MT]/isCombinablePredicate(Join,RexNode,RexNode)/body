{
  final JoinPredicateInfo joinPredInfo=HiveCalciteUtil.JoinPredicateInfo.constructJoinPredicateInfo(join,condition);
  final JoinPredicateInfo otherJoinPredInfo=HiveCalciteUtil.JoinPredicateInfo.constructJoinPredicateInfo(join,otherCondition);
  if (joinPredInfo.getProjsFromLeftPartOfJoinKeysInJoinSchema().equals(otherJoinPredInfo.getProjsFromLeftPartOfJoinKeysInJoinSchema())) {
    return false;
  }
  if (joinPredInfo.getProjsFromRightPartOfJoinKeysInJoinSchema().equals(otherJoinPredInfo.getProjsFromRightPartOfJoinKeysInJoinSchema())) {
    return false;
  }
  return true;
}
