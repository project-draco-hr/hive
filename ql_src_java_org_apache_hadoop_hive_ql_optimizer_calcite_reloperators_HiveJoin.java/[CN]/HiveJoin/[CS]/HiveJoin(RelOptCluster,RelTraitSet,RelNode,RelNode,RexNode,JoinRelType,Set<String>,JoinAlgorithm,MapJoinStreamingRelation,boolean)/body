{
  super(cluster,TraitsUtil.getDefaultTraitSet(cluster),left,right,condition,joinType,variablesStopped);
  this.joinAlgorithm=joinAlgo;
  this.mapJoinStreamingSide=streamingSideForMapJoin;
  this.leftSemiJoin=leftSemiJoin;
  this.maxMemory=(double)HiveConf.getLongVar(cluster.getPlanner().getContext().unwrap(HiveConf.class),HiveConf.ConfVars.HIVECONVERTJOINNOCONDITIONALTASKTHRESHOLD);
}
