{
  try {
    Set<String> variablesStopped=Collections.emptySet();
    return new HiveJoin(cluster,null,left,right,condition,joinType,variablesStopped,JoinAlgorithm.NONE,MapJoinStreamingRelation.NONE,ImmutableBitSet.of(),leftSemiJoin);
  }
 catch (  InvalidRelException e) {
    throw new RuntimeException(e);
  }
}
