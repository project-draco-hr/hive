{
  try {
    Set<String> variablesStopped=Collections.emptySet();
    HiveJoin join=new HiveJoin(cluster,null,left,right,condition,joinType,variablesStopped,DefaultJoinAlgorithm.INSTANCE,leftSemiJoin);
    return join;
  }
 catch (  InvalidRelException|CalciteSemanticException e) {
    throw new RuntimeException(e);
  }
}
