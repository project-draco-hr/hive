{
  try {
    Set<String> variablesStopped=Collections.emptySet();
    HiveJoin join=new HiveJoin(cluster,null,left,right,condition,joinType,variablesStopped,JoinAlgorithm.NONE,chooseStreamingSide(left,right),leftSemiJoin);
    return join;
  }
 catch (  InvalidRelException e) {
    throw new RuntimeException(e);
  }
}
