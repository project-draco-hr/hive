{
  return ("Field= " + hf.getName() + ", isPrimitive="+ hf.isPrimitive()+ ", isList="+ hf.isList()+ (hf.isList() ? " of " + hf.getListElementType().getName() : "")+ ", isMap="+ hf.isMap()+ (hf.isMap() ? " of <" + hf.getMapKeyType().getName() + ","+ hf.getMapValueType().getName()+ ">" : "")+ ", type="+ hf.getType().getName());
}
