{
  InStream present=StreamUtils.createInStream(OrcProto.Stream.Kind.PRESENT.name(),fileName,null,bufferSize,presentStream,presentCBIdx);
  InStream data=StreamUtils.createInStream(OrcProto.Stream.Kind.DATA.name(),fileName,null,bufferSize,dataStream,secondsCBIdx);
  InStream nanos=StreamUtils.createInStream(OrcProto.Stream.Kind.SECONDARY.name(),fileName,null,bufferSize,nanosStream,nanosCBIdx);
  boolean isFileCompressed=compressionCodec != null;
  return new TimestampStreamReader(columnIndex,present,data,nanos,isFileCompressed,columnEncoding,skipCorrupt,rowIndex);
}
