{
  if (rel instanceof HiveJoin) {
    HiveJoin hjRel=(HiveJoin)rel;
    if (hjRel.isLeftSemiJoin()) {
      return mq.getDistinctRowCount(hjRel.getLeft(),groupKey,rel.getCluster().getRexBuilder().makeLiteral(true));
    }
 else {
      return RelMdUtil.getJoinDistinctRowCount(mq,rel,rel.getJoinType(),groupKey,predicate,true);
    }
  }
  return mq.getDistinctRowCount(rel,groupKey,predicate);
}
