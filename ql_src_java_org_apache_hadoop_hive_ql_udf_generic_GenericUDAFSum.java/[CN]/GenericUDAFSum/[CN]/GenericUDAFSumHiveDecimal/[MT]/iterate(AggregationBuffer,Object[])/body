{
  assert(parameters.length == 1);
  try {
    if (isEligibleValue((SumHiveDecimalAgg)agg,parameters[0])) {
      ((SumHiveDecimalAgg)agg).empty=false;
      ((SumHiveDecimalAgg)agg).sum=((SumHiveDecimalAgg)agg).sum.add(PrimitiveObjectInspectorUtils.getHiveDecimal(parameters[0],inputOI));
    }
  }
 catch (  NumberFormatException e) {
    if (!warned) {
      warned=true;
      LOG.warn(getClass().getSimpleName() + " " + StringUtils.stringifyException(e));
      LOG.warn(getClass().getSimpleName() + " ignoring similar exceptions.");
    }
  }
}
