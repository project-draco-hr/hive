{
  if (partial != null) {
    SumHiveDecimalAgg myagg=(SumHiveDecimalAgg)agg;
    if (myagg.sum == null) {
      return;
    }
    myagg.empty=false;
    try {
      myagg.sum=myagg.sum.add(PrimitiveObjectInspectorUtils.getHiveDecimal(partial,inputOI));
    }
 catch (    NumberFormatException e) {
      myagg.sum=null;
    }
  }
}
