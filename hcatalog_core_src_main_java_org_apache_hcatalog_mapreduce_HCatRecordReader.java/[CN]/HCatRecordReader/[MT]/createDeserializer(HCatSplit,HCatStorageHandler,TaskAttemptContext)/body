{
  deserializer=ReflectionUtils.newInstance(storageHandler.getSerDeClass(),taskContext.getConfiguration());
  try {
    InternalUtil.initializeDeserializer(deserializer,storageHandler.getConf(),hcatSplit.getPartitionInfo().getTableInfo(),hcatSplit.getPartitionInfo().getPartitionSchema());
  }
 catch (  SerDeException e) {
    throw new IOException("Failed initializing deserializer " + storageHandler.getSerDeClass().getName(),e);
  }
}
