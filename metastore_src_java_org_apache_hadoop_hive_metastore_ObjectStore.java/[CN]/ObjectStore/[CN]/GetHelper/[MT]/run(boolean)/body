{
  try {
    start(initTable);
    if (doUseDirectSql) {
      try {
        directSql.prepareTxn();
        this.results=getSqlResult(this);
      }
 catch (      Exception ex) {
        handleDirectSqlError(ex);
      }
    }
    if (!doUseDirectSql) {
      this.results=getJdoResult(this);
    }
    return commit();
  }
 catch (  NoSuchObjectException ex) {
    throw ex;
  }
catch (  MetaException ex) {
    throw ex;
  }
catch (  Exception ex) {
    LOG.error("",ex);
    throw new MetaException(ex.getMessage());
  }
 finally {
    close();
  }
}
