{
  if (mtbl == null)   return null;
  return new Table(mtbl.getTableName(),mtbl.getDatabase().getName(),mtbl.getOwner(),mtbl.getCreateTime(),mtbl.getLastAccessTime(),mtbl.getRetention(),convertToStorageDescriptor(mtbl.getSd()),convertToFieldSchemas(mtbl.getPartitionKeys()),mtbl.getParameters());
}
