{
  boolean success=false;
  boolean commited=false;
  try {
    openTransaction();
    MPartition mpart=convertToMPart(part);
    pm.makePersistent(mpart);
    commited=commitTransaction();
    success=true;
  }
  finally {
    if (!commited) {
      rollbackTransaction();
    }
  }
  return success;
}
