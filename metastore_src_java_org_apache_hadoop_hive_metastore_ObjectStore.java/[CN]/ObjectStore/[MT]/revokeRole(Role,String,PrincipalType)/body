{
  boolean success=false;
  try {
    openTransaction();
    MRoleMap roleMember=getMSecurityUserRoleMap(userName,principalType,role.getRoleName());
    pm.deletePersistent(roleMember);
    success=commitTransaction();
  }
  finally {
    if (!success) {
      rollbackTransaction();
    }
  }
  return success;
}
