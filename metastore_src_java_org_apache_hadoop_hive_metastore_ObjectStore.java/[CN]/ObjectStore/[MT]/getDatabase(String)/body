{
  MetaException ex=null;
  Database db=null;
  try {
    db=getDatabaseInternal(name);
  }
 catch (  MetaException e) {
    ex=e;
  }
  if (db == null) {
    LOG.warn("Failed to get database " + name + ", returning NoSuchObjectException",ex);
    throw new NoSuchObjectException(name + (ex == null ? "" : (": " + ex.getMessage())));
  }
  return db;
}
