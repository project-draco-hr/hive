{
  boolean success=false;
  try {
    openTransaction();
    MTable tbl=getMTable(dbName,tableName);
    pm.retrieve(tbl);
    if (tbl != null) {
      pm.deletePersistentAll(listMPartitions(dbName,tableName,-1));
      pm.deletePersistent(tbl);
    }
    success=commitTransaction();
  }
  finally {
    if (!success) {
      rollbackTransaction();
    }
  }
  return success;
}
