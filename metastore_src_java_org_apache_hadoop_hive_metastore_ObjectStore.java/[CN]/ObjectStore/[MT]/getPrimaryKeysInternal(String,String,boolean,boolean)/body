{
  return new GetListHelper<SQLPrimaryKey>(db_name,tbl_name,allowSql,allowJdo){
    @Override protected List<SQLPrimaryKey> getSqlResult(    GetHelper<List<SQLPrimaryKey>> ctx) throws MetaException {
      return directSql.getPrimaryKeys(db_name,tbl_name);
    }
    @Override protected List<SQLPrimaryKey> getJdoResult(    GetHelper<List<SQLPrimaryKey>> ctx) throws MetaException, NoSuchObjectException {
      return getPrimaryKeysViaJdo(db_name,tbl_name);
    }
  }
.run(false);
}
