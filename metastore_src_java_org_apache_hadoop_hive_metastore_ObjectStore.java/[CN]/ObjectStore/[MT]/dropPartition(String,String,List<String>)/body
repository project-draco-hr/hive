{
  boolean success=false;
  try {
    openTransaction();
    MPartition part=this.getMPartition(dbName,tableName,part_vals);
    if (part != null)     pm.deletePersistent(part);
    success=commitTransaction();
  }
  finally {
    if (!success) {
      rollbackTransaction();
    }
  }
  return success;
}
