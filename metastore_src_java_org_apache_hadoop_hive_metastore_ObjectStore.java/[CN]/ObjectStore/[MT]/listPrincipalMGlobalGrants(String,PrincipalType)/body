{
  boolean commited=false;
  List<MGlobalPrivilege> userNameDbPriv=null;
  try {
    openTransaction();
    if (principalName != null) {
      Query query=pm.newQuery(MGlobalPrivilege.class,"principalName == t1 && principalType == t2 ");
      query.declareParameters("java.lang.String t1, java.lang.String t2");
      userNameDbPriv=(List<MGlobalPrivilege>)query.executeWithArray(principalName,principalType.toString());
      pm.retrieveAll(userNameDbPriv);
    }
    commited=commitTransaction();
  }
  finally {
    if (!commited) {
      rollbackTransaction();
    }
  }
  return userNameDbPriv;
}
