{
  MRole mrole=null;
  boolean commited=false;
  Query query=null;
  try {
    openTransaction();
    query=pm.newQuery(MRole.class,"roleName == t1");
    query.declareParameters("java.lang.String t1");
    query.setUnique(true);
    mrole=(MRole)query.execute(roleName);
    pm.retrieve(mrole);
    commited=commitTransaction();
  }
  finally {
    if (!commited) {
      rollbackTransaction();
    }
    if (query != null) {
      query.closeAll();
    }
  }
  return mrole;
}
