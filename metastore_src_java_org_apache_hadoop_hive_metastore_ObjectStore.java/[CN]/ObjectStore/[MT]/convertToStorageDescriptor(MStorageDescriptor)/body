{
  if (msd == null)   return null;
  return new StorageDescriptor(convertToFieldSchemas(msd.getCols()),msd.getLocation(),msd.getInputFormat(),msd.getOutputFormat(),msd.isCompressed(),msd.getNumBuckets(),converToSerDeInfo(msd.getSerDeInfo()),msd.getBucketCols(),convertToOrders(msd.getSortCols()),msd.getParameters());
}
