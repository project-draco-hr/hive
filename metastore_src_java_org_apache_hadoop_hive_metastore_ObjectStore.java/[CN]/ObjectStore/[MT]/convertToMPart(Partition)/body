{
  if (part == null) {
    return null;
  }
  MTable mt=getMTable(part.getDbName(),part.getTableName());
  if (mt == null) {
    throw new InvalidObjectException("Partition doesn't have a valid table or database name");
  }
  return new MPartition(Warehouse.makePartName(convertToFieldSchemas(mt.getPartitionKeys()),part.getValues()),mt,part.getValues(),part.getCreateTime(),part.getLastAccessTime(),convertToMStorageDescriptor(part.getSd()),part.getParameters());
}
