{
  boolean success=false;
  try {
    openTransaction();
    MFunction mfunc=getMFunction(dbName,funcName);
    pm.retrieve(mfunc);
    if (mfunc != null) {
      pm.deletePersistentAll(mfunc);
    }
    success=commitTransaction();
  }
  finally {
    if (!success) {
      rollbackTransaction();
    }
  }
}
