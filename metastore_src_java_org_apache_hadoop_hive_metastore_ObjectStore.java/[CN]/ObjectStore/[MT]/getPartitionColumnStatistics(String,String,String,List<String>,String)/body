{
  ColumnStatistics statsObj;
  MPartitionColumnStatistics mStatsObj=getMPartitionColumnStatistics(dbName,tableName,partName,partVal,colName);
  if (mStatsObj == null) {
    throw new NoSuchObjectException("Statistics for dbName=" + dbName + " tableName="+ tableName+ " partName= "+ partName+ " columnName="+ colName+ " doesn't exist.");
  }
  statsObj=convertToPartColumnStatistics(mStatsObj);
  return statsObj;
}
