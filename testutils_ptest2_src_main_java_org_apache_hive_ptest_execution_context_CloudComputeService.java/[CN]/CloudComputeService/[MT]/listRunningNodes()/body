{
  Set<NodeMetadata> result=Sets.newHashSet();
  result.addAll(mComputeService.listNodesDetailsMatching(new Predicate<ComputeMetadata>(){
    @Override public boolean apply(    ComputeMetadata computeMetadata){
      NodeMetadata nodeMetadata=(NodeMetadata)computeMetadata;
      return nodeMetadata.getStatus() == Status.RUNNING && (mGroupName.equalsIgnoreCase(nodeMetadata.getGroup()) || nodeMetadata.getTags().contains(mGroupTag));
    }
  }
));
  return result;
}
