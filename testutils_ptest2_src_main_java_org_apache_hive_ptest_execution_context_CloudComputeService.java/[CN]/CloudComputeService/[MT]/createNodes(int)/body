{
  Set<NodeMetadata> result=Sets.newHashSet();
  Template template=mComputeService.templateBuilder().hardwareId(mInstanceType).imageId(mImageId).build();
  template.getOptions().as(AWSEC2TemplateOptions.class).keyPair(mkeyPair).securityGroupIds(mSecurityGroup).blockOnPort(22,60).spotPrice(mMaxBid).tags(Collections.singletonList(mGroupTag));
  result.addAll(mComputeService.createNodesInGroup(mGroupName,count,template));
  return result;
}
