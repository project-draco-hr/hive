{
  Set<NodeMetadata> result=Sets.newHashSet();
  Template template=mComputeService.templateBuilder().hardwareId(InstanceType.M1_XLARGE).imageId(mImageId).build();
  template.getOptions().as(AWSEC2TemplateOptions.class).keyPair(mkeyPair).securityGroupIds(mSecurityGroup).blockOnPort(22,60).spotPrice(mMaxBid);
  result.addAll(mComputeService.createNodesInGroup(mGroupName,count,template));
  return result;
}
