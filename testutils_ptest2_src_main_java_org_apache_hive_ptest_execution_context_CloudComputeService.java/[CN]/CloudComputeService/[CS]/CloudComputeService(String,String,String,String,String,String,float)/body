{
  mGroupName=groupName;
  mImageId=imageId;
  mkeyPair=keyPair;
  mSecurityGroup=securityGroup;
  mMaxBid=maxBid;
  mComputeServiceContext=ContextBuilder.newBuilder("aws-ec2").credentials(apiKey,accessKey).modules(ImmutableSet.of(new Log4JLoggingModule())).buildView(ComputeServiceContext.class);
  mComputeService=mComputeServiceContext.getComputeService();
}
