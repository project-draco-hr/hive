{
  if (!isOpen()) {
    return;
  }
  LOG.info("Closing Tez Session");
  try {
    session.stop();
  }
 catch (  SessionNotRunning nr) {
  }
  if (!keepTmpDir) {
    cleanupScratchDir();
  }
  session=null;
  tezScratchDir=null;
  conf=null;
  appJarLr=null;
  additionalFilesNotFromConf.clear();
  localizedResources.clear();
}
