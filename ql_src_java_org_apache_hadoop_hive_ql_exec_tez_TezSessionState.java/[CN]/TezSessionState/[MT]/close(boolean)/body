{
  if (!isOpen()) {
    return;
  }
  LOG.info("Closing Tez Session");
  try {
    session.stop();
  }
 catch (  SessionNotRunning nr) {
  }
  if (!keepTmpDir) {
    FileSystem fs=tezScratchDir.getFileSystem(conf);
    fs.delete(tezScratchDir,true);
  }
  session=null;
  tezScratchDir=null;
  conf=null;
  appJarLr=null;
}
