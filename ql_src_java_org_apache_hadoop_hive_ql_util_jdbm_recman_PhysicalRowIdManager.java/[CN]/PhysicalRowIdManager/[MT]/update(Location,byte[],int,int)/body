{
  BlockIo block=file.get(rowid.getBlock());
  RecordHeader head=new RecordHeader(block,rowid.getOffset());
  if (length > head.getAvailableSize()) {
    file.release(block);
    free(rowid);
    rowid=alloc(length);
  }
 else {
    file.release(block);
  }
  write(rowid,data,start,length);
  return rowid;
}
