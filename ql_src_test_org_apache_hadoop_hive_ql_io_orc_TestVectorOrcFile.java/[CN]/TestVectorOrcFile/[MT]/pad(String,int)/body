{
  if (value.length() == length) {
    return value;
  }
 else   if (value.length() > length) {
    return value.substring(0,length);
  }
 else {
    StringBuilder buf=new StringBuilder();
    buf.append(value);
    for (int i=0; i < length - value.length(); ++i) {
      buf.append(' ');
    }
    return buf.toString();
  }
}
