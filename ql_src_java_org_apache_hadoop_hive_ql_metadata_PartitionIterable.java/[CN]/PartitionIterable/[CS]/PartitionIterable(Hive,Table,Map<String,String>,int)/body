{
  this.currType=Type.LAZY_FETCH_PARTITIONS;
  this.db=db;
  this.table=table;
  this.partialPartitionSpec=partialPartitionSpec;
  this.batch_size=batch_size;
  if (this.partialPartitionSpec == null) {
    partitionNames=db.getPartitionNames(table.getDbName(),table.getTableName(),(short)-1);
  }
 else {
    partitionNames=db.getPartitionNames(table.getDbName(),table.getTableName(),partialPartitionSpec,(short)-1);
  }
}
