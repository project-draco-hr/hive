{
  VectorExpressionDescriptor.Builder b=new VectorExpressionDescriptor.Builder();
  b.setMode(VectorExpressionDescriptor.Mode.PROJECTION).setNumArguments(1).setArgumentTypes(VectorExpressionDescriptor.ArgumentType.STRING).setInputExpressionTypes(VectorExpressionDescriptor.InputExpressionType.COLUMN);
  return b.build();
}
