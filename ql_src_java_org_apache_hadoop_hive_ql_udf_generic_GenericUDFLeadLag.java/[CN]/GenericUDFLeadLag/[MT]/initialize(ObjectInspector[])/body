{
  if (arguments[1] instanceof PrimitiveObjectInspector) {
    amtOI=(PrimitiveObjectInspector)arguments[1];
  }
 else {
    throw new UDFArgumentTypeException(1,"Primitive Type is expected but " + arguments[1].getTypeName() + "\" is found");
  }
  firstArgOI=arguments[0];
  return ObjectInspectorUtils.getStandardObjectInspector(firstArgOI,ObjectInspectorCopyOption.WRITABLE);
}
