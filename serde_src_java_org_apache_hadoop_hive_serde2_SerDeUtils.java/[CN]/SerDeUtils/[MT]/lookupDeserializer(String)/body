{
  Class<?> c;
  if (serdes.containsKey(name)) {
    c=serdes.get(name);
  }
 else {
    try {
      c=Class.forName(name);
    }
 catch (    ClassNotFoundException e) {
      throw new SerDeException("SerDe " + name + " does not exist");
    }
  }
  try {
    return (Deserializer)c.newInstance();
  }
 catch (  Exception e) {
    throw new SerDeException(e);
  }
}
