{
  StringBuilder ret=new StringBuilder("[");
  boolean first=true;
  if (op.getParentOperators() != null) {
    for (    Operator<? extends Serializable> parent : op.getParentOperators()) {
      if (!first) {
        ret.append(",");
      }
      ret.append(opMap.get(parent));
      first=false;
    }
  }
  ret.append("]");
  return ret.toString();
}
