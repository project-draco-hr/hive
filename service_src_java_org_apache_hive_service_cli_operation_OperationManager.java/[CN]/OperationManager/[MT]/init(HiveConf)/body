{
  if (hiveConf.getBoolVar(HiveConf.ConfVars.HIVE_SERVER2_LOGGING_OPERATION_ENABLED)) {
    initOperationLogCapture(hiveConf.getVar(HiveConf.ConfVars.HIVE_SERVER2_LOGGING_OPERATION_LEVEL));
  }
 else {
    LOG.debug("Operation level logging is turned off");
  }
  if (hiveConf.isWebUiQueryInfoCacheEnabled()) {
    historicSqlOperations=new SQLOperationDisplayCache(hiveConf.getIntVar(ConfVars.HIVE_SERVER2_WEBUI_MAX_HISTORIC_QUERIES));
  }
  super.init(hiveConf);
}
