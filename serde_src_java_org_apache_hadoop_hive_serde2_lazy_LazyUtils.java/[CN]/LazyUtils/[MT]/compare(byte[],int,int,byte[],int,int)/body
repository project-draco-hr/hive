{
  int min=Math.min(length1,length2);
  for (int i=0; i < min; i++) {
    if (b1[start1 + i] == b2[start2 + i]) {
      continue;
    }
    if (b1[start1 + i] < b2[start2 + i]) {
      return -1;
    }
 else {
      return 1;
    }
  }
  if (length1 < length2)   return -1;
  if (length1 > length2)   return 1;
  return 0;
}
