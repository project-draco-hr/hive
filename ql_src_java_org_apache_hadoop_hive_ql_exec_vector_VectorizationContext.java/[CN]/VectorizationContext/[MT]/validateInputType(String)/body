{
  if (!(inputType.equalsIgnoreCase("float") || inputType.equalsIgnoreCase("double") || inputType.equalsIgnoreCase("string")|| inputType.equalsIgnoreCase("tinyint")|| inputType.equalsIgnoreCase("smallint")|| inputType.equalsIgnoreCase("short")|| inputType.equalsIgnoreCase("byte")|| inputType.equalsIgnoreCase("int")|| inputType.equalsIgnoreCase("long")|| inputType.equalsIgnoreCase("bigint")|| inputType.equalsIgnoreCase("boolean")|| inputType.equalsIgnoreCase("timestamp"))) {
    throw new HiveException("Unsupported input type: " + inputType);
  }
}
