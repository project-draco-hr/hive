{
  VectorExpressionDescriptor.ArgumentType argType=VectorExpressionDescriptor.ArgumentType.fromHiveTypeName(hiveTypeName);
switch (argType) {
case INT_FAMILY:
    return "Long";
case FLOAT_FAMILY:
  return "Double";
case DECIMAL:
return "Decimal";
case STRING:
return "String";
case CHAR:
return "Char";
case VARCHAR:
return "VarChar";
case DATE:
return "Date";
case TIMESTAMP:
return "Timestamp";
default :
return "None";
}
}
