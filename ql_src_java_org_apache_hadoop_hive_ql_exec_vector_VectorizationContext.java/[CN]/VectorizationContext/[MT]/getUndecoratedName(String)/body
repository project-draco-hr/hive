{
  VectorExpressionDescriptor.ArgumentType argType=VectorExpressionDescriptor.ArgumentType.fromHiveTypeName(hiveTypeName);
switch (argType) {
case INT_FAMILY:
    return "Long";
case FLOAT_FAMILY:
  return "Double";
case DECIMAL:
return "Decimal";
case STRING:
return "String";
case CHAR:
return "Char";
case VARCHAR:
return "VarChar";
case DATE:
return "Date";
case TIMESTAMP:
return "Timestamp";
case INTERVAL_YEAR_MONTH:
case INTERVAL_DAY_TIME:
return hiveTypeName;
default :
return "None";
}
}
