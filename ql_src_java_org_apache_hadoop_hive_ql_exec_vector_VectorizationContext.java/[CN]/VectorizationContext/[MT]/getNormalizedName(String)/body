{
  VectorExpressionDescriptor.ArgumentType argType=VectorExpressionDescriptor.ArgumentType.fromHiveTypeName(hiveTypeName);
switch (argType) {
case INT_FAMILY:
    return "Long";
case FLOAT_FAMILY:
  return "Double";
case DECIMAL:
return hiveTypeName;
case STRING:
return "String";
case CHAR:
return hiveTypeName;
case VARCHAR:
return hiveTypeName;
case DATE:
return "Date";
case TIMESTAMP:
return "Timestamp";
case INTERVAL_YEAR_MONTH:
case INTERVAL_DAY_TIME:
return hiveTypeName;
default :
throw new HiveException("Unexpected hive type name " + hiveTypeName);
}
}
