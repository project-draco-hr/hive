{
  VectorExpressionDescriptor.ArgumentType argType=VectorExpressionDescriptor.ArgumentType.fromHiveTypeName(hiveTypeName);
switch (argType) {
case INT_FAMILY:
    return "Long";
case FLOAT_FAMILY:
  return "Double";
case DECIMAL:
return hiveTypeName;
case STRING:
return "String";
case CHAR:
return hiveTypeName;
case VARCHAR:
return hiveTypeName;
case DATE:
return "Date";
case TIMESTAMP:
return "Timestamp";
default :
return "None";
}
}
