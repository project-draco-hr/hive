{
  StringBuilder b=new StringBuilder();
  b.append("org.apache.hadoop.hive.ql.exec.vector.expressions.gen.");
  b.append(this.getNormalizedTypeName(scalarType));
  b.append("Scalar");
  b.append(method);
  b.append(this.getNormalizedTypeName(colType));
  b.append("Column");
  return b.toString();
}
