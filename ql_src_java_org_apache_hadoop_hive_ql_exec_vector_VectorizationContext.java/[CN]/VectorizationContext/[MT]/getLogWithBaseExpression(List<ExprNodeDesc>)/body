{
  if (childExpr.size() == 1) {
    return getUnaryFunctionExpression("FuncLn","Double",childExpr,GENERATED_EXPR_PACKAGE);
  }
 else   if (childExpr.size() == 2) {
    String argType=childExpr.get(0).getTypeString();
    double base=getDoubleScalar(childExpr.get(1));
    VectorExpression e=getUnaryFunctionExpression("FuncLogWithBase","Double",childExpr,CUSTOM_EXPR_PACKAGE);
    ((ISetDoubleArg)e).setArg(base);
    return e;
  }
  throw new HiveException("Udf: Log could not be vectorized");
}
