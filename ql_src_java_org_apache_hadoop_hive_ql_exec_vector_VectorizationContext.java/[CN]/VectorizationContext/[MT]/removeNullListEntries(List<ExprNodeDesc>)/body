{
  boolean hasNulls=false;
  for (  ExprNodeDesc e : childrenForInList) {
    if (e instanceof ExprNodeNullDesc) {
      hasNulls=true;
      break;
    }
  }
  if (!hasNulls) {
    return childrenForInList;
  }
 else {
    List<ExprNodeDesc> nullFreeList=new ArrayList<ExprNodeDesc>();
    for (    ExprNodeDesc e : childrenForInList) {
      if (!(e instanceof ExprNodeNullDesc)) {
        nullFreeList.add(e);
      }
    }
    return nullFreeList;
  }
}
