{
  String inputType=childExpr.get(0).getTypeString();
  if (inputType.equals("boolean")) {
    return getUnaryFunctionExpression("CastBooleanToStringVia","String",childExpr,CUSTOM_EXPR_PACKAGE);
  }
 else   if (isIntFamily(inputType)) {
    return getUnaryFunctionExpression("Cast","String",childExpr,CUSTOM_EXPR_PACKAGE);
  }
  throw new HiveException("Unhandled cast input type: " + inputType);
}
