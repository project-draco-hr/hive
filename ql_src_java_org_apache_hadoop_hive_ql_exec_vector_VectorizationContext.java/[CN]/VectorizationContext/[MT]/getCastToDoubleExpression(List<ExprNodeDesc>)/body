{
  String inputType=childExpr.get(0).getTypeString();
  if (isIntFamily(inputType)) {
    return getUnaryFunctionExpression("Cast","Double",childExpr,GENERATED_EXPR_PACKAGE);
  }
 else   if (inputType.equals("timestamp")) {
    return getUnaryFunctionExpression("CastTimestampToDoubleVia","Double",childExpr,GENERATED_EXPR_PACKAGE);
  }
 else   if (isFloatFamily(inputType)) {
    return getIdentityExpression(childExpr);
  }
  throw new HiveException("Unhandled cast input type: " + inputType);
}
