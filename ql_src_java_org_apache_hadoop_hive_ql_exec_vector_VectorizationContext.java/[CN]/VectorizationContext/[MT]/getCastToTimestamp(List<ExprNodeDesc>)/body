{
  String inputType=childExpr.get(0).getTypeString();
  if (isIntFamily(inputType)) {
    return getUnaryFunctionExpression("CastLongToTimestampVia","Long",childExpr,GENERATED_EXPR_PACKAGE);
  }
 else   if (isFloatFamily(inputType)) {
    return getUnaryFunctionExpression("CastDoubleToTimestampVia","Long",childExpr,GENERATED_EXPR_PACKAGE);
  }
  throw new HiveException("Unhandled cast input type: " + inputType);
}
