{
  if (method.equalsIgnoreCase("divide") || inputType1.equalsIgnoreCase("double") || inputType2.equalsIgnoreCase("double")) {
    return "double";
  }
 else {
    if (inputType1.equalsIgnoreCase("string") || inputType2.equalsIgnoreCase("string")) {
      return "string";
    }
 else {
      return "long";
    }
  }
}
