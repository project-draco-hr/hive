{
  validateInputType(inputType1);
  validateInputType(inputType2);
  if (method.equalsIgnoreCase("divide") || inputType1.equalsIgnoreCase("double") || inputType2.equalsIgnoreCase("double")|| inputType1.equalsIgnoreCase("float")|| inputType2.equalsIgnoreCase("float")) {
    return "double";
  }
 else {
    if (inputType1.equalsIgnoreCase("string") || inputType2.equalsIgnoreCase("string")) {
      return "string";
    }
 else {
      return "long";
    }
  }
}
