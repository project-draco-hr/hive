{
  boolean ret=castExpressionUdfs.contains(genericUDF.getClass()) || (genericUDF instanceof GenericUDFRound) || (genericUDF instanceof GenericUDFBetween);
  if (ret) {
    return ret;
  }
  if (genericUDF instanceof GenericUDFBridge) {
    Class<?> udfClass=((GenericUDFBridge)genericUDF).getUdfClass();
    return castExpressionUdfs.contains(udfClass) || UDFSign.class.isAssignableFrom(udfClass);
  }
  return false;
}
