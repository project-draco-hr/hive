{
  int columnNum=columnMap.get(exprDesc.getColumn());
  VectorExpression expr=null;
switch (opType) {
case FILTER:
    expr=new SelectColumnIsTrue(columnNum);
  break;
case SELECT:
case GROUPBY:
case REDUCESINK:
expr=new IdentityExpression(columnNum,exprDesc.getTypeString());
break;
}
return expr;
}
