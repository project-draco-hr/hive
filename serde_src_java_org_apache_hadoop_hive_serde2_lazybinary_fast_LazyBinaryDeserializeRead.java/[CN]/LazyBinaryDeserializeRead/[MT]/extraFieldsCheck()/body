{
  if (offset < end) {
    if (!bufferRangeHasExtraDataWarned) {
      int length=end - start;
      int remaining=end - offset;
      LOG.info("Not all fields were read in the buffer range! Buffer range " + start + " for length "+ length+ " but "+ remaining+ " bytes remain. "+ "(total buffer length "+ bytes.length+ ")"+ "  Ignoring similar problems.");
      bufferRangeHasExtraDataWarned=true;
    }
  }
}
