{
  if (offset + 2 > end) {
    warnBeyondEof();
  }
  short result=LazyBinaryUtils.byteArrayToShort(bytes,offset);
  offset+=2;
  fieldIndex++;
  if (fieldIndex < fieldCount) {
    if ((fieldIndex % 8) == 0) {
      if (offset >= end) {
        warnBeyondEof();
      }
      nullByte=bytes[offset++];
    }
  }
  return result;
}
