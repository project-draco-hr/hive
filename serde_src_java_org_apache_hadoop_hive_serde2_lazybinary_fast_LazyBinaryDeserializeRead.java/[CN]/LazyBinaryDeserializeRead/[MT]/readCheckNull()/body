{
  if (fieldIndex >= fieldCount) {
    if (!readBeyondConfiguredFieldsWarned) {
      LOG.info("Reading beyond configured fields! Configured " + fieldCount + " fields but "+ " reading more (NULLs returned).  Ignoring similar problems.");
      readBeyondConfiguredFieldsWarned=true;
    }
    return true;
  }
  if (fieldIndex == 0) {
    if (offset >= end) {
      warnBeyondEof();
    }
    nullByte=bytes[offset++];
  }
  if ((nullByte & (1 << (fieldIndex % 8))) != 0) {
    if (offset >= end) {
      warnBeyondEof();
    }
    if (((PrimitiveTypeInfo)typeInfos[fieldIndex]).getPrimitiveCategory() != PrimitiveCategory.DECIMAL) {
      return false;
    }
    return earlyReadHiveDecimal();
  }
  fieldIndex++;
  if (fieldIndex < fieldCount) {
    if ((fieldIndex % 8) == 0) {
      if (offset >= end) {
        warnBeyondEof();
      }
      nullByte=bytes[offset++];
    }
  }
  return true;
}
