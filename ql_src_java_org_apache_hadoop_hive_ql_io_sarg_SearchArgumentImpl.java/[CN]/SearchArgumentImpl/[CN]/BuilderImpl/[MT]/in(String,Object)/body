{
  ExpressionTree parent=currentTree.getFirst();
  if (literal.length == 0) {
    throw new IllegalArgumentException("Can't create in expression with " + "no arguments");
  }
  List<Object> argList=new ArrayList<Object>();
  for (  Object lit : literal) {
    argList.add(boxLiteral(lit));
  }
  PredicateLeaf leaf=new PredicateLeafImpl(PredicateLeaf.Operator.IN,getType(argList.get(0)),column,null,argList);
  leaves.add(leaf);
  parent.getChildren().add(new ExpressionTree(leaves.size() - 1));
  return this;
}
