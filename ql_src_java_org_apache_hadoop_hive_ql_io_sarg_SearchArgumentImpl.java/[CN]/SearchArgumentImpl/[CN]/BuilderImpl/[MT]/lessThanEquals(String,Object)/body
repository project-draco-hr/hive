{
  ExpressionTree parent=currentTree.getFirst();
  Object box=boxLiteral(literal);
  PredicateLeaf leaf=new PredicateLeafImpl(PredicateLeaf.Operator.LESS_THAN_EQUALS,getType(box),column,box,null);
  leaves.add(leaf);
  parent.getChildren().add(new ExpressionTree(leaves.size() - 1));
  return this;
}
