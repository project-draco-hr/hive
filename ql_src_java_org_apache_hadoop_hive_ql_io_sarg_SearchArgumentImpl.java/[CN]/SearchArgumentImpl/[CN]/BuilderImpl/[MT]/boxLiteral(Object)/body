{
  if (literal instanceof String || literal instanceof Long || literal instanceof Double|| literal instanceof DateWritable|| literal instanceof Timestamp|| literal instanceof HiveDecimal|| literal instanceof BigDecimal) {
    return literal;
  }
 else   if (literal instanceof HiveChar || literal instanceof HiveVarchar) {
    return StringUtils.stripEnd(literal.toString(),null);
  }
 else   if (literal instanceof Byte || literal instanceof Short || literal instanceof Integer) {
    return Long.valueOf(literal.toString());
  }
 else   if (literal instanceof Float) {
    return Double.valueOf((Float)literal);
  }
 else {
    throw new IllegalArgumentException("Unknown type for literal " + literal);
  }
}
