{
  Object result=null;
  List<ExprNodeDesc> children=expr.getChildren();
  if (children.size() != 2) {
    return null;
  }
  for (  ExprNodeDesc child : children) {
    if (child instanceof ExprNodeConstantDesc) {
      if (result != null) {
        return null;
      }
      result=boxLiteral((ExprNodeConstantDesc)child);
    }
  }
  return result;
}
