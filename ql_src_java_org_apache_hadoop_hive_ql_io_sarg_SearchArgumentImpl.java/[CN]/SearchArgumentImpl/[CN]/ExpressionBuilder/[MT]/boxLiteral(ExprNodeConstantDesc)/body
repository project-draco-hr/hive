{
switch (getType(lit)) {
case INTEGER:
    return ((Number)lit.getValue()).longValue();
case STRING:
  return StringUtils.stripEnd(lit.getValue().toString(),null);
case FLOAT:
return Double.parseDouble(lit.getValue().toString());
case DATE:
case TIMESTAMP:
case DECIMAL:
case BOOLEAN:
return lit;
default :
throw new IllegalArgumentException("Unknown literal " + getType(lit));
}
}
