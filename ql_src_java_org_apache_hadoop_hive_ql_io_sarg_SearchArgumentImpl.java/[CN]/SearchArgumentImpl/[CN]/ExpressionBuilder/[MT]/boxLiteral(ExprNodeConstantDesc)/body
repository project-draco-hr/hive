{
switch (getType(lit)) {
case INTEGER:
    return ((Number)lit.getValue()).longValue();
case STRING:
  return StringUtils.stripEnd(lit.getValue().toString(),null);
case FLOAT:
return ((Number)lit.getValue()).doubleValue();
case DATE:
case DECIMAL:
return lit;
default :
throw new IllegalArgumentException("Unknown literal " + getType(lit));
}
}
