{
  int result=-1;
  List<ExprNodeDesc> children=expr.getChildren();
  for (int i=0; i < children.size(); ++i) {
    ExprNodeDesc child=children.get(i);
    if (child instanceof ExprNodeColumnDesc) {
      if (result != -1) {
        return -1;
      }
 else {
        result=i;
      }
    }
  }
  return result;
}
