{
  if (literal instanceof java.util.Date) {
    return new Timestamp(((java.util.Date)literal).getTime());
  }
switch (format) {
case ORC:
    if (literal instanceof Integer) {
      return ((Number)literal).longValue();
    }
  return literal;
case PARQUET:
return literal;
default :
throw new RuntimeException("File format " + format + "is not support to build search arguments");
}
}
