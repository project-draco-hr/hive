{
  int lenSub=byteSub.length;
  if (len < lenSub) {
    return false;
  }
  int end=start + len - lenSub + 1;
  boolean match=false;
  for (int i=start; (i < end) && (!match); i++) {
    match=true;
    for (int j=0; j < lenSub; j++) {
      if (byteS[i + j] != byteSub[j]) {
        match=false;
        break;
      }
    }
  }
  return match;
}
