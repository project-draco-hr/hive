{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  QueryFragmentInfo that=(QueryFragmentInfo)o;
  if (fragmentNumber != that.fragmentNumber) {
    return false;
  }
  if (attemptNumber != that.attemptNumber) {
    return false;
  }
  return vertexName.equals(that.vertexName);
}
