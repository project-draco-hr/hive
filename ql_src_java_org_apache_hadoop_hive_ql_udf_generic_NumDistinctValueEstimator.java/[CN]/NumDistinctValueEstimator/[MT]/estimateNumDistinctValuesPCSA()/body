{
  double numDistinctValues=0.0;
  long S=0;
  for (int i=0; i < numBitVectors; i++) {
    int index=0;
    while (bitVector[i].get(index) && index < bitVectorSize) {
      index=index + 1;
    }
    S=S + index;
  }
  numDistinctValues=((numBitVectors / phi) * Math.pow(2.0,S / numBitVectors));
  return ((long)numDistinctValues);
}
