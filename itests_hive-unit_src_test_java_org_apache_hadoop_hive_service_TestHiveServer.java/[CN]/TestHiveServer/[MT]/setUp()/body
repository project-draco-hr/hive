{
  super.setUp();
  if (standAloneServer) {
    try {
      transport=new TSocket(host,port);
      TProtocol protocol=new TBinaryProtocol(transport);
      client=new HiveClient(protocol);
      transport.open();
    }
 catch (    Throwable e) {
      e.printStackTrace();
    }
  }
 else {
    client=new HiveServer.HiveServerHandler();
  }
}
