{
  super.parse(args);
  args=commandLine.getArgs();
  if (args.length >= 1) {
    System.err.println("This usage has been deprecated, consider using the new command " + "line syntax (run with -h to see usage information)");
    port=Integer.parseInt(args[0]);
  }
  if (args.length >= 2) {
    minWorkerThreads=Integer.parseInt(args[1]);
  }
  if (commandLine.hasOption('p')) {
    port=Integer.parseInt(commandLine.getOptionValue('p'));
  }
 else {
    String hivePort=System.getenv("HIVE_PORT");
    if (hivePort != null) {
      port=Integer.parseInt(hivePort);
    }
  }
  if (commandLine.hasOption(OPTION_MIN_WORKER_THREADS)) {
    minWorkerThreads=Integer.parseInt(commandLine.getOptionValue(OPTION_MIN_WORKER_THREADS));
  }
  if (commandLine.hasOption(OPTION_MAX_WORKER_THREADS)) {
    maxWorkerThreads=Integer.parseInt(commandLine.getOptionValue(OPTION_MAX_WORKER_THREADS));
  }
}
