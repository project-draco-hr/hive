{
  if (!isHiveQuery) {
    return "";
  }
  ArrayList<String> result=new ArrayList<String>();
  driver.setMaxRows(1);
  try {
    if (driver.getResults(result)) {
      return result.get(0);
    }
    return "";
  }
 catch (  IOException e) {
    HiveServerException ex=new HiveServerException();
    ex.setMessage(e.getMessage());
    throw ex;
  }
}
