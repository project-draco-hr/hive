{
  TableScanOperator node=(TableScanOperator)nd;
  WalkerCtx walkerCtx=(WalkerCtx)procCtx;
  if (((node.getNeededColumnIDs() == null) || (node.getNeededColumnIDs().size() == 0)) && ((node.getConf() == null) || (node.getConf().getVirtualCols() == null) || (node.getConf().getVirtualCols().isEmpty()))) {
    walkerCtx.setMayBeMetadataOnly(node);
  }
  return nd;
}
