{
  if (val == null) {
    return;
  }
  PrintWriter out=null;
  try {
    Path keyfile=new Path(getPath(type) + "/" + id+ "/"+ key);
    out=new PrintWriter(new OutputStreamWriter(fs.create(keyfile)));
    out.write(val);
  }
 catch (  IOException e) {
    LOG.info("Couldn't write to " + getPath(type) + "/"+ id+ ": "+ e.getMessage());
  }
 finally {
    try {
      out.flush();
      out.close();
    }
 catch (    Exception e) {
    }
  }
}
