{
  StringBuilder sb=new StringBuilder("DropPartitionsExpr(");
  boolean first=true;
  sb.append("expr:");
  if (this.expr == null) {
    sb.append("null");
  }
 else {
    org.apache.thrift.TBaseHelper.toString(this.expr,sb);
  }
  first=false;
  if (isSetPartArchiveLevel()) {
    if (!first)     sb.append(", ");
    sb.append("partArchiveLevel:");
    sb.append(this.partArchiveLevel);
    first=false;
  }
  sb.append(")");
  return sb.toString();
}
