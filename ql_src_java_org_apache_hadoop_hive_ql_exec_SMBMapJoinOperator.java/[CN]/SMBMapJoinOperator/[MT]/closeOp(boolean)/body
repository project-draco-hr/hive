{
  if (closeCalled) {
    return;
  }
  closeCalled=true;
  if ((this.getExecContext() != null && this.getExecContext().inputFileChanged()) || !firstFetchHappened) {
    for (    Map.Entry<String,FetchOperator> entry : fetchOperators.entrySet()) {
      String alias=entry.getKey();
      FetchOperator fetchOp=entry.getValue();
      fetchOp.clearFetchContext();
      setUpFetchOpContext(fetchOp,alias);
    }
    firstFetchHappened=true;
    for (    Byte t : order) {
      if (t != (byte)posBigTable) {
        fetchNextGroup(t);
      }
    }
  }
  joinFinalLeftData();
  for (  Byte alias : order) {
    if (alias != (byte)posBigTable) {
      fetchOpDone.put(alias,Boolean.FALSE);
      ;
    }
    foundNextKeyGroup.put(alias,Boolean.FALSE);
  }
  localWorkInited=false;
  super.closeOp(abort);
  if (fetchOperators != null) {
    for (    Map.Entry<String,FetchOperator> entry : fetchOperators.entrySet()) {
      Operator<? extends Serializable> forwardOp=localWork.getAliasToWork().get(entry.getKey());
      forwardOp.close(abort);
    }
  }
}
