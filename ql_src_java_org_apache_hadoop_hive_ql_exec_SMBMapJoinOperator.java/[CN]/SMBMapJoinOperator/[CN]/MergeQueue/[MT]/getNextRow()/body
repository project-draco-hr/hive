{
  if (currentMinSegment != null) {
    adjustPriorityQueue(currentMinSegment);
  }
  Integer current=top();
  if (current == null) {
    if (isLogInfoEnabled) {
      LOG.info("MergeQueue forwarded " + counter + " rows");
    }
    return null;
  }
  counter++;
  return keys[currentMinSegment=current].getSecond();
}
