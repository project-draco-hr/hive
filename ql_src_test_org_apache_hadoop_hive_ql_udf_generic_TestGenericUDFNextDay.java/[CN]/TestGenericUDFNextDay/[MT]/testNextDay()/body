  runAndVerify("2015-01-14",null,null,udf);
  runAndVerify(null,"SU",null,udf);
  runAndVerify(null,null,null,udf);
  runAndVerify("01/14/2015","TU",null,udf);
  runAndVerify("2015-01-14","VT",null,udf);
