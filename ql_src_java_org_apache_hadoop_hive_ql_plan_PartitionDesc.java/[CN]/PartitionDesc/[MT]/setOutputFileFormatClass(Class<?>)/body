{
  Class<? extends HiveOutputFormat> outputClass=outputFileFormatClass == null ? null : HiveFileFormatUtils.getOutputFormatSubstitute(outputFileFormatClass,false);
  if (outputClass != null) {
    this.outputFileFormatClass=(Class<? extends HiveOutputFormat>)CLASS_INTERNER.intern(outputClass);
  }
 else {
    this.outputFileFormatClass=outputClass;
  }
}
