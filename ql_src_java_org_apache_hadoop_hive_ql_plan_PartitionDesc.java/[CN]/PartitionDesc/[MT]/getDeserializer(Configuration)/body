{
  try {
    Deserializer deserializer=ReflectionUtils.newInstance(conf.getClassByName(getProperties().getProperty(serdeConstants.SERIALIZATION_LIB)).asSubclass(Deserializer.class),conf);
    deserializer.initialize(conf,getProperties());
    return deserializer;
  }
 catch (  Exception e) {
    return null;
  }
}
