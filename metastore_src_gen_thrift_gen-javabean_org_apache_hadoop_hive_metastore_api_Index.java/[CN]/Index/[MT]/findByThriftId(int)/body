{
switch (fieldId) {
case 1:
    return INDEX_NAME;
case 2:
  return INDEX_HANDLER_CLASS;
case 3:
return DB_NAME;
case 4:
return ORIG_TABLE_NAME;
case 5:
return CREATE_TIME;
case 6:
return LAST_ACCESS_TIME;
case 7:
return INDEX_TABLE_NAME;
case 8:
return SD;
case 9:
return PARAMETERS;
case 10:
return DEFERRED_REBUILD;
default :
return null;
}
}
