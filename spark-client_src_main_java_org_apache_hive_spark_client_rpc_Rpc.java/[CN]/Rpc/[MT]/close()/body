{
  if (!rpcClosed.compareAndSet(false,true)) {
    return;
  }
  try {
    channel.close().sync();
  }
 catch (  InterruptedException ie) {
    Thread.interrupted();
  }
 finally {
synchronized (listeners) {
      for (      Listener l : listeners) {
        try {
          l.rpcClosed(this);
        }
 catch (        Exception e) {
          LOG.warn("Error caught in Rpc.Listener invocation.",e);
        }
      }
    }
  }
}
