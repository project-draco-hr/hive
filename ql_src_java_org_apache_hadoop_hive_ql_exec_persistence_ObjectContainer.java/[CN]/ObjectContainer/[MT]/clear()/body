{
  readCursor=rowsInReadBuffer=rowsOnDisk=0;
  readBufferUsed=false;
  if (parentDir != null) {
    if (input != null) {
      try {
        input.close();
      }
 catch (      Throwable ignored) {
      }
      input=null;
    }
    if (output != null) {
      try {
        output.close();
      }
 catch (      Throwable ignored) {
      }
      output=null;
    }
    try {
      FileUtil.fullyDelete(parentDir);
    }
 catch (    Throwable ignored) {
    }
    parentDir=null;
    tmpFile=null;
  }
}
