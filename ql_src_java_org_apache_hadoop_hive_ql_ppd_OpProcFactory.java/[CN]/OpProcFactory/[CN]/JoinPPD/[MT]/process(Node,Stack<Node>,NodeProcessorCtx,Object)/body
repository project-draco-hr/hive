{
  LOG.info("Processing for " + nd.getName() + "("+ ((Operator)nd).getIdentifier()+ ")");
  OpWalkerInfo owi=(OpWalkerInfo)procCtx;
  Set<String> aliases=getQualifiedAliases((JoinOperator)nd,owi.getRowResolver(nd));
  boolean hasUnpushedPredicates=mergeWithChildrenPred(nd,owi,null,aliases,false);
  if (HiveConf.getBoolVar(owi.getParseContext().getConf(),HiveConf.ConfVars.HIVEPPDREMOVEDUPLICATEFILTERS)) {
    if (hasUnpushedPredicates) {
      aliases=null;
    }
    ExprWalkerInfo unpushedPreds=mergeChildrenPred(nd,owi,aliases,false);
    return createFilter((Operator)nd,unpushedPreds,owi);
  }
  return null;
}
