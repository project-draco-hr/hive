{
  if (nd.getChildren() == null) {
    return null;
  }
  Operator<? extends Serializable> op=(Operator<? extends Serializable>)nd;
  ExprWalkerInfo ewi=new ExprWalkerInfo();
  for (  Operator<? extends Serializable> child : op.getChildOperators()) {
    ExprWalkerInfo childPreds=owi.getPrunedPreds(child);
    if (childPreds == null) {
      continue;
    }
    for (    Entry<String,List<ExprNodeDesc>> e : childPreds.getFinalCandidates().entrySet()) {
      if (ignoreAliases || excludedAliases == null || !excludedAliases.contains(e.getKey()) || e.getKey() == null) {
        ewi.addPushDowns(e.getKey(),e.getValue());
        logExpr(nd,ewi);
      }
    }
  }
  return ewi;
}
