{
  if (doSleep) {
    try {
      Thread.sleep(100);
    }
 catch (    InterruptedException e) {
    }
  }
  int logSize=(int)offset.get();
  for (int i=0; i < logSize; ++i) {
    LOG.info("CAS history dump: " + FixedSizedObjectPool.toString(log[i << 1]) + " => "+ FixedSizedObjectPool.toString(log[(i << 1) + 1]));
  }
  offset.set(0);
}
