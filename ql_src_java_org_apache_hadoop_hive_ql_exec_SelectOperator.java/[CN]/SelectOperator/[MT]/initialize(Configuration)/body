{
  super.initialize(hconf);
  try {
    eval=new ExprNodeEvaluator[conf.getColList().size()];
    int i=0;
    for (    exprNodeDesc e : conf.getColList()) {
      eval[i++]=ExprNodeEvaluatorFactory.get(e);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw new RuntimeException(e);
  }
}
