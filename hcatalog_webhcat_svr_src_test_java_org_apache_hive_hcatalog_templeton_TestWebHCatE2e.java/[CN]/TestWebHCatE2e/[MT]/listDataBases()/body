{
  LOG.debug("+listDataBases()");
  MethodCallRetVal p=doHttpCall(templetonBaseUrl + "/ddl/database",HTTP_METHOD_TYPE.GET);
  Assert.assertEquals(p.getAssertMsg(),HttpStatus.OK_200,p.httpStatusCode);
