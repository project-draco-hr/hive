{
  if (exec.signals.empty()) {
    return 0;
  }
  if (exec.conf.onError == OnError.SETERROR || exec.conf.onError == OnError.STOP) {
    exec.signals.pop();
    return 0;
  }
  if (ctx.L_ID().toString().equalsIgnoreCase("OTHERS")) {
    trace(ctx,"EXCEPTION HANDLER");
    exec.signals.pop();
    enterScope(Scope.Type.HANDLER);
    visit(ctx.block());
    leaveScope();
  }
  return 0;
}
