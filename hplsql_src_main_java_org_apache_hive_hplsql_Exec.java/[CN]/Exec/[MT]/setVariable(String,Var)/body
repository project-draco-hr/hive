{
  if (value == null || value == Var.Empty) {
    if (exec.stack.empty()) {
      return Var.Empty;
    }
    value=exec.stack.pop();
  }
  if (name.startsWith("hplsql.")) {
    exec.conf.setOption(name,value.toString());
    return Var.Empty;
  }
  Var var=findVariable(name);
  if (var != null) {
    var.cast(value);
  }
 else {
    var=new Var(value);
    var.setName(name);
    exec.currentScope.addVariable(var);
  }
  return var;
}
