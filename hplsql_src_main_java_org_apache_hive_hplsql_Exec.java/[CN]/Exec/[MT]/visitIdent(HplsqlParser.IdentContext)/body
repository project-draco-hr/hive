{
  String ident=ctx.getText();
  Var var=findVariable(ident);
  if (var != null) {
    if (!exec.buildSql) {
      exec.stackPush(var);
    }
 else {
      exec.stackPush(new Var(ident,Var.Type.STRING,var.toSqlString()));
    }
  }
 else {
    if (!exec.buildSql && !exec.inCallStmt && exec.function.isProc(ident)&& exec.function.execProc(ident,null)) {
      return 0;
    }
 else {
      exec.stackPush(new Var(Var.Type.IDENT,ident));
    }
  }
  return 0;
}
