{
  Scope cur=exec.currentScope;
  String name2=null;
  if (name.startsWith(":")) {
    name2=name.substring(1);
  }
  while (cur != null) {
    for (    Var v : cur.vars) {
      if (name.equalsIgnoreCase(v.getName()) || (name2 != null && name2.equalsIgnoreCase(v.getName()))) {
        return v;
      }
    }
    cur=cur.parent;
  }
  return null;
}
