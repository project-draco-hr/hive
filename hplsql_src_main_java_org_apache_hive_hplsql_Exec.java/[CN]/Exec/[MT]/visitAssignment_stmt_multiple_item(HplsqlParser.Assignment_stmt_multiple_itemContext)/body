{
  int cnt=ctx.ident().size();
  int ecnt=ctx.expr().size();
  for (int i=0; i < cnt; i++) {
    String name=ctx.ident(i).getText();
    if (i < ecnt) {
      visit(ctx.expr(i));
      Var var=setVariable(name);
      if (trace) {
        trace(ctx,"SET " + name + " = "+ var.toString());
      }
    }
  }
  return 0;
}
