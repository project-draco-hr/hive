{
  if (this.negative) {
    if (this.mag.equals(SqlMathUtil.FULLBITS_32,SqlMathUtil.FULLBITS_32,SqlMathUtil.FULLBITS_32,SqlMathUtil.FULLBITS_31)) {
      SqlMathUtil.throwOverflowException();
    }
    this.mag.incrementDestructive();
    assert(this.mag.getV3() >= 0);
  }
 else {
    if (this.mag.isZero()) {
      this.negative=true;
      this.mag.incrementDestructive();
    }
 else {
      this.mag.decrementDestructive();
    }
  }
}
