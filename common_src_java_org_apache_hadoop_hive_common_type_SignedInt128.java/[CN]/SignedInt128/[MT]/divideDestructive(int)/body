{
  int ret;
  if (right < 0) {
    ret=this.mag.divideDestructive(-right);
    this.negative=!this.negative;
  }
 else {
    ret=this.mag.divideDestructive(right);
  }
  ret=ret & SqlMathUtil.FULLBITS_31;
  if (this.negative) {
    ret=-ret;
  }
  if (this.mag.isZero()) {
    this.negative=false;
  }
  return ret;
}
