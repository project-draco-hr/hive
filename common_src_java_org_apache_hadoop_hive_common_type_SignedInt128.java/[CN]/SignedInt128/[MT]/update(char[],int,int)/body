{
  if (length == 0) {
    this.zeroClear();
    return;
  }
  this.negative=false;
  if (str[offset] == '-') {
    this.negative=true;
    ++offset;
    --length;
  }
 else   if (str[offset] == '+') {
    ++offset;
    --length;
  }
  this.mag.update(str,offset,length);
  if (this.mag.isZero()) {
    this.negative=false;
  }
}
