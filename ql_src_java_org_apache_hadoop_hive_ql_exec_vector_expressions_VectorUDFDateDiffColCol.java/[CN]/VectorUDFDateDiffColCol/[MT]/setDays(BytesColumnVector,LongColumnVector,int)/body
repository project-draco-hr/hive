{
  if (input.isNull[i]) {
    output.isNull[i]=true;
    return;
  }
  String string=new String(input.vector[i],input.start[i],input.length[i]);
  try {
    date.setTime(formatter.parse(string).getTime());
    output.vector[i]=DateWritable.dateToDays(date);
  }
 catch (  ParseException e) {
    output.isNull[i]=true;
  }
}
