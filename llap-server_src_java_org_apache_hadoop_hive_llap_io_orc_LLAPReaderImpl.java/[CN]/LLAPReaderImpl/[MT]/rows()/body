{
  Reader.Options options=new Options();
  boolean[] include=options.getInclude();
  if (include == null) {
    include=new boolean[footer.getTypesCount()];
    Arrays.fill(include,true);
    options.include(include);
  }
  return new LLAPRecordReaderImpl(this.getStripes(),fileSystem,path,options,footer.getTypesList(),codec,bufferSize,footer.getRowIndexStride(),conf);
}
