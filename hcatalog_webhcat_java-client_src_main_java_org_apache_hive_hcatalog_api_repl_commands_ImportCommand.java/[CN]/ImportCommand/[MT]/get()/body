{
  StringBuilder sb=new StringBuilder();
  sb.append("IMPORT TABLE ");
  sb.append(dbName);
  sb.append('.');
  sb.append(tableName);
  sb.append(ReplicationUtils.partitionDescriptor(ptnDesc));
  sb.append(" FROM '");
  sb.append(importLocation);
  sb.append('\'');
  return Collections.singletonList(sb.toString());
}
