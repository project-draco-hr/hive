{
  StringBuilder sb=new StringBuilder();
  sb.append("TotalTasks: ");
  sb.append(getTotalTaskCount());
  sb.append(" Succeeded: ");
  sb.append(getSucceededTaskCount());
  sb.append(" Running: ");
  sb.append(getRunningTaskCount());
  sb.append(" Failed: ");
  sb.append(getFailedTaskCount());
  sb.append(" Killed: ");
  sb.append(getKilledTaskCount());
  return sb.toString();
}
