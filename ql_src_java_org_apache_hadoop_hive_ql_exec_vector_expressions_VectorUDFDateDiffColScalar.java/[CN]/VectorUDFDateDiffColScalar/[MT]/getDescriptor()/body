{
  VectorExpressionDescriptor.Builder b=new VectorExpressionDescriptor.Builder();
  b.setMode(VectorExpressionDescriptor.Mode.PROJECTION).setNumArguments(2).setArgumentTypes(VectorExpressionDescriptor.ArgumentType.ANY,VectorExpressionDescriptor.ArgumentType.ANY).setInputExpressionTypes(VectorExpressionDescriptor.InputExpressionType.COLUMN,VectorExpressionDescriptor.InputExpressionType.SCALAR);
  return b.build();
}
