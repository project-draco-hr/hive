{
  if (that == null)   return false;
  boolean this_present_message=true && (this.message != null);
  boolean that_present_message=true && (that.message != null);
  if (this_present_message || that_present_message) {
    if (!(this_present_message && that_present_message))     return false;
    if (!this.message.equals(that.message))     return false;
  }
  return true;
}
