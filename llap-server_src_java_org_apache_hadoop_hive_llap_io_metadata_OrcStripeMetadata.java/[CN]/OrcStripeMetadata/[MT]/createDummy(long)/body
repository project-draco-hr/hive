{
  OrcStripeMetadata dummy=new OrcStripeMetadata(id);
  dummy.encodings.add(ColumnEncoding.getDefaultInstance());
  dummy.streams.add(Stream.getDefaultInstance());
  RowIndex ri=RowIndex.newBuilder().addEntry(RowIndexEntry.newBuilder().addPositions(1).setStatistics(OrcFileMetadata.createStatsDummy())).build();
  BloomFilterIndex bfi=BloomFilterIndex.newBuilder().addBloomFilter(BloomFilter.newBuilder().addBitset(0)).build();
  dummy.rowIndex=new RecordReaderImpl.Index(new RowIndex[]{ri},new BloomFilterIndex[]{bfi});
  return dummy;
}
