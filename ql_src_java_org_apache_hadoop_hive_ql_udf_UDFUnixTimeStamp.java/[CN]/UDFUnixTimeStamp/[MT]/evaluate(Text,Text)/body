{
  if (dateText == null || patternText == null) {
    return null;
  }
  try {
    if (!patternText.equals(lastPatternText)) {
      formatter.applyPattern(patternText.toString());
      lastPatternText.set(patternText);
    }
  }
 catch (  Exception e) {
    return null;
  }
  return evaluate(dateText);
}
