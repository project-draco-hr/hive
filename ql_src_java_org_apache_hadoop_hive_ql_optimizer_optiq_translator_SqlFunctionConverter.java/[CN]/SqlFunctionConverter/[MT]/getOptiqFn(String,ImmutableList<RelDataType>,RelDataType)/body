{
  SqlOperator optiqOp=hiveToOptiq.get(hiveUdfName);
  if (optiqOp == null) {
    OptiqUDFInfo uInf=getUDFInfo(hiveUdfName,optiqArgTypes,optiqRetType);
    optiqOp=new SqlFunction(uInf.m_udfName,SqlKind.OTHER_FUNCTION,uInf.m_returnTypeInference,uInf.m_operandTypeInference,uInf.m_operandTypeChecker,SqlFunctionCategory.USER_DEFINED_FUNCTION);
  }
  return optiqOp;
}
