{
  SqlAggFunction optiqAggFn=(SqlAggFunction)hiveToOptiq.get(hiveUdfName);
  if (optiqAggFn == null) {
    OptiqUDFInfo uInf=getUDFInfo(hiveUdfName,optiqArgTypes,optiqRetType);
    optiqAggFn=new OptiqUDAF(uInf.udfName,uInf.returnTypeInference,uInf.operandTypeInference,uInf.operandTypeChecker,uInf.argTypes,uInf.retType);
  }
  return optiqAggFn;
}
