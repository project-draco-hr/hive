{
switch (opNode.getType()) {
case HiveParser.KW_EXISTS:
    return EXISTS;
case HiveParser.TOK_SUBQUERY_OP_NOTEXISTS:
  return NOT_EXISTS;
case HiveParser.KW_IN:
return IN;
case HiveParser.TOK_SUBQUERY_OP_NOTIN:
return NOT_IN;
default :
throw new SemanticException(SemanticAnalyzer.generateErrorMessage(opNode,"Operator not supported in SubQuery use."));
}
}
