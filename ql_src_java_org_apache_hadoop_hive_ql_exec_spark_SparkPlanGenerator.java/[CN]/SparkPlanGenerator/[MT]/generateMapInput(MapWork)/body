{
  JobConf jobConf=cloneJobConf(mapWork);
  Class ifClass=getInputFormat(jobConf,mapWork);
  JavaPairRDD<WritableComparable,Writable> hadoopRDD=sc.hadoopRDD(jobConf,ifClass,WritableComparable.class,Writable.class);
  StorageLevel level=cloneToWork.containsKey(mapWork) ? getStorageLevel(jobConf) : null;
  MapInput result=new MapInput(hadoopRDD,level);
  return result;
}
