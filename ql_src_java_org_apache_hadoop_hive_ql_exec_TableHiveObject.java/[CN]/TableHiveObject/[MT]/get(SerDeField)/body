{
  try {
    Object o=field.get(rowObject);
    if (o == null)     return new NullHiveObject();
    if (field.isPrimitive())     return new PrimitiveHiveObject(o);
 else     return new TableHiveObject(o,tableSerDe,field,rowObject,partCols,partFields);
  }
 catch (  SerDeException e) {
    throw new HiveException(e);
  }
}
