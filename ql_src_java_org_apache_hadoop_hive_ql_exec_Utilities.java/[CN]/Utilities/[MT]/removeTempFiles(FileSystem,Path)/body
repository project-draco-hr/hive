{
  if (path == null)   return;
  FileStatus items[]=fs.listStatus(path);
  if (items == null)   return;
  for (  FileStatus one : items) {
    if (isTempPath(one)) {
      if (!fs.delete(one.getPath(),true)) {
        throw new IOException("Unable to delete tmp file: " + one.getPath());
      }
    }
  }
}
