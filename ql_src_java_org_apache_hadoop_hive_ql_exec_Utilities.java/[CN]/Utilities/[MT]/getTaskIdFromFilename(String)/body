{
  Matcher m=fileNameTaskIdRegex.matcher(filename);
  if (!m.matches()) {
    LOG.warn("Unable to get task id from file name: " + filename + ". Using full filename as task id.");
    return filename;
  }
 else {
    String taskId=m.group(1);
    LOG.debug("TaskId for " + filename + " = "+ taskId);
    return taskId;
  }
}
