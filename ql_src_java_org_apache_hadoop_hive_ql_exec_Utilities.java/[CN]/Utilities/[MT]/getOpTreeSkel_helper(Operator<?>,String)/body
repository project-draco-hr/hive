{
  if (op == null) {
    return "";
  }
  StringBuilder sb=new StringBuilder();
  sb.append(indent);
  sb.append(op.toString());
  sb.append("\n");
  if (op.getChildOperators() != null) {
    for (    Object child : op.getChildOperators()) {
      sb.append(getOpTreeSkel_helper((Operator<?>)child,indent + "  "));
    }
  }
  return sb.toString();
}
