{
  String useName=conf.get(INPUT_NAME);
  if (useName == null) {
    useName="mapreduce";
  }
  conf.set(INPUT_NAME,useName);
  setMapWork(conf,w.getMapWork(),hiveScratchDir,true);
  if (w.getReduceWork() != null) {
    conf.set(INPUT_NAME,useName);
    setReduceWork(conf,w.getReduceWork(),hiveScratchDir,true);
  }
}
