{
  List<String> names=new ArrayList<String>();
  String colNames=props.getProperty(Constants.LIST_COLUMNS);
  String[] cols=colNames.trim().split(",");
  if (cols != null) {
    for (    String col : cols) {
      if (col != null && !col.trim().equals("")) {
        names.add(col);
      }
    }
  }
  return names;
}
