{
  if (op == null)   return "";
  StringBuffer sb=new StringBuffer();
  sb.append(indent);
  sb.append(op.toString());
  sb.append("\n");
  if (op.getChildOperators() != null)   for (  Object child : op.getChildOperators()) {
    sb.append(getOpTreeSkel_helper((Operator)child,indent + "  "));
  }
  return sb.toString();
}
