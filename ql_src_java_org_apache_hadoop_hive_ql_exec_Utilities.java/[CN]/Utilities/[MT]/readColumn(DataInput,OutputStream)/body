{
  while (true) {
    int b;
    try {
      b=(int)in.readByte();
    }
 catch (    EOFException e) {
      return streamStatus.EOF;
    }
    if (b == Utilities.newLineCode) {
      return streamStatus.TERMINATED;
    }
    if (b == Utilities.ctrlaCode) {
      return streamStatus.NORMAL;
    }
    out.write(b);
  }
}
