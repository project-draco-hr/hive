{
  try {
    Path planPath=new Path(HiveConf.getVar(job,HiveConf.ConfVars.PLAN));
    FileSystem fs=planPath.getFileSystem(job);
    if (fs.exists(planPath)) {
      try {
        fs.delete(planPath,true);
      }
 catch (      IOException e) {
        e.printStackTrace();
      }
    }
  }
 catch (  Exception e) {
  }
 finally {
    String jobID=getHiveJobID(job);
    if (jobID != null) {
      gWorkMap.remove(jobID);
    }
  }
}
