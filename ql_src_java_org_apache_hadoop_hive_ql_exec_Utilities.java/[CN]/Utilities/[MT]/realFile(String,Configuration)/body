{
  Path path=new Path(newFile);
  URI pathURI=path.toUri();
  FileSystem fs;
  if (pathURI.getScheme() == null) {
    fs=FileSystem.getLocal(conf);
  }
 else {
    fs=path.getFileSystem(conf);
  }
  if (!fs.exists(path)) {
    return null;
  }
  try {
    fs.close();
  }
 catch (  IOException e) {
  }
  ;
  return (path.makeQualified(fs).toString());
}
