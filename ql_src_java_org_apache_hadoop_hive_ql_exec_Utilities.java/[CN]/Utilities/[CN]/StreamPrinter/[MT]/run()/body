{
  try {
    InputStreamReader isr=new InputStreamReader(is);
    BufferedReader br=new BufferedReader(isr);
    String line=null;
    if (type != null) {
      while ((line=br.readLine()) != null)       os.println(type + ">" + line);
    }
 else {
      while ((line=br.readLine()) != null)       os.println(line);
    }
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
}
