{
  NodeList nodeList=xpath.evalNodeList(xml,path);
  if (nodeList == null) {
    return emptyResult;
  }
  result.clear();
  for (int i=0; i < nodeList.getLength(); i++) {
    String value=nodeList.item(i).getNodeValue();
    if (value != null) {
      result.add(new Text(value));
    }
  }
  return result;
}
