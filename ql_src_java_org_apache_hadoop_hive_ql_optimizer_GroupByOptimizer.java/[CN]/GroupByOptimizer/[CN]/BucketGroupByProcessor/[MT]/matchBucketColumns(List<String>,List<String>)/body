{
  if (tblBucketCols == null || tblBucketCols.size() == 0 || grpCols.size() == 0 || grpCols.size() != tblBucketCols.size()) {
    return false;
  }
  for (int i=0; i < grpCols.size(); i++) {
    String tblCol=grpCols.get(i);
    if (!tblBucketCols.contains(tblCol)) {
      return false;
    }
  }
  return true;
}
