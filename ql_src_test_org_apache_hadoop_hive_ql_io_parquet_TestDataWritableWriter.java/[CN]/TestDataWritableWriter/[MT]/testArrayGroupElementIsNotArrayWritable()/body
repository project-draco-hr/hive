{
  String schemaStr="message hive_schema {\n" + "  optional group array_of_arrays (LIST) {\n" + "    repeated group array {\n"+ "      required group element (LIST) {\n"+ "        required int32 element;\n"+ "      }\n"+ "    }\n"+ "  }\n"+ "}\n";
  ArrayWritable hiveRecord=createGroup(createGroup(createArray(createInt(1))));
  try {
    writeParquetRecord(schemaStr,hiveRecord);
    fail();
  }
 catch (  RuntimeException e) {
    assertEquals("Parquet record is malformed: Field value is not an ArrayWritable object: " + "required group element (LIST) {\n  required int32 element;\n}",e.getMessage());
  }
}
