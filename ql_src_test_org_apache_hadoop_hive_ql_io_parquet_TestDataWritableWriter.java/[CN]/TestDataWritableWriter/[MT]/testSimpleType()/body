{
  String schemaStr="message hive_schema {\n" + "  optional int32 int;\n" + "  optional double double;\n"+ "  optional boolean boolean;\n"+ "  optional float float;\n"+ "  optional binary string;\n"+ "}\n";
  ArrayWritable hiveRecord=createGroup(createInt(1),createDouble(1.0),createBoolean(true),createFloat(1.0f),createString("one"));
  writeParquetRecord(schemaStr,hiveRecord);
  startMessage();
  startField("int",0);
  addInteger(1);
  endField("int",0);
  startField("double",1);
  addDouble(1.0);
  endField("double",1);
  startField("boolean",2);
  addBoolean(true);
  endField("boolean",2);
  startField("float",3);
  addFloat(1.0f);
  endField("float",3);
  startField("string",4);
  addString("one");
  endField("string",4);
  endMessage();
}
