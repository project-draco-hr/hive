{
  PrimitiveCategory primitiveCategory=typeSpec.getPrimitiveCategory();
  BaseTypeParams typeParams=typeSpec.getTypeParams();
  PrimitiveObjectInspector poi=cachedParameterizedLazyObjectInspectors.getObjectInspector(typeSpec);
  if (poi == null) {
switch (primitiveCategory) {
default :
      throw new RuntimeException("Primitve type " + primitiveCategory + " should not take parameters");
  }
}
return poi;
}
