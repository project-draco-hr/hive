{
  if (this.pathNames == null) {
    return null;
  }
  if (pathNames.length == 1) {
    return pathNames[0];
  }
 else   if (pathNames.length == 2) {
    return pathNames[0] + "@" + pathNames[1];
  }
  String ret=pathNames[0] + "@" + pathNames[1]+ "@";
  boolean first=true;
  for (int i=2; i < pathNames.length; i++) {
    if (!first) {
      ret=ret + "/";
    }
 else {
      first=false;
    }
    ret=ret + pathNames[i];
  }
  return ret;
}
