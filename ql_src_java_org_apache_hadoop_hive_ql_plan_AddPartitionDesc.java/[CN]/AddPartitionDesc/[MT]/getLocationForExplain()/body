{
  if (this.partitions == null || this.partitions.isEmpty())   return "<no partition>";
  boolean isFirst=true;
  StringBuilder sb=new StringBuilder();
  for (  OnePartitionDesc desc : this.partitions) {
    if (!isFirst) {
      sb.append(", ");
    }
    isFirst=false;
    sb.append(desc.location);
  }
  return sb.toString();
}
