{
  String d=null;
  if (type.T_VARCHAR2() != null) {
    d="STRING";
  }
 else   if (type.T_NUMBER() != null) {
    d="DECIMAL";
    if (len != null) {
      d+=exec.getText(len);
    }
  }
  if (d != null) {
    return d;
  }
 else   if (len != null) {
    return exec.getText(type,type.getStart(),len.getStop());
  }
  return exec.getText(type,type.getStart(),type.getStop());
}
