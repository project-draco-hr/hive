{
  try {
    if (handleGroupKey && groupKey != null) {
      reducer.endGroup();
    }
  }
 catch (  Exception e) {
    if (!abort) {
      l4j.error("Hit error while closing operators - failing tree");
      throw new RuntimeException("Hive Runtime Error while closing operators: " + e.getMessage(),e);
    }
  }
  return abort;
}
