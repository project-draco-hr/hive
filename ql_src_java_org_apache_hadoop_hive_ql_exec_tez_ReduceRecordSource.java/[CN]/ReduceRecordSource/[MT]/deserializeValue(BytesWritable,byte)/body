{
  try {
    return inputValueDeserializer.deserialize(valueWritable);
  }
 catch (  SerDeException e) {
    throw new HiveException("Hive Runtime Error: Unable to deserialize reduce input value (tag=" + tag + ") from "+ Utilities.formatBinaryString(valueWritable.getBytes(),0,valueWritable.getLength())+ " with properties "+ valueTableDesc.getProperties(),e);
  }
}
