def recv_execute(self):
    iprot = self._iprot
    (fname, mtype, rseqid) = iprot.readMessageBegin()
    if (mtype == TMessageType.EXCEPTION):
        x = TApplicationException()
        x.read(iprot)
        iprot.readMessageEnd()
        raise x
    result = execute_result()
    result.read(iprot)
    iprot.readMessageEnd()
    if (result.ex is not None):
        raise result.ex
    return
