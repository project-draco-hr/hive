{
  if (files.size() <= 0) {
    return false;
  }
  for (int fileId=0; fileId < files.size(); fileId++) {
    RCFile.Reader reader=null;
    try {
      reader=new RCFile.Reader(fs,files.get(fileId).getPath(),conf);
      reader.close();
      reader=null;
    }
 catch (    IOException e) {
      return false;
    }
 finally {
      if (null != reader) {
        reader.close();
      }
    }
  }
  return true;
}
