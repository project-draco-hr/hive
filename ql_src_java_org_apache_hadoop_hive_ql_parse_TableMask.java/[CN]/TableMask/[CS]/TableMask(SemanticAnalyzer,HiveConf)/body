{
  try {
    authorizer=SessionState.get().getAuthorizerV2();
    if (authorizer != null && needTransform()) {
      enable=true;
      translator=new UnparseTranslator(conf);
      translator.enable();
    }
  }
 catch (  Exception e) {
    LOG.warn("Failed to initialize masking policy");
    throw new SemanticException(e);
  }
}
