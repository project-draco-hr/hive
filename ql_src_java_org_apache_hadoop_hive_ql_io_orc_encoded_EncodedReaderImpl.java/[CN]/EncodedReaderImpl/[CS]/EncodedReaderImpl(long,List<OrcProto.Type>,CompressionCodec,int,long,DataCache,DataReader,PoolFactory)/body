{
  this.fileId=fileId;
  this.codec=codec;
  this.types=types;
  this.bufferSize=bufferSize;
  this.rowIndexStride=strideRate;
  this.cache=cache;
  this.dataReader=dataReader;
  if (POOLS != null)   return;
  if (pf == null) {
    pf=new NoopPoolFactory();
  }
  Pools pools=createPools(pf);
synchronized (POOLS_CREATION_LOCK) {
    if (POOLS != null)     return;
    POOLS=pools;
  }
}
