{
  GenMRProcContext ctx=(GenMRProcContext)procCtx;
  Map<Operator<? extends Serializable>,GenMapRedCtx> mapCurrCtx=ctx.getMapCurrCtx();
  GenMapRedCtx mapredCtx=mapCurrCtx.get((Operator<? extends Serializable>)stack.get(stack.size() - 2));
  mapCurrCtx.put((Operator<? extends Serializable>)nd,new GenMapRedCtx(mapredCtx.getCurrTask(),mapredCtx.getCurrTopOp(),mapredCtx.getCurrAliasId()));
  return null;
}
