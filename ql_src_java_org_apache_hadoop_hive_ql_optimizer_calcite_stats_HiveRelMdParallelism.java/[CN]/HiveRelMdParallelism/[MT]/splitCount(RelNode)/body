{
  Boolean newPhase=RelMetadataQuery.isPhaseTransition(rel);
  if (newPhase == null) {
    return null;
  }
  if (newPhase) {
    return splitCountRepartition(rel);
  }
  Integer splitCount=0;
  for (  RelNode input : rel.getInputs()) {
    splitCount+=RelMetadataQuery.splitCount(input);
  }
  return splitCount;
}
