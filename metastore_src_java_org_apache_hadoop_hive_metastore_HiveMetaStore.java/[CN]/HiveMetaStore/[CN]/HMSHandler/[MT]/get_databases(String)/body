{
  startFunction("get_databases",": " + pattern);
  List<String> ret=null;
  Exception ex=null;
  try {
    ret=getMS().getDatabases(pattern);
  }
 catch (  Exception e) {
    ex=e;
    if (e instanceof MetaException) {
      throw (MetaException)e;
    }
 else {
      throw newMetaException(e);
    }
  }
 finally {
    endFunction("get_databases",ret != null,ex);
  }
  return ret;
}
