{
  startFunction("get_database",": " + name);
  Database db=null;
  try {
    db=getMS().getDatabase(name);
  }
 catch (  MetaException e) {
    throw e;
  }
catch (  NoSuchObjectException e) {
    throw e;
  }
catch (  Exception e) {
    assert(e instanceof RuntimeException);
    throw (RuntimeException)e;
  }
 finally {
    endFunction("get_database",db != null);
  }
  return db;
}
