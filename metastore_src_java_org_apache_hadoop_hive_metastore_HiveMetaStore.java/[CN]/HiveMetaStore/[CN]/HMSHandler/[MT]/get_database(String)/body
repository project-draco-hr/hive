{
  startFunction("get_database",": " + name);
  Database db=null;
  Exception ex=null;
  try {
    db=get_database_core(name);
    firePreEvent(new PreReadDatabaseEvent(db,this));
  }
 catch (  MetaException e) {
    ex=e;
    throw e;
  }
catch (  NoSuchObjectException e) {
    ex=e;
    throw e;
  }
 finally {
    endFunction("get_database",db != null,ex);
  }
  return db;
}
