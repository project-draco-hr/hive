{
  incrementCounter("get_partition_by_name");
  logStartFunction("get_partition_by_name: db=" + db_name + " tbl="+ tbl_name+ " part="+ part_name);
  Partition ret=null;
  try {
    ret=executeWithRetry(new Command<Partition>(){
      @Override Partition run(      RawStore ms) throws Exception {
        return get_partition_by_name_core(ms,db_name,tbl_name,part_name);
      }
    }
);
  }
 catch (  MetaException e) {
    throw e;
  }
catch (  NoSuchObjectException e) {
    throw e;
  }
catch (  TException e) {
    throw e;
  }
catch (  Exception e) {
    assert(e instanceof RuntimeException);
    throw (RuntimeException)e;
  }
  return ret;
}
