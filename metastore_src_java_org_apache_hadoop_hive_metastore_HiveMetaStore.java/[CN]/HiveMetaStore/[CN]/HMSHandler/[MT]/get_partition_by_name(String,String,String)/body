{
  startFunction("get_partition_by_name",": db=" + db_name + " tbl="+ tbl_name+ " part="+ part_name);
  Partition ret=null;
  Exception ex=null;
  try {
    ret=get_partition_by_name_core(getMS(),db_name,tbl_name,part_name);
  }
 catch (  Exception e) {
    ex=e;
    rethrowException(e);
  }
 finally {
    endFunction("get_partition_by_name",ret != null,ex,tbl_name);
  }
  return ret;
}
