{
  incrementCounter("get_index_by_name");
  logStartFunction("get_index_by_name: db=" + dbName + " tbl="+ tblName+ " index="+ indexName);
  Index ret=null;
  try {
    ret=executeWithRetry(new Command<Index>(){
      @Override Index run(      RawStore ms) throws Exception {
        return get_index_by_name_core(ms,dbName,tblName,indexName);
      }
    }
);
  }
 catch (  MetaException e) {
    throw e;
  }
catch (  NoSuchObjectException e) {
    throw e;
  }
catch (  TException e) {
    throw e;
  }
catch (  Exception e) {
    assert(e instanceof RuntimeException);
    throw (RuntimeException)e;
  }
  return ret;
}
