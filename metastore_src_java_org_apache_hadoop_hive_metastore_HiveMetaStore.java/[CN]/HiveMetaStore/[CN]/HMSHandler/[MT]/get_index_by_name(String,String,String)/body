{
  startFunction("get_index_by_name",": db=" + dbName + " tbl="+ tblName+ " index="+ indexName);
  Index ret=null;
  try {
    ret=executeWithRetry(new Command<Index>(){
      @Override public Index run(      RawStore ms) throws Exception {
        return get_index_by_name_core(ms,dbName,tblName,indexName);
      }
    }
);
  }
 catch (  MetaException e) {
    throw e;
  }
catch (  NoSuchObjectException e) {
    throw e;
  }
catch (  TException e) {
    throw e;
  }
catch (  Exception e) {
    assert(e instanceof RuntimeException);
    throw (RuntimeException)e;
  }
 finally {
    endFunction("drop_index_by_name",ret != null);
  }
  return ret;
}
