{
  incrementCounter("revoke_privileges");
  firePreEvent(new PreAuthorizationCallEvent(this));
  Boolean ret=null;
  try {
    ret=getMS().revokePrivileges(privileges,grantOption);
  }
 catch (  MetaException e) {
    throw e;
  }
catch (  Exception e) {
    throw new RuntimeException(e);
  }
  return ret;
}
