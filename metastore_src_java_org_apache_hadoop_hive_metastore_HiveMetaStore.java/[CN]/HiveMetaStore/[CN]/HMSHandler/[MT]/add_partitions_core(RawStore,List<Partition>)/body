{
  String db=parts.get(0).getDbName();
  String tbl=parts.get(0).getTableName();
  logInfo("add_partitions : db=" + db + " tbl="+ tbl);
  boolean success=false;
  try {
    ms.openTransaction();
    for (    Partition part : parts) {
      add_partition(part);
    }
    success=true;
    ms.commitTransaction();
  }
  finally {
    if (!success) {
      ms.rollbackTransaction();
    }
  }
  return parts.size();
}
