{
  if (newRole.equals(curRole)) {
    return true;
  }
  List<MRoleMap> parentRoleMaps=getMS().listRoles(curRole,PrincipalType.ROLE);
  for (  MRoleMap parentRole : parentRoleMaps) {
    if (isNewRoleAParent(newRole,parentRole.getRole().getRoleName())) {
      return true;
    }
  }
  return false;
}
