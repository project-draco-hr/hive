{
  dbName=dbName.toLowerCase();
  tableName=tableName.toLowerCase();
  if (colName != null) {
    colName=colName.toLowerCase();
  }
  startFunction("delete_column_statistics_by_table: db=" + dbName + " table="+ tableName+ " column="+ colName);
  boolean ret=false;
  try {
    ret=getMS().deleteTableColumnStatistics(dbName,tableName,colName);
  }
  finally {
    endFunction("delete_column_statistics_by_table: ",ret != false,null);
  }
  return ret;
}
