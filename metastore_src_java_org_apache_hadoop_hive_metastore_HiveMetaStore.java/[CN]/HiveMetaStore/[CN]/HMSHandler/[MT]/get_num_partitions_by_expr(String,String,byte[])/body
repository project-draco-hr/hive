{
  startTableFunction("get_num_partitions_by_expr",dbName,tblName);
  int ret=-1;
  Exception ex=null;
  try {
    ret=getMS().getNumPartitionsByExpr(dbName,tblName,expr);
  }
 catch (  Exception e) {
    ex=e;
    rethrowException(e);
  }
 finally {
    endFunction("get_num_partitions_by_expr",ret != -1,ex,tblName);
  }
  return ret;
}
