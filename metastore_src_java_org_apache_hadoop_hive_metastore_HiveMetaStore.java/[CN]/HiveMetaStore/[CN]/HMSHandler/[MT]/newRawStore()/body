{
  LOG.info(addPrefix("Opening raw store with implemenation class:" + rawStoreClassName));
  Configuration conf=getConf();
  RawStore ms=(RawStore)ReflectionUtils.newInstance(getClass(rawStoreClassName,RawStore.class),conf);
  return RetryingRawStore.getProxy(hiveConf,conf,ms,threadLocalId.get());
}
