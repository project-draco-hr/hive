{
  startFunction("update_master_key.");
  Exception ex=null;
  try {
    getMS().updateMasterKey(seq_number,key);
  }
 catch (  Exception e) {
    ex=e;
    if (e instanceof MetaException) {
      throw (MetaException)e;
    }
 else {
      throw newMetaException(e);
    }
  }
 finally {
    endFunction("update_master_key.",ex == null,ex);
  }
}
