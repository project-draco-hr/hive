{
  startFunction("drop_partition_by_name",": db=" + db_name + " tbl="+ tbl_name+ " part="+ part_name);
  boolean ret=false;
  try {
    ret=drop_partition_by_name_core(getMS(),db_name,tbl_name,part_name,deleteData);
  }
 catch (  IOException e) {
    throw new MetaException(e.getMessage());
  }
 finally {
    endFunction("drop_partition_by_name",ret);
  }
  return ret;
}
