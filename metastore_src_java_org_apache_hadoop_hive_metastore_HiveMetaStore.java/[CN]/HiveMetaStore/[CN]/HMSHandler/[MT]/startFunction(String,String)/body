{
  incrementCounter(function);
  logInfo((getIpAddress() == null ? "" : "source:" + getIpAddress() + " ") + function + extraLogInfo);
  if (hiveConf.getBoolVar(ConfVars.METASTORE_METRICS)) {
    try {
      MetricsFactory.getMetricsInstance().startScope(function);
    }
 catch (    IOException e) {
      LOG.debug("Exception when starting metrics scope" + e.getClass().getName() + " "+ e.getMessage(),e);
    }
  }
  return function;
}
