{
  if (partPaths != null && !partPaths.isEmpty()) {
    for (    Path partPath : partPaths) {
      try {
        wh.deleteDir(partPath,true,ifPurge);
      }
 catch (      Exception e) {
        LOG.error("Failed to delete partition directory: " + partPath + " "+ e.getMessage());
      }
    }
  }
}
