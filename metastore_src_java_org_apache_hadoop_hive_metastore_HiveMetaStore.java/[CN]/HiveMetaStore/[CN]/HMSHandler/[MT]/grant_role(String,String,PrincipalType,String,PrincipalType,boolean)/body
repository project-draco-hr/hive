{
  incrementCounter("add_role_member");
  Boolean ret=null;
  try {
    RawStore ms=getMS();
    Role role=ms.getRole(roleName);
    ret=ms.grantRole(role,userName,principalType,grantor,grantorType,grantOption);
  }
 catch (  MetaException e) {
    throw e;
  }
catch (  Exception e) {
    throw new RuntimeException(e);
  }
  return ret;
}
