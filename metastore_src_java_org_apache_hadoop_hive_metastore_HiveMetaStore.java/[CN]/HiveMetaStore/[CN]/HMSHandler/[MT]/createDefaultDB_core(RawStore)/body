{
  try {
    ms.getDatabase(MetaStoreUtils.DEFAULT_DATABASE_NAME);
  }
 catch (  NoSuchObjectException e) {
    ms.createDatabase(new Database(MetaStoreUtils.DEFAULT_DATABASE_NAME,wh.getDefaultDatabasePath(MetaStoreUtils.DEFAULT_DATABASE_NAME).toString()));
  }
  HMSHandler.createDefaultDB=true;
}
