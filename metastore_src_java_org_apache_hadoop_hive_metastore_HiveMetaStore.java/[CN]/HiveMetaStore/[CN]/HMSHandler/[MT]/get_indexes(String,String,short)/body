{
  startTableFunction("get_indexes",dbName,tblName);
  List<Index> ret=null;
  Exception ex=null;
  try {
    ret=getMS().getIndexes(dbName,tblName,maxIndexes);
  }
 catch (  Exception e) {
    ex=e;
    rethrowException(e);
  }
 finally {
    endFunction("get_indexes",ret != null,ex,tblName);
  }
  return ret;
}
