{
  incrementCounter("remove_role_member");
  Boolean ret=null;
  try {
    RawStore ms=getMS();
    Role mRole=ms.getRole(roleName);
    ret=ms.revokeRole(mRole,userName,principalType);
  }
 catch (  MetaException e) {
    throw e;
  }
catch (  Exception e) {
    throw new RuntimeException(e);
  }
  return ret;
}
