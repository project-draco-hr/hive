{
  startFunction("add_master_key.");
  int ret=-1;
  Exception ex=null;
  try {
    ret=getMS().addMasterKey(key);
  }
 catch (  Exception e) {
    ex=e;
    if (e instanceof MetaException) {
      throw (MetaException)e;
    }
 else {
      throw newMetaException(e);
    }
  }
 finally {
    endFunction("add_master_key.",ex == null,ex);
  }
  return ret;
}
