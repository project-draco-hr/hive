{
  if (tenScale == 0) {
    return difference(left,right,result);
  }
  int[] r=right.v.clone();
  if (tenScale < 0) {
    scaleDownTenArray4RoundUp(r,(short)-tenScale);
  }
 else {
    boolean overflow=scaleUpTenArray(r,tenScale);
    if (overflow) {
      SqlMathUtil.throwOverflowException();
    }
  }
  return differenceInternal(left,r,result);
}
