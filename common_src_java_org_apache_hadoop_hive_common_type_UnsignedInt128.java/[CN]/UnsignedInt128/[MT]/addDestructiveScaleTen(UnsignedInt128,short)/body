{
  if (tenScale == 0) {
    addDestructive(right);
    return;
  }
  final int[] r=right.v.clone();
  if (tenScale < 0) {
    scaleDownTenArray4RoundUp(r,(short)-tenScale);
  }
 else   if (tenScale > 0) {
    boolean overflow=scaleUpTenArray(r,tenScale);
    if (overflow) {
      SqlMathUtil.throwOverflowException();
    }
  }
  addDestructive(r);
}
