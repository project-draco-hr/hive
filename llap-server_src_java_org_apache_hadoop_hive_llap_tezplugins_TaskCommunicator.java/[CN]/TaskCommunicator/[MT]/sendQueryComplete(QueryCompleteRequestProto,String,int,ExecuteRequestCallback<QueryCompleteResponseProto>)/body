{
  ListenableFuture<QueryCompleteResponseProto> future=executor.submit(new SendQueryCompleteCallable(host,port,request));
  Futures.addCallback(future,new FutureCallback<QueryCompleteResponseProto>(){
    @Override public void onSuccess(    QueryCompleteResponseProto result){
      callback.setResponse(result);
    }
    @Override public void onFailure(    Throwable t){
      callback.indicateError(t);
    }
  }
);
}
