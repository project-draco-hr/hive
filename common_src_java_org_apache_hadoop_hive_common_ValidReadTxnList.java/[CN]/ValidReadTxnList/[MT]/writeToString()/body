{
  StringBuilder buf=new StringBuilder();
  buf.append(highWatermark);
  buf.append(':');
  buf.append(minOpenTxn);
  if (exceptions.length == 0) {
    buf.append(':');
  }
 else {
    for (    long except : exceptions) {
      buf.append(':');
      buf.append(except);
    }
  }
  return buf.toString();
}
