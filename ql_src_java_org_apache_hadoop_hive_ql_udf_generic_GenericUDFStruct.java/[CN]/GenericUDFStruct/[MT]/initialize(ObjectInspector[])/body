{
  int numFields=arguments.length;
  ret=new Object[numFields];
  ArrayList<String> fname=new ArrayList<String>(numFields);
  for (int f=1; f <= numFields; f++) {
    fname.add("col" + f);
  }
  StructObjectInspector soi=ObjectInspectorFactory.getStandardStructObjectInspector(fname,Arrays.asList(arguments));
  return soi;
}
