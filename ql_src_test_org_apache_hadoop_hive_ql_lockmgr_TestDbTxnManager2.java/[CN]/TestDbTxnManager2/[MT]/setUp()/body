{
  SessionState.start(conf);
  ctx=new Context(conf);
  driver=new Driver(conf);
  driver.init();
  TxnDbUtil.cleanDb();
  TxnDbUtil.prepDb();
  SessionState ss=SessionState.get();
  ss.initTxnMgr(conf);
  txnMgr=ss.getTxnMgr();
  Assert.assertTrue(txnMgr instanceof DbTxnManager);
  txnHandler=TxnUtils.getTxnStore(conf);
}
