{
  String id=null;
  if (ids != null) {
    for (int i=0; i < ids.size(); i++) {
      if (i == 0) {
        id="" + ids.get(i);
      }
 else {
        id=id + StringUtils.COMMA_STR + ids.get(i);
      }
    }
  }
  if (id == null || id.length() <= 0) {
    conf.set(READ_COLUMN_IDS_CONF_STR,"");
    return;
  }
  conf.set(READ_COLUMN_IDS_CONF_STR,id);
}
