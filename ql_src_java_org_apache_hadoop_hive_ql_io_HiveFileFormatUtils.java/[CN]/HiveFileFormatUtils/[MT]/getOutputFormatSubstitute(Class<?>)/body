{
  if (origin == null || HiveOutputFormat.class.isAssignableFrom(origin)) {
    return (Class<? extends OutputFormat>)origin;
  }
  Class<? extends OutputFormat> substitute=FileChecker.getInstance().getOutputFormatSubstiture(origin);
  if (substitute != null) {
    return substitute;
  }
  return (Class<? extends OutputFormat>)origin;
}
