{
  if (origin == null || HiveOutputFormat.class.isAssignableFrom(origin)) {
    return (Class<? extends OutputFormat>)origin;
  }
  Class<? extends OutputFormat> substitute=outputFormatSubstituteMap.get(origin);
  if (substitute != null) {
    return substitute;
  }
  return (Class<? extends OutputFormat>)origin;
}
