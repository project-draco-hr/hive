{
  data.write(dataOutput);
  if (partVal == null) {
    dataOutput.writeBoolean(false);
  }
 else {
    dataOutput.writeBoolean(true);
    partVal.write(dataOutput);
  }
  if (recId == null) {
    dataOutput.writeBoolean(false);
  }
 else {
    dataOutput.writeBoolean(true);
    recId.write(dataOutput);
  }
}
