{
  ByteArrayOutputStream out=new ByteArrayOutputStream();
  try {
    jobConf.write(new DataOutputStream(out));
  }
 catch (  IOException e) {
    e.printStackTrace();
    return null;
  }
 finally {
    try {
      out.close();
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
  return out.toByteArray();
}
