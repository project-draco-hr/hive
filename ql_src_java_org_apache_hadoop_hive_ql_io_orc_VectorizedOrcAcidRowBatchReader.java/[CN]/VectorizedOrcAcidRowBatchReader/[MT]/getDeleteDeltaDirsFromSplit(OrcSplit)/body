{
  Path path=orcSplit.getPath();
  Path root;
  if (orcSplit.hasBase()) {
    if (orcSplit.isOriginal()) {
      root=path.getParent();
    }
 else {
      root=path.getParent().getParent();
    }
  }
 else {
    root=path;
  }
  return AcidUtils.deserializeDeleteDeltas(root,orcSplit.getDeltas());
}
