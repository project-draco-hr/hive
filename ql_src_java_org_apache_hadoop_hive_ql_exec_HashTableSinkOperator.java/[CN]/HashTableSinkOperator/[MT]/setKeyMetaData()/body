{
  TableDesc keyTableDesc=conf.getKeyTblDesc();
  SerDe keySerializer=(SerDe)ReflectionUtils.newInstance(keyTableDesc.getDeserializerClass(),null);
  keySerializer.initialize(null,keyTableDesc.getProperties());
  MapJoinMetaData.clear();
  MapJoinMetaData.put(Integer.valueOf(metadataKeyTag),new HashTableSinkObjectCtx(ObjectInspectorUtils.getStandardObjectInspector(keySerializer.getObjectInspector(),ObjectInspectorCopyOption.WRITABLE),keySerializer,keyTableDesc,false,hconf));
}
