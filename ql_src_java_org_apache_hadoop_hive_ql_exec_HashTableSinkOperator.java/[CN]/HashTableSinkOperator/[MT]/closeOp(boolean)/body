{
  try {
    if (mapJoinTables == null) {
      if (isLogDebugEnabled) {
        LOG.debug("mapJoinTables is null");
      }
    }
 else {
      flushToFile();
    }
    super.closeOp(abort);
  }
 catch (  HiveException e) {
    throw e;
  }
catch (  Exception e) {
    throw new HiveException(e);
  }
}
