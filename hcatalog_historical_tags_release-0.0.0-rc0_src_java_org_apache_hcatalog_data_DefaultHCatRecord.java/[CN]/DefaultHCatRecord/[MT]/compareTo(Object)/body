{
  if (that instanceof HCatRecord) {
    HCatRecord other=(HCatRecord)that;
    int mySz=this.size();
    int urSz=other.size();
    if (mySz != urSz) {
      return mySz - urSz;
    }
 else {
      for (int i=0; i < mySz; i++) {
        int c=DataType.compare(get(i),other.get(i));
        if (c != 0) {
          return c;
        }
      }
    }
    return 0;
  }
 else {
    return DataType.compare(this,that);
  }
}
