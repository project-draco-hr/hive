{
  Table table;
  try {
    table=Hive.get().getTable(dbName,tabName);
  }
 catch (  HiveException e) {
    return 0;
  }
  Map<String,String> params=table.getParameters();
  long result=0;
  if (params != null) {
    try {
      result=Long.parseLong(params.get(statType));
    }
 catch (    NumberFormatException e) {
      result=0;
    }
  }
  return result;
}
