{
  if (csPK != null && csFK != null) {
    if (csPK.isPrimaryKey()) {
      if (csPK.getRange() != null && csFK.getRange() != null) {
        ColStatistics.Range pkRange=csPK.getRange();
        ColStatistics.Range fkRange=csFK.getRange();
        return isWithin(fkRange,pkRange);
      }
    }
  }
  return false;
}
