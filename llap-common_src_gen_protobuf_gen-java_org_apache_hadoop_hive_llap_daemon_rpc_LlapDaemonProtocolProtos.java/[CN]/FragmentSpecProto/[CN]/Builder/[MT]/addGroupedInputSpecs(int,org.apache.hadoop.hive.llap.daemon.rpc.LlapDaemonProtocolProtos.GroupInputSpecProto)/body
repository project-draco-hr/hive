{
  if (groupedInputSpecsBuilder_ == null) {
    if (value == null) {
      throw new NullPointerException();
    }
    ensureGroupedInputSpecsIsMutable();
    groupedInputSpecs_.add(index,value);
    onChanged();
  }
 else {
    groupedInputSpecsBuilder_.addMessage(index,value);
  }
  return this;
}
