{
  LlapNodeId llapNodeId=getNodeIfForTaskAttempt(taskAttemptId);
  if (llapNodeId != null) {
    BiMap<TezTaskAttemptID,ContainerId> bMap=nodeMap.get(llapNodeId).inverse();
    if (bMap != null) {
      return bMap.get(taskAttemptId);
    }
 else {
      return null;
    }
  }
 else {
    return null;
  }
}
