{
  communicator.sendSourceStateUpdate(request,host,port,new LlapDaemonProtocolClientProxy.ExecuteRequestCallback<SourceStateUpdatedResponseProto>(){
    @Override public void setResponse(    SourceStateUpdatedResponseProto response){
    }
    @Override public void indicateError(    Throwable t){
      LOG.error("Failed to send state update to node: " + host + ":"+ port+ ", StateUpdate="+ request,t);
    }
  }
);
}
