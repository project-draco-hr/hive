{
  try {
    if (currRecReader != null) {
      currRecReader.close();
      currRecReader=null;
    }
    if (operator != null) {
      operator.close(false);
      operator=null;
    }
    if (context != null) {
      context.clear();
      context=null;
    }
    this.currPath=null;
    this.iterPath=null;
    this.iterPartDesc=null;
    this.iterSplits=Iterators.emptyIterator();
  }
 catch (  Exception e) {
    throw new HiveException("Failed with exception " + e.getMessage() + StringUtils.stringifyException(e));
  }
}
