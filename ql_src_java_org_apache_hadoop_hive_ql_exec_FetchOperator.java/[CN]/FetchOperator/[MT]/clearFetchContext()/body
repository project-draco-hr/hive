{
  try {
    if (currRecReader != null) {
      currRecReader.close();
      currRecReader=null;
    }
    this.currPath=null;
    this.iterPath=null;
    this.iterPartDesc=null;
  }
 catch (  Exception e) {
    throw new HiveException("Failed with exception " + e.getMessage() + org.apache.hadoop.util.StringUtils.stringifyException(e));
  }
}
