{
  if (partDirs != null) {
    StringBuilder buff=new StringBuilder();
    boolean first=true;
    for (    Path p : partDirs) {
      if (first) {
        first=false;
      }
 else {
        buff.append('\t');
      }
      buff.append(StringEscapeUtils.escapeJava(p.toString()));
    }
    conf.set(FETCH_OPERATOR_DIRECTORY_LIST,buff.toString());
  }
}
