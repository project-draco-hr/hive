{
  String pcols=table.getProperties().getProperty(org.apache.hadoop.hive.metastore.api.Constants.META_TABLE_PARTITION_COLUMNS);
  String[] partKeys=pcols.trim().split("/");
  if (partSpec != null) {
    rowWithPart[1]=createPartValue(partKeys,partSpec);
  }
  rowObjectInspector=createRowInspector(partKeys);
}
