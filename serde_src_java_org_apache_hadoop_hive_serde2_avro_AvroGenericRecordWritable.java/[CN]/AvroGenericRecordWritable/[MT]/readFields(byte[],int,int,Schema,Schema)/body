{
  fileSchema=writerSchema;
  record=new GenericData.Record(writerSchema);
  binaryDecoder=DecoderFactory.get().binaryDecoder(bytes,offset,length - offset,binaryDecoder);
  GenericDatumReader<GenericRecord> gdr=new GenericDatumReader<GenericRecord>(writerSchema,readerSchema);
  record=gdr.read(null,binaryDecoder);
}
