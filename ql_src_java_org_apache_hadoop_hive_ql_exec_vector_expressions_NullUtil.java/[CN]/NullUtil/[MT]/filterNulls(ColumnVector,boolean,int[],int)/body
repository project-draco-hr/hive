{
  int newSize=0;
  if (v.noNulls) {
    return n;
  }
  if (v.isRepeating) {
    return v.isNull[0] ? 0 : n;
  }
  if (selectedInUse) {
    for (int j=0; j != n; j++) {
      int i=sel[j];
      if (!v.isNull[i]) {
        sel[newSize++]=i;
      }
    }
  }
 else {
    for (int i=0; i != n; i++) {
      if (!v.isNull[i]) {
        sel[newSize++]=i;
      }
    }
  }
  return newSize;
}
