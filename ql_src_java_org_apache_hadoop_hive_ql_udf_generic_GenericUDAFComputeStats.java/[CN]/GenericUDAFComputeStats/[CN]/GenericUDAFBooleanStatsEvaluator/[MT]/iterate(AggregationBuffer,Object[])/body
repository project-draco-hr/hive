{
  Object p=parameters[0];
  BooleanStatsAgg myagg=(BooleanStatsAgg)agg;
  if (p == null) {
    myagg.countNulls++;
  }
 else {
    try {
      boolean v=PrimitiveObjectInspectorUtils.getBoolean(p,inputOI);
      if (v == false) {
        myagg.countFalses++;
      }
 else       if (v == true) {
        myagg.countTrues++;
      }
    }
 catch (    NumberFormatException e) {
      if (!warned) {
        warned=true;
        LOG.warn(getClass().getSimpleName() + " " + StringUtils.stringifyException(e));
        LOG.warn(getClass().getSimpleName() + " ignoring similar exceptions.");
      }
    }
  }
}
