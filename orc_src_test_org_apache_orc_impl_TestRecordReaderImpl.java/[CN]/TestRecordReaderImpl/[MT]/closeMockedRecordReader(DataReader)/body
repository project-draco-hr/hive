{
  Configuration conf=new Configuration();
  Path path=new Path(workDir,"empty.orc");
  FileSystem.get(conf).delete(path,true);
  Writer writer=OrcFile.createWriter(path,OrcFile.writerOptions(conf).setSchema(TypeDescription.createLong()));
  writer.close();
  Reader reader=OrcFile.createReader(path,OrcFile.readerOptions(conf));
  RecordReader recordReader=reader.rows(new Reader.Options().dataReader(mockedDataReader));
  recordReader.close();
}
