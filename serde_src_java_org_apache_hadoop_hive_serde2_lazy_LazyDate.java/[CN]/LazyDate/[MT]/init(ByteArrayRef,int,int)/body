{
  String s=null;
  if (!LazyUtils.isDateMaybe(bytes.getData(),start,length)) {
    isNull=true;
    return;
  }
  try {
    s=Text.decode(bytes.getData(),start,length);
    data.set(Date.valueOf(s));
    isNull=false;
  }
 catch (  Exception e) {
    isNull=true;
    logExceptionMessage(bytes,start,length,"DATE");
  }
}
