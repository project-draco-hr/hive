{
  if (arguments.length != 2) {
    throw new UDFArgumentLengthException("The function SPLIT(s, regexp) takes exactly 2 arguments.");
  }
  if (!(arguments[0] instanceof WritableStringObjectInspector)) {
    throw new UDFArgumentTypeException(0,"Arguments of SPLIT(s, regexp) must be strings.");
  }
  if (!(arguments[1] instanceof WritableStringObjectInspector)) {
    throw new UDFArgumentTypeException(1,"Arguments of SPLIT(s, regexp) must be strings.");
  }
  return ObjectInspectorFactory.getStandardListObjectInspector(PrimitiveObjectInspectorFactory.writableStringObjectInspector);
}
