{
  DataBuilder db=new DataBuilder();
  db.setColumnNames("a","b","c");
  db.setColumnTypes(PrimitiveObjectInspectorFactory.javaStringObjectInspector,PrimitiveObjectInspectorFactory.javaIntObjectInspector,PrimitiveObjectInspectorFactory.javaDoubleObjectInspector);
  db.addRow("one",1,new Double("1.1"));
  db.addRow(null,null,null);
  db.addRow("two",2,new Double("2.1"));
  return db.createRows();
}
