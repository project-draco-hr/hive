{
  if (level.index >= mLevel.index) {
    mLog.print(mDateFormatter.format(new Date()));
    mLog.print(" ");
    mLog.print(String.format("%5s",level.name()));
    mLog.print(" ");
    mLog.print("[");
    mLog.print(getThreadName());
    mLog.print("]");
    mLog.print(" ");
    mLog.print(getCaller());
    mLog.print(" ");
    mLog.print(msg);
    if (t != null) {
      mLog.print(" ");
      t.printStackTrace(mLog);
    }
    mLog.print("\n");
    mLog.flush();
  }
}
