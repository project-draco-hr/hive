{
  try {
    return outf.getRecordUpdater(partitionPath,new AcidOutputFormat.Options(conf).inspector(getSerde().getObjectInspector()).bucket(bucketId).minimumTransactionId(minTxnId).maximumTransactionId(maxTxnID));
  }
 catch (  SerDeException e) {
    throw new SerializationError("Failed to get object inspector from Serde " + getSerde().getClass().getName(),e);
  }
}
