{
  StringBuilder buffer=new StringBuilder();
  buffer.append("[");
  boolean isFirst=true;
  while (range != null) {
    if (!isFirst) {
      buffer.append(", {");
    }
 else {
      buffer.append("{");
    }
    isFirst=false;
    buffer.append(range.toString());
    buffer.append("}");
    range=range.next;
  }
  buffer.append("]");
  return buffer.toString();
}
