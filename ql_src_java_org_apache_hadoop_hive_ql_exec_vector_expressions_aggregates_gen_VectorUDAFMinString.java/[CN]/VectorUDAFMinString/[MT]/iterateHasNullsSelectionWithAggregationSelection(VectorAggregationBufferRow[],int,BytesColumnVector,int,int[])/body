{
  for (int i=0; i < batchSize; ++i) {
    int row=selection[i];
    if (!inputColumn.isNull[row]) {
      Aggregation myagg=getCurrentAggregationBuffer(aggregationBufferSets,aggregrateIndex,i);
      myagg.checkValue(inputColumn.vector[row],inputColumn.start[row],inputColumn.length[row]);
    }
  }
}
