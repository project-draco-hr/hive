{
  System.out.println("Evaluating " + expr + " for "+ times+ " times");
  new InspectableObject();
  ObjectInspector resultOI=eval.initialize(input.oi);
  Object resultO=null;
  long start=System.currentTimeMillis();
  for (int i=0; i < times; i++) {
    resultO=eval.evaluate(input.o);
  }
  long end=System.currentTimeMillis();
  assertEquals(standardJavaOutput,ObjectInspectorUtils.copyToStandardObject(resultO,resultOI,ObjectInspectorCopyOption.JAVA));
  System.out.println("Evaluation finished: " + String.format("%2.3f",(end - start) * 0.001) + " seconds, "+ String.format("%2.3f",(end - start) * 1000.0 / times)+ " seconds/million call.");
}
