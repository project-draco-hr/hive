{
  StringBuilder sb=new StringBuilder();
  sb.append(UDFClass.toString());
  sb.append(".");
  sb.append(UDFMethod.toString());
  sb.append("(");
  for (int i=0; i < children.size(); i++) {
    if (i > 0)     sb.append(", ");
    sb.append(children.get(i).toString());
  }
  sb.append("(");
  sb.append(")");
  return sb.toString();
}
