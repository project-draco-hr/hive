{
  if (distinctVals.isEmpty()) {
    return 2;
  }
  if (distinctVals.size() <= 2) {
    return Collections.max(distinctVals);
  }
 else {
    long denom=1;
    for (int i=0; i < distinctVals.size() - 1; i++) {
      long v1=distinctVals.get(i);
      long v2=distinctVals.get(i + 1);
      if (v1 >= v2) {
        denom*=v1;
      }
 else {
        denom*=v2;
      }
    }
    return denom;
  }
}
