{
  long time=0;
  try {
    time=hmsClient.renewDelegationToken(tokenStrForm);
  }
 catch (  MetaException e) {
    throw new HCatException("MetaException while renewing delegation token.",e);
  }
catch (  TException e) {
    throw new ConnectionFailureException("TException while renewing delegation token.",e);
  }
  return time;
}
