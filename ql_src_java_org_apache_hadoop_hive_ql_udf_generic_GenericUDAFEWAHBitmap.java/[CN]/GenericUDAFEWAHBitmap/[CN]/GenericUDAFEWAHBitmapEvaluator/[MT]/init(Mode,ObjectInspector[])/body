{
  super.init(m,parameters);
  if (m == Mode.PARTIAL1) {
    inputOI=(PrimitiveObjectInspector)parameters[0];
    return ObjectInspectorFactory.getStandardListObjectInspector(PrimitiveObjectInspectorFactory.writableLongObjectInspector);
  }
 else   if (m == Mode.PARTIAL2 || m == Mode.FINAL) {
    internalMergeOI=(StandardListObjectInspector)parameters[0];
    bitmapLongOI=PrimitiveObjectInspectorFactory.writableLongObjectInspector;
    inputOI=PrimitiveObjectInspectorFactory.writableByteObjectInspector;
    loi=(StandardListObjectInspector)ObjectInspectorFactory.getStandardListObjectInspector(PrimitiveObjectInspectorFactory.writableLongObjectInspector);
    return loi;
  }
 else {
    bitmapLongOI=PrimitiveObjectInspectorFactory.writableLongObjectInspector;
    inputOI=PrimitiveObjectInspectorFactory.writableByteObjectInspector;
    loi=(StandardListObjectInspector)ObjectInspectorFactory.getStandardListObjectInspector(PrimitiveObjectInspectorFactory.writableLongObjectInspector);
    return loi;
  }
}
