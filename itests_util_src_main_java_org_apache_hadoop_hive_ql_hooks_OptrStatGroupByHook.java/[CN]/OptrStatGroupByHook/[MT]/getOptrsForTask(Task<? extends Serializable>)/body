{
  Collection<Operator<? extends OperatorDesc>> topOptrs=task.getTopOperators();
  Set<Operator<? extends OperatorDesc>> allOptrs=new HashSet<Operator<? extends OperatorDesc>>();
  Queue<Operator<? extends OperatorDesc>> opsToVisit=new LinkedList<Operator<? extends OperatorDesc>>();
  if (topOptrs != null) {
    opsToVisit.addAll(topOptrs);
    addChildOptrs(opsToVisit,allOptrs);
  }
  return allOptrs;
}
