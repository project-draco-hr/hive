{
  Object[] os=new Object[fs.size()];
  int i=0;
  for (  Future<?> f : fs) {
    try {
      if (abortOp.get()) {
        f.cancel(true);
      }
 else {
        os[i++]=f.get();
      }
    }
 catch (    Exception e) {
      throw new HiveException(e);
    }
  }
  completeInitializationOp(os);
}
