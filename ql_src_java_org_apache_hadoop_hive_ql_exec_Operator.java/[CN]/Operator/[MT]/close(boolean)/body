{
  if (state == state.CLOSE)   return;
  try {
    logStats();
    if (childOperators == null)     return;
    for (    Operator<? extends Serializable> op : childOperators) {
      op.close(abort);
    }
    state=State.CLOSE;
    LOG.info("Close done");
  }
 catch (  HiveException e) {
    e.printStackTrace();
    throw e;
  }
}
