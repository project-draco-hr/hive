{
  if (state == State.CLOSE) {
    return;
  }
  if (!allInitializedParentsAreClosed()) {
    return;
  }
  state=State.CLOSE;
  LOG.info(id + " finished. closing... ");
  closeOp(abort);
  reporter=null;
  try {
    logStats();
    if (childOperators == null) {
      return;
    }
    for (    Operator<? extends OperatorDesc> op : childOperators) {
      op.close(abort);
    }
    LOG.info(id + " Close done");
  }
 catch (  HiveException e) {
    e.printStackTrace();
    throw e;
  }
}
