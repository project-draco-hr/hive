{
  String counterName="CNTR_NAME_" + getOperatorId() + "_"+ name;
  ProgressCounter pc=counterNameToEnum.get(counterName);
  if (pc == null)   LOG.warn("Using too many counters. Increase the total number of counters for " + counterName);
 else   if (reporter != null)   reporter.incrCounter(pc,amount);
}
