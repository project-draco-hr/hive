{
  for (  BaseWork wrk : tzWrk.getAllWork()) {
    if (wrk instanceof MergeJoinWork) {
      wrk=((MergeJoinWork)wrk).getMainWork();
    }
    List<String> warnings=new MapJoinCheck(wrk.getName()).analyze(wrk);
    if (!warnings.isEmpty()) {
      for (      String w : warnings) {
        warn(w);
      }
    }
  }
}
