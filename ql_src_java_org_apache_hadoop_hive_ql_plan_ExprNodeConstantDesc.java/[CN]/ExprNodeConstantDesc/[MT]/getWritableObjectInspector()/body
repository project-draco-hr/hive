{
  PrimitiveCategory pc=((PrimitiveTypeInfo)getTypeInfo()).getPrimitiveCategory();
  Object writableValue=PrimitiveObjectInspectorFactory.getPrimitiveJavaObjectInspector(pc).getPrimitiveWritableObject(getValue());
  return PrimitiveObjectInspectorFactory.getPrimitiveWritableConstantObjectInspector((PrimitiveTypeInfo)getTypeInfo(),writableValue);
}
