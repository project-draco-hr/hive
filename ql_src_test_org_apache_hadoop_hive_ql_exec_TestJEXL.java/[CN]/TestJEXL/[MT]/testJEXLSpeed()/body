{
  try {
    int basetimes=100000;
    JexlContext jc=JexlHelper.createContext();
    jc.getVars().put("__udf__concat",UDFRegistry.getUDFClass("concat").newInstance());
    measureSpeed("1 + 2",basetimes * 100,ExpressionFactory.createExpression("1 + 2"),jc,Long.valueOf(1 + 2));
    measureSpeed("__udf__concat.evaluate(\"1\", \"2\")",basetimes * 10,ExpressionFactory.createExpression("__udf__concat.evaluate(\"1\", \"2\")"),jc,"12");
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw e;
  }
}
