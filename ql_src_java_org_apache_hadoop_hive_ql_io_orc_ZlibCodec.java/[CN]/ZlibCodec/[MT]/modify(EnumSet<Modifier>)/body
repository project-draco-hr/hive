{
  int l=this.level;
  int s=this.strategy;
  for (  Modifier m : modifiers) {
switch (m) {
case BINARY:
      s=Deflater.FILTERED;
    break;
case TEXT:
  s=Deflater.DEFAULT_STRATEGY;
break;
case FASTEST:
l=Deflater.BEST_SPEED;
break;
case FAST:
l=Deflater.BEST_SPEED + 1;
break;
case DEFAULT:
l=Deflater.DEFAULT_COMPRESSION;
break;
default :
break;
}
}
return new ZlibCodec(l,s);
}
