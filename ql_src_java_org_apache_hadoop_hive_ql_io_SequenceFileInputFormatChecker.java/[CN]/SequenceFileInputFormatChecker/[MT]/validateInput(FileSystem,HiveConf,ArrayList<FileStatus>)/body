{
  if (files.size() <= 0) {
    return false;
  }
  for (int fileId=0; fileId < files.size(); fileId++) {
    try {
      SequenceFile.Reader reader=new SequenceFile.Reader(fs,files.get(fileId).getPath(),conf);
      reader.close();
    }
 catch (    IOException e) {
      return false;
    }
  }
  return true;
}
