{
  MockFile file=null;
  for (  MockFile currentFile : files) {
    if (currentFile.path.equals(path)) {
      file=currentFile;
      break;
    }
  }
  if (file == null) {
    file=new MockFile(path.toString(),(int)blockSize,new byte[0]);
    files.add(file);
  }
  return new MockOutputStream(file);
}
