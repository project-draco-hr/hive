{
  if (!objInspector.getCategory().equals(Category.PRIMITIVE)) {
    try {
      serialize(out,SerDeUtils.getJSONString(obj,objInspector),PrimitiveObjectInspectorFactory.javaStringObjectInspector,serdeParams.getSeparators(),1,serdeParams.getNullSequence(),serdeParams.isEscaped(),serdeParams.getEscapeChar(),serdeParams.getNeedsEscape());
    }
 catch (    IOException e) {
      throw new SerDeException(e);
    }
  }
 else {
    super.serializeField(out,obj,objInspector,serdeParams);
  }
}
