{
  SessionState ss=SessionState.get();
  LogHelper console=SessionState.getConsole();
  Configuration conf=(ss == null) ? new Configuration() : ss.getConf();
  try {
    if (Utilities.realFile(newFile,conf) != null)     return newFile;
 else {
      console.printError(newFile + " does not exist");
      return null;
    }
  }
 catch (  IOException e) {
    console.printError("Unable to validate " + newFile + "\nException: "+ e.getMessage(),"\n" + org.apache.hadoop.util.StringUtils.stringifyException(e));
    return null;
  }
}
