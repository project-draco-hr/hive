{
  LogHelper console=getConsole();
  try {
    Utilities.addToClassPath(StringUtils.split(newJar,","));
    console.printInfo("Added " + newJar + " to class path");
    return true;
  }
 catch (  Exception e) {
    console.printError("Unable to register " + newJar + "\nException: "+ e.getMessage(),"\n" + org.apache.hadoop.util.StringUtils.stringifyException(e));
    return false;
  }
}
