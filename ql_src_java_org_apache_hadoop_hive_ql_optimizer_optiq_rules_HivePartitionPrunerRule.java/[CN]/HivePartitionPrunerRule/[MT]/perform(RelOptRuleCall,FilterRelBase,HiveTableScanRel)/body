{
  RelOptHiveTable hiveTable=(RelOptHiveTable)tScan.getTable();
  RexNode predicate=filter.getCondition();
  Pair<RexNode,RexNode> predicates=PartitionPruner.extractPartitionPredicates(filter.getCluster(),hiveTable,predicate);
  RexNode partColExpr=predicates.left;
  RexNode remainingExpr=predicates.right;
  remainingExpr=remainingExpr == null ? filter.getCluster().getRexBuilder().makeLiteral(true) : remainingExpr;
  if (partColExpr == null || InputFinder.bits(partColExpr).length() == 0) {
    return;
  }
  try {
    hiveTable.computePartitionList(conf,partColExpr);
  }
 catch (  HiveException he) {
    throw new RuntimeException(he);
  }
}
