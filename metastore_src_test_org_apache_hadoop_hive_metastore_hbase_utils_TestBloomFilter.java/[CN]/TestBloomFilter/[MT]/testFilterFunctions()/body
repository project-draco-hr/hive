{
  for (  String item : items) {
    bloomFilter.addToFilter(item.getBytes());
  }
  for (  String item : items) {
    Assert.assertTrue(bloomFilter.contains(item.getBytes()));
  }
  bloomFilter.getBitVector().clearAll();
  for (  String item : items) {
    Assert.assertFalse(bloomFilter.contains(item.getBytes()));
  }
}
