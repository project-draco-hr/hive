{
  if (s == null) {
    return null;
  }
  result.set(s);
  byte[] data=result.getBytes();
  int prev=0;
  for (int i=1; i < result.getLength(); i++) {
    if (GenericUDFUtils.isUtfStartByte(data[i])) {
      reverse(data,prev,i - 1);
      prev=i;
    }
  }
  reverse(data,prev,result.getLength() - 1);
  reverse(data,0,result.getLength() - 1);
  return result;
}
