{
  debugLog("DHMAP.authorizecols " + "tbl:" + table.getCompleteName() + " , part: "+ part.getName()+ " . cols: "+ columns.toString()+ " , read:"+ debugPrivPrint(readRequiredPriv)+ " , write:"+ debugPrivPrint(writeRequiredPriv));
  List<Object> authObjects=new ArrayList<Object>();
  authObjects.add(table);
  authObjects.add(part);
  authCalls.add(new AuthCallContext(AuthCallContextType.TABLE_AND_PARTITION,authObjects,readRequiredPriv,writeRequiredPriv));
}
