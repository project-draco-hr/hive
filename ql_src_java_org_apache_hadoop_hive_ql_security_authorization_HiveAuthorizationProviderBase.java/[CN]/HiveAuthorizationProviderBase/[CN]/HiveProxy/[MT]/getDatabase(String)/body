{
  if (!isRunFromMetaStore()) {
    return hiveClient.getDatabase(dbName);
  }
 else {
    try {
      return handler.get_database(dbName);
    }
 catch (    NoSuchObjectException e) {
      throw new HiveException(e);
    }
catch (    MetaException e) {
      throw new HiveException(e);
    }
  }
}
