{
  if (this.tblDesc != null) {
    if (addCursor >= blockSize) {
      spillBlock(currentWriteBlock,addCursor);
      addCursor=0;
      if (numFlushedBlocks == 1)       currentWriteBlock=(Row[])new ArrayList[blockSize];
    }
    currentWriteBlock[addCursor++]=t;
  }
 else   if (t != null) {
    this.dummyRow=t;
  }
  ++size;
}
