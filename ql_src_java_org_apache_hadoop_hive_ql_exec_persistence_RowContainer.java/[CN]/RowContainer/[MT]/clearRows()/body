{
  itrCursor=0;
  addCursor=0;
  numFlushedBlocks=0;
  this.readBlockSize=0;
  this.actualSplitNum=0;
  this.currentSplitPointer=-1;
  this.firstCalled=false;
  this.inputSplits=null;
  tempOutPath=null;
  addCursor=0;
  size=0;
  try {
    if (rw != null) {
      rw.close(false);
    }
    if (rr != null) {
      rr.close();
    }
  }
 catch (  Exception e) {
    LOG.error(e.toString());
    throw new HiveException(e);
  }
 finally {
    rw=null;
    rr=null;
    tmpFile=null;
    deleteLocalFile(parentDir,true);
    parentDir=null;
  }
}
