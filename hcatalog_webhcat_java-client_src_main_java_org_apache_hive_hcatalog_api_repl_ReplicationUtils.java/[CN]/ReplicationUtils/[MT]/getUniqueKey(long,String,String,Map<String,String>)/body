{
  StringBuilder sb=new StringBuilder();
  sb.append(eventId);
  sb.append('.');
  sb.append(toStringWordCharsOnly(db));
  sb.append('.');
  sb.append(toStringWordCharsOnly(table));
  sb.append('.');
  sb.append(toStringWordCharsOnly(ptnDesc));
  sb.append('.');
  sb.append(Objects.hashCode(db,table,ptnDesc));
  return sb.toString();
}
