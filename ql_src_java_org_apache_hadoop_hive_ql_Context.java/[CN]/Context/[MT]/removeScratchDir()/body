{
  for (  Map.Entry<String,Path> entry : fsScratchDirs.entrySet()) {
    try {
      Path p=entry.getValue();
      p.getFileSystem(conf).delete(p,true);
    }
 catch (    Exception e) {
      LOG.warn("Error Removing Scratch: " + StringUtils.stringifyException(e));
    }
  }
  fsScratchDirs.clear();
}
