{
  for (  Map.Entry<String,String> entry : fsScratchDirs.entrySet()) {
    try {
      Path p=new Path(entry.getValue());
      p.getFileSystem(conf).delete(p,true);
    }
 catch (    Exception e) {
      LOG.warn("Error Removing Scratch: " + StringUtils.stringifyException(e));
    }
  }
  fsScratchDirs.clear();
}
