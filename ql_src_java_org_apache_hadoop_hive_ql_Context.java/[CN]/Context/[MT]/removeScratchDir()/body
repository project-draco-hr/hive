{
  if (explain) {
    try {
      if (localScratchDir != null)       FileSystem.getLocal(conf).delete(localScratchDir,true);
    }
 catch (    Exception e) {
      LOG.warn("Error Removing Scratch: " + StringUtils.stringifyException(e));
    }
  }
 else {
    for (    Path p : allScratchDirs) {
      try {
        p.getFileSystem(conf).delete(p,true);
      }
 catch (      Exception e) {
        LOG.warn("Error Removing Scratch: " + StringUtils.stringifyException(e));
      }
    }
  }
  MRScratchDir=null;
  localScratchDir=null;
}
