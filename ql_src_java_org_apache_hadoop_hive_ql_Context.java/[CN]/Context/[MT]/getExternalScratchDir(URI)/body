{
  try {
    for (    Path p : allScratchDirs) {
      URI pURI=p.toUri();
      if (strEquals(pURI.getScheme(),extURI.getScheme()) && strEquals(pURI.getAuthority(),extURI.getAuthority())) {
        return p.toString();
      }
    }
    return makeExternalScratchDir(extURI).toString();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
