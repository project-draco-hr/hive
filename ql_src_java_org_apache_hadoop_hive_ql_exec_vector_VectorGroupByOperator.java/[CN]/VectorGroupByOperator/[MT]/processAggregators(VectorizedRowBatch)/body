{
  VectorAggregationBufferRow[] aggregationBufferSets=aggregationBatchInfo.getAggregationBuffers();
  if (aggregationBatchInfo.getDistinctBufferSetCount() == 1) {
    VectorAggregateExpression.AggregationBuffer[] aggregationBuffers=aggregationBufferSets[0].getAggregationBuffers();
    for (int i=0; i < aggregators.length; ++i) {
      aggregators[i].aggregateInput(aggregationBuffers[i],batch);
    }
  }
 else {
    for (int i=0; i < aggregators.length; ++i) {
      aggregators[i].aggregateInputSelection(aggregationBufferSets,i,batch);
    }
  }
}
