{
  if (batch.size == 0) {
    return false;
  }
  if (numEntriesSinceCheck >= this.checkInterval) {
    updateAvgVariableSize(batch);
    numEntriesSinceCheck=0;
  }
  if (numEntriesHashTable > this.maxHtEntries || numEntriesHashTable * (fixedHashEntrySize + avgVariableSize) > maxHashTblMemory) {
    return true;
  }
 else {
    return (gcCanary.get() == null);
  }
}
