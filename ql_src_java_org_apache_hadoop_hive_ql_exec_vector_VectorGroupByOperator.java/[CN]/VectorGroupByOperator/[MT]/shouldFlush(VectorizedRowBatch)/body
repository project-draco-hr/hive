{
  if (batch.size == 0) {
    return false;
  }
  if (numEntriesSinceCheck >= this.checkInterval) {
    updateAvgVariableSize(batch);
    numEntriesSinceCheck=0;
  }
  return numEntriesHashTable > this.maxHtEntries || numEntriesHashTable * (fixedHashEntrySize + avgVariableSize) > maxHashTblMemory;
}
