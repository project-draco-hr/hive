{
  try {
    Pair<String,String> dbAndTableName=HCatUtil.getDbAndTableName(tableDesc.getTableName());
    InputJobInfo info=InputJobInfo.create(dbAndTableName.first,dbAndTableName.second,"");
    jobProperties.put(HCatConstants.HCAT_KEY_JOB_INFO,InitializeInput.getSerializedHcatKeyJobInfo(null,info,tableDesc.getProperties().getProperty("location")));
  }
 catch (  Exception e) {
    throw new IOException(e);
  }
}
