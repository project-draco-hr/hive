{
  FileSystem fs=inputPath.getFileSystem(job);
  FSDataInputStream input=fs.open(inputPath);
  try {
    int count=input.readInt();
    for (int i=0; i < count; i++) {
      byte[] key=new byte[input.readInt()];
      input.readFully(key);
      sampled.add(key);
    }
  }
  finally {
    IOUtils.closeStream(input);
  }
}
