{
switch (field) {
case MESSAGE:
    if (value == null) {
      unsetMessage();
    }
 else {
      setMessage((String)value);
    }
  break;
case ERROR_CODE:
if (value == null) {
  unsetErrorCode();
}
 else {
  setErrorCode((Integer)value);
}
break;
case SQLSTATE:
if (value == null) {
unsetSQLState();
}
 else {
setSQLState((String)value);
}
break;
}
}
