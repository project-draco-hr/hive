{
  try {
    r=new CompositeHiveObject[5];
    for (int i=0; i < 5; i++) {
      r[i]=new CompositeHiveObject(3);
      for (int j=0; j < 3; j++) {
        r[i].addHiveObject(new PrimitiveHiveObject(Integer.valueOf(i - 1 + j)));
      }
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
