{
switch (type) {
case "BROADCAST_EDGE":
    this.edgeType=EdgeType.BROADCAST;
  break;
case "SIMPLE_EDGE":
this.edgeType=EdgeType.SHUFFLE;
break;
case "CUSTOM_SIMPLE_EDGE":
this.edgeType=EdgeType.PARTITION_ONLY_SHUFFLE;
break;
case "CUSTOM_EDGE":
this.edgeType=EdgeType.MULTICAST;
break;
default :
this.edgeType=EdgeType.UNKNOWN;
}
}
