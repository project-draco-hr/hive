{
  if (TempletonUtils.isset(appConf.sqoopArchive())) {
    if (!TempletonUtils.isset(appConf.sqoopPath()) && !TempletonUtils.isset(appConf.sqoopHome())) {
      throw new IllegalStateException("If '" + AppConfig.SQOOP_ARCHIVE_NAME + "' is defined, '"+ AppConfig.SQOOP_PATH_NAME+ "' and '"+ AppConfig.SQOOP_HOME_PATH+ "' must be defined");
    }
  }
  runAs=user;
  List<String> args=makeArgs(command,optionsFile,otherFiles,statusdir,completedUrl,enablelog,libdir);
  return enqueueController(user,userArgs,callback,args);
}
