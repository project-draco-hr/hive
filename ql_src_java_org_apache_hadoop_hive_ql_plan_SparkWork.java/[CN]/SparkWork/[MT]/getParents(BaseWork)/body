{
  Preconditions.checkArgument(invertedWorkGraph.containsKey(work),"AssertionError: expected invertedWorkGraph.containsKey(work) to be true");
  Preconditions.checkArgument(invertedWorkGraph.get(work) != null,"AssertionError: expected invertedWorkGraph.get(work) to be not null");
  return new LinkedList<BaseWork>(invertedWorkGraph.get(work));
}
