{
  super.positionReaders(positionProvider);
  if (isFileCompressed) {
    positionProvider.getNext();
  }
  dataReader.seek(positionProvider);
  if (kind.equals(OrcProto.ColumnEncoding.Kind.DIRECT) || kind.equals(OrcProto.ColumnEncoding.Kind.DIRECT_V2)) {
    if (isFileCompressed) {
      positionProvider.getNext();
    }
    lengthReader.seek(positionProvider);
  }
}
