def __init__(self, d=None):
    self.name = None
    self.return_type = None
    self.arguments = None
    if isinstance(d, dict):
        if ('name' in d):
            self.name = d['name']
        if ('return_type' in d):
            self.return_type = d['return_type']
        if ('arguments' in d):
            self.arguments = d['arguments']
