def __init__(self, d=None):
    self.name = None
    self.methods = None
    self.fully_reflected = None
    if isinstance(d, dict):
        if ('name' in d):
            self.name = d['name']
        if ('methods' in d):
            self.methods = d['methods']
        if ('fully_reflected' in d):
            self.fully_reflected = d['fully_reflected']
