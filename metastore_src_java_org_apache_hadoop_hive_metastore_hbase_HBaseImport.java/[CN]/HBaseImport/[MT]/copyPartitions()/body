{
  screen("Copying partitions");
  for (  Table table : tables) {
    System.out.print("Copying partitions for table " + table.getDbName() + "."+ table.getTableName());
    for (    Partition part : rdbmsStore.getPartitions(table.getDbName(),table.getTableName(),-1)) {
      LOG.info("Copying " + table.getTableName() + "."+ table.getTableName()+ "."+ StringUtils.join(part.getValues(),':'));
      System.out.print('.');
      hbaseStore.addPartition(part);
    }
    System.out.println();
  }
}
