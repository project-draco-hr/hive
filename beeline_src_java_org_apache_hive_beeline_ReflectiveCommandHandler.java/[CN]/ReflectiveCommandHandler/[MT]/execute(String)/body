{
  lastException=null;
  ClientHook hook=ClientCommandHookFactory.get().getHook(beeLine,line);
  try {
    Object ob=beeLine.getCommands().getClass().getMethod(getName(),new Class[]{String.class}).invoke(beeLine.getCommands(),new Object[]{line});
    boolean result=(ob != null && ob instanceof Boolean && ((Boolean)ob).booleanValue());
    if (hook != null && result) {
      hook.postHook(beeLine);
    }
    return result;
  }
 catch (  Throwable e) {
    lastException=e;
    return beeLine.error(e);
  }
}
