{
  if (filterMap == null) {
    return null;
  }
  filterMap=compactFilter(filterMap);
  Map<Integer,String> result=new LinkedHashMap<Integer,String>();
  for (int i=0; i < filterMap.length; i++) {
    if (filterMap[i] == null) {
      continue;
    }
    result.put(i,Arrays.toString(filterMap[i]));
  }
  return result.isEmpty() ? null : result;
}
