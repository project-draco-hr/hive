 else {
    sb.append(this.taskId);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("taskType:");
  if (this.taskType == null) {
    sb.append("null");
  }
 else {
    sb.append(this.taskType);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("taskAttributes:");
  if (this.taskAttributes == null) {
    sb.append("null");
  }
 else {
