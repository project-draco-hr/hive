      sb.append("null");
    }
 else {
      sb.append(this.operatorList);
    }
    first=false;
  }
  if (!first)   sb.append(", ");
  sb.append("done:");
  sb.append(this.done);
  first=false;
  if (!first)   sb.append(", ");
  sb.append("started:");
  sb.append(this.started);
  first=false;
  sb.append(")");
  return sb.toString();
}
