{
switch (field) {
case TASK_ID:
    return getTaskId();
case TASK_TYPE:
  return getTaskType();
case TASK_ATTRIBUTES:
return getTaskAttributes();
case TASK_COUNTERS:
return getTaskCounters();
case OPERATOR_GRAPH:
return getOperatorGraph();
case OPERATOR_LIST:
return getOperatorList();
case DONE:
return new Boolean(isDone());
case STARTED:
return new Boolean(isStarted());
}
throw new IllegalStateException();
}
