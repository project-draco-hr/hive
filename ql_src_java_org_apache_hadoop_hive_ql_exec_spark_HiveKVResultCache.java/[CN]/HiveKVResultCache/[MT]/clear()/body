{
  writeCursor=readCursor=rowsInReadBuffer=0;
  readBufferUsed=false;
  if (parentFile != null) {
    if (input != null) {
      try {
        input.close();
      }
 catch (      Throwable ignored) {
      }
      input=null;
    }
    if (output != null) {
      try {
        output.close();
      }
 catch (      Throwable ignored) {
      }
      output=null;
    }
    try {
      FileUtil.fullyDelete(parentFile);
    }
 catch (    Throwable ignored) {
    }
    parentFile=null;
    tmpFile=null;
  }
}
