{
  if (!more)   return false;
  long pos=in.getPosition();
  boolean hasMore=in.next(key);
  if (hasMore) {
    in.getCurrentRow(value);
  }
  if (pos >= end && in.syncSeen() && !in.hasRecordsInBuffer()) {
    more=false;
    if (firstSeen) {
      firstSeen=false;
      return true;
    }
  }
 else {
    more=hasMore;
  }
  return more;
}
