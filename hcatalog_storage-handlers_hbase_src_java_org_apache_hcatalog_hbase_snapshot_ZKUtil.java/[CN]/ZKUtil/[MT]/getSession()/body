{
  if (zkSession == null || zkSession.getState() == States.CLOSED) {
synchronized (this) {
      if (zkSession == null || zkSession.getState() == States.CLOSED) {
        zkSession=new ZooKeeper(this.connectString,this.DEFAULT_SESSION_TIMEOUT,new ZKWatcher());
      }
    }
  }
  return zkSession;
}
