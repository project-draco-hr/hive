{
  if (field != null && HCatContext.INSTANCE.getConf().isPresent()) {
    Configuration conf=HCatContext.INSTANCE.getConf().get();
    if (field instanceof Boolean && conf.getBoolean(HCatConstants.HCAT_DATA_CONVERT_BOOLEAN_TO_INTEGER,HCatConstants.HCAT_DATA_CONVERT_BOOLEAN_TO_INTEGER_DEFAULT)) {
      return ((Boolean)field) ? 1 : 0;
    }
 else     if (field instanceof Short && conf.getBoolean(HCatConstants.HCAT_DATA_TINY_SMALL_INT_PROMOTION,HCatConstants.HCAT_DATA_TINY_SMALL_INT_PROMOTION_DEFAULT)) {
      return new Integer((Short)field);
    }
 else     if (field instanceof Byte && conf.getBoolean(HCatConstants.HCAT_DATA_TINY_SMALL_INT_PROMOTION,HCatConstants.HCAT_DATA_TINY_SMALL_INT_PROMOTION_DEFAULT)) {
      return new Integer((Byte)field);
    }
  }
  return ((PrimitiveObjectInspector)fieldObjectInspector).getPrimitiveJavaObject(field);
}
