{
  executeCMD(options,CMD,retCode);
  String output=os.toString();
  LOG.debug(output);
  if (contains) {
    Assert.assertTrue("The expected keyword \"" + keywords + "\" doesn't occur in the output: "+ output,output.contains(keywords));
  }
 else {
    Assert.assertFalse("The expected keyword \"" + keywords + "\" doesn't occur in the output: "+ output,output.contains(keywords));
  }
}
