{
  try {
    @SuppressWarnings("unchecked") Class<? extends LlapTokenProvider> clazz=(Class<? extends LlapTokenProvider>)Class.forName(TOKEN_CLASS);
    Constructor<? extends LlapTokenProvider> ctor=clazz.getDeclaredConstructor(Configuration.class);
    ctor.setAccessible(true);
    return ctor.newInstance(conf);
  }
 catch (  Exception e) {
    throw new RuntimeException("Failed to create token provider class",e);
  }
}
