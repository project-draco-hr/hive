{
  this.inpJobInfo=inputJobInfo;
  this.conf=conf;
  String snapshotString=conf.get(HBaseConstants.PROPERTY_TABLE_SNAPSHOT_KEY);
  HCatTableSnapshot hcatSnapshot=(HCatTableSnapshot)HCatUtil.deserialize(snapshotString);
  this.snapshot=HBaseRevisionManagerUtil.convertSnapshot(hcatSnapshot,inpJobInfo.getTableInfo());
}
