{
  List<? extends Node> children=nd.getChildren();
  opStack.push(nd);
  dispatch(nd,opStack);
  if ((children == null) || ((nd instanceof ReduceSinkOperator) && (getDispatchedList().containsAll(children)))) {
    opStack.pop();
    return;
  }
  for (  Node ch : children)   walk(ch);
  opStack.pop();
}
