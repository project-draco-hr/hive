{
  List<Operator<? extends Serializable>> children=op.getChildOperators();
  opStack.push(op);
  dispatch(op,opStack);
  if ((children == null) || ((op instanceof ReduceSinkOperator) && (getDispatchedList().containsAll(children)))) {
    opStack.pop();
    return;
  }
  for (  Operator<? extends Serializable> ch : children)   walk(ch);
  opStack.pop();
}
