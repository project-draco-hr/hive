{
  if (doMerge && tail != null && tail.merge(offset,end))   return;
  if (doLogNew) {
    LOG.info("Creating new range; last range (which can include some previous adds) was " + tail);
  }
  DiskRangeList node=new DiskRangeList(offset,end);
  if (tail == null) {
    head=tail=node;
  }
 else {
    tail=tail.insertAfter(node);
  }
}
