{
  final SequenceFile.Writer outStream=Utilities.createSequenceWriter(jc,FileSystem.get(jc),finalOutPath,BytesWritable.class,valueClass,isCompressed);
  return new RecordWriter(){
    public void write(    Writable r) throws IOException {
      outStream.append(EMPTY_KEY,r);
    }
    public void close(    boolean abort) throws IOException {
      outStream.close();
    }
  }
;
}
