{
  if (arguments.length != 0) {
    throw new UDFArgumentLengthException("The function CURRENT_USER does not take any arguments, but found " + arguments.length);
  }
  if (currentUser == null) {
    String sessUserFromAuth=SessionState.getUserFromAuthenticator();
    if (sessUserFromAuth != null) {
      currentUser=new Text(sessUserFromAuth);
    }
  }
  return PrimitiveObjectInspectorFactory.writableStringObjectInspector;
}
