{
  if (o instanceof MapJoinObjectKey) {
    MapJoinObjectKey mObj=(MapJoinObjectKey)o;
    if (mObj.getMetadataTag() == metadataTag) {
      if ((obj == null) && (mObj.getObj() == null))       return true;
      if ((obj != null) && (mObj.getObj() != null) && (mObj.getObj().equals(obj)))       return true;
    }
  }
  return false;
}
