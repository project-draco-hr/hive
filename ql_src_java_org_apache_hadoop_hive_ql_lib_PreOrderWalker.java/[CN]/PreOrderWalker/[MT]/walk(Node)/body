{
  opStack.push(nd);
  dispatch(nd,opStack);
  if (nd.getChildren() != null) {
    for (    Node n : nd.getChildren()) {
      walk(n);
    }
  }
 else   if (nd instanceof ConditionalTask) {
    for (    Task n : ((ConditionalTask)nd).getListTasks()) {
      if (n.getParentTasks() == null || n.getParentTasks().isEmpty()) {
        walk(n);
      }
    }
  }
  opStack.pop();
}
