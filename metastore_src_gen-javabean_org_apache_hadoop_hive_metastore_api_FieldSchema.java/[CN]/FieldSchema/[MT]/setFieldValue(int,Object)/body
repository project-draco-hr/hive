{
switch (fieldID) {
case NAME:
    if (value == null) {
      unsetName();
    }
 else {
      setName((String)value);
    }
  break;
case TYPE:
if (value == null) {
  unsetType();
}
 else {
  setType((String)value);
}
break;
case COMMENT:
if (value == null) {
unsetComment();
}
 else {
setComment((String)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
