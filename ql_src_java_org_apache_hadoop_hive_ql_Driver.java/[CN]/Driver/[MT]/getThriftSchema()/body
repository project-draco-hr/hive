{
  Schema schema;
  try {
    schema=this.getSchema();
    if (schema != null) {
      List<FieldSchema> lst=schema.getFieldSchemas();
      if (lst != null) {
        for (        FieldSchema f : lst) {
          f.setType(MetaStoreUtils.typeToThriftType(f.getType()));
        }
      }
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw e;
  }
  LOG.info("Returning Thrift schema: " + schema);
  return schema;
}
