{
  Iterator<TaskResult> resultIterator=results.iterator();
  while (true) {
    while (resultIterator.hasNext()) {
      TaskResult tskRes=resultIterator.next();
      if (!tskRes.isRunning()) {
        return tskRes;
      }
    }
    try {
      Thread.sleep(SLEEP_TIME);
    }
 catch (    InterruptedException ie) {
      ;
    }
    resultIterator=results.iterator();
  }
}
