{
  if (plan != null && plan.getFetchTask() != null) {
    try {
      plan.getFetchTask().clearFetch();
    }
 catch (    Exception e) {
      throw new IOException("Error closing the current fetch task",e);
    }
    plan.getFetchTask().initialize(conf,plan,null);
  }
 else {
    ctx.resetStream();
    resStream=null;
  }
}
