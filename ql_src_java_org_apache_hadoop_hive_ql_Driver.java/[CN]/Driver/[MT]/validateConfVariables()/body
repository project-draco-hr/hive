{
  boolean valid=true;
  if ((!conf.getBoolVar(HiveConf.ConfVars.HIVE_HADOOP_SUPPORTS_SUBDIRECTORIES)) && ((conf.getBoolVar(HiveConf.ConfVars.HADOOPMAPREDINPUTDIRRECURSIVE)) || (conf.getBoolVar(HiveConf.ConfVars.HIVEOPTLISTBUCKETING)))) {
    errorMessage="FAILED: Hive Internal Error: " + ErrorMsg.SUPPORT_DIR_MUST_TRUE_FOR_LIST_BUCKETING.getMsg();
    SQLState=ErrorMsg.findSQLState(errorMessage);
    console.printError(errorMessage + "\n");
    valid=false;
  }
  return valid;
}
