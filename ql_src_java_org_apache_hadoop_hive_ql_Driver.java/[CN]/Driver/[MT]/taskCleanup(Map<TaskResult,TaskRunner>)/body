{
  for (  Map.Entry<TaskResult,TaskRunner> entry : running.entrySet()) {
    if (entry.getKey().isRunning()) {
      Task<?> task=entry.getValue().getTask();
      try {
        task.shutdown();
      }
 catch (      Exception e) {
        console.printError("Exception on shutting down task " + task.getId() + ": "+ e);
      }
    }
  }
  running.clear();
}
