{
  errorMessage=null;
  SQLState=null;
  int ret=compile(command);
  if (ret != 0) {
    return new CommandProcessorResponse(ret,errorMessage,SQLState);
  }
  ret=execute();
  if (ret != 0) {
    return new CommandProcessorResponse(ret,errorMessage,SQLState);
  }
  return new CommandProcessorResponse(ret);
}
