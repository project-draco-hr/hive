{
  if (endOfResult) {
    return false;
  }
  if (nextRow == null) {
    try {
      if (rs.next()) {
        nextRow=new Row(labelRow.sizes.length,rs);
        if (normalizingWidths) {
          nextRow.sizes=labelRow.sizes;
        }
      }
 else {
        endOfResult=true;
      }
    }
 catch (    SQLException ex) {
      throw new RuntimeException(ex.toString());
    }
  }
  return (nextRow != null);
}
