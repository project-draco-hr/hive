{
  LOG.info("Closing the session manager.");
  if (createdSessions != null) {
synchronized (createdSessions) {
      Iterator<SparkSession> it=createdSessions.iterator();
      while (it.hasNext()) {
        SparkSession session=it.next();
        session.close();
      }
      createdSessions.clear();
    }
  }
  inited=false;
}
