{
  for (int j=0; j < allocs.length; ++j) {
    LlapCacheableBuffer mem=(LlapCacheableBuffer)allocs[j];
    int pos=mem.byteBuffer.position();
    assertEquals("Failed to match (" + pos + ") on "+ j+ "/"+ allocs.length,testValues[j],mem.byteBuffer.getLong(pos));
    int halfLength=mem.byteBuffer.remaining() >> 1;
    if (halfLength + 8 <= mem.byteBuffer.remaining()) {
      assertEquals("Failed to match half (" + (pos + halfLength) + ") on "+ j+ "/"+ allocs.length,testValues[j],mem.byteBuffer.getLong(pos + halfLength));
    }
    a.deallocate(mem);
  }
}
