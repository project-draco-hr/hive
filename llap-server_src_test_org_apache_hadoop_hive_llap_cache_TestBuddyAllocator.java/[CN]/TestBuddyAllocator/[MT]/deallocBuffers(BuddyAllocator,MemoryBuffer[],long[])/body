{
  for (int j=0; j < allocs.length; ++j) {
    LlapDataBuffer mem=(LlapDataBuffer)allocs[j];
    int pos=mem.getByteBufferRaw().position();
    assertEquals("Failed to match (" + pos + ") on "+ j+ "/"+ allocs.length,testValues[j],mem.getByteBufferRaw().getLong(pos));
    int halfLength=mem.getByteBufferRaw().remaining() >> 1;
    if (halfLength + 8 <= mem.getByteBufferRaw().remaining()) {
      assertEquals("Failed to match half (" + (pos + halfLength) + ") on "+ j+ "/"+ allocs.length,testValues[j],mem.getByteBufferRaw().getLong(pos + halfLength));
    }
    a.deallocate(mem);
  }
}
