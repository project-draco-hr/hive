{
  int res=-1;
  int maxSize=0;
  for (int i=0; i < hashPartitions.length; i++) {
    int size;
    if (isOnDisk(i)) {
      continue;
    }
 else {
      size=hashPartitions[i].hashMap.getNumValues();
    }
    if (size > maxSize) {
      maxSize=size;
      res=i;
    }
  }
  if (res == -1) {
    for (int i=0; i < hashPartitions.length; i++) {
      if (!isOnDisk(i)) {
        return i;
      }
    }
  }
  return res;
}
