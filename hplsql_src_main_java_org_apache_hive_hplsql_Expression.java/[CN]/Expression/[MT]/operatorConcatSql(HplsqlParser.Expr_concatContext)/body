{
  StringBuilder sql=new StringBuilder();
  sql.append("CONCAT(");
  int cnt=ctx.expr_concat_item().size();
  for (int i=0; i < cnt; i++) {
    sql.append(evalPop(ctx.expr_concat_item(i)).toString());
    if (i + 1 < cnt) {
      sql.append(", ");
    }
  }
  sql.append(")");
  exec.stackPush(sql);
}
