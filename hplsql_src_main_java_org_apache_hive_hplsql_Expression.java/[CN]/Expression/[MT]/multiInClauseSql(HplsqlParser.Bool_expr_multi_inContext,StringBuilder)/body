{
  int cnt=ctx.expr().size();
  sql.append("(");
  for (int i=0; i < cnt; i++) {
    sql.append(evalPop(ctx.expr(i)).toString());
    if (i + 1 < cnt) {
      sql.append(", ");
    }
  }
  sql.append(")");
  if (ctx.T_NOT() != null) {
    sql.append(" " + ctx.T_NOT().getText());
  }
  sql.append(" " + ctx.T_IN().getText() + " (");
  if (ctx.select_stmt() != null) {
    sql.append(evalPop(ctx.select_stmt()));
  }
  sql.append(")");
}
