{
  boolean val=false;
  if (ctx.T_IS() != null) {
    val=evalPop(ctx.expr(0)).isNull();
    if (ctx.T_NOT() != null) {
      val=!val;
    }
  }
 else   if (ctx.T_BETWEEN() != null) {
    Var v=evalPop(ctx.expr(0));
    Var v1=evalPop(ctx.expr(1));
    int cmp=v.compareTo(v1);
    if (cmp >= 0) {
      Var v2=evalPop(ctx.expr(2));
      cmp=v.compareTo(v2);
      if (cmp <= 0) {
        val=true;
      }
    }
  }
  exec.stackPush(val);
  return 0;
}
