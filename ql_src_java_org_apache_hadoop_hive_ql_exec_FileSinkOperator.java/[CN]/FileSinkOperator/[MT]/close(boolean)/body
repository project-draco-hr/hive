{
  if (state == State.CLOSE)   return;
  if (counterNameToEnum != null) {
    incrCounter(numInputRowsCntr,inputRows);
    incrCounter(numOutputRowsCntr,outputRows);
    incrCounter(timeTakenCntr,totalTime);
  }
  state=State.CLOSE;
  if (!abort) {
    if (outWriter != null) {
      try {
        outWriter.close(abort);
        commit();
      }
 catch (      IOException e) {
        throw new HiveException(e);
      }
    }
  }
 else {
    try {
      outWriter.close(abort);
      if (!autoDelete)       fs.delete(outPath,true);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
