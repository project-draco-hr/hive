{
  if (state == state.CLOSE)   return;
  state=state.CLOSE;
  if (!abort) {
    if (outWriter != null) {
      try {
        outWriter.close(abort);
        commit();
      }
 catch (      IOException e) {
        return;
      }
    }
  }
 else {
    try {
      outWriter.close(abort);
      if (!autoDelete)       fs.delete(outPath,true);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
