{
  try {
    reporter.progress();
    recordValue=serializer.serialize(row,rowInspector);
    if (row_count != null) {
      row_count.set(row_count.get() + 1);
    }
    outWriter.write(recordValue);
  }
 catch (  IOException e) {
    throw new HiveException(e);
  }
catch (  SerDeException e) {
    throw new HiveException(e);
  }
}
