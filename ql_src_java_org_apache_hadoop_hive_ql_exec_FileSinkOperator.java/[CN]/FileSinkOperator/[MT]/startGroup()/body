{
  if (!conf.getDpSortState().equals(DPSortState.NONE)) {
    keyOI=getGroupKeyObjectInspector();
    keys.clear();
    keyWritables.clear();
    ObjectInspectorUtils.partialCopyToStandardObject(keyWritables,getGroupKeyObject(),0,numKeyColToRead,(StructObjectInspector)keyOI,ObjectInspectorCopyOption.WRITABLE);
    for (    Object o : keyWritables) {
      if (o == null || o.toString().length() == 0) {
        keys.add(dpCtx.getDefaultPartitionName());
      }
 else {
        keys.add(o.toString());
      }
    }
    fpaths=getDynOutPaths(keys,null);
  }
}
