{
  for (int idx=0; idx < outWriters.length; idx++) {
    if (outWriters[idx] != null) {
      try {
        outWriters[idx].close(abort);
        updateProgress();
      }
 catch (      IOException e) {
        throw new HiveException(e);
      }
    }
  }
  try {
    if ("org.apache.hadoop.hive.llap.LlapStorageHandler".equals(getConf().getTableInfo().getProperties().get(hive_metastoreConstants.META_TABLE_STORAGE))) {
      (new LlapOutputFormat()).getRecordWriter(null,hconf instanceof JobConf ? (JobConf)hconf : new JobConf(hconf),null,null).close(null);
    }
  }
 catch (  IOException e) {
  }
  try {
    for (int i=0; i < updaters.length; i++) {
      if (updaters[i] != null) {
        updaters[i].close(abort);
      }
    }
  }
 catch (  IOException e) {
    throw new HiveException(e);
  }
}
