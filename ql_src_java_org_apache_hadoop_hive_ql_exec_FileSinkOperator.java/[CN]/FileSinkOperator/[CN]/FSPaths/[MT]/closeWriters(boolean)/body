{
  for (int idx=0; idx < outWriters.length; idx++) {
    if (outWriters[idx] != null) {
      try {
        outWriters[idx].close(abort);
        updateProgress();
      }
 catch (      IOException e) {
        throw new HiveException(e);
      }
    }
  }
  try {
    for (int i=0; i < updaters.length; i++) {
      if (updaters[i] != null) {
        updaters[i].close(abort);
      }
    }
  }
 catch (  IOException e) {
    throw new HiveException(e);
  }
}
