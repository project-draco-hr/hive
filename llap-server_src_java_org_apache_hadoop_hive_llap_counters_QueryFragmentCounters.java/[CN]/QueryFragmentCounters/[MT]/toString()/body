{
  StringBuilder sb=new StringBuilder("Fragment counters for [");
  for (int i=0; i < descs.length; ++i) {
    if (i != 0) {
      sb.append(", ");
    }
    if (descs[i] != null) {
      sb.append(descs[i]);
    }
  }
  sb.append("]: [ ");
  for (int i=0; i < fixedCounters.length(); ++i) {
    if (i != 0) {
      sb.append(", ");
    }
    sb.append(LlapIOCounters.values()[i].name()).append("=").append(fixedCounters.get(i));
  }
  sb.append(" ]");
  return sb.toString();
}
