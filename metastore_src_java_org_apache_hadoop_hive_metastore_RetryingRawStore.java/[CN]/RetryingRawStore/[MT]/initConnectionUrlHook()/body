{
  String className=hiveConf.get(HiveConf.ConfVars.METASTORECONNECTURLHOOK.toString(),"").trim();
  if (className.equals("")) {
    urlHookClassName="";
    urlHook=null;
    return;
  }
  boolean urlHookChanged=!urlHookClassName.equals(className);
  if (urlHook == null || urlHookChanged) {
    urlHookClassName=className.trim();
    Class<?> urlHookClass=Class.forName(urlHookClassName,true,JavaUtils.getClassLoader());
    urlHook=(JDOConnectionURLHook)ReflectionUtils.newInstance(urlHookClass,null);
  }
  return;
}
