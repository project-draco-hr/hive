{
  Class<? extends RawStore> baseClass=(Class<? extends RawStore>)MetaStoreUtils.getClass(rawStoreClassName);
  RetryingRawStore handler=new RetryingRawStore(hiveConf,conf,baseClass,id);
  return (RawStore)Proxy.newProxyInstance(RetryingRawStore.class.getClassLoader(),getAllInterfaces(baseClass),handler);
}
