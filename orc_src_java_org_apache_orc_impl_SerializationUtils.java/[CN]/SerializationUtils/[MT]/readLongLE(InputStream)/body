{
  readFully(in,readBuffer,0,8);
  return (((readBuffer[0] & 0xff) << 0) + ((readBuffer[1] & 0xff) << 8) + ((readBuffer[2] & 0xff) << 16)+ ((long)(readBuffer[3] & 0xff) << 24)+ ((long)(readBuffer[4] & 0xff) << 32)+ ((long)(readBuffer[5] & 0xff) << 40)+ ((long)(readBuffer[6] & 0xff) << 48)+ ((long)(readBuffer[7] & 0xff) << 56));
}
