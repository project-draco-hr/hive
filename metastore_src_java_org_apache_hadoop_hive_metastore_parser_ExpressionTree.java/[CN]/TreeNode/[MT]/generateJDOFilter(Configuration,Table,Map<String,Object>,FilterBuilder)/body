{
  if (filterBuffer.hasError())   return;
  if (lhs != null) {
    filterBuffer.append(" (");
    lhs.generateJDOFilter(conf,table,params,filterBuffer);
    if (rhs != null) {
      if (andOr == LogicalOperator.AND) {
        filterBuffer.append(" && ");
      }
 else {
        filterBuffer.append(" || ");
      }
      rhs.generateJDOFilter(conf,table,params,filterBuffer);
    }
    filterBuffer.append(") ");
  }
}
