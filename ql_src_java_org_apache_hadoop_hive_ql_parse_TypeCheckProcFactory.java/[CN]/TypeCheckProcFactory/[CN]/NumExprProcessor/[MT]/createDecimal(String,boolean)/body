{
  HiveDecimal hd=HiveDecimal.create(strVal);
  if (notNull && hd == null)   return null;
  int prec=1;
  int scale=0;
  if (hd != null) {
    prec=hd.precision();
    scale=hd.scale();
  }
  DecimalTypeInfo typeInfo=TypeInfoFactory.getDecimalTypeInfo(prec,scale);
  return new ExprNodeConstantDesc(typeInfo,hd);
}
