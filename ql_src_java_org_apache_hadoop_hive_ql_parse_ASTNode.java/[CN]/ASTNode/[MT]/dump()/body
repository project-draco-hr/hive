{
  StringBuilder sb=new StringBuilder();
  sb.append('(');
  sb.append(toString());
  ArrayList<Node> children=getChildren();
  if (children != null) {
    for (    Node node : getChildren()) {
      if (node instanceof ASTNode) {
        sb.append(((ASTNode)node).dump());
      }
 else {
        sb.append("NON-ASTNODE!!");
      }
    }
  }
  sb.append(')');
  return sb.toString();
}
