{
  sb.append(ws);
  sb.append(toString());
  sb.append("\n");
  ArrayList<Node> children=getChildren();
  if (children != null) {
    for (    Node node : getChildren()) {
      if (node instanceof ASTNode) {
        ((ASTNode)node).dump(sb,ws + "   ");
      }
 else {
        sb.append(ws);
        sb.append("   NON-ASTNODE!!");
        sb.append("\n");
      }
    }
  }
  return sb;
}
