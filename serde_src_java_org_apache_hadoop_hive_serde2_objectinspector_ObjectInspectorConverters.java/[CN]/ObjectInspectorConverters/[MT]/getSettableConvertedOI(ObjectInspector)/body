{
switch (inputOI.getCategory()) {
case PRIMITIVE:
    PrimitiveObjectInspector primInputOI=(PrimitiveObjectInspector)inputOI;
  return PrimitiveObjectInspectorFactory.getPrimitiveWritableObjectInspector(primInputOI.getPrimitiveCategory());
case STRUCT:
return inputOI;
case LIST:
return inputOI;
case MAP:
return inputOI;
default :
throw new RuntimeException("Hive internal error: desired OI of " + inputOI.getTypeName() + " not supported yet.");
}
}
