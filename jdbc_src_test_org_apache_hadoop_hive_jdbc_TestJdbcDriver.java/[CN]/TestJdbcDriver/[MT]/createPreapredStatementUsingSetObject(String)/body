{
  PreparedStatement ps=con.prepareStatement(sql);
  ps.setObject(1,true);
  ps.setObject(2,true);
  ps.setObject(3,Short.valueOf("1"));
  ps.setObject(4,2);
  ps.setObject(5,3f);
