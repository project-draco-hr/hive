{
  PreparedStatement ps=con.prepareStatement(sql);
  ps.setBoolean(1,true);
  ps.setBoolean(2,true);
  ps.setShort(3,Short.valueOf("1"));
  ps.setInt(4,2);
  ps.setFloat(5,3f);
  ps.setDouble(6,Double.valueOf(4));
  ps.setString(7,"test'string\"");
  ps.setLong(8,5L);
  ps.setByte(9,(byte)1);
  ps.setByte(10,(byte)1);
  ps.setMaxRows(2);
  return ps;
}
