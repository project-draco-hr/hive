{
  beforeRun();
  try {
    Metrics metrics=MetricsFactory.getInstance();
    if (metrics != null) {
      try {
        metrics.incrementCounter(MetricsConstant.OPEN_OPERATIONS);
      }
 catch (      Exception e) {
        LOG.warn("Error Reporting open operation to Metrics system",e);
      }
    }
    runInternal();
  }
  finally {
    afterRun();
  }
}
