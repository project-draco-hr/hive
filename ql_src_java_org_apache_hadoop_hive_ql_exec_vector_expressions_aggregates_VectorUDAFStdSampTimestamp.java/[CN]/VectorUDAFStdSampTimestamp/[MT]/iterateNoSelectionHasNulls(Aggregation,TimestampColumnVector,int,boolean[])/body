{
  for (int i=0; i < batchSize; ++i) {
    if (!isNull[i]) {
      double value=inputColVector.getDouble(i);
      if (myagg.isNull) {
        myagg.init();
      }
      myagg.sum+=value;
      myagg.count+=1;
      if (myagg.count > 1) {
        double t=myagg.count * value - myagg.sum;
        myagg.variance+=(t * t) / ((double)myagg.count * (myagg.count - 1));
      }
    }
  }
}
