{
  if (hiveConf.get("spark.master").startsWith("yarn-") && !remoteClient.isActive()) {
    close();
    createRemoteClient();
  }
  try {
    return submit(driverContext,sparkWork);
  }
 catch (  Throwable cause) {
    throw new Exception("Failed to submit Spark work, please retry later",cause);
  }
}
