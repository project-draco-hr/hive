{
  if (i == 0) {
    result[0]='0';
    return 1;
  }
  int current=0;
  if (i < 0) {
    result[current++]='-';
  }
 else {
    i=-i;
  }
  long start=1000000000000000000L;
  while (i / start == 0) {
    start/=10;
  }
  while (start > 0) {
    result[current++]=(byte)('0' - (i / start % 10));
    start/=10;
  }
  return current;
}
