{
  if (s == null) {
    return null;
  }
  if (value.length < s.getLength() * 2) {
    value=new byte[s.getLength() * 2];
  }
  byte[] str=s.getBytes();
  for (int i=0; i < s.getLength(); i++) {
    value[i * 2]=(byte)Character.toUpperCase(Character.forDigit((str[i] & 0xF0) >>> 4,16));
    value[i * 2 + 1]=(byte)Character.toUpperCase(Character.forDigit(str[i] & 0x0F,16));
  }
  result.set(value,0,s.getLength() * 2);
  return result;
}
