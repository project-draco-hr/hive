{
  if (converters == null) {
    return parameters;
  }
 else {
    assert(parameters.length == convertedParameters.length);
    for (int i=0; i < parameters.length; i++) {
      convertedParameters[i]=converters[i] == null ? parameters[i] : converters[i].convert(parameters[i]);
    }
    return convertedParameters;
  }
}
