{
  this.m=m;
  this.parameters=parameters;
  Type[] acceptedParameters=m.getGenericParameterTypes();
  assert(parameters.length == acceptedParameters.length);
  for (int i=0; i < parameters.length; i++) {
    ObjectInspector acceptedParameterOI=PrimitiveObjectInspectorFactory.getPrimitiveObjectInspectorFromClass((Class<?>)acceptedParameters[i]);
    Converter pc=ObjectInspectorConverters.getConverter(parameters[i],acceptedParameterOI);
    if (pc != null) {
      if (converters == null) {
        converters=new Converter[parameters.length];
        convertedParameters=new Object[parameters.length];
      }
      converters[i]=pc;
    }
  }
}
