{
  for (  String file : files) {
    try {
      printJsonData(conf,file);
      if (files.size() > 1) {
        System.out.println(Strings.repeat("=",80) + "\n");
      }
    }
 catch (    Exception e) {
      System.err.println("Unable to dump data for file: " + file);
      e.printStackTrace();
      System.err.println(Strings.repeat("=",80) + "\n");
      continue;
    }
  }
}
