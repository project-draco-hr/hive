{
  VectorizedRowBatch batch=new VectorizedRowBatch(2);
  LongColumnVector lcv;
  DoubleColumnVector dcv;
  lcv=new LongColumnVector();
  dcv=new DoubleColumnVector();
  dcv.vector[0]=-1.5d;
  dcv.vector[1]=-0.5d;
  dcv.vector[2]=-0.1d;
  dcv.vector[3]=0d;
  dcv.vector[4]=0.5d;
  dcv.vector[5]=0.7d;
  dcv.vector[6]=1.5d;
  batch.cols[0]=dcv;
  batch.cols[1]=lcv;
  batch.size=7;
  return batch;
}
