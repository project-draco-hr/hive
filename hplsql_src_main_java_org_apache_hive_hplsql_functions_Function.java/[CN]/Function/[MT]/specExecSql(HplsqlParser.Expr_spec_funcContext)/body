{
  String name=ctx.start.getText().toUpperCase();
  if (trace && ctx.parent.parent instanceof HplsqlParser.Expr_stmtContext) {
    trace(ctx,"FUNC " + name);
  }
  FuncSpecCommand func=specSqlMap.get(name);
  if (func != null) {
    func.run(ctx);
  }
 else {
    exec.stackPush(exec.getFormattedText(ctx));
  }
}
