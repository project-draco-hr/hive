{
  if (execUserSql(ctx,name)) {
    return;
  }
  StringBuilder sql=new StringBuilder();
  sql.append(name);
  sql.append("(");
  int cnt=ctx.expr().size();
  for (int i=0; i < cnt; i++) {
    sql.append(evalPop(ctx.expr(i)));
    if (i + 1 < cnt) {
      sql.append(", ");
    }
  }
  sql.append(")");
  exec.stackPush(sql);
}
