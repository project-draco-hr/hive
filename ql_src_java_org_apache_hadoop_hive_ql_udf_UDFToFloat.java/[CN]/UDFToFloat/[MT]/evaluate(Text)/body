{
  if (i == null) {
    return null;
  }
 else {
    if (!LazyUtils.isNumberMaybe(i.getBytes(),0,i.getLength())) {
      return null;
    }
    try {
      floatWritable.set(Float.parseFloat(i.toString()));
      return floatWritable;
    }
 catch (    NumberFormatException e) {
      return null;
    }
  }
}
