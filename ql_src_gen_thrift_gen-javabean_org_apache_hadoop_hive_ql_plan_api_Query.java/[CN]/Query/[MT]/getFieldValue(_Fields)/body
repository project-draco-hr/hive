{
switch (field) {
case QUERY_ID:
    return getQueryId();
case QUERY_TYPE:
  return getQueryType();
case QUERY_ATTRIBUTES:
return getQueryAttributes();
case QUERY_COUNTERS:
return getQueryCounters();
case STAGE_GRAPH:
return getStageGraph();
case STAGE_LIST:
return getStageList();
case DONE:
return new Boolean(isDone());
case STARTED:
return new Boolean(isStarted());
}
throw new IllegalStateException();
}
