{
  int size=keyFields.size();
  if (key == null || key.getKey().length != size) {
    key=new MapJoinKey(new Object[size]);
  }
  Object[] array=key.getKey();
  for (int keyIndex=0; keyIndex < size; keyIndex++) {
    array[keyIndex]=(ObjectInspectorUtils.copyToStandardObject(keyFields.get(keyIndex).evaluate(row),keyFieldsOI.get(keyIndex),ObjectInspectorCopyOption.WRITABLE));
  }
  return key;
}
