{
  File scriptFile=File.createTempFile(getClass().getName() + "-" + id+ "-",".log");
  scriptFile.deleteOnExit();
  toBeDeletedList.add(scriptFile);
  PrintStream os=new PrintStream(new FileOutputStream(scriptFile));
  try {
    os.print(content);
  }
  finally {
    os.close();
  }
  return scriptFile;
}
