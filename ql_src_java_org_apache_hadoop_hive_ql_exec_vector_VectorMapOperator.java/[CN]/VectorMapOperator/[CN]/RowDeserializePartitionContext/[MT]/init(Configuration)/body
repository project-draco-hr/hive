{
  VectorPartitionDesc vectorPartDesc=partDesc.getVectorPartitionDesc();
  partDeserializer=partDesc.getDeserializer(hconf);
  if (partDeserializer instanceof OrcSerde) {
    partRawRowObjectInspector=(StructObjectInspector)OrcStruct.createObjectInspector(tableStructTypeInfo);
  }
 else {
    partRawRowObjectInspector=(StructObjectInspector)partDeserializer.getObjectInspector();
  }
  TypeInfo[] dataTypeInfos=vectorPartDesc.getDataTypeInfos();
  vectorAssign=new VectorAssignRow();
  readerColumnCount=vectorAssign.initConversion(dataTypeInfos,tableRowTypeInfos,dataColumnsToIncludeTruncated);
}
