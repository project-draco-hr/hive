{
  if (!abort && oneRootOperator != null && !oneRootOperator.getDone() && currentReadType != VectorMapOperatorReadType.VECTORIZED_INPUT_FILE_FORMAT) {
    if (deserializerBatch.size > 0) {
      batchCounter++;
      oneRootOperator.process(deserializerBatch,0);
      deserializerBatch.size=0;
    }
  }
  super.closeOp(abort);
}
