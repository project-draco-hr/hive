{
  if (oc == null) {
    l4j.trace("Close called no row");
  }
  try {
    if (groupKey != null) {
      l4j.trace("End Group");
      reducer.endGroup();
    }
    if (isLogInfoEnabled) {
      l4j.info("ExecReducer: processed " + cntr + " rows: used memory = "+ memoryMXBean.getHeapMemoryUsage().getUsed());
    }
    reducer.close(abort);
    reportStats rps=new reportStats(rp);
    reducer.preorderMap(rps);
  }
 catch (  Exception e) {
    if (!abort) {
      l4j.error("Hit error while closing operators - failing tree");
      throw new RuntimeException("Hive Runtime Error while closing operators: " + e.getMessage(),e);
    }
  }
 finally {
    MapredContext.close();
    Utilities.clearWorkMap();
  }
}
