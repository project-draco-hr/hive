{
  if (cOffset < 0)   return ranges.next();
  DiskRange current=null;
  boolean doCallNext=false;
  if (ranges.hasNext()) {
    current=ranges.next();
  }
 else   if (ranges.hasPrevious()) {
    current=ranges.previous();
    doCallNext=true;
  }
  while (current.end <= cOffset) {
    current=ranges.next();
    doCallNext=false;
  }
  while (current.offset > cOffset) {
    current=ranges.previous();
    doCallNext=true;
  }
  if (doCallNext) {
    ranges.next();
  }
  return current;
}
