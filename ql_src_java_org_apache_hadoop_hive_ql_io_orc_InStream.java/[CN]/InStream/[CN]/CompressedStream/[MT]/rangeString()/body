{
  StringBuilder builder=new StringBuilder();
  for (int i=0; i < bytes.size(); ++i) {
    if (i != 0) {
      builder.append("; ");
    }
    DiskRange range=bytes.get(i);
    builder.append(" range " + i + " = "+ range.offset+ " to "+ (range.end - range.offset));
  }
  return builder.toString();
}
