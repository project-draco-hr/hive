{
  cacheBuffer=null;
  uncompressed=null;
  compressed=null;
  currentRange=bytes.size();
  currentOffset=length;
  if (doManageBuffers) {
    for (    DiskRange range : bytes) {
      if (range instanceof CacheChunk) {
        cache.releaseBuffer(((CacheChunk)range).buffer);
      }
    }
  }
  bytes.clear();
}
