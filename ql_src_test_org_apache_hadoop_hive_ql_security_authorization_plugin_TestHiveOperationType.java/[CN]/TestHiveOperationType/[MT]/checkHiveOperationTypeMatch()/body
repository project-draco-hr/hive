{
  for (  HiveOperation op : HiveOperation.values()) {
    try {
      HiveOperationType.valueOf(op.name());
    }
 catch (    IllegalArgumentException ex) {
      fail("Unable to find corresponding type in HiveOperationType for " + op + " : "+ ex);
    }
  }
  assertEquals("Check if HiveOperation, HiveOperationType have same number of instances",HiveOperation.values().length,HiveOperationType.values().length);
}
