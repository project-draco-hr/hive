{
  String tokenStoreClassName=conf.get(DELEGATION_TOKEN_STORE_CLS,"");
  if (StringUtils.isBlank(tokenStoreClassName)) {
    return new MemoryTokenStore();
  }
  try {
    Class<? extends TokenStoreDelegationTokenSecretManager.TokenStore> storeClass=Class.forName(tokenStoreClassName).asSubclass(TokenStoreDelegationTokenSecretManager.TokenStore.class);
    return ReflectionUtils.newInstance(storeClass,conf);
  }
 catch (  ClassNotFoundException e) {
    throw new IOException("Error initializing delegation token store: " + tokenStoreClassName,e);
  }
}
