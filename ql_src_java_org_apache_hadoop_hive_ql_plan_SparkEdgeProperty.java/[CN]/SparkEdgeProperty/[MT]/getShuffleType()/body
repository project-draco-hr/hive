{
  if (isShuffleNone()) {
    return "NONE";
  }
  StringBuilder sb=new StringBuilder();
  if (isShuffleGroup()) {
    sb.append("GROUP");
  }
  if (isMRShuffle()) {
    if (sb.length() != 0) {
      sb.append(" ");
    }
    sb.append("PARTITION-LEVEL SORT");
  }
  if (isShuffleSort()) {
    if (sb.length() != 0) {
      sb.append(" ");
    }
    sb.append("SORT");
  }
  return sb.toString();
}
