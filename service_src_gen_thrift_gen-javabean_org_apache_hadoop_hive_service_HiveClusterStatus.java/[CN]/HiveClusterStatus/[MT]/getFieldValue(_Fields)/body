{
switch (field) {
case TASK_TRACKERS:
    return getTaskTrackers();
case MAP_TASKS:
  return getMapTasks();
case REDUCE_TASKS:
return getReduceTasks();
case MAX_MAP_TASKS:
return getMaxMapTasks();
case MAX_REDUCE_TASKS:
return getMaxReduceTasks();
case STATE:
return getState();
}
throw new IllegalStateException();
}
