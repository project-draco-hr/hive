{
  List<T> results=Lists.newArrayList();
  for (  T result : Futures.allAsList(futures).get()) {
    if (result != null) {
      if (result.getException() != null || result.getExitCode() != 0) {
        throw new SSHExecutionException(result);
      }
      results.add(result);
    }
  }
  return results;
}
