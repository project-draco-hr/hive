{
  if (fromTypeInfo.equals(toTypeInfo)) {
    return false;
  }
  if (fromTypeInfo.getCategory() == Category.PRIMITIVE && toTypeInfo.getCategory() == Category.PRIMITIVE) {
    PrimitiveCategory fromPrimitiveCategory=((PrimitiveTypeInfo)fromTypeInfo).getPrimitiveCategory();
    PrimitiveCategory toPrimitiveCategory=((PrimitiveTypeInfo)toTypeInfo).getPrimitiveCategory();
    PrimitiveCategory[] toPrimitiveCategories=validFromPrimitiveMap.get(fromPrimitiveCategory);
    if (toPrimitiveCategories == null || !ArrayUtils.contains(toPrimitiveCategories,toPrimitiveCategory)) {
      invalidFromTypeInfo=fromTypeInfo;
      invalidToTypeInfo=toTypeInfo;
      validConversion=false;
      return false;
    }
  }
 else {
  }
  return true;
}
