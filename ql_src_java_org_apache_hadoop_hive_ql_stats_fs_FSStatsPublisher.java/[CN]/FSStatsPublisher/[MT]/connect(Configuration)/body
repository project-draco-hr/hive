{
  conf=hconf;
  Path statsDir=new Path(hconf.get(StatsSetupConst.STATS_TMP_LOC));
  LOG.debug("Connecting to : " + statsDir);
  statsMap=new HashMap<String,Map<String,String>>();
  try {
    return statsDir.getFileSystem(conf).exists(statsDir);
  }
 catch (  IOException e) {
    LOG.error(e);
    return false;
  }
}
