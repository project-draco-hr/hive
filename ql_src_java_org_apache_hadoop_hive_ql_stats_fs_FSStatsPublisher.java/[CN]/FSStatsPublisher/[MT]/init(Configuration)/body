{
  Path statsDir=new Path(hconf.get(StatsSetupConst.STATS_TMP_LOC));
  LOG.debug("Initing FSStatsPublisher with : " + statsDir);
  try {
    statsDir.getFileSystem(hconf).mkdirs(statsDir);
    LOG.info("created : " + statsDir);
    return true;
  }
 catch (  IOException e) {
    LOG.error(e);
    return false;
  }
}
