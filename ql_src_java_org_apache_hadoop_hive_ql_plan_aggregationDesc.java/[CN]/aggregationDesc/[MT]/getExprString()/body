{
  FunctionInfo fI=FunctionRegistry.getInfo(aggregationClass);
  StringBuilder sb=new StringBuilder();
  sb.append(fI.getDisplayName());
  sb.append("(");
  if (distinct) {
    sb.append("DISTINCT ");
  }
  boolean first=true;
  for (  exprNodeDesc exp : parameters) {
    if (!first) {
      sb.append(", ");
    }
    sb.append(exp.getExprString());
    first=false;
  }
  sb.append(")");
  return sb.toString();
}
