{
  StringBuilder sb=new StringBuilder();
  sb.append(genericUDAFName);
  sb.append("(");
  if (distinct) {
    sb.append("DISTINCT ");
  }
  boolean first=true;
  for (  exprNodeDesc exp : parameters) {
    if (first) {
      first=false;
    }
 else {
      sb.append(", ");
    }
    sb.append(exp.getExprString());
  }
  sb.append(")");
  return sb.toString();
}
