{
  miniHS2.setConfProperty(ConfVars.HIVE_SERVER2_USE_SSL.varname,"true");
  miniHS2.setConfProperty(ConfVars.HIVE_SERVER2_SSL_KEYSTORE_PATH.varname,dataFileDir + File.separator + KEY_STORE_NAME);
  miniHS2.setConfProperty(ConfVars.HIVE_SERVER2_SSL_KEYSTORE_PASSWORD.varname,KEY_STORE_PASSWORD);
  miniHS2.start();
}
