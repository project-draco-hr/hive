{
  int exitCode;
  try {
    String output=Shell.execCommand("bash","-c",cmd);
    LOG.info("Output from '" + cmd + "': "+ output);
    exitCode=0;
  }
 catch (  Shell.ExitCodeException e) {
    exitCode=e.getExitCode();
    LOG.info("Error executing '" + cmd + "', exitCode = "+ exitCode,e);
  }
  return exitCode;
}
