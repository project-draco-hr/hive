{
  String columns=JDBCStatsUtils.getIdColumnName();
  String values="?";
  for (int i=0; i < supportedStats.size(); i++) {
    columns+=", " + getStatColumnName(supportedStats.get(i));
    values+=", ?";
  }
  String insert="INSERT INTO /* " + comment + " */ "+ getStatTableName()+ "("+ columns+ ") VALUES ("+ values+ ")";
  return insert;
}
