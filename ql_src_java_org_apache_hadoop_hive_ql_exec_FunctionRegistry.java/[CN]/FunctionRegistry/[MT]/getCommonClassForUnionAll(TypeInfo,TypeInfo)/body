{
  if (a.equals(b)) {
    return a;
  }
  if (FunctionRegistry.implicitConvertable(a,b)) {
    return b;
  }
  if (FunctionRegistry.implicitConvertable(b,a)) {
    return a;
  }
  for (  TypeInfo t : numericTypeList) {
    if (FunctionRegistry.implicitConvertable(a,t) && FunctionRegistry.implicitConvertable(b,t)) {
      return t;
    }
  }
  return null;
}
