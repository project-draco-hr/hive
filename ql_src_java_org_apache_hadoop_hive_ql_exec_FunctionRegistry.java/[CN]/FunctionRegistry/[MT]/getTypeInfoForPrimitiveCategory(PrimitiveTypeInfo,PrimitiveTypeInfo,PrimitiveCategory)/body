{
switch (typeCategory) {
case VARCHAR:
    int maxLength=getCommonLength(TypeInfoUtils.getCharacterLengthForType(a),TypeInfoUtils.getCharacterLengthForType(b));
  return TypeInfoFactory.getVarcharTypeInfo(maxLength);
default :
return TypeInfoFactory.getPrimitiveTypeInfo(PrimitiveObjectInspectorUtils.getTypeEntryFromPrimitiveCategory(typeCategory).typeName);
}
}
