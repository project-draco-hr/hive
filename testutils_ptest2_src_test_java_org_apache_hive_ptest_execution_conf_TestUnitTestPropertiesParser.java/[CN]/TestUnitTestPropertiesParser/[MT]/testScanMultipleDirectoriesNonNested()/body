{
  File baseDir=getFakeTestBaseDir();
  Context context=getDefaultContext();
  FileListProvider flProvider=getTestFileListProvider(baseDir,13,8);
  context.put(getUtRootPropertyName(UnitTestPropertiesParser.PROP_DIRECTORIES),"./" + MODULE1_NAME + " "+ "./"+ MODULE2_NAME);
  UnitTestPropertiesParser parser=new UnitTestPropertiesParser(context,TEST_CASE_PROPERT_NAME,baseDir,LOG,flProvider,null,true);
  Collection<TestBatch> testBatchCollection=parser.generateTestBatches();
  verifyBatches(testBatchCollection,3,new String[]{MODULE1_NAME,MODULE1_NAME,MODULE2_NAME},new int[]{10,3,8},new boolean[]{true,true,true});
}
