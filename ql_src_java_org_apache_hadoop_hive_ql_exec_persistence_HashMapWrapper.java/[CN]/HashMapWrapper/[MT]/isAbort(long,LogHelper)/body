{
  int size=mHash.size();
  long usedMemory=memoryMXBean.getHeapMemoryUsage().getUsed();
  double rate=(double)usedMemory / (double)maxMemory;
  console.printInfo(Utilities.now() + "\tProcessing rows:\t" + numRows+ "\tHashtable size:\t"+ size+ "\tMemory usage:\t"+ usedMemory+ "\trate:\t"+ num.format(rate));
  if (rate > (double)maxMemoryUsage) {
    return true;
  }
  return false;
}
