{
  for (int j=0; j < offsets.length; ++j) {
    offsets[j]=rdm.nextInt(offsetsToUse);
  }
  Arrays.sort(offsets);
  int check=0, insert=0, count=offsets.length;
  while (check < (offsets.length - 1)) {
    if (offsets[check] == offsets[check + 1]) {
      --insert;
      --count;
    }
    offsets[++insert]=offsets[++check];
  }
  return count;
}
