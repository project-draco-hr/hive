{
  TCloseSessionResp resp=new TCloseSessionResp();
  try {
    SessionHandle sessionHandle=new SessionHandle(req.getSessionHandle());
    cliService.closeSession(sessionHandle);
    resp.setStatus(OK_STATUS);
  }
 catch (  Exception e) {
    LOG.warn("Error closing session: ",e);
    resp.setStatus(HiveSQLException.toTStatus(e));
  }
 finally {
    if (!(isEmbedded) && (hiveConf.getBoolVar(ConfVars.HIVE_SERVER2_SUPPORT_DYNAMIC_SERVICE_DISCOVERY)) && (!hiveServer2.isRegisteredWithZooKeeper())) {
      if (cliService.getSessionManager().getOpenSessionCount() == 0) {
        LOG.info("This instance of HiveServer2 has been removed from the list of server " + "instances available for dynamic service discovery. " + "The last client session has ended - will shutdown now.");
        Thread shutdownThread=new Thread(){
          @Override public void run(){
            hiveServer2.stop();
          }
        }
;
        shutdownThread.start();
      }
    }
  }
  return resp;
}
