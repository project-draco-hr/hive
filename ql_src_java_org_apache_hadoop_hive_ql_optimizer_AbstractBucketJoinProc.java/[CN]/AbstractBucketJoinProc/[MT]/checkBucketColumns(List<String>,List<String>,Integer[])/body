{
  if (keys == null || bucketColumns == null || bucketColumns.isEmpty()) {
    return false;
  }
  for (int i=0; i < keys.size(); i++) {
    int index=bucketColumns.indexOf(keys.get(i));
    if (orders[i] != null && orders[i] != index) {
      return false;
    }
    orders[i]=index;
  }
  return keys.containsAll(bucketColumns);
}
