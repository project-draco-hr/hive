{
  for (  List<String> colValue : this.getSkewedColValues()) {
    if (colValue.size() != this.getSkewedColNames().size()) {
      throw new SemanticException(ErrorMsg.CREATE_SKEWED_TABLE_SKEWED_COL_NAME_VALUE_MISMATCH_3.getMsg() + this.getSkewedColNames().size() + " : "+ colValue.size());
    }
  }
}
