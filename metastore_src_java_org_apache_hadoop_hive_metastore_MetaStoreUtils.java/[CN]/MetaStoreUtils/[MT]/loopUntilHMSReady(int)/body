{
  int retries=0;
  Exception exc=null;
  while (true) {
    try {
      Socket socket=new Socket();
      socket.connect(new InetSocketAddress(port),5000);
      socket.close();
      return;
    }
 catch (    Exception e) {
      if (retries++ > 6) {
        exc=e;
        break;
      }
      Thread.sleep(10000);
    }
  }
  throw exc;
}
