{
  try {
    Deserializer deserializer=ReflectionUtils.newInstance(conf.getClassByName(schema.getProperty(serdeConstants.SERIALIZATION_LIB)).asSubclass(Deserializer.class),conf);
    deserializer.initialize(conf,schema);
    return deserializer;
  }
 catch (  Exception e) {
    LOG.error("error in initSerDe: " + e.getClass().getName() + " "+ e.getMessage());
    MetaStoreUtils.printStackTrace(e);
    throw new MetaException(e.getClass().getName() + " " + e.getMessage());
  }
}
