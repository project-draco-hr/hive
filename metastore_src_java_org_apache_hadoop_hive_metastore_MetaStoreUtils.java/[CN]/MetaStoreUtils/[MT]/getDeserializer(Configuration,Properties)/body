{
  try {
    String clazzName=schema.getProperty(serdeConstants.SERIALIZATION_LIB);
    if (clazzName == null) {
      throw new IllegalStateException("Property " + serdeConstants.SERIALIZATION_LIB + " cannot be null");
    }
    Deserializer deserializer=ReflectionUtils.newInstance(conf.getClassByName(clazzName).asSubclass(Deserializer.class),conf);
    deserializer.initialize(conf,schema);
    return deserializer;
  }
 catch (  Exception e) {
    LOG.error("error in initSerDe: " + e.getClass().getName() + " "+ e.getMessage(),e);
    throw new MetaException(e.getClass().getName() + " " + e.getMessage());
  }
}
