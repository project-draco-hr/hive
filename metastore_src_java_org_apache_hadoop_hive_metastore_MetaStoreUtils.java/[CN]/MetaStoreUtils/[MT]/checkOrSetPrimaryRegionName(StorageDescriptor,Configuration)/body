{
  String defaultRegionName=HiveConf.getVar(conf,HiveConf.ConfVars.HIVE_DEFAULT_REGION_NAME);
  if (sd.getPrimaryRegionName() != null && !sd.getPrimaryRegionName().equals(defaultRegionName)) {
    throw new MetaException("Only default region can be used");
  }
 else {
    sd.setPrimaryRegionName(defaultRegionName);
  }
}
