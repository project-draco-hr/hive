{
  ScanPlan newPlan=new ScanPlan();
  ScanMarker greaterStartMarker=getComparedMarker(this.getStartMarker(),other.getStartMarker(),true);
  newPlan.setStartMarker(greaterStartMarker);
  ScanMarker lesserEndMarker=getComparedMarker(this.getEndMarker(),other.getEndMarker(),false);
  newPlan.setEndMarker(lesserEndMarker);
  newPlan.setFilter(createCombinedFilter(this.getFilter(),other.getFilter()));
  return newPlan;
}
