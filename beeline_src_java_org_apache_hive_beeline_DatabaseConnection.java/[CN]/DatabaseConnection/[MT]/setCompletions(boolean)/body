{
  final String extraNameCharacters=getDatabaseMetaData() == null || getDatabaseMetaData().getExtraNameCharacters() == null ? "" : getDatabaseMetaData().getExtraNameCharacters();
  sqlCompletor=new ArgumentCompletor(new SQLCompletor(beeLine,skipmeta),new ArgumentCompletor.AbstractArgumentDelimiter(){
    @Override public boolean isDelimiterChar(    String buf,    int pos){
      char c=buf.charAt(pos);
      if (Character.isWhitespace(c)) {
        return true;
      }
      return !(Character.isLetterOrDigit(c)) && c != '_' && extraNameCharacters.indexOf(c) == -1;
    }
  }
);
  ((ArgumentCompletor)sqlCompletor).setStrict(false);
}
