{
  if (name == null) {
    inputFormatClass=null;
    tTable.getSd().setInputFormat(null);
    return;
  }
  try {
    setInputFormatClass((Class<? extends InputFormat<WritableComparable,Writable>>)Class.forName(name,true,Utilities.getSessionSpecifiedClassLoader()));
  }
 catch (  ClassNotFoundException e) {
    throw new HiveException("Class not found: " + name,e);
  }
}
