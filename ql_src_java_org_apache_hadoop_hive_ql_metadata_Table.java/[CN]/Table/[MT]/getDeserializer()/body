{
  if (deserializer == null) {
    try {
      deserializer=MetaStoreUtils.getDeserializer(Hive.get().getConf(),tTable);
    }
 catch (    MetaException e) {
      throw new RuntimeException(e);
    }
catch (    HiveException e) {
      throw new RuntimeException(e);
    }
  }
  return deserializer;
}
