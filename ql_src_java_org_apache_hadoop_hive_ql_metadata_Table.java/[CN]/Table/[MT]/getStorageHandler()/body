{
  if (storageHandler != null || !isNonNative()) {
    return storageHandler;
  }
  try {
    storageHandler=HiveUtils.getStorageHandler(SessionState.getSessionConf(),getProperty(org.apache.hadoop.hive.metastore.api.hive_metastoreConstants.META_TABLE_STORAGE));
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  return storageHandler;
}
