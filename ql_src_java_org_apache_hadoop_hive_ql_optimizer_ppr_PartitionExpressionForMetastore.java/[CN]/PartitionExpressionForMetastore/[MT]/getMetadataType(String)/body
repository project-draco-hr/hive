{
  try {
    Class<?> ifClass=Class.forName(inputFormat);
    if (OrcInputFormat.class.isAssignableFrom(ifClass)) {
      return FileMetadataExprType.ORC_SARG;
    }
    return null;
  }
 catch (  Throwable t) {
    LOG.warn("Can't create the class for input format " + inputFormat,t);
    return null;
  }
}
