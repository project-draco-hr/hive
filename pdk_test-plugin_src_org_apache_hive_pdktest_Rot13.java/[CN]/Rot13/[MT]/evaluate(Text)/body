{
  StringBuilder out=new StringBuilder(s.getLength());
  char[] ca=s.toString().toCharArray();
  for (  char c : ca) {
    if (c >= 'a' && c <= 'm') {
      c+=13;
    }
 else     if (c >= 'n' && c <= 'z') {
      c-=13;
    }
 else     if (c >= 'A' && c <= 'M') {
      c+=13;
    }
 else     if (c >= 'N' && c <= 'Z') {
      c-=13;
    }
    out.append(c);
  }
  t.set(out.toString());
  return t;
}
