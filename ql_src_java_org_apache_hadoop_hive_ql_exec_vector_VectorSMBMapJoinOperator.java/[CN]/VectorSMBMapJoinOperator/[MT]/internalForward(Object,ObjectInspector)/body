{
  Object[] values=(Object[])row;
  VectorColumnAssign[] vcas=outputVectorAssigners.get(outputOI);
  if (null == vcas) {
    Map<String,Map<String,Integer>> allColumnMaps=Utilities.getScratchColumnMap(hconf);
    Map<String,Integer> columnMap=allColumnMaps.get(fileKey);
    vcas=VectorColumnAssignFactory.buildAssigners(outputBatch,outputOI,columnMap,conf.getOutputColumnNames());
    outputVectorAssigners.put(outputOI,vcas);
  }
  for (int i=0; i < values.length; ++i) {
    vcas[i].assignObjectValue(values[i],outputBatch.size);
  }
  ++outputBatch.size;
  if (outputBatch.size == VectorizedRowBatch.DEFAULT_SIZE) {
    flushOutput();
  }
}
