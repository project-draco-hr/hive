{
  if (columnNames == null) {
    return null;
  }
  if (columnNames.isEmpty()) {
    return "";
  }
  StringBuffer sb=new StringBuffer();
  int numStrings=columnNames.split(",").length;
  sb.append("_col0");
  for (int i=1; i < numStrings; i++) {
    sb.append(",");
    sb.append(HiveConf.getColumnInternalName(i));
  }
  return sb.toString();
}
