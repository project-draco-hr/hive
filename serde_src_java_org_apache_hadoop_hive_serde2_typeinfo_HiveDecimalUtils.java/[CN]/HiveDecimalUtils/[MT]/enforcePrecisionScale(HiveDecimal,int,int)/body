{
  if (dec == null) {
    return null;
  }
  if (dec.precision() - dec.scale() <= maxPrecision - maxScale && dec.scale() <= maxScale) {
    return dec;
  }
  BigDecimal bd=HiveDecimal.enforcePrecisionScale(dec.bigDecimalValue(),maxPrecision,maxScale);
  if (bd == null) {
    return null;
  }
  return HiveDecimal.create(bd);
}
