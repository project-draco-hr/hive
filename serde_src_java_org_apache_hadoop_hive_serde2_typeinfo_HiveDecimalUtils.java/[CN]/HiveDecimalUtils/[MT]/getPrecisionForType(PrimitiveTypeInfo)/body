{
switch (typeInfo.getPrimitiveCategory()) {
case DECIMAL:
    return ((DecimalTypeInfo)typeInfo).precision();
case FLOAT:
  return 7;
case DOUBLE:
return 15;
case BYTE:
return 3;
case SHORT:
return 5;
case INT:
return 10;
case LONG:
return 19;
case VOID:
return 1;
default :
return HiveDecimal.MAX_PRECISION;
}
}
