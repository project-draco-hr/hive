{
  LinkedHashMap<Operator<? extends OperatorDesc>,OpParseContext> opParseCtxMap=newWork.getMapWork().getOpParseCtxMap();
  QBJoinTree newJoinTree=newWork.getMapWork().getJoinTree();
  MapJoinOperator newMapJoinOp=MapJoinProcessor.convertMapJoin(conf,opParseCtxMap,op,newJoinTree,mapJoinPos,true,false);
  genLocalWorkForMapJoin(newWork,newMapJoinOp,mapJoinPos);
}
