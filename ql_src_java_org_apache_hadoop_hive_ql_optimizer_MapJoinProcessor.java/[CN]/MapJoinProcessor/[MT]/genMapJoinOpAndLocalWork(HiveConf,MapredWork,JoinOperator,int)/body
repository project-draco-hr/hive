{
  LinkedHashMap<Operator<? extends OperatorDesc>,OpParseContext> opParseCtxMap=newWork.getMapWork().getOpParseCtxMap();
  MapJoinOperator newMapJoinOp=new MapJoinProcessor().convertMapJoin(conf,opParseCtxMap,op,newWork.getMapWork().isLeftInputJoin(),newWork.getMapWork().getBaseSrc(),newWork.getMapWork().getMapAliases(),mapJoinPos,true,false);
  genLocalWorkForMapJoin(newWork,newMapJoinOp,mapJoinPos);
}
