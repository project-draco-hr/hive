{
  int mapJoinPos=-1;
  if (joinTree.isMapSideJoin()) {
    int pos=0;
    if (joinTree.getJoinSrc() != null) {
      mapJoinPos=pos;
    }
    for (    String src : joinTree.getBaseSrc()) {
      if (src != null) {
        if (!joinTree.getMapAliases().contains(src)) {
          if (mapJoinPos >= 0) {
            return -1;
          }
          mapJoinPos=pos;
        }
      }
      pos++;
    }
    if (mapJoinPos == -1) {
      throw new SemanticException(ErrorMsg.INVALID_MAPJOIN_HINT.getMsg(Arrays.toString(joinTree.getBaseSrc())));
    }
  }
  return mapJoinPos;
}
