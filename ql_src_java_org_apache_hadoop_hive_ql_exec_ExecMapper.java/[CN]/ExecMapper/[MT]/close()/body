{
  try {
    mo.close(abort);
    reportStats rps=new reportStats(rp);
    mo.preorderMap(rps);
    return;
  }
 catch (  HiveException e) {
    if (!abort) {
      l4j.error("Hit error while closing operators - failing tree");
      throw new RuntimeException("Error while closing operators");
    }
  }
}
