{
  if (!op.opAllowedConvertMapJoin()) {
    return false;
  }
  if (op.getChildOperators() == null) {
    return true;
  }
  for (  Operator<? extends OperatorDesc> childOp : op.getChildOperators()) {
    if (!checkOperatorOKMapJoinConversion(childOp)) {
      return false;
    }
  }
  return true;
}
