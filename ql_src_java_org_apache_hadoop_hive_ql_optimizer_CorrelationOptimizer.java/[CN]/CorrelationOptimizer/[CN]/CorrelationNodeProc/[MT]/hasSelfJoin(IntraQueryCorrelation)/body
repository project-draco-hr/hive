{
  boolean hasSelfJoin=false;
  for (  Entry<String,List<ReduceSinkOperator>> entry : correlation.getTable2CorrelatedRSops().entrySet()) {
    for (    ReduceSinkOperator rsop : entry.getValue()) {
      Set<ReduceSinkOperator> intersection=new HashSet<ReduceSinkOperator>(CorrelationOptimizerUtils.findPeerReduceSinkOperators(rsop));
      intersection.retainAll(entry.getValue());
      if (intersection.size() > 1) {
        hasSelfJoin=true;
        return hasSelfJoin;
      }
    }
  }
  return hasSelfJoin;
}
