{
  Metrics metrics=MetricsFactory.getInstance();
  try {
    if (metrics != null) {
      for (      MetricsScope openScope : openScopes.values()) {
        metrics.endScope(openScope);
      }
    }
  }
 catch (  IOException e) {
    LOG.warn("Error cleaning up metrics",e);
  }
  openScopes.clear();
}
