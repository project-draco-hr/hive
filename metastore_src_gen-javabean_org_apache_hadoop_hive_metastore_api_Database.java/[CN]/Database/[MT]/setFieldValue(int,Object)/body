{
switch (fieldID) {
case NAME:
    if (value == null) {
      unsetName();
    }
 else {
      setName((String)value);
    }
  break;
case DESCRIPTION:
if (value == null) {
  unsetDescription();
}
 else {
  setDescription((String)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
