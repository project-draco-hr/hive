{
  PredicateLeaf pred=TestSearchArgumentImpl.createPredicateLeaf(PredicateLeaf.Operator.EQUALS,PredicateLeaf.Type.INTEGER,"x",15L,null);
  assertEquals(TruthValue.NO,RecordReaderImpl.evaluatePredicate(createIntStats(20L,30L),pred));
  assertEquals(TruthValue.YES_NO,RecordReaderImpl.evaluatePredicate(createIntStats(15L,30L),pred));
  assertEquals(TruthValue.YES_NO,RecordReaderImpl.evaluatePredicate(createIntStats(10L,30L),pred));
  assertEquals(TruthValue.YES_NO,RecordReaderImpl.evaluatePredicate(createIntStats(10L,15L),pred));
  assertEquals(TruthValue.NO,RecordReaderImpl.evaluatePredicate(createIntStats(0L,10L),pred));
  assertEquals(TruthValue.YES,RecordReaderImpl.evaluatePredicate(createIntStats(15L,15L),pred));
}
