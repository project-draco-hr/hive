{
  List<Object> args=new ArrayList<Object>();
  args.add(10L);
  args.add(20L);
  PredicateLeaf pred=TestSearchArgumentImpl.createPredicateLeaf(PredicateLeaf.Operator.BETWEEN,PredicateLeaf.Type.INTEGER,"x",null,args);
  assertEquals(TruthValue.NO,RecordReaderImpl.evaluatePredicateProto(createIntStats(0L,5L),pred,null));
  assertEquals(TruthValue.NO,RecordReaderImpl.evaluatePredicateProto(createIntStats(30L,40L),pred,null));
  assertEquals(TruthValue.YES_NO,RecordReaderImpl.evaluatePredicateProto(createIntStats(5L,15L),pred,null));
  assertEquals(TruthValue.YES_NO,RecordReaderImpl.evaluatePredicateProto(createIntStats(15L,25L),pred,null));
  assertEquals(TruthValue.YES_NO,RecordReaderImpl.evaluatePredicateProto(createIntStats(5L,25L),pred,null));
  assertEquals(TruthValue.YES,RecordReaderImpl.evaluatePredicateProto(createIntStats(10L,20L),pred,null));
  assertEquals(TruthValue.YES,RecordReaderImpl.evaluatePredicateProto(createIntStats(12L,18L),pred,null));
}
