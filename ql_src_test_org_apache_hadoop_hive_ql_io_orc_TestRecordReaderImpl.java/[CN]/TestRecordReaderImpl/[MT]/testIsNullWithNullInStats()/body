{
  PredicateLeaf pred=TestSearchArgumentImpl.createPredicateLeaf(PredicateLeaf.Operator.IS_NULL,PredicateLeaf.Type.STRING,"x",null,null);
  assertEquals(TruthValue.YES,RecordReaderImpl.evaluatePredicate(createStringStats("c","d",true),pred));
  assertEquals(TruthValue.NO,RecordReaderImpl.evaluatePredicate(createStringStats("c","d",false),pred));
}
