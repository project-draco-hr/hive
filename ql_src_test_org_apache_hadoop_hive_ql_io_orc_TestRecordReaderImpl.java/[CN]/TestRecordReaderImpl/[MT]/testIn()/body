{
  List<Object> args=new ArrayList<Object>();
  args.add(10L);
  args.add(20L);
  PredicateLeaf pred=TestSearchArgumentImpl.createPredicateLeaf(PredicateLeaf.Operator.IN,PredicateLeaf.Type.INTEGER,"x",null,args);
  assertEquals(TruthValue.YES,RecordReaderImpl.evaluatePredicateProto(createIntStats(20L,20L),pred,null));
  assertEquals(TruthValue.NO,RecordReaderImpl.evaluatePredicateProto(createIntStats(30L,30L),pred,null));
  assertEquals(TruthValue.YES_NO,RecordReaderImpl.evaluatePredicateProto(createIntStats(10L,30L),pred,null));
  assertEquals(TruthValue.NO,RecordReaderImpl.evaluatePredicateProto(createIntStats(12L,18L),pred,null));
}
