{
  List<Object> args=new ArrayList<Object>();
  args.add(10L);
  args.add(20L);
  PredicateLeaf pred=TestSearchArgumentImpl.createPredicateLeaf(PredicateLeaf.Operator.IN,PredicateLeaf.Type.INTEGER,"x",null,args);
  assertEquals(TruthValue.YES_NULL,RecordReaderImpl.evaluatePredicate(createIntStats(20L,20L),pred));
  assertEquals(TruthValue.NO_NULL,RecordReaderImpl.evaluatePredicate(createIntStats(30L,30L),pred));
  assertEquals(TruthValue.YES_NO_NULL,RecordReaderImpl.evaluatePredicate(createIntStats(10L,30L),pred));
  assertEquals(TruthValue.NO_NULL,RecordReaderImpl.evaluatePredicate(createIntStats(12L,18L),pred));
}
