{
  this.taskState=tTask.getTaskState();
  if (externalHandle == null && tTask.getExternalHandle() != null) {
    this.externalHandle=tTask.getExternalHandle();
  }
switch (taskState) {
case RUNNING:
    beginTime=System.currentTimeMillis();
  break;
case FINISHED:
endTime=System.currentTimeMillis();
break;
}
}
