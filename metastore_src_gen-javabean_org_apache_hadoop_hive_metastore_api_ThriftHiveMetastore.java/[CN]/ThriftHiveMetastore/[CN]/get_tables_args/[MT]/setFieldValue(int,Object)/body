{
switch (fieldID) {
case DB_NAME:
    if (value == null) {
      unsetDb_name();
    }
 else {
      setDb_name((String)value);
    }
  break;
case PATTERN:
if (value == null) {
  unsetPattern();
}
 else {
  setPattern((String)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
