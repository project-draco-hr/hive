{
  StringBuilder sb=new StringBuilder("create_database_args(");
  boolean first=true;
  sb.append("name:");
  if (this.name == null) {
    sb.append("null");
  }
 else {
    sb.append(this.name);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("description:");
  if (this.description == null) {
    sb.append("null");
  }
 else {
    sb.append(this.description);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}
