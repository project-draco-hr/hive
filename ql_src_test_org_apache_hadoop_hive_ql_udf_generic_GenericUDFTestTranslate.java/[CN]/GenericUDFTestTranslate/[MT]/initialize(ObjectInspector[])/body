{
  if (arguments.length != 3) {
    throw new UDFArgumentLengthException("The function TRANSLATE(expr,from_string,to_string) accepts exactly 3 arguments, but " + arguments.length + " arguments is found.");
  }
  for (int i=0; i < 3; i++) {
    if (arguments[i].getTypeName() != Constants.STRING_TYPE_NAME && arguments[i].getTypeName() != Constants.VOID_TYPE_NAME) {
      throw new UDFArgumentTypeException(i,"The " + getOrdinal(i + 1) + " argument of function TRANSLATE is expected to \""+ Constants.STRING_TYPE_NAME+ "\", but \""+ arguments[i].getTypeName()+ "\" is found");
    }
  }
  this.argumentOIs=arguments;
  return PrimitiveObjectInspectorFactory.writableStringObjectInspector;
}
