{
  HashMap<String,String> map=new HashMap<String,String>();
  try {
    for (    String node : zk.getChildren(makeZnode(type,id),false)) {
      byte[] b=zk.getData(makeFieldZnode(type,id,node),false,null);
      map.put(node,new String(b,ENCODING));
    }
  }
 catch (  Exception e) {
    return map;
  }
  return map;
}
