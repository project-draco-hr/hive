{
switch (field) {
case REQUEST_TYPE:
    if (value == null) {
      unsetRequestType();
    }
 else {
      setRequestType((GrantRevokeType)value);
    }
  break;
case PRIVILEGES:
if (value == null) {
  unsetPrivileges();
}
 else {
  setPrivileges((PrivilegeBag)value);
}
break;
case REVOKE_GRANT_OPTION:
if (value == null) {
unsetRevokeGrantOption();
}
 else {
setRevokeGrantOption((Boolean)value);
}
break;
}
}
