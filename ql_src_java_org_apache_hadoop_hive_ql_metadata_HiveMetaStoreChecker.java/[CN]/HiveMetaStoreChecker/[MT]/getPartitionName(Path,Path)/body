{
  String result=null;
  Path currPath=partitionPath;
  while (currPath != null && !tablePath.equals(currPath)) {
    if (result == null) {
      result=currPath.getName();
    }
 else {
      result=currPath.getName() + Path.SEPARATOR + result;
    }
    currPath=currPath.getParent();
  }
  return result;
}
