{
  basePlan=hepPlan(basePlan,mdProvider,HivePushFilterPastJoinRule.FILTER_ON_JOIN,HivePushFilterPastJoinRule.JOIN,new HivePartitionPrunerRule(SemanticAnalyzer.this.conf));
  HiveRelFieldTrimmer fieldTrimmer=new HiveRelFieldTrimmer(null);
  basePlan=fieldTrimmer.trim(basePlan);
  return basePlan;
}
