{
  ExprNodeDesc filterCondn=genExprNodeDesc(filterExpr,m_relToHiveRR.get(srcRel));
  ImmutableMap<String,Integer> hiveColNameOptiqPosMap=this.m_relToHiveColNameOptiqPosMap.get(srcRel);
  RexNode convertedFilterExpr=new RexNodeConverter(m_cluster,srcRel.getRowType(),hiveColNameOptiqPosMap,0,true).convert(filterCondn);
  RelNode filterRel=new HiveFilterRel(m_cluster,m_cluster.traitSetOf(HiveRel.CONVENTION),srcRel,convertedFilterExpr);
  this.m_relToHiveColNameOptiqPosMap.put(filterRel,hiveColNameOptiqPosMap);
  m_relToHiveRR.put(filterRel,m_relToHiveRR.get(srcRel));
  m_relToHiveColNameOptiqPosMap.put(filterRel,hiveColNameOptiqPosMap);
  return filterRel;
}
