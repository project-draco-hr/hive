{
  String qIdSupport=HiveConf.getVar(conf,HiveConf.ConfVars.HIVE_QUOTEDID_SUPPORT);
  if ("column".equals(qIdSupport)) {
    return false;
  }
  for (int i=0; i < pattern.length(); i++) {
    if (!Character.isLetterOrDigit(pattern.charAt(i)) && pattern.charAt(i) != '_') {
      return true;
    }
  }
  return false;
}
