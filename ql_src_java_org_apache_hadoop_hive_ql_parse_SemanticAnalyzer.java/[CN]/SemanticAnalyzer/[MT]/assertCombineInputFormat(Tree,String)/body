{
  String inputFormat=conf.getVar(HiveConf.ConfVars.HIVE_EXECUTION_ENGINE).equals("tez") ? HiveConf.getVar(conf,HiveConf.ConfVars.HIVETEZINPUTFORMAT) : HiveConf.getVar(conf,HiveConf.ConfVars.HIVEINPUTFORMAT);
  if (!inputFormat.equals(CombineHiveInputFormat.class.getName())) {
    throw new SemanticException(generateErrorMessage((ASTNode)numerator,message + " sampling is not supported in " + inputFormat));
  }
}
