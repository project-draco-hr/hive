{
  if ((task instanceof MapRedTask) || (task instanceof ExecDriver)) {
    mapredWork work=(mapredWork)task.getWork();
    work.deriveExplainAttributes();
    HashMap<String,Operator<? extends Serializable>> opMap=work.getAliasToWork();
    if (!opMap.isEmpty())     for (    Operator<? extends Serializable> op : opMap.values())     GenMapRedUtils.setKeyAndValueDesc(work,op);
  }
 else   if (task instanceof ConditionalTask) {
    List<Task<? extends Serializable>> listTasks=((ConditionalTask)task).getListTasks();
    for (    Task<? extends Serializable> tsk : listTasks)     setKeyDescTaskTree(tsk);
  }
  if (task.getChildTasks() == null)   return;
  for (  Task<? extends Serializable> childTask : task.getChildTasks())   setKeyDescTaskTree(childTask);
}
