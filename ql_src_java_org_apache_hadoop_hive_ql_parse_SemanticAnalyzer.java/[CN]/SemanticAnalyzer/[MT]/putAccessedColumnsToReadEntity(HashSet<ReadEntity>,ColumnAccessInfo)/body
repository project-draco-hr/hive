{
  Map<String,List<String>> tableToColumnAccessMap=columnAccessInfo.getTableToColumnAccessMap();
  if (tableToColumnAccessMap != null && !tableToColumnAccessMap.isEmpty()) {
    for (    ReadEntity entity : inputs) {
switch (entity.getType()) {
case TABLE:
        entity.getAccessedColumns().addAll(tableToColumnAccessMap.get(entity.getTable().getCompleteName()));
      break;
case PARTITION:
    entity.getAccessedColumns().addAll(tableToColumnAccessMap.get(entity.getPartition().getTable().getCompleteName()));
  break;
default :
}
}
}
}
