{
  OpParseContext inputCtx=opParseCtx.get(input);
  RowResolver inputRR=inputCtx.getRR();
  Operator output=putOpInsertMap(OperatorFactory.getAndMakeChild(new filterDesc(genExprNodeDesc(qb.getMetaData(),condn,inputRR)),new RowSchema(inputRR.getColumnInfos()),input),inputRR);
  LOG.debug("Created Filter Plan for " + qb.getId() + " row schema: "+ inputRR.toString());
  return output;
}
