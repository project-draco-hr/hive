{
  if (name.equalsIgnoreCase("AND")) {
    return "&&";
  }
 else   if (name.equalsIgnoreCase("OR")) {
    return "||";
  }
 else   if (name.equalsIgnoreCase("NOT")) {
    return "!";
  }
 else   if (name.equalsIgnoreCase("=")) {
    return "==";
  }
 else   if (name.equalsIgnoreCase("<>")) {
    return "!=";
  }
 else   if (name.equalsIgnoreCase("NULL")) {
    return "== NULL";
  }
 else   if (name.equalsIgnoreCase("NOT NULL")) {
    return "!= NULL";
  }
 else {
    return name;
  }
}
