{
  RowResolver inputRR=opParseCtx.get(input).getRowResolver();
  ArrayList<ExprNodeDesc> colList=new ArrayList<ExprNodeDesc>();
  ArrayList<String> columnNames=new ArrayList<String>();
  Map<String,ExprNodeDesc> colExprMap=new HashMap<String,ExprNodeDesc>();
  for (  ASTNode field : fields) {
    ExprNodeColumnDesc exprNode=(ExprNodeColumnDesc)genExprNodeDesc(field,inputRR);
    colList.add(exprNode);
    columnNames.add(exprNode.getColumn());
    colExprMap.put(exprNode.getColumn(),exprNode);
  }
  Operator output=putOpInsertMap(OperatorFactory.getAndMakeChild(new SelectDesc(colList,columnNames,false),new RowSchema(inputRR.getColumnInfos()),input),inputRR);
  output.setColumnExprMap(colExprMap);
  return output;
}
