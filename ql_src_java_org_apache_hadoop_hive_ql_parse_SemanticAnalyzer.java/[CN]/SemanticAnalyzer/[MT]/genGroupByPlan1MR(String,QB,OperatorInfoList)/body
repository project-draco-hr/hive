{
  if (inputList.size() != 1) {
    throw new SemanticException("Select has more than one inputs");
  }
  OperatorInfo inputOperatorInfo=inputList.get(0);
  QBParseInfo parseInfo=qb.getParseInfo();
  OperatorInfo reduceSinkOperatorInfo=genGroupByPlanReduceSinkOperator(parseInfo,dest,inputOperatorInfo,getGroupByForClause(parseInfo,dest).size());
  OperatorInfo groupByOperatorInfo=genGroupByPlanGroupByOperator(parseInfo,dest,reduceSinkOperatorInfo,groupByDesc.Mode.COMPLETE);
  OperatorInfo selectOperatorInfo=genGroupByPlanSelectOperator(parseInfo,dest,groupByOperatorInfo);
  OperatorInfoList output=new OperatorInfoList();
  output.add(selectOperatorInfo);
  return output;
}
