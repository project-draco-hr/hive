{
  if (qbexpr.getOpcode() == QBExpr.Opcode.NULLOP) {
    return genPlan(qbexpr.getQB());
  }
  if (qbexpr.getOpcode() == QBExpr.Opcode.UNION) {
    Operator qbexpr1Ops=genPlan(qbexpr.getQBExpr1());
    Operator qbexpr2Ops=genPlan(qbexpr.getQBExpr2());
    return genUnionPlan(qbexpr.getAlias(),qbexpr.getQBExpr1().getAlias(),qbexpr1Ops,qbexpr.getQBExpr2().getAlias(),qbexpr2Ops);
  }
  return null;
}
