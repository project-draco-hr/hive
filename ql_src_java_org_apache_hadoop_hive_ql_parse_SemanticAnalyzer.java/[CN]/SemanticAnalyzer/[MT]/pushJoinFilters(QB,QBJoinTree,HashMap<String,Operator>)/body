{
  Vector<Vector<CommonTree>> filters=joinTree.getFilters();
  if (joinTree.getJoinSrc() != null)   pushJoinFilters(qb,joinTree.getJoinSrc(),map);
  int pos=0;
  for (  String src : joinTree.getBaseSrc()) {
    if (src != null) {
      Operator srcOp=map.get(src);
      Vector<CommonTree> filter=filters.get(pos);
      for (      CommonTree cond : filter)       srcOp=genFilterPlan(qb,cond,srcOp);
      map.put(src,srcOp);
    }
    pos++;
  }
}
