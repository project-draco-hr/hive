{
  OpParseContext inputCtx=opParseCtx.get(input);
  RowResolver inputRR=inputCtx.getRowResolver();
  Operator output=putOpInsertMap(OperatorFactory.getAndMakeChild(new FilterDesc(genExprNodeDesc(condn,inputRR,useCaching),false),new RowSchema(inputRR.getColumnInfos()),input),inputRR);
  if (LOG.isDebugEnabled()) {
    LOG.debug("Created Filter Plan for " + qb.getId() + " row schema: "+ inputRR.toString());
  }
  return output;
}
