{
  OperatorInfo limitMap=(OperatorInfo)input.clone();
  limitMap.setOp(OperatorFactory.getAndMakeChild(new limitDesc(limit),new RowSchema(limitMap.getRowResolver().getColumnInfos()),input.getOp()));
  LOG.debug("Created LimitOperator Plan for clause: " + dest + " row schema: "+ limitMap.getRowResolver().toString());
  return limitMap;
}
