{
  ASTNode whereExpr=qb.getParseInfo().getWhrForClause(dest);
  OpParseContext inputCtx=opParseCtx.get(input);
  RowResolver inputRR=inputCtx.getRR();
  Operator output=putOpInsertMap(OperatorFactory.getAndMakeChild(new filterDesc(genExprNodeDesc((ASTNode)whereExpr.getChild(0),inputRR)),new RowSchema(inputRR.getColumnInfos()),input),inputRR);
  LOG.debug("Created Filter Plan for " + qb.getId() + ":"+ dest+ " row schema: "+ inputRR.toString());
  return output;
}
