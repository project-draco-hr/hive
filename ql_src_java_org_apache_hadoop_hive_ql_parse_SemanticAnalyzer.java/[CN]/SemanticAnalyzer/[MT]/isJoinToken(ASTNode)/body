{
  if ((node.getToken().getType() == HiveParser.TOK_JOIN) || (node.getToken().getType() == HiveParser.TOK_LEFTOUTERJOIN) || (node.getToken().getType() == HiveParser.TOK_RIGHTOUTERJOIN)|| (node.getToken().getType() == HiveParser.TOK_FULLOUTERJOIN)|| (node.getToken().getType() == HiveParser.TOK_LEFTSEMIJOIN)|| (node.getToken().getType() == HiveParser.TOK_UNIQUEJOIN))   return true;
  return false;
}
