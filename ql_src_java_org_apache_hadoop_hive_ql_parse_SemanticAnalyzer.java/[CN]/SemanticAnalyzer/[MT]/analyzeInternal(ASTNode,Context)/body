{
  this.ctx=ctx;
  reset();
  QB qb=new QB(null,null,false);
  this.qb=qb;
  this.ast=ast;
  LOG.info("Starting Semantic Analysis");
  doPhase1(ast,qb,initPhase1Ctx());
  LOG.info("Completed phase 1 of Semantic Analysis");
  getMetaData(qb);
  LOG.info("Completed getting MetaData in Semantic Analysis");
  genPlan(qb);
  ParseContext pCtx=new ParseContext(conf,qb,ast,aliasToPruner,aliasToSamplePruner,topOps,topSelOps,opParseCtx,loadTableWork,loadFileWork,ctx);
  Optimizer optm=new Optimizer();
  optm.setPctx(pCtx);
  optm.initialize();
  pCtx=optm.optimize();
  init(pCtx);
  qb=pCtx.getQB();
  genPartitionPruners(qb);
  LOG.info("Completed partition pruning");
  genSamplePruners(qb);
  LOG.info("Completed sample pruning");
  genMapRedTasks(qb);
  LOG.info("Completed plan generation");
  return;
}
