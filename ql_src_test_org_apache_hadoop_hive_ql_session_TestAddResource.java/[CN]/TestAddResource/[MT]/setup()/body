{
  conf=new HiveConf();
  t=ResourceType.JAR;
  for (int i=1; i <= 5; i++) {
    Writer output=null;
    String dataFile=TEST_JAR_DIR + "testjar" + i+ ".jar";
    File file=new File(dataFile);
    output=new BufferedWriter(new FileWriter(file));
    output.write("sample");
    output.close();
  }
}
