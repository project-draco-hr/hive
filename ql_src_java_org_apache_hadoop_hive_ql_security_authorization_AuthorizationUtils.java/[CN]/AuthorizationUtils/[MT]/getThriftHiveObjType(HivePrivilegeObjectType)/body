{
  if (type == null) {
    return null;
  }
switch (type) {
case DATABASE:
    return HiveObjectType.DATABASE;
case TABLE_OR_VIEW:
  return HiveObjectType.TABLE;
case PARTITION:
return HiveObjectType.PARTITION;
case LOCAL_URI:
case DFS_URI:
throw new HiveException("Unsupported type " + type);
default :
throw new AssertionError("Unsupported type " + type);
}
}
