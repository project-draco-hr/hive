{
  String columnHeaders[]=MetaDataFormatUtils.getColumnsHeader(null);
  formatOutputPretty(columnHeaders[0],columnHeaders[1],columnHeaders[2],columnInformation,maxColNameLen,prettyOutputNumCols);
  columnInformation.append(MetaDataFormatUtils.FIELD_DELIM).append(MetaDataFormatUtils.FIELD_DELIM).append(MetaDataFormatUtils.LINE_DELIM);
}
