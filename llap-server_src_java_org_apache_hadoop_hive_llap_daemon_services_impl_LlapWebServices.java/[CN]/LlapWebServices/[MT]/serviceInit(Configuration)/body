{
  this.conf=new Configuration(conf);
  this.conf.addResource(YarnConfiguration.YARN_SITE_CONFIGURATION_FILE);
  this.port=conf.getInt(LlapConfiguration.LLAP_DAEMON_SERVICE_PORT,LlapConfiguration.LLAP_DAEMON_SERVICE_PORT_DEFAULT);
  this.ssl=conf.getBoolean(LlapConfiguration.LLAP_DAEMON_SERVICE_SSL,LlapConfiguration.LLAP_DAEMON_SERVICE_SSL_DEFAULT);
  this.webAppInstance=new LlapWebApp();
}
