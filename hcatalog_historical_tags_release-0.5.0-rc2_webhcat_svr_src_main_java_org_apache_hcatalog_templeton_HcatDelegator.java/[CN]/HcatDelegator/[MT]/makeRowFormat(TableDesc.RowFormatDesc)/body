{
  String res=makeTermBy(desc.fieldsTerminatedBy,"fields") + makeTermBy(desc.collectionItemsTerminatedBy,"collection items") + makeTermBy(desc.mapKeysTerminatedBy,"map keys")+ makeTermBy(desc.linesTerminatedBy,"lines");
  if (TempletonUtils.isset(res))   return "row format delimited" + res;
 else   if (desc.serde != null)   return makeSerdeFormat(desc.serde);
 else   return "";
}
