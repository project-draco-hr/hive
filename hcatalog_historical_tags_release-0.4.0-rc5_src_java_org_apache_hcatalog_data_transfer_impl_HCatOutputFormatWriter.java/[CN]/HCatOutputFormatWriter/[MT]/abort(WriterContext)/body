{
  try {
    new HCatOutputFormat().getOutputCommitter(new TaskAttemptContext(context.getConf(),new TaskAttemptID())).abortJob(new JobContext(context.getConf(),null),State.FAILED);
  }
 catch (  IOException e) {
    throw new HCatException(ErrorType.ERROR_NOT_INITIALIZED,e);
  }
catch (  InterruptedException e) {
    throw new HCatException(ErrorType.ERROR_NOT_INITIALIZED,e);
  }
}
