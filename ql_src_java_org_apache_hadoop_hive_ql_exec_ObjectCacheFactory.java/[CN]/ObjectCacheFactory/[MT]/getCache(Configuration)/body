{
  if (HiveConf.getVar(conf,HiveConf.ConfVars.HIVE_EXECUTION_ENGINE).equals("tez") && HiveConf.getVar(conf,HiveConf.ConfVars.HIVE_EXECUTION_MODE).equals("container")) {
    return new org.apache.hadoop.hive.ql.exec.tez.ObjectCache();
  }
 else {
    return new org.apache.hadoop.hive.ql.exec.mr.ObjectCache();
  }
}
