{
  try {
    metadataTag=in.readInt();
    MapJoinObjectCtx ctx=MapJoinOperator.getMapMetadata().get(Integer.valueOf(metadataTag));
    val.readFields(in);
    obj=(ArrayList<Object>)ctx.getDeserializer().deserialize(val);
  }
 catch (  Exception e) {
    throw new IOException(e.getMessage());
  }
}
