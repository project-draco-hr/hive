{
  TableDesc tblDesc=getTableDesc(LazySimpleSerDe.class,"" + Utilities.ctrlaCode,cols,colTypes,false,false,fileFormat);
  tblDesc.getProperties().setProperty(serdeConstants.ESCAPE_CHAR,"\\");
  tblDesc.getProperties().setProperty(serdeConstants.SERIALIZATION_ESCAPE_CRLF,"true");
  tblDesc.getProperties().setProperty(LazySerDeParameters.SERIALIZATION_EXTEND_ADDITIONAL_NESTING_LEVELS,"true");
  return tblDesc;
}
