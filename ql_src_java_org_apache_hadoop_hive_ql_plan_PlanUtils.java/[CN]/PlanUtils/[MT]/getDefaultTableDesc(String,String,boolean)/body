{
  Properties properties=Utilities.makeProperties(Constants.SERIALIZATION_FORMAT,separatorCode,"columns",columns);
  if (lastColumnTakesRestOfTheLine) {
    properties.setProperty(Constants.SERIALIZATION_LAST_COLUMN_TAKES_REST,"true");
  }
  return new tableDesc(LazySimpleSerDe.class,TextInputFormat.class,IgnoreKeyTextOutputFormat.class,properties);
}
