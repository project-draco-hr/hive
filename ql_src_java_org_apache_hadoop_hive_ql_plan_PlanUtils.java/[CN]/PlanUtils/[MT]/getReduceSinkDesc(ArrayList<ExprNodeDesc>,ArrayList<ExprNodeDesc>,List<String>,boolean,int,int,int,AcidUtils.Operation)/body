{
  return getReduceSinkDesc(keyCols,keyCols.size(),valueCols,new ArrayList<List<Integer>>(),includeKey ? outputColumnNames.subList(0,keyCols.size()) : new ArrayList<String>(),includeKey ? outputColumnNames.subList(keyCols.size(),outputColumnNames.size()) : outputColumnNames,includeKey,tag,numPartitionFields,numReducers,writeType);
}
