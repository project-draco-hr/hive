{
  return new reduceSinkDesc(keyCols,valueCols,tag,numPartitionFields,numReducers,inferNumReducers,getDefaultTableDesc("" + Utilities.ctrlaCode,ObjectInspectorUtils.getIntegerCSV(keyCols.size())),getDefaultTableDesc("" + Utilities.ctrlaCode,ObjectInspectorUtils.getIntegerCSV(valueCols.size())));
}
