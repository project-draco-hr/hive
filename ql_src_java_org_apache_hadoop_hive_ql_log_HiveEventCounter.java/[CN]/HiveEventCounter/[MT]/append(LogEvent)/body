{
  Level level=event.getLevel();
  if (level.equals(Level.INFO)) {
    counts.incr(INFO);
  }
 else   if (level.equals(Level.WARN)) {
    counts.incr(WARN);
  }
 else   if (level.equals(Level.ERROR)) {
    counts.incr(ERROR);
  }
 else   if (level.equals(Level.FATAL)) {
    counts.incr(FATAL);
  }
}
