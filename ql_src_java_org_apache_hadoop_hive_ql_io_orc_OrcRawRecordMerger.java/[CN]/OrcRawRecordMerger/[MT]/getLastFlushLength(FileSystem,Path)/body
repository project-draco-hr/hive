{
  Path lengths=OrcRecordUpdater.getSideFile(deltaFile);
  long result=Long.MAX_VALUE;
  try (FSDataInputStream stream=fs.open(lengths)){
    result=-1;
    while (stream.available() > 0) {
      result=stream.readLong();
    }
    return result;
  }
 catch (  IOException ioe) {
    return result;
  }
}
