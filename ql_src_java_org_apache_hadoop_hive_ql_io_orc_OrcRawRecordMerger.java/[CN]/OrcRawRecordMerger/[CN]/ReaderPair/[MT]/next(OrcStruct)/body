{
  if (recordReader.hasNext()) {
    nextRecord=(OrcStruct)recordReader.next(next);
    key.setValues(OrcRecordUpdater.getOriginalTransaction(nextRecord),OrcRecordUpdater.getBucket(nextRecord),OrcRecordUpdater.getRowId(nextRecord),OrcRecordUpdater.getCurrentTransaction(nextRecord));
    if (maxKey != null && key.compareRow(maxKey) > 0) {
      LOG.debug("key " + key + " > maxkey "+ maxKey);
      nextRecord=null;
      recordReader.close();
    }
  }
 else {
    nextRecord=null;
    recordReader.close();
  }
}
