{
  boolean logCreated=false;
  BufferedReader buf=new BufferedReader(new InputStreamReader(process.getInputStream()));
  String line="";
  while ((line=buf.readLine()) != null) {
    line=line.replace("//","/");
    if (line.equals(logFile)) {
      logCreated=true;
    }
  }
  assertEquals(true,logCreated);
}
