{
  Converter converter;
  if (type.isRepetition(Type.Repetition.REPEATED)) {
    if (type.isPrimitive()) {
      converter=new Repeated.RepeatedPrimitiveConverter(type.asPrimitiveType(),this,fieldIndex,hiveTypeInfo);
    }
 else {
      converter=new Repeated.RepeatedGroupConverter(type.asGroupType(),this,fieldIndex,hiveTypeInfo == null ? null : ((ListTypeInfo)hiveTypeInfo).getListElementTypeInfo());
    }
    repeatedConverters.add((Repeated)converter);
  }
 else {
    converter=getConverterFromDescription(type,fieldIndex,this,hiveTypeInfo);
  }
  return converter;
}
