{
  if (exec == null) {
    exec=new Exec();
    String query=queryOI.getPrimitiveJavaObject(arguments[0].get());
    String[] args={"-e",query,"-trace"};
    try {
      exec.setUdfRun(true);
      exec.init(args);
    }
 catch (    Exception e) {
      throw new HiveException(e.getMessage());
    }
  }
  if (arguments.length > 1) {
    setParameters(arguments);
  }
  Var result=exec.run();
  if (result != null) {
    return result.toString();
  }
  return null;
}
