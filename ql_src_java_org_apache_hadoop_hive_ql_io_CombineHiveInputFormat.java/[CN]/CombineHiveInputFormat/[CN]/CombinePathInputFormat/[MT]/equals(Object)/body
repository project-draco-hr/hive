{
  if (o instanceof CombinePathInputFormat) {
    CombinePathInputFormat mObj=(CombinePathInputFormat)o;
    if (mObj == null) {
      return false;
    }
    return (opList.equals(mObj.opList)) && (inputFormatClassName.equals(mObj.inputFormatClassName)) && (deserializerClassName == null ? (mObj.deserializerClassName == null) : deserializerClassName.equals(mObj.deserializerClassName));
  }
  return false;
}
