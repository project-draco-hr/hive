{
  for (  Path filterPath : filterPaths) {
    try {
      path=path.makeQualified(filterPath.getFileSystem(conf));
    }
 catch (    IOException e) {
      throw new RuntimeException("Error making path " + path + " qualified ",e);
    }
    if (filterPath.equals(path)) {
      return true;
    }
  }
  return false;
}
