{
  if (queryString == null) {
    return null;
  }
  Map<String,String[]> params=javax.servlet.http.HttpUtils.parseQueryString(queryString);
  Set<String> keySet=params.keySet();
  for (  String key : keySet) {
    if (key.equalsIgnoreCase("doAs")) {
      return params.get(key)[0];
    }
  }
  return null;
}
