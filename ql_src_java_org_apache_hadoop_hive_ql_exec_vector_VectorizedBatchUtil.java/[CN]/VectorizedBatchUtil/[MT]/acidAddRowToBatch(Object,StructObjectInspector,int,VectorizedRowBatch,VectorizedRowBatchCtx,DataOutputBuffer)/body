{
  List<? extends StructField> fieldRefs=oi.getAllStructFieldRefs();
  for (int i=0; i < fieldRefs.size(); i++) {
    if (batch.cols[i] == null) {
      continue;
    }
    if (context.isPartitionCol(i)) {
      continue;
    }
    setVector(row,oi,fieldRefs.get(i),batch,buffer,rowIndex,i,0);
  }
}
