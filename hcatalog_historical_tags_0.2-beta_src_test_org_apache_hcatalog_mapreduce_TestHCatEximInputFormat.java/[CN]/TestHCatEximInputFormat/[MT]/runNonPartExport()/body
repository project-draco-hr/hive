{
  if (fs.exists(inputLocation)) {
    fs.delete(inputLocation,true);
  }
  setupMRExport(new String[]{"237,Krishna,01/01/1990,M,IN,TN\n","238,Kalpana,01/01/2000,F,IN,KA\n","239,Satya,01/01/2001,M,US,TN\n","240,Kavya,01/01/2002,F,US,KA\n"});
  HCatEximOutputFormat.setOutput(job,dbName,tblName,inputLocation.toString(),null,null,schema);
  job.waitForCompletion(true);
  HCatEximOutputCommitter committer=new HCatEximOutputCommitter(job,null);
  committer.cleanupJob(job);
}
