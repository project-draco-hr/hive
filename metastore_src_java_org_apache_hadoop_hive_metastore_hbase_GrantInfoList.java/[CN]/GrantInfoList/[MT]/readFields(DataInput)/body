{
  int size=in.readInt();
  if (size == 0) {
    grantInfos=new ArrayList<GrantInfoWritable>();
  }
 else {
    grantInfos=new ArrayList<GrantInfoWritable>(size);
    for (int i=0; i < size; i++) {
      GrantInfoWritable info=new GrantInfoWritable();
      info.readFields(in);
      grantInfos.add(info);
    }
  }
}
