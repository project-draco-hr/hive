{
  FileSplit fileSplit=(FileSplit)inputSplit;
  Path path=fileSplit.getPath();
  FileSystem fs=path.getFileSystem(ShimLoader.getHadoopShims().getConfiguration(context));
  return new OrcRecordReader(OrcFile.createReader(fs,path),ShimLoader.getHadoopShims().getConfiguration(context),fileSplit.getStart(),fileSplit.getLength());
}
