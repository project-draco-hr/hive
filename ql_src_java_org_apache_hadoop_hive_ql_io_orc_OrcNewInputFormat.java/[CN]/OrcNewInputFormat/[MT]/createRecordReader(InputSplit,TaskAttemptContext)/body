{
  FileSplit fileSplit=(FileSplit)inputSplit;
  Path path=fileSplit.getPath();
  Configuration conf=ShimLoader.getHadoopShims().getConfiguration(context);
  FileSystem fs=path.getFileSystem(conf);
  return new OrcRecordReader(OrcFile.createReader(fs,path,conf),ShimLoader.getHadoopShims().getConfiguration(context),fileSplit.getStart(),fileSplit.getLength());
}
