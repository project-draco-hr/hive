{
  int val=value;
  if (value < 0) {
    val*=-1;
  }
  int digitCount=0;
  for (int v=val; v != 0; v/=10) {
    digitCount++;
  }
  final int retainCount=digitCount < charCount ? 0 : (digitCount - charCount);
  int ret=0;
  int pos=1;
  for (int i=0; val != 0; i++) {
    if (i >= retainCount) {
      ret+=maskedNumber * pos;
    }
 else {
      ret+=(val % 10) * pos;
    }
    val/=10;
    pos*=10;
  }
  if (value < 0) {
    ret*=-1;
  }
  return ret;
}
