{
  if (entry == _first) {
    _first=entry.getNext();
  }
  if (_last == entry) {
    _last=entry.getPrevious();
  }
  CacheEntry previous=entry.getPrevious();
  CacheEntry next=entry.getNext();
  if (previous != null) {
    previous.setNext(next);
  }
  if (next != null) {
    next.setPrevious(previous);
  }
  entry.setPrevious(null);
  entry.setNext(null);
}
