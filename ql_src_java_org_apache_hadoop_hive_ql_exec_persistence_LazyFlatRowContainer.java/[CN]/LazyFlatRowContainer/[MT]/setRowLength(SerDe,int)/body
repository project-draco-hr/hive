{
  try {
    rowLength=ObjectInspectorUtils.getStructSize(serde.getObjectInspector());
  }
 catch (  SerDeException ex) {
    throw new HiveException("Get structure size error",ex);
  }
  if (rowLength == 0) {
    rowLength=-rowCount;
    array=EMPTY_OBJECT_ARRAY;
    return false;
  }
  return true;
}
