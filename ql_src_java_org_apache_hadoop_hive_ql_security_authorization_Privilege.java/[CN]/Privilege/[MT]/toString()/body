{
switch (this.priv) {
case ALL:
    return "All";
case ALTER_DATA:
  return "Update";
case ALTER_METADATA:
return "Alter";
case CREATE:
return "Create";
case DROP:
return "Drop";
case INDEX:
return "Index";
case LOCK:
return "Lock";
case SELECT:
return "Select";
case SHOW_DATABASE:
return "Show_Database";
default :
return "Unknown";
}
}
