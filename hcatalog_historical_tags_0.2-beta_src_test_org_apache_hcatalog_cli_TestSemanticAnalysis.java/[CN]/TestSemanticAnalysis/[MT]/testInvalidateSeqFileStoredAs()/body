{
  hcatDriver.run("drop table junit_sem_analysis");
  query="create table junit_sem_analysis (a int) partitioned by (b string)  stored as SEQUENCEFILE";
  CommandProcessorResponse response=hcatDriver.run(query);
  assertEquals(10,response.getResponseCode());
  assertEquals("FAILED: Error in semantic analysis: Operation not supported. HCatalog doesn't support Sequence File by default yet. You may specify it through INPUT/OUTPUT storage drivers.",response.getErrorMessage());
}
