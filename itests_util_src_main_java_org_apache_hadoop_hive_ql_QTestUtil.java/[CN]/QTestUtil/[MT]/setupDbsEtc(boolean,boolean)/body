{
  if (!dbEtcSetup || force) {
    if (isNewDb) {
      db=Hive.get(conf);
    }
 else {
      db=Hive.getWithFastCheck(conf,false);
    }
    LOG.info("Obtained db");
    drv=new Driver(conf);
    drv.init();
    pd=new ParseDriver();
    sem=new SemanticAnalyzer(conf);
    dbEtcSetup=true;
  }
}
