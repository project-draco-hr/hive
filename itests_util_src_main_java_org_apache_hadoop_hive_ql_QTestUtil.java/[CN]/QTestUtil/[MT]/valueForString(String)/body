{
  if (type.equals("miniMR")) {
    return mr;
  }
 else   if (type.equals("tez")) {
    return tez;
  }
 else   if (type.equals("spark")) {
    return spark;
  }
 else   if (type.equals("miniSparkOnYarn")) {
    return miniSparkOnYarn;
  }
 else   if (type.equals("llap")) {
    return llap;
  }
 else {
    return none;
  }
}
