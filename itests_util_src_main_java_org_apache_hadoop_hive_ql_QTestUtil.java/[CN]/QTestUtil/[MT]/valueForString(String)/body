{
  if (type.equals("miniMR")) {
    return mr;
  }
 else   if (type.equals("tez")) {
    return tez;
  }
 else   if (type.equals("spark")) {
    return spark;
  }
 else   if (type.equals("encrypted")) {
    return encrypted;
  }
 else   if (type.equals("tezlocal")) {
    return tezlocal;
  }
 else {
    return none;
  }
}
