{
  if (c == null)   throw new NullPointerException();
  if (c == this)   throw new IllegalArgumentException();
  lock.lock();
  try {
    for (    E e : list) {
      c.add(e);
    }
    int n=list.size();
    list.clear();
    notFull.signalAll();
    return n;
  }
  finally {
    lock.unlock();
  }
}
