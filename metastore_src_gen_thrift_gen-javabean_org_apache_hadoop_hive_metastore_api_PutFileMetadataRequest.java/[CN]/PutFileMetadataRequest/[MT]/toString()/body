{
  StringBuilder sb=new StringBuilder("PutFileMetadataRequest(");
  boolean first=true;
  sb.append("fileIds:");
  if (this.fileIds == null) {
    sb.append("null");
  }
 else {
    sb.append(this.fileIds);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("metadata:");
  if (this.metadata == null) {
    sb.append("null");
  }
 else {
    org.apache.thrift.TBaseHelper.toString(this.metadata,sb);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}
