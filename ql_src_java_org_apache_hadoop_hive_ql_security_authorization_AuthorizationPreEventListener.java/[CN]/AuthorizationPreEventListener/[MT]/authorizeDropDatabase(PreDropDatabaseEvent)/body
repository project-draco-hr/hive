{
  try {
    tAuthorizer.get().authorize(new Database(context.getDatabase()),HiveOperation.DROPDATABASE.getInputRequiredPrivileges(),HiveOperation.DROPDATABASE.getOutputRequiredPrivileges());
  }
 catch (  AuthorizationException e) {
    throw invalidOperationException(e);
  }
catch (  HiveException e) {
    throw metaException(e);
  }
}
