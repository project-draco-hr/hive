{
  try {
    tAuthorizer.get().authorize(new TableWrapper(context.getTable()),HiveOperation.CREATETABLE.getInputRequiredPrivileges(),HiveOperation.CREATETABLE.getOutputRequiredPrivileges());
  }
 catch (  AuthorizationException e) {
    throw invalidOperationException(e);
  }
catch (  HiveException e) {
    throw metaException(e);
  }
}
