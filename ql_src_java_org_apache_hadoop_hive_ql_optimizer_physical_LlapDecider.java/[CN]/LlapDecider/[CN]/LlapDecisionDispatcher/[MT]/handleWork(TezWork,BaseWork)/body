{
  if (evaluateWork(tezWork,work)) {
    convertWork(tezWork,work);
  }
 else {
    if (mode == all) {
      throw new SemanticException("Llap mode is set to all but cannot run work in llap mode." + "Set " + HiveConf.ConfVars.LLAP_EXECUTION_MODE + " = auto or set "+ HiveConf.ConfVars.HIVE_EXECUTION_MODE+ " = container");
    }
  }
}
