{
  if (enablePreemption) {
    String state=reason == null ? "FAILED" : reason.name();
    preemptionQueue.remove(task.getRequest());
    if (isInfoEnabled) {
      LOG.info(TaskRunnerCallable.getTaskIdentifierString(task.getRequest()) + " request " + state+ "! Removed from preemption list.");
    }
  }
  numSlotsAvailable.incrementAndGet();
  if (!waitQueue.isEmpty()) {
synchronized (waitLock) {
      waitLock.notify();
    }
  }
}
