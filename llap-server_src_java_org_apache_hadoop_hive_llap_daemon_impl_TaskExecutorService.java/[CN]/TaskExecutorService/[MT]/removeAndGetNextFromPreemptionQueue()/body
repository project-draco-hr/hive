{
  TaskWrapper taskWrapper;
synchronized (lock) {
    taskWrapper=preemptionQueue.poll();
    if (taskWrapper != null) {
      taskWrapper.setIsInPreemptableQueue(false);
      if (metrics != null) {
        metrics.setExecutorNumPreemptableRequests(preemptionQueue.size());
      }
    }
  }
  return taskWrapper;
}
