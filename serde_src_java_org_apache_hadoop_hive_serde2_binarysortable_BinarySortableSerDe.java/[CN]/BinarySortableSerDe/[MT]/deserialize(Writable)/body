{
  BytesWritable data=(BytesWritable)blob;
  inputByteBuffer.reset(data.getBytes(),0,data.getLength());
  try {
    for (int i=0; i < columnNames.size(); i++) {
      row.set(i,deserialize(inputByteBuffer,columnTypes.get(i),columnSortOrderIsDesc[i],columnNullMarker[i],columnNotNullMarker[i],row.get(i)));
    }
  }
 catch (  IOException e) {
    throw new SerDeException(e);
  }
  return row;
}
