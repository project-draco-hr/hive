{
  accepted.close().awaitUninterruptibly(10,TimeUnit.SECONDS);
  if (selector != null) {
    ServerBootstrap bootstrap=new ServerBootstrap(selector);
    bootstrap.releaseExternalResources();
  }
  if (pipelineFact != null) {
    pipelineFact.destroy();
  }
  if (dirWatcher != null) {
    dirWatcher.stop();
  }
}
