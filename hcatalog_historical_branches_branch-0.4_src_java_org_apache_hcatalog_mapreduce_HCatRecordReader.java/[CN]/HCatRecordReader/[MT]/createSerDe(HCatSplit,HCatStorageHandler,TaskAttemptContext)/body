{
  SerDe serde=ReflectionUtils.newInstance(storageHandler.getSerDeClass(),taskContext.getConfiguration());
  try {
    InternalUtil.initializeInputSerDe(serde,storageHandler.getConf(),hcatSplit.getPartitionInfo().getTableInfo(),hcatSplit.getPartitionInfo().getPartitionSchema());
  }
 catch (  SerDeException e) {
    throw new IOException("Failed initializing SerDe " + storageHandler.getSerDeClass().getName(),e);
  }
  return serde;
}
