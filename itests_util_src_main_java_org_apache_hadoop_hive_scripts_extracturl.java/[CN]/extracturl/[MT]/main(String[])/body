{
  String input;
  try {
    while ((input=in.readLine()) != null) {
      Matcher m=pattern.matcher(input);
      while (m.find()) {
        String url=input.substring(m.start(1),m.end(1));
        System.out.println(url + "\t" + "1");
      }
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    System.exit(1);
  }
}
