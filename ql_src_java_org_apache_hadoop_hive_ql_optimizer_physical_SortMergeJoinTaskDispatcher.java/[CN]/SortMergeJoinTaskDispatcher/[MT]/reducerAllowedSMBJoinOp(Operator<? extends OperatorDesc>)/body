{
  while (reducer != null) {
    if (!reducer.opAllowedBeforeSortMergeJoin()) {
      return false;
    }
    List<Operator<? extends OperatorDesc>> childOps=reducer.getChildOperators();
    if ((childOps == null) || (childOps.isEmpty())) {
      return true;
    }
    if (childOps.size() > 1) {
      return false;
    }
    reducer=childOps.get(0);
  }
  return true;
}
