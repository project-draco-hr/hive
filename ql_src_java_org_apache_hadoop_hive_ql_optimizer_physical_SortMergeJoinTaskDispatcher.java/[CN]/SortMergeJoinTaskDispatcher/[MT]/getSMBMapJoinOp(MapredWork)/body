{
  if (work != null) {
    Operator<? extends OperatorDesc> reducer=work.getReducer();
    for (    Operator<? extends OperatorDesc> op : work.getAliasToWork().values()) {
      SMBMapJoinOperator smbMapJoinOp=getSMBMapJoinOp(op,reducer);
      if (smbMapJoinOp != null) {
        return smbMapJoinOp;
      }
    }
  }
  return null;
}
