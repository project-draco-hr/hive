{
  byte[] byteS=s.getBytes();
  byte[] byteSub=sub.getBytes();
  int lenSub=sub.getLength();
  boolean match=false;
  for (int i=startS; (i < endS - lenSub + 1) && (!match); i++) {
    match=true;
    for (int j=0; j < lenSub; j++) {
      if (byteS[j + i] != byteSub[j]) {
        match=false;
        break;
      }
    }
  }
  return match;
}
