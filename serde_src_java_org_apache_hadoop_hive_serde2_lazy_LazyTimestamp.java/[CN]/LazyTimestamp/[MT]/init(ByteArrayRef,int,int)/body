{
  String s=null;
  try {
    s=new String(bytes.getData(),start,length,"US-ASCII");
  }
 catch (  UnsupportedEncodingException e) {
    LOG.error(e);
    s="";
  }
  Timestamp t;
  if (s.compareTo("NULL") == 0) {
    t=null;
    logExceptionMessage(bytes,start,length,"TIMESTAMP");
  }
 else {
    t=Timestamp.valueOf(s);
  }
  data.set(t);
}
