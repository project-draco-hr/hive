{
  StringBuilder sql=new StringBuilder();
  sql.append(ctx.T_FROM().getText()).append(" ");
  sql.append(evalPop(ctx.from_table_clause()));
  int cnt=ctx.from_join_clause().size();
  for (int i=0; i < cnt; i++) {
    sql.append(evalPop(ctx.from_join_clause(i)));
  }
  exec.stackPush(sql);
  return 0;
}
