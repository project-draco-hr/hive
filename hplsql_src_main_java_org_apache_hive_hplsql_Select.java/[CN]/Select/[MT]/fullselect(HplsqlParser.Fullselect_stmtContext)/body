{
  int cnt=ctx.fullselect_stmt_item().size();
  StringBuilder sql=new StringBuilder();
  for (int i=0; i < cnt; i++) {
    String part=evalPop(ctx.fullselect_stmt_item(i)).toString();
    sql.append(part);
    if (i + 1 != cnt) {
      sql.append("\n" + getText(ctx.fullselect_set_clause(i)) + "\n");
    }
  }
  exec.stackPush(sql);
  return 0;
}
