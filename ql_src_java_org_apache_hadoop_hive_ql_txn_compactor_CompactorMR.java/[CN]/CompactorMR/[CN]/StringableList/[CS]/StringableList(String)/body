{
  String[] parts=s.split(":",2);
  int numElements=Integer.valueOf(parts[0]);
  s=parts[1];
  for (int i=0; i < numElements; i++) {
    parts=s.split(":",2);
    int len=Integer.valueOf(parts[0]);
    String val=parts[1].substring(0,len);
    s=parts[1].substring(len);
    add(new Path(val));
  }
}
