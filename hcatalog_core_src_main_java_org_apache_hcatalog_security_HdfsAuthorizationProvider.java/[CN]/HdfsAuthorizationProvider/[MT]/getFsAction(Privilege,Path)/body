{
switch (priv.getPriv()) {
case ALL:
    throw new AuthorizationException("no matching Action for Privilege.All");
case ALTER_DATA:
  return FsAction.WRITE;
case ALTER_METADATA:
return FsAction.WRITE;
case CREATE:
return FsAction.WRITE;
case DROP:
return FsAction.WRITE;
case INDEX:
return FsAction.WRITE;
case LOCK:
return FsAction.WRITE;
case SELECT:
return FsAction.READ;
case SHOW_DATABASE:
return FsAction.READ;
case UNKNOWN:
default :
throw new AuthorizationException("Unknown privilege");
}
}
