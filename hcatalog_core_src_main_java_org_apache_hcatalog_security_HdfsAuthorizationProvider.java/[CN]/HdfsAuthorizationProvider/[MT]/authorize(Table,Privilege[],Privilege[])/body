{
  if (table == null) {
    return;
  }
  Path path=null;
  try {
    if (table.getTTable().getSd().getLocation() == null || table.getTTable().getSd().getLocation().isEmpty()) {
      path=wh.getTablePath(hive_db.getDatabase(table.getDbName()),table.getTableName());
    }
 else {
      path=table.getPath();
    }
  }
 catch (  MetaException ex) {
    throw new HiveException(ex);
  }
  authorize(path,readRequiredPriv,writeRequiredPriv);
}
