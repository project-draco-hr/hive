{
  super.afterExecute(r,t);
  if (t == null && r instanceof Future<?>) {
    try {
      Future<?> future=(Future<?>)r;
      if (future.isDone()) {
        future.get();
      }
    }
 catch (    InterruptedException ie) {
      Thread.currentThread().interrupt();
    }
catch (    Throwable t2) {
      t=t2;
    }
  }
  if (t instanceof OutOfMemoryError) {
    oomHook.run();
  }
}
