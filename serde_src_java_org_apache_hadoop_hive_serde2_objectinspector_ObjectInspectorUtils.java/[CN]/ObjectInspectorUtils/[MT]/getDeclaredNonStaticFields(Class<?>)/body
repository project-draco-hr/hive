{
  Field[] f=c.getDeclaredFields();
  ArrayList<Field> af=new ArrayList<Field>();
  for (int i=0; i < f.length; ++i) {
    if (!Modifier.isStatic(f[i].getModifiers())) {
      af.add(f[i]);
    }
  }
  Field[] r=new Field[af.size()];
  for (int i=0; i < af.size(); ++i) {
    r[i]=af.get(i);
  }
  return r;
}
