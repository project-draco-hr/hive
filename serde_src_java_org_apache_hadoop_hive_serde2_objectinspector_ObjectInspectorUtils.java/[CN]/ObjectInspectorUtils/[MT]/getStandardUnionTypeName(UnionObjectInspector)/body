{
  StringBuilder sb=new StringBuilder();
  sb.append(serdeConstants.UNION_TYPE_NAME + "<");
  List<ObjectInspector> ois=uoi.getObjectInspectors();
  for (int i=0; i < ois.size(); i++) {
    if (i > 0) {
      sb.append(",");
    }
    sb.append(ois.get(i).getTypeName());
  }
  sb.append(">");
  return sb.toString();
}
