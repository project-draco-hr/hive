{
  while (integerCSVCache.size() <= size) {
    integerCSVCache.add(null);
  }
  String result=integerCSVCache.get(size);
  if (result == null) {
    StringBuilder sb=new StringBuilder();
    for (int i=0; i < size; i++) {
      if (i > 0)       sb.append(",");
      sb.append("" + i);
    }
    result=sb.toString();
    integerCSVCache.set(size,result);
  }
  return result;
}
