{
  forwardedRow.clear();
  List<Object> thisRow=(List<Object>)row;
  forwardedRow.add(thisRow.get(0));
  forwardedRow.add(thisRow.get(1));
  forwardedRow.add(oldTagByteWritable);
  if (isLogInfoEnabled) {
    cntr++;
    if (cntr == nextCntr) {
      log.info(id + " (newTag, childIndex, oldTag)=(" + newTag+ ", "+ childIndex+ ", "+ oldTag+ "), forwarding "+ cntr+ " rows");
      nextCntr=getNextCntr(cntr);
    }
  }
  return forwardedRow;
}
