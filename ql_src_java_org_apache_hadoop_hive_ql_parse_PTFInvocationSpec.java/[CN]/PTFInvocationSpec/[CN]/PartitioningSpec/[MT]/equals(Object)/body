{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  PartitioningSpec other=(PartitioningSpec)obj;
  if (orderSpec == null) {
    if (other.orderSpec != null) {
      return false;
    }
  }
 else   if (!orderSpec.equals(other.orderSpec)) {
    return false;
  }
  if (partSpec == null) {
    if (other.partSpec != null) {
      return false;
    }
  }
 else   if (!partSpec.equals(other.partSpec)) {
    return false;
  }
  return true;
}
