{
  if (!ShimLoader.getHadoopShims().isSecurityEnabled()) {
    return;
  }
  try {
    ShimLoader.getHadoopShims().reLoginUserFromKeytab();
  }
 catch (  IOException e) {
    String msg="Error doing relogin using keytab " + e.getMessage();
    LOG.error(msg,e);
    throw new MetaException(msg);
  }
}
