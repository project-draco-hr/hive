{
  writer.name("edges");
  writer.beginArray();
  for (  Edge edge : edges) {
    writer.beginObject();
    writer.name("sources");
    writer.beginArray();
    for (    Vertex vertex : edge.sources) {
      writer.value(vertex.id);
    }
    writer.endArray();
    writer.name("targets");
    writer.beginArray();
    for (    Vertex vertex : edge.targets) {
      writer.value(vertex.id);
    }
    writer.endArray();
    if (edge.expr != null) {
      writer.name("expression").value(edge.expr);
    }
    writer.name("edgeType").value(edge.type.name());
    writer.endObject();
  }
  writer.endArray();
}
