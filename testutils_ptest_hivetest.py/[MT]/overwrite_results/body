def overwrite_results():
    local.cd(code_path)
    expanded_path = local.run('pwd', abandon_output=False)
    print(('\n-- Copying generated `.q.out` files to master repository: ' + expanded_path))
    for name in ['clientpositive', 'clientnegative']:
        local.cd(((report_path + '/out/') + name))
        local.run((((('cp * "' + code_path) + '/ql/src/test/results/') + name) + '"'), warn_only=True)
