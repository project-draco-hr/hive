{
  lock.lock();
  try {
    if (index == paths.length) {
      getLocks(paths,verify,fetchData,locks,conf);
      return;
    }
    Node child=children.get(paths[index]);
    if (child != null) {
      child.getLocks(paths,index + 1,verify,fetchData,locks,conf);
    }
  }
  finally {
    lock.unlock();
  }
}
