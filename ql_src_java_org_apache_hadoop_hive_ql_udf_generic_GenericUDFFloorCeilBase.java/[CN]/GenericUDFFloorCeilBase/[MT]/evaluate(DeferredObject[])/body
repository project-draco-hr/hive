{
  if (arguments[0] == null) {
    return null;
  }
  Object input=arguments[0].get();
  if (input == null) {
    return null;
  }
  input=converter.convert(input);
  if (input == null) {
    return null;
  }
switch (resultOI.getPrimitiveCategory()) {
case LONG:
    return evaluate((DoubleWritable)input);
case DECIMAL:
  return evaluate((HiveDecimalWritable)input);
default :
throw new IllegalStateException("Unexpected type in evaluating " + opName + ": "+ inputOI.getPrimitiveCategory());
}
}
