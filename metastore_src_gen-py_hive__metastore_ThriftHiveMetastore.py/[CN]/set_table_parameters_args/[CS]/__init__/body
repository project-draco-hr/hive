def __init__(self, d=None):
    self.dbname = None
    self.tbl_name = None
    self.params = None
    if isinstance(d, dict):
        if ('dbname' in d):
            self.dbname = d['dbname']
        if ('tbl_name' in d):
            self.tbl_name = d['tbl_name']
        if ('params' in d):
            self.params = d['params']
