def __init__(self, d=None):
    self.dbname = None
    self.tbl_name = None
    self.new_tbl = None
    if isinstance(d, dict):
        if ('dbname' in d):
            self.dbname = d['dbname']
        if ('tbl_name' in d):
            self.tbl_name = d['tbl_name']
        if ('new_tbl' in d):
            self.new_tbl = d['new_tbl']
