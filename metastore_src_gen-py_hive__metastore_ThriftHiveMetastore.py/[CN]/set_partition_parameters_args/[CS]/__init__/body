def __init__(self, d=None):
    self.db_name = None
    self.tbl_name = None
    self.pname = None
    self.params = None
    if isinstance(d, dict):
        if ('db_name' in d):
            self.db_name = d['db_name']
        if ('tbl_name' in d):
            self.tbl_name = d['tbl_name']
        if ('pname' in d):
            self.pname = d['pname']
        if ('params' in d):
            self.params = d['params']
