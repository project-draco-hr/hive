def __init__(self, d=None):
    self.db_name = None
    self.tbl_name = None
    self.max_parts = (-1)
    if isinstance(d, dict):
        if ('db_name' in d):
            self.db_name = d['db_name']
        if ('tbl_name' in d):
            self.tbl_name = d['tbl_name']
        if ('max_parts' in d):
            self.max_parts = d['max_parts']
