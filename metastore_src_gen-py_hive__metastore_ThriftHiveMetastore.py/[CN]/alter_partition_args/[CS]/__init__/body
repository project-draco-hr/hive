def __init__(self, d=None):
    self.db_name = None
    self.tbl_name = None
    self.new_part = None
    if isinstance(d, dict):
        if ('db_name' in d):
            self.db_name = d['db_name']
        if ('tbl_name' in d):
            self.tbl_name = d['tbl_name']
        if ('new_part' in d):
            self.new_part = d['new_part']
