{
  Hive db=Hive.get();
  for (  String dbName : db.getAllDatabases()) {
    for (    String functionName : db.getFunctions(dbName,"*")) {
      Function function=db.getFunction(dbName,functionName);
      try {
        FunctionRegistry.registerPermanentFunction(FunctionUtils.qualifyFunctionName(functionName,dbName),function.getClassName(),false,FunctionTask.toFunctionResource(function.getResourceUris()));
      }
 catch (      Exception e) {
        LOG.warn("Failed to register persistent function " + functionName + ":"+ function.getClassName()+ ". Ignore and continue.");
      }
    }
  }
}
