{
  try {
    msc.dropTable(tableName,deleteData);
  }
 catch (  NoSuchObjectException e) {
    if (!ignoreUnknownTab) {
      throw new HiveException(e);
    }
  }
catch (  UnknownTableException e) {
    if (!ignoreUnknownTab) {
      throw new HiveException(e);
    }
  }
catch (  Exception e) {
    throw new HiveException(e);
  }
}
