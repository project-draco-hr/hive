{
  try {
    tbl.initSerDe();
    if (tbl.getCols().size() == 0) {
      tbl.setFields(MetaStoreUtils.getFieldsFromDeserializer(tbl.getName(),tbl.getDeserializer()));
    }
    tbl.checkValidity();
    getMSC().createTable(tbl.getTTable());
  }
 catch (  HiveException e) {
    throw e;
  }
catch (  Exception e) {
    throw new HiveException(e);
  }
}
