{
  String[] names=getQualifiedNames(tableName);
switch (names.length) {
case 2:
    return new Table(names[0],names[1]);
case 1:
  return new Table(SessionState.get().getCurrentDatabase(),names[0]);
default :
try {
  throw new HiveException("Invalid table name: " + tableName);
}
 catch (Exception e) {
  e.printStackTrace();
}
throw new HiveException("Invalid table name: " + tableName);
}
}
