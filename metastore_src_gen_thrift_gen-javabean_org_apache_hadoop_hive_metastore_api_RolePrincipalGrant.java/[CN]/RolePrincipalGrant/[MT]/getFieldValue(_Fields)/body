{
switch (field) {
case ROLE_NAME:
    return getRoleName();
case PRINCIPAL_NAME:
  return getPrincipalName();
case PRINCIPAL_TYPE:
return getPrincipalType();
case GRANT_OPTION:
return isGrantOption();
case GRANT_TIME:
return getGrantTime();
case GRANTOR_NAME:
return getGrantorName();
case GRANTOR_PRINCIPAL_TYPE:
return getGrantorPrincipalType();
}
throw new IllegalStateException();
}
