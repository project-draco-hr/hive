{
  if (instances <= 0) {
    throw new ParseException("Invalid configuration: " + instances + " (should be greater than 0)");
  }
  this.instances=instances;
  this.directory=directory;
  this.name=name;
  this.executors=executors;
  this.cache=cache;
  this.size=size;
  this.xmx=xmx;
  this.conf=hiveconf;
}
