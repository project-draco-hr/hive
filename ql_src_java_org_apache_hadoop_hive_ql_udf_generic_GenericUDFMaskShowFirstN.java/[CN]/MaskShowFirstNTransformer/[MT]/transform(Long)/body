{
  long val=value;
  if (value < 0) {
    val*=-1;
  }
  int digitCount=0;
  for (long v=val; v != 0; v/=10) {
    digitCount++;
  }
  final int maskCount=digitCount - charCount;
  if (maskCount <= 0) {
    return value;
  }
  long ret=0;
  long pos=1;
  for (int i=0; val != 0; i++) {
    if (i < maskCount) {
      ret+=(maskedNumber * pos);
    }
 else {
      ret+=((val % 10) * pos);
    }
    val/=10;
    pos*=10;
  }
  if (value < 0) {
    ret*=-1;
  }
  return ret;
}
