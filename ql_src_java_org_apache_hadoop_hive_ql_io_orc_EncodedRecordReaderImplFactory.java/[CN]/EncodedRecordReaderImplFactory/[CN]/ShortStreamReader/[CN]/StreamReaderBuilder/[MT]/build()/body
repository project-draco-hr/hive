{
  SettableUncompressedStream present=StreamUtils.createLlapInStream(OrcProto.Stream.Kind.PRESENT.name(),fileId,presentStream);
  SettableUncompressedStream data=StreamUtils.createLlapInStream(OrcProto.Stream.Kind.DATA.name(),fileId,dataStream);
  boolean isFileCompressed=compressionCodec != null;
  return new ShortStreamReader(columnIndex,present,data,isFileCompressed,columnEncoding);
}
