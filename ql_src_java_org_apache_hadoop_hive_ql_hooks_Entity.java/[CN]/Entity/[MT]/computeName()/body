{
switch (typ) {
case DATABASE:
    return "database:" + database.getName();
case TABLE:
  return t.getDbName() + "@" + t.getTableName();
case PARTITION:
return t.getDbName() + "@" + t.getTableName()+ "@"+ p.getName();
case DUMMYPARTITION:
return p.getName();
case FUNCTION:
if (database != null) {
return database.getName() + "." + stringObject;
}
return stringObject;
default :
return d.toString();
}
}
