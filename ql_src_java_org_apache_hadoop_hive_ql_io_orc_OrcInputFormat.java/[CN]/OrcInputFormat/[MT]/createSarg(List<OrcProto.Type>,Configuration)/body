{
  String serializedPushdown=conf.get(TableScanDesc.FILTER_EXPR_CONF_STR);
  if (serializedPushdown == null || conf.get(ColumnProjectionUtils.READ_COLUMN_NAMES_CONF_STR) == null) {
    LOG.debug("No ORC pushdown predicate");
    return null;
  }
  SearchArgument sarg=SearchArgument.FACTORY.create(Utilities.deserializeExpression(serializedPushdown));
  LOG.info("ORC pushdown predicate: " + sarg);
  return sarg;
}
