{
  if (Utilities.isVectorMode(conf)) {
    return new VectorizedOrcInputFormat().validateInput(fs,conf,files);
  }
  if (files.size() <= 0) {
    return false;
  }
  for (  FileStatus file : files) {
    try {
      OrcFile.createReader(file.getPath(),OrcFile.readerOptions(conf).filesystem(fs));
    }
 catch (    IOException e) {
      return false;
    }
  }
  return true;
}
