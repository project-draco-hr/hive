{
  boolean vectorPath=conf.getBoolean(HiveConf.ConfVars.HIVE_VECTORIZATION_ENABLED.toString(),false);
  if (vectorPath) {
    RecordReader<NullWritable,VectorizedRowBatch> vorr=voif.getRecordReader(inputSplit,conf,reporter);
    return (RecordReader)vorr;
  }
  FileSplit fileSplit=(FileSplit)inputSplit;
  Path path=fileSplit.getPath();
  FileSystem fs=path.getFileSystem(conf);
  reporter.setStatus(fileSplit.toString());
  return new OrcRecordReader(OrcFile.createReader(fs,path),conf,fileSplit.getStart(),fileSplit.getLength());
}
