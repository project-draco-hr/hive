{
  List<OrcSplit> splits=Lists.newArrayList();
  for (  FileStatus fileStatus : fileStatuses) {
    String[] hosts=SHIMS.getLocationsWithOffset(fs,fileStatus).firstEntry().getValue().getHosts();
    OrcSplit orcSplit=new OrcSplit(fileStatus.getPath(),0,fileStatus.getLen(),hosts,null,isOriginal,true,deltas,-1);
    splits.add(orcSplit);
  }
  splits.addAll(super.getSplits());
  return splits;
}
