{
  if ((files.size() + otherFiles.size()) > ETL_COMBINE_FILE_LIMIT || this.isOriginal != isOriginal) {
    return (files.size() > otherFiles.size()) ? CombineResult.NO_AND_SWAP : CombineResult.NO_AND_CONTINUE;
  }
  files.addAll(otherFiles);
  dirs.add(new ETLDir(dir,fs,otherFiles.size()));
  return CombineResult.YES;
}
