{
  List<RootInputLeafOutput<InputDescriptor,InputInitializerDescriptor>> inputs=vertex.getInputs();
  Preconditions.checkState(inputs.size() == 1);
  String inputName=inputs.get(0).getName();
  EventMetaData destMeta=new EventMetaData(EventMetaData.EventProducerConsumerType.INPUT,vertex.getName(),inputName,null);
  return destMeta;
}
