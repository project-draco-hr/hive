{
  if (!isOpen) {
    return false;
  }
  if (readNextIndex >= readNextCount) {
    bufferedRead();
    if (readNextIndex >= readNextCount) {
      return false;
    }
  }
  currentBytes=readNextBytes[readNextIndex];
  currentOffset=readNextOffsets[readNextIndex];
  currentLength=readNextLengths[readNextIndex];
  readNextIndex++;
  return true;
}
