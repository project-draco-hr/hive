{
  if (fileInputStream != null) {
    try {
      fileInputStream.close();
    }
 catch (    Throwable ignored) {
    }
    fileInputStream=null;
  }
  if (fileOutputStream != null) {
    try {
      fileOutputStream.close();
    }
 catch (    Throwable ignored) {
    }
    fileOutputStream=null;
  }
  if (parentDir != null) {
    try {
      FileUtil.fullyDelete(parentDir);
    }
 catch (    Throwable ignored) {
    }
  }
  parentDir=null;
  tmpFile=null;
  isOpen=false;
  totalWriteLength=0;
}
