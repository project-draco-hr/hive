{
  Map<String,String> old_map=t.getParameters();
  if (old_map != null) {
    String timeStr=old_map.get(LAST_QUERY_TIME);
    if (timeStr != null) {
      long time=Long.parseLong(timeStr);
      long cur_time=System.currentTimeMillis();
      if (cur_time - time < 1 * 60 * 60* 1000) {
        return false;
      }
    }
  }
  t.setParameters(modifyParams(old_map,LAST_QUERY_TIME,Long.toString(System.currentTimeMillis())));
  return true;
}
