{
  int tag=GenericData.get().resolveUnion(recordSchema,datum);
  Object desered=worker(datum,fileSchema == null ? null : fileSchema.getTypes().get(tag),recordSchema.getTypes().get(tag),columnType.getAllUnionObjectTypeInfos().get(tag));
  return new StandardUnionObjectInspector.StandardUnion((byte)tag,desered);
}
