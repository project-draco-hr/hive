{
  if (trace) {
    trace(ctx,"USE");
  }
  String sql=ctx.T_USE().toString() + " " + evalPop(ctx.expr()).toString();
  if (trace) {
    trace(ctx,"Query: " + sql);
  }
  Query query=exec.executeSql(ctx,sql,exec.conf.defaultConnection);
  if (query.error()) {
    exec.signal(query);
    return 1;
  }
  exec.setSqlCode(0);
  exec.closeQuery(query,exec.conf.defaultConnection);
  return 0;
}
