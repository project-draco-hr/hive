{
  trace(ctx,"WHILE - ENTERED");
  String label=exec.labelPop();
  while (true) {
    if (evalPop(ctx.bool_expr()).isTrue()) {
      exec.enterScope(Scope.Type.LOOP);
      visit(ctx.block());
      exec.leaveScope();
      if (canContinue(label)) {
        continue;
      }
    }
    break;
  }
  trace(ctx,"WHILE - LEFT");
  return 0;
}
