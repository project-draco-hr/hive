{
  super(PrimitiveObjectInspectorUtils.varcharTypeEntry);
  this.value=value;
  if (typeParams == null) {
    typeParams=new VarcharTypeParams();
    typeParams.length=this.value.getCharacterLength();
  }
  setTypeParams(typeParams);
  this.typeEntry=PrimitiveObjectInspectorUtils.getTypeEntryFromTypeSpecs(PrimitiveCategory.VARCHAR,typeParams);
}
