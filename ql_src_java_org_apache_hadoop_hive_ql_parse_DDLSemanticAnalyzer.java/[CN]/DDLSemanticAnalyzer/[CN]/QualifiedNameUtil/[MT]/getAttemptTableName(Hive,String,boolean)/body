{
  String tableName=qualifiedName.substring(0,qualifiedName.indexOf('.') == -1 ? qualifiedName.length() : qualifiedName.indexOf('.'));
  try {
    Table tab=db.getTable(tableName);
    if (tab != null) {
      if (isColumn) {
        return qualifiedName;
      }
 else {
        return tableName;
      }
    }
  }
 catch (  HiveException e) {
    return null;
  }
  return null;
}
