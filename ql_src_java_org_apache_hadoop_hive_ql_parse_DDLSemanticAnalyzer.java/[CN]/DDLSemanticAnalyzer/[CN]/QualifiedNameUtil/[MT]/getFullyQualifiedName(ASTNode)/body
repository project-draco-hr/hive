{
  if (ast.getChildCount() == 0) {
    return ast.getText();
  }
 else   if (ast.getChildCount() == 2) {
    return getFullyQualifiedName((ASTNode)ast.getChild(0)) + "." + getFullyQualifiedName((ASTNode)ast.getChild(1));
  }
 else   if (ast.getChildCount() == 3) {
    return getFullyQualifiedName((ASTNode)ast.getChild(0)) + "." + getFullyQualifiedName((ASTNode)ast.getChild(1))+ "."+ getFullyQualifiedName((ASTNode)ast.getChild(2));
  }
 else {
    return null;
  }
}
