{
  String dbName=null;
  String fullyQualifiedName=getFullyQualifiedName(ast);
  if (ast.getChildCount() >= 2) {
    dbName=fullyQualifiedName.substring(0,fullyQualifiedName.indexOf('.') == -1 ? fullyQualifiedName.length() : fullyQualifiedName.indexOf('.'));
    try {
      if (!db.databaseExists(dbName)) {
        return null;
      }
    }
 catch (    HiveException e) {
      return null;
    }
  }
 else {
    return null;
  }
  return dbName;
}
