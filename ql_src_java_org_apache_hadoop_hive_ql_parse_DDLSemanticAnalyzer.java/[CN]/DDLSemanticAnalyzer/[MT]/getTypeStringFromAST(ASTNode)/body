{
switch (typeNode.getType()) {
case HiveParser.TOK_LIST:
    return Constants.LIST_TYPE_NAME + "<" + getTypeStringFromAST((ASTNode)typeNode.getChild(0))+ ">";
case HiveParser.TOK_MAP:
  return Constants.MAP_TYPE_NAME + "<" + getTypeStringFromAST((ASTNode)typeNode.getChild(0))+ ","+ getTypeStringFromAST((ASTNode)typeNode.getChild(1))+ ">";
case HiveParser.TOK_STRUCT:
return getStructTypeStringFromAST(typeNode);
default :
return getTypeName(typeNode.getType());
}
}
