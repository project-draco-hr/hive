{
  RoleDDLDesc showRolesDesc=new RoleDDLDesc(null,null,RoleDDLDesc.RoleOperation.SHOW_ROLES,null);
  showRolesDesc.setResFile(ctx.getResFile().toString());
  rootTasks.add(TaskFactory.get(new DDLWork(getInputs(),getOutputs(),showRolesDesc),conf));
  setFetchTask(createFetchTask(RoleDDLDesc.getRoleNameSchema()));
}
