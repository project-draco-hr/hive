{
switch (ast.getChildCount()) {
case 0:
    ctx.setResFile(ctx.getLocalTmpPath());
  rootTasks.add(hiveAuthorizationTaskFactory.createShowCurrentRoleTask(getInputs(),getOutputs(),ctx.getResFile()));
setFetchTask(createFetchTask(RoleDDLDesc.getRoleNameSchema()));
break;
case 1:
rootTasks.add(hiveAuthorizationTaskFactory.createSetRoleTask(BaseSemanticAnalyzer.unescapeIdentifier(ast.getChild(0).getText()),getInputs(),getOutputs()));
break;
default :
throw new SemanticException("Internal error. ASTNode expected to have 0 or 1 child. " + ast.dump());
}
}
