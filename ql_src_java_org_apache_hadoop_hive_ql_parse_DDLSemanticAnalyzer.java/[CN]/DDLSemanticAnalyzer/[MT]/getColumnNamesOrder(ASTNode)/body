{
  List<Order> colList=new ArrayList<Order>();
  int numCh=ast.getChildCount();
  for (int i=0; i < numCh; i++) {
    ASTNode child=(ASTNode)ast.getChild(i);
    if (child.getToken().getType() == HiveParser.TOK_TABSORTCOLNAMEASC)     colList.add(new Order(unescapeIdentifier(child.getChild(0).getText()),1));
 else     colList.add(new Order(unescapeIdentifier(child.getChild(0).getText()),0));
  }
  return colList;
}
