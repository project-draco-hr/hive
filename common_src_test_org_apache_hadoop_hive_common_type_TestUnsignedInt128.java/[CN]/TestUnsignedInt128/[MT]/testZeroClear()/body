{
  assertFalse(one.isZero());
  assertFalse(two.isZero());
  assertNotEquals(0L,one.asLong());
  assertNotEquals(0L,two.asLong());
  two.zeroClear();
  assertNotEquals(0L,one.asLong());
  assertEquals(0L,two.asLong());
  assertFalse(one.isZero());
  assertTrue(two.isZero());
  one.zeroClear();
  assertEquals(0L,one.asLong());
  assertEquals(0L,two.asLong());
  assertTrue(one.isZero());
  assertTrue(two.isZero());
}
