{
  super.next(previous);
  OrcUnion result=null;
  if (valuePresent) {
    if (previous == null) {
      result=new OrcUnion();
    }
 else {
      result=(OrcUnion)previous;
    }
    byte tag=tags.next();
    Object previousVal=result.getObject();
    result.set(tag,fields[tag].next(tag == result.getTag() ? previousVal : null));
  }
  return result;
}
