{
  super.startStripe(streams,encodings);
  lengths=createIntegerReader(encodings.get(columnId).getKind(),streams.get(new StreamName(columnId,OrcProto.Stream.Kind.LENGTH)),false);
  if (keyReader != null) {
    keyReader.startStripe(streams,encodings);
  }
  if (valueReader != null) {
    valueReader.startStripe(streams,encodings);
  }
}
