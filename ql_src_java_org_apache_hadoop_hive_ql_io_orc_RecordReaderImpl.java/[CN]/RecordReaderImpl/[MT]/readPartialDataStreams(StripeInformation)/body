{
  List<OrcProto.Stream> streamList=stripeFooter.getStreamsList();
  List<DiskRange> chunks=planReadPartialDataStreams(streamList,indexes,included,includedRowGroups,codec != null,stripeFooter.getColumnsList(),types,bufferSize);
  if (LOG.isDebugEnabled()) {
    LOG.debug("chunks = " + stringifyDiskRanges(chunks));
  }
  mergeDiskRanges(chunks);
  if (LOG.isDebugEnabled()) {
    LOG.debug("merge = " + stringifyDiskRanges(chunks));
  }
  byte[][] bytes=readDiskRanges(file,stripe.getOffset(),chunks);
  createStreams(streamList,chunks,bytes,included,codec,bufferSize,streams);
}
