{
  for (  InStream is : streams.values()) {
    is.close();
  }
  if (bufferChunks != null) {
    if (zcr != null) {
      for (DiskRangeList range=bufferChunks; range != null; range=range.next) {
        if (!(range instanceof BufferChunk))         continue;
        zcr.releaseBuffer(((BufferChunk)range).chunk);
      }
    }
    bufferChunks=null;
  }
  streams.clear();
}
