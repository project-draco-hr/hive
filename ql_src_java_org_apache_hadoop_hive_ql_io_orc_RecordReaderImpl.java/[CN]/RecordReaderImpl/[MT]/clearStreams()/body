{
  for (  InStream is : streams.values()) {
    is.close();
  }
  if (bufferChunks != null) {
    if (zcr != null) {
      DiskRangeList range=bufferChunks;
      while (range != null) {
        if (range instanceof BufferChunk) {
          zcr.releaseBuffer(((BufferChunk)range).chunk);
        }
        range=range.next;
      }
    }
    bufferChunks=null;
  }
  streams.clear();
}
