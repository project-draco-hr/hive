{
  validate();
  oprot.writeStructBegin(STRUCT_DESC);
  if (this.db_name != null) {
    oprot.writeFieldBegin(DB_NAME_FIELD_DESC);
    oprot.writeString(this.db_name);
    oprot.writeFieldEnd();
  }
