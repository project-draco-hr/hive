{
  if (that == null)   return false;
  boolean this_present_db_name=true && this.isSetDb_name();
  boolean that_present_db_name=true && that.isSetDb_name();
  if (this_present_db_name || that_present_db_name) {
    if (!(this_present_db_name && that_present_db_name))     return false;
    if (!this.db_name.equals(that.db_name))     return false;
  }
  boolean this_present_pattern=true && this.isSetPattern();
  boolean that_present_pattern=true && that.isSetPattern();
  if (this_present_pattern || that_present_pattern) {
    if (!(this_present_pattern && that_present_pattern))     return false;
    if (!this.pattern.equals(that.pattern))     return false;
  }
  return true;
}
