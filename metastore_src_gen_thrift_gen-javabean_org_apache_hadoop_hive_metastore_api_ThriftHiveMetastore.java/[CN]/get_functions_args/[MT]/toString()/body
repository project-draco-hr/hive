{
  StringBuilder sb=new StringBuilder("get_functions_args(");
  boolean first=true;
  sb.append("dbName:");
  if (this.dbName == null) {
    sb.append("null");
  }
 else {
    sb.append(this.dbName);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("pattern:");
  if (this.pattern == null) {
    sb.append("null");
  }
 else {
    sb.append(this.pattern);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}
