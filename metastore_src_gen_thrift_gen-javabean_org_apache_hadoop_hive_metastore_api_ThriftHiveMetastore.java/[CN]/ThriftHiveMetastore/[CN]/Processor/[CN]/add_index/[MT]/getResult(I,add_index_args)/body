{
  add_index_result result=new add_index_result();
  try {
    result.success=iface.add_index(args.new_index,args.index_table);
  }
 catch (  InvalidObjectException o1) {
    result.o1=o1;
  }
catch (  AlreadyExistsException o2) {
    result.o2=o2;
  }
catch (  MetaException o3) {
    result.o3=o3;
  }
  return result;
}
