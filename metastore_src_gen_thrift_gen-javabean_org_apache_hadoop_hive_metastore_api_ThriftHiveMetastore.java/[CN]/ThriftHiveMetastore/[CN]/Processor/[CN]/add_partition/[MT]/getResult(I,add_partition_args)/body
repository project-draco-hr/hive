{
  add_partition_result result=new add_partition_result();
  try {
    result.success=iface.add_partition(args.new_part);
  }
 catch (  InvalidObjectException o1) {
    result.o1=o1;
  }
catch (  AlreadyExistsException o2) {
    result.o2=o2;
  }
catch (  MetaException o3) {
    result.o3=o3;
  }
  return result;
}
