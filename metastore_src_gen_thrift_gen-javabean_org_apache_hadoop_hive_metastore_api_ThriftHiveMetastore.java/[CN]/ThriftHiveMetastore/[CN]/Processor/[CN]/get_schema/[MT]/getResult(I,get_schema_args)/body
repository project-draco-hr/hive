{
  get_schema_result result=new get_schema_result();
  try {
    result.success=iface.get_schema(args.db_name,args.table_name);
  }
 catch (  MetaException o1) {
    result.o1=o1;
  }
catch (  UnknownTableException o2) {
    result.o2=o2;
  }
catch (  UnknownDBException o3) {
    result.o3=o3;
  }
  return result;
}
