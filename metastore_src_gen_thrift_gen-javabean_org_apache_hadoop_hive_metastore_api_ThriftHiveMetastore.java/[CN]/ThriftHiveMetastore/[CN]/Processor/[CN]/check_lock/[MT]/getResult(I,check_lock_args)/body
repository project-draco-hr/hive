{
  check_lock_result result=new check_lock_result();
  try {
    result.success=iface.check_lock(args.rqst);
  }
 catch (  NoSuchTxnException o1) {
    result.o1=o1;
  }
catch (  TxnAbortedException o2) {
    result.o2=o2;
  }
catch (  NoSuchLockException o3) {
    result.o3=o3;
  }
  return result;
}
