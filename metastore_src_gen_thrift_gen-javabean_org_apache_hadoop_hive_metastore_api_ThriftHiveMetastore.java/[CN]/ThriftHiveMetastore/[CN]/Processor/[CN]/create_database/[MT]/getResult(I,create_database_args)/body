{
  create_database_result result=new create_database_result();
  try {
    iface.create_database(args.database);
  }
 catch (  AlreadyExistsException o1) {
    result.o1=o1;
  }
catch (  InvalidObjectException o2) {
    result.o2=o2;
  }
catch (  MetaException o3) {
    result.o3=o3;
  }
  return result;
}
