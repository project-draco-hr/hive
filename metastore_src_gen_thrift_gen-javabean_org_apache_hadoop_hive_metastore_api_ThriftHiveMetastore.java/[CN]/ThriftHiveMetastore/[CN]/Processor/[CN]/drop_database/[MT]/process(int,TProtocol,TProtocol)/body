  }
  iprot.readMessageEnd();
  drop_database_result result=new drop_database_result();
  try {
    iface_.drop_database(args.name,args.deleteData,args.cascade);
  }
 catch (  NoSuchObjectException o1) {
    result.o1=o1;
  }
catch (  InvalidOperationException o2) {
    result.o2=o2;
  }
