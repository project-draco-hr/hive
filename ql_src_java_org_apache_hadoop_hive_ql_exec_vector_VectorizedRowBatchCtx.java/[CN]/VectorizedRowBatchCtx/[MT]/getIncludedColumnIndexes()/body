{
  int colCount=(colsToInclude == null) ? rowOI.getAllStructFieldRefs().size() : colsToInclude.size();
  int[] result=new int[colCount];
  if (colsToInclude == null) {
    for (int i=0; i < result.length; ++i) {
      result[i]=i;
    }
  }
 else {
    int i=-1;
    for (    int colIx : colsToInclude) {
      result[++i]=colIx;
    }
  }
  return result;
}
