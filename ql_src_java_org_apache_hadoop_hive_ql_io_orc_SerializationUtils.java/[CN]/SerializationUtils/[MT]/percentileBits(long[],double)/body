{
  if ((p > 1.0) || (p <= 0.0)) {
    return -1;
  }
  int[] hist=new int[32];
  for (  long l : data) {
    int idx=encodeBitWidth(findClosestNumBits(l));
    hist[idx]+=1;
  }
  int len=data.length;
  int perLen=(int)(len * (1.0 - p));
  for (int i=hist.length - 1; i >= 0; i--) {
    perLen-=hist[i];
    if (perLen < 0) {
      return decodeBitWidth(i);
    }
  }
  return 0;
}
