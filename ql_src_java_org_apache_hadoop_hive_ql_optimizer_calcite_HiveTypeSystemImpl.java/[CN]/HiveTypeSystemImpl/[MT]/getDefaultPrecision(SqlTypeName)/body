{
switch (typeName) {
case CHAR:
case VARCHAR:
case BINARY:
case VARBINARY:
case TIME:
case TIMESTAMP:
    return getMaxPrecision(typeName);
case DECIMAL:
  return DEFAULT_DECIMAL_PRECISION;
case INTERVAL_DAY_TIME:
case INTERVAL_YEAR_MONTH:
return SqlTypeName.DEFAULT_INTERVAL_START_PRECISION;
default :
return -1;
}
}
