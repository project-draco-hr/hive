{
  flattenPush();
  if (isRepeating) {
    isRepeating=false;
    if (noNulls || (!noNulls && !isNull[0])) {
      if (selectedInUse) {
        for (int j=1; j < size; j++) {
          int i=sel[j];
          this.setRef(i,vector[0],start[0],length[0]);
        }
      }
 else {
        for (int i=1; i < size; i++) {
          this.setRef(i,vector[0],start[0],length[0]);
        }
      }
    }
    flattenRepeatingNulls(selectedInUse,sel,size);
  }
  flattenNoNulls(selectedInUse,sel,size);
}
