{
  if (len < lenSub) {
    return false;
  }
  int end=start + len - lenSub + 1;
  boolean match=false;
  for (int i=start; i < end; i++) {
    match=true;
    for (int j=0; j < lenSub; j++) {
      if (byteS[i + j] != byteSub[j]) {
        match=false;
        break;
      }
    }
    if (match) {
      return true;
    }
  }
  return match;
}
