{
  if (filter != null) {
    inputJobInfo=InputJobInfo.create(inputJobInfo.getDatabaseName(),inputJobInfo.getTableName(),filter,inputJobInfo.getProperties());
    try {
      InitializeInput.setInput(conf,inputJobInfo);
    }
 catch (    Exception e) {
      throw new IOException(e);
    }
  }
  return this;
}
