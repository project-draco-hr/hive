{
  if (authorizeUserAndDBPriv(hive_db.getDatabase(table.getDbName()),inputRequiredPriv,outputRequiredPriv,inputCheck,outputCheck)) {
    return true;
  }
  PrincipalPrivilegeSet tablePrivileges=hive_db.get_privilege_set(HiveObjectType.TABLE,table.getDbName(),table.getTableName(),null,null,this.getAuthenticator().getUserName(),this.getAuthenticator().getGroupNames());
  if (authorizePrivileges(tablePrivileges,inputRequiredPriv,inputCheck,outputRequiredPriv,outputCheck)) {
    return true;
  }
  return false;
}
