{
  try {
    DB db=DB.createDB("foo1",conf_);
{
      List<String> tables=db.getTables(".+");
      assertTrue(tables.size() == 0);
    }
    Table bar1=Table.create(db,"bar1",createSchema("foo1","bar1"),conf_);
{
      List<String> tables=db.getTables(".+");
      assertTrue(tables.size() == 1);
    }
    bar1.drop();
{
      List<String> tables=db.getTables(".+");
      assertTrue(tables.size() == 0);
    }
    cleanup();
  }
 catch (  MetaException e) {
    e.printStackTrace();
    throw e;
  }
}
