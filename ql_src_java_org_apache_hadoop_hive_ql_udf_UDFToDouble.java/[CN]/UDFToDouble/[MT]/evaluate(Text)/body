{
  if (i == null) {
    return null;
  }
 else {
    if (!LazyUtils.isNumberMaybe(i.getBytes(),0,i.getLength())) {
      return null;
    }
    try {
      doubleWritable.set(Double.valueOf(i.toString()));
      return doubleWritable;
    }
 catch (    NumberFormatException e) {
      return null;
    }
  }
}
