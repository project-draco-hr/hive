{
  if (HiveConf.getVar(job,HiveConf.ConfVars.HIVE_EXECUTION_ENGINE).equals("tez")) {
    mrwork=(MapWork)Utilities.getMergeWork(job);
    if (mrwork == null) {
      mrwork=Utilities.getMapWork(job);
    }
  }
 else {
    mrwork=Utilities.getMapWork(job);
  }
  pathToPartitionInfo=mrwork.getPathToPartitionInfo();
}
