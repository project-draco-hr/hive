{
  PartitionDesc partDesc=pathToPartitionInfo.get(dir.toString());
  if (partDesc == null) {
    partDesc=pathToPartitionInfo.get(dir.toUri().getPath());
  }
  if (partDesc == null) {
    throw new IOException("cannot find dir = " + dir.toString() + " in partToPartitionInfo!");
  }
  return partDesc;
}
