{
  if ((a == null) || (b == null)) {
    return null;
  }
  if (b.getHiveDecimal().compareTo(HiveDecimal.ZERO) == 0) {
    return null;
  }
  HiveDecimal dec=a.getHiveDecimal().divide(b.getHiveDecimal());
  if (dec == null) {
    return null;
  }
  decimalWritable.set(dec);
  return decimalWritable;
}
