{
switch (fieldID) {
case MESSAGE:
    if (value == null) {
      unsetMessage();
    }
 else {
      setMessage((String)value);
    }
  break;
case ERRORCODE:
if (value == null) {
  unsetErrorCode();
}
 else {
  setErrorCode((Integer)value);
}
break;
case SQLSTATE:
if (value == null) {
unsetSQLState();
}
 else {
setSQLState((String)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
