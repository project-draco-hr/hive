{
  int status=OK;
  if (map.containsKey("error"))   status=SERVER_ERROR;
  Object o=map.get("errorCode");
  if (o != null) {
    try {
      status=Integer.parseInt(o.toString());
    }
 catch (    Exception e) {
      if (o instanceof Number)       status=((Number)o).intValue();
    }
  }
  return buildResponse(status);
}
