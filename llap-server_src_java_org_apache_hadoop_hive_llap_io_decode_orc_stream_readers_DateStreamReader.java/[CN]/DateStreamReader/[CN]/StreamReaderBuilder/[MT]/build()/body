{
  InStream present=StreamUtils.createInStream(OrcProto.Stream.Kind.PRESENT.name(),fileName,null,bufferSize,presentStream,presentCBIdx);
  InStream data=StreamUtils.createInStream(OrcProto.Stream.Kind.DATA.name(),fileName,null,bufferSize,dataStream,presentCBIdx);
  boolean isFileCompressed=compressionCodec != null;
  return new DateStreamReader(columnIndex,present,data,isFileCompressed,columnEncoding,rowIndex);
}
