{
  verifyUser();
  verifyParam(jar,"jar");
  verifyParam(mainClass,"class");
  checkEnableLogPrerequisite(enablelog,statusdir);
  JarDelegator d=new JarDelegator(appConf);
  return d.run(getDoAsUser(),jar,mainClass,libjars,files,args,defines,statusdir,callback,getCompletedUrl(),enablelog,JobType.JAR);
}
