{
  verifyUser();
  if (execute == null && srcFile == null)   throw new BadParam("Either execute or file parameter required");
  checkEnableLogPrerequisite(enablelog,statusdir);
  HiveDelegator d=new HiveDelegator(appConf);
  return d.run(getDoAsUser(),execute,srcFile,defines,hiveArgs,otherFiles,statusdir,callback,getCompletedUrl(),enablelog);
}
