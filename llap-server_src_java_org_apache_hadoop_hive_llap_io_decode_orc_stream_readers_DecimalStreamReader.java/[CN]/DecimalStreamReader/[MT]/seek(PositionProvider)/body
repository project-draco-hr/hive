{
  if (present != null) {
    if (_isFileCompressed) {
      index.getNext();
    }
    present.seek(index);
  }
  if (_valueStream.available() > 0) {
    if (_isFileCompressed) {
      index.getNext();
    }
    value.seek(index);
  }
  if (_scaleStream.available() > 0) {
    if (_isFileCompressed) {
      index.getNext();
    }
    scaleReader.seek(index);
  }
}
