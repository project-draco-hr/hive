{
  setState(state);
  if (shouldRunAsync()) {
    Future<?> backgroundHandle=getBackgroundHandle();
    if (backgroundHandle != null) {
      backgroundHandle.cancel(true);
    }
  }
  if (driver != null) {
    driver.close();
    driver.destroy();
  }
  driver=null;
  SessionState ss=SessionState.get();
  ss.deleteTmpOutputFile();
  ss.deleteTmpErrOutputFile();
}
