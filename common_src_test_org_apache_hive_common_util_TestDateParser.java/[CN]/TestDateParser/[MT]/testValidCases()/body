  checkValidCase(" 1946-01-01",Date.valueOf("1946-01-01"));
  checkValidCase(" 2001-11-12 01:02:03",Date.valueOf("2001-11-12"));
  checkValidCase("2001-13-12",Date.valueOf("2002-01-12"));
