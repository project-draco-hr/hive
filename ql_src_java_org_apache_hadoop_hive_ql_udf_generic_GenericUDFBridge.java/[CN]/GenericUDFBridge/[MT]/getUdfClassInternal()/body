{
  @SuppressWarnings("unchecked") Class<? extends UDF> clazz=(Class<? extends UDF>)Class.forName(udfClassName,true,Utilities.getSessionSpecifiedClassLoader());
  if (udfChecker != null && !udfChecker.isUdfAllowed(clazz)) {
    throw new SecurityException("UDF " + clazz.getCanonicalName() + " is not allowed");
  }
  return clazz;
}
