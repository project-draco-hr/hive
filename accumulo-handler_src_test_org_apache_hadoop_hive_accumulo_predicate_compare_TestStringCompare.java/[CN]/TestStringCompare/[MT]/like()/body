{
  Like likeObj=new Like(strCompare);
  String condition="%a";
  assertTrue(likeObj.accept(condition.getBytes()));
  condition="%a%";
  assertTrue(likeObj.accept(condition.getBytes()));
  condition="a%";
  assertTrue(likeObj.accept(condition.getBytes()));
  condition="a%aa";
  assertFalse(likeObj.accept(condition.getBytes()));
  condition="b%";
  assertFalse(likeObj.accept(condition.getBytes()));
  condition="%ab%";
  assertFalse(likeObj.accept(condition.getBytes()));
  condition="%ba";
  assertFalse(likeObj.accept(condition.getBytes()));
}
