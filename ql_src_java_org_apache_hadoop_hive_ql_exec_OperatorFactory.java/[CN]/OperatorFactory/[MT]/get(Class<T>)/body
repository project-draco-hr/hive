{
  for (  opTuple o : opvec) {
    if (o.descClass == opClass) {
      try {
        Operator<T> op=(Operator<T>)o.opClass.newInstance();
        op.initializeCounters();
        return op;
      }
 catch (      Exception e) {
        e.printStackTrace();
        throw new RuntimeException(e);
      }
    }
  }
  throw new RuntimeException("No operator for descriptor class " + opClass.getName());
}
