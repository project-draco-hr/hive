{
  for (  opTuple o : opvec) {
    if (o.descClass == opClass) {
      try {
        return (Operator<T>)o.opClass.newInstance();
      }
 catch (      Exception e) {
        e.printStackTrace();
        throw new RuntimeException(e);
      }
    }
  }
  throw new RuntimeException("No operator for descriptor class " + opClass.getName());
}
