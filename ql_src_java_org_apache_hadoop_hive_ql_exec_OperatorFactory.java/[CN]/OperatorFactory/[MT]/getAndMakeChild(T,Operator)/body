{
  Operator<T> ret=get((Class<T>)conf.getClass());
  ret.setConf(conf);
  if (oplist.length == 0)   return (ret);
  for (  Operator op : oplist) {
    List<Operator> children=op.getChildOperators();
    if (children == null) {
      children=new ArrayList<Operator>();
    }
    children.add(ret);
    op.setChildOperators(children);
  }
  return (ret);
}
