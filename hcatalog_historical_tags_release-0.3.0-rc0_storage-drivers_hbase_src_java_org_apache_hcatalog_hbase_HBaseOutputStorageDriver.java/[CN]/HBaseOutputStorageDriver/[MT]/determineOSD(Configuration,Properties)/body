{
  if (activeOSD != null)   return;
  String bulkMode=conf.get(HBaseConstants.PROPERTY_OSD_BULK_MODE_KEY);
  if (bulkMode == null && prop != null)   bulkMode=prop.getProperty(HBaseConstants.PROPERTY_OSD_BULK_MODE_KEY);
  if (bulkMode != null && !Boolean.valueOf(bulkMode)) {
    activeOSD=directOSD;
    bulkOSD=null;
  }
 else {
    activeOSD=bulkOSD;
    directOSD=null;
  }
}
