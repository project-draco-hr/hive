{
  RowResolver parentRowResolver=parseCtx.getOpParseCtx().get(parent).getRowResolver();
  TableScanOperator tableScanOp=(TableScanOperator)GenMapRedUtils.putOpInsertMap(GenMapRedUtils.createTemporaryTableScanOperator(parent.getSchema()),parentRowResolver,parseCtx);
  tableScanOp.setChildOperators(Utilities.makeList(child));
  child.setParentOperators(Utilities.makeList(tableScanOp));
  return tableScanOp;
}
