{
  lock.lock();
  try {
    Set<String> funcNames=new TreeSet<String>();
    for (    String funcName : mFunctions.keySet()) {
      if (funcName.contains(WINDOW_FUNC_PREFIX)) {
        continue;
      }
      if (funcPattern == null || funcPattern.matcher(funcName).matches()) {
        funcNames.add(funcName);
      }
    }
    return funcNames;
  }
  finally {
    lock.unlock();
  }
}
