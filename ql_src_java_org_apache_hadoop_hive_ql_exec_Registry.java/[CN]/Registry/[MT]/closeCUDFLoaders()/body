{
  lock.lock();
  try {
    try {
      for (      ClassLoader loader : mSessionUDFLoaders) {
        JavaUtils.closeClassLoader(loader);
      }
    }
 catch (    IOException ie) {
      LOG.error("Error in close loader: " + ie);
    }
    mSessionUDFLoaders.clear();
  }
  finally {
    lock.unlock();
  }
}
