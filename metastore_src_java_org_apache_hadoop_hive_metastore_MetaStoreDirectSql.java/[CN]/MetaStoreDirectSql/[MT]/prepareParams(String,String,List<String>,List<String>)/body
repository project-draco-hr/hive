{
  Object[] params=new Object[colNames.size() + partNames.size() + 2];
  int paramI=0;
  params[paramI++]=dbName;
  params[paramI++]=tableName;
  for (  String colName : colNames) {
    params[paramI++]=colName;
  }
  for (  String partName : partNames) {
    params[paramI++]=partName;
  }
  return params;
}
