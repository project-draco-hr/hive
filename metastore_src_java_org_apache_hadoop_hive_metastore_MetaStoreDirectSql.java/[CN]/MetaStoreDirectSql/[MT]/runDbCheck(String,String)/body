{
  Transaction tx=pm.currentTransaction();
  if (!tx.isActive()) {
    tx.begin();
  }
  try {
    executeNoResult(queryText);
    return true;
  }
 catch (  Throwable t) {
    LOG.debug(name + " check failed, assuming we are not on " + name+ ": "+ t.getMessage());
    tx.rollback();
    tx=pm.currentTransaction();
    tx.begin();
    return false;
  }
}
