{
  for (  String table : tableNames) {
    try {
      if (hmsc.getTable(DATABASE,table) != null) {
        hmsc.dropTable(DATABASE,table);
      }
    }
 catch (    NoSuchObjectException ignored) {
    }
  }
  for (int i=0; i < tableNames.length; i++) {
    createTable(tableNames[i],tablePerms[i]);
  }
}
