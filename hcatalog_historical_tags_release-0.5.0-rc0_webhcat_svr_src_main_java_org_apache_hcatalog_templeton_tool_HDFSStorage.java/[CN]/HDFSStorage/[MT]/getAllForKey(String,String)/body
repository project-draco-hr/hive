{
  ArrayList<String> allNodes=new ArrayList<String>();
  try {
    for (    Type type : Type.values()) {
      allNodes.addAll(getAllForTypeAndKey(type,key,value));
    }
  }
 catch (  Exception e) {
    LOG.trace("Couldn't find children for key " + key + ": "+ e.getMessage());
  }
  return allNodes;
}
