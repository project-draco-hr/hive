{
  int idx=pItr.getIndex() - 1;
  ArrayList<Object> path=new ArrayList<Object>();
  path.add(ObjectInspectorUtils.copyToStandardObject(currRow,rowOI));
  int pSz=1;
  while (pSz < sz && pItr.hasNext()) {
    currRow=pItr.next();
    path.add(ObjectInspectorUtils.copyToStandardObject(currRow,rowOI));
    pSz++;
  }
  pItr.resetToIndex(idx);
  return path;
}
