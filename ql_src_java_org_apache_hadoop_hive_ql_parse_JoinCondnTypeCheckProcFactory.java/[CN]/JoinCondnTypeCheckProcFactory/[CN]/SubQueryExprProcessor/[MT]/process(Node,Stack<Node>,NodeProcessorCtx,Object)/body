{
  JoinTypeCheckCtx ctx=(JoinTypeCheckCtx)procCtx;
  if (ctx.getError() != null) {
    return null;
  }
  ASTNode expr=(ASTNode)nd;
  ASTNode sqNode=(ASTNode)expr.getParent().getChild(1);
  ctx.setError(ErrorMsg.UNSUPPORTED_SUBQUERY_EXPRESSION.getMsg(sqNode,"SubQuery expressions are npt supported in Join Condition"),sqNode);
  return null;
}
