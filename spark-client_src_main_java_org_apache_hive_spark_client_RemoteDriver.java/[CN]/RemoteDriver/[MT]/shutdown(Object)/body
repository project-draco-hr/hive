{
  if (running) {
    LOG.info("Shutting down remote driver.");
    running=false;
    for (    JobWrapper<?> job : activeJobs.values()) {
      cancelJob(job);
    }
    if (msg != null) {
      client.tell(msg,actor);
    }
    if (jc != null) {
      jc.stop();
    }
    system.shutdown();
synchronized (shutdownLock) {
      shutdownLock.notifyAll();
    }
  }
}
