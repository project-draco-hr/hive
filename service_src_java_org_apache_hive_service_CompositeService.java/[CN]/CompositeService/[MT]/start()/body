{
  int i=0;
  try {
    for (int n=serviceList.size(); i < n; i++) {
      Service service=serviceList.get(i);
      service.start();
    }
    super.start();
  }
 catch (  Throwable e) {
    LOG.error("Error starting services " + getName(),e);
    stop(i);
    throw new ServiceException("Failed to Start " + getName(),e);
  }
}
