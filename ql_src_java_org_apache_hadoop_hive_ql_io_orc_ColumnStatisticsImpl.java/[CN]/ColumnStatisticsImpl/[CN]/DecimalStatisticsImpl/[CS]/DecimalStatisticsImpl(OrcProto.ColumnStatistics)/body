{
  super(stats);
  OrcProto.DecimalStatistics dec=stats.getDecimalStatistics();
  if (dec.hasMaximum()) {
    maximum=new HiveDecimal(dec.getMaximum());
  }
  if (dec.hasMinimum()) {
    minimum=new HiveDecimal(dec.getMinimum());
  }
  if (dec.hasSum()) {
    sum=new HiveDecimal(dec.getSum());
  }
 else {
    sum=null;
  }
}
