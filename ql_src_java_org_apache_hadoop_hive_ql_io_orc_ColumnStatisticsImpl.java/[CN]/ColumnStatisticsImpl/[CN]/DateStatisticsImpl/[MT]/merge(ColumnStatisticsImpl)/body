{
  super.merge(other);
  DateStatisticsImpl dateStats=(DateStatisticsImpl)other;
  if (minimum == null) {
    minimum=dateStats.minimum;
    maximum=dateStats.maximum;
  }
 else   if (dateStats.minimum != null) {
    if (minimum > dateStats.minimum) {
      minimum=dateStats.minimum;
    }
    if (maximum < dateStats.maximum) {
      maximum=dateStats.maximum;
    }
  }
}
