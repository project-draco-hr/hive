{
  super.merge(other);
  DoubleStatisticsImpl dbl=(DoubleStatisticsImpl)other;
  if (!hasMinimum) {
    hasMinimum=dbl.hasMinimum;
    minimum=dbl.minimum;
    maximum=dbl.maximum;
  }
 else   if (dbl.hasMinimum) {
    if (dbl.minimum < minimum) {
      minimum=dbl.minimum;
    }
    if (dbl.maximum > maximum) {
      maximum=dbl.maximum;
    }
  }
  sum+=dbl.sum;
}
