{
  Properties tbl=new Properties();
  tbl.setProperty(Constants.SERIALIZATION_FORMAT,"9");
  tbl.setProperty(Constants.LIST_COLUMNS,"key,valint,valbyte,valshort,vallong,valfloat,valdouble,valbool");
  tbl.setProperty(Constants.LIST_COLUMN_TYPES,"string:map<int,int>:map<tinyint,tinyint>:map<smallint,smallint>:map<bigint,bigint>:" + "map<float,float>:map<double,double>:map<boolean,boolean>");
  tbl.setProperty(HBaseSerDe.HBASE_COLUMNS_MAPPING,":key#-,cf-int:#b:b,cf-byte:#b:b,cf-short:#b:b,cf-long:#b:b,cf-float:#b:b,cf-double:#b:b," + "cf-bool:#b:b");
  return tbl;
}
