{
  Comparator<T> comp;
  if (ascending) {
    comp=new Comparator<T>(){
      public int compare(      T o1,      T o2){
        return o1.compareTo(o2);
      }
    }
;
  }
 else {
    comp=new Comparator<T>(){
      public int compare(      T o1,      T o2){
        return o2.compareTo(o1);
      }
    }
;
  }
  return comp;
}
