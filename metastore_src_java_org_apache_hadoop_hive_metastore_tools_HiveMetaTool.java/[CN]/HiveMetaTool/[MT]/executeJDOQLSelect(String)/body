{
  Collection<?> result=objStore.executeJDOQLSelect(query);
  if (result != null) {
    Iterator<?> iter=result.iterator();
    while (iter.hasNext()) {
      Object o=iter.next();
      System.out.println(o.toString());
    }
  }
 else {
    System.err.println("HiveMetaTool:Encountered error during executeJDOQLSelect -" + "commit of JDO transaction failed.");
  }
}
