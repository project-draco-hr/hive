{
  Set<String> hdfsRoots=objStore.listFSRoots();
  if (hdfsRoots != null) {
    System.out.println("HiveMetaTool:Listing FS Roots..");
    for (    String s : hdfsRoots) {
      System.out.println(s);
    }
  }
 else {
    System.err.println("HiveMetaTool:Encountered error during listFSRoot - " + "commit of JDO transaction failed");
  }
}
