{
  boolean walkChildren=true;
  if (op instanceof SelectOperator) {
    for (    Operator<? extends Serializable> child : op.getChildOperators()) {
      if ((child instanceof FileSinkOperator) || (child instanceof ScriptOperator))       walkChildren=false;
    }
  }
  if ((op.getChildOperators() == null) || getDispatchedList().containsAll(op.getChildOperators()) || !walkChildren) {
    dispatch(op,null);
    return;
  }
  getToWalk().removeAll(op.getChildOperators());
  getToWalk().addAll(0,op.getChildOperators());
  getToWalk().add(op);
}
