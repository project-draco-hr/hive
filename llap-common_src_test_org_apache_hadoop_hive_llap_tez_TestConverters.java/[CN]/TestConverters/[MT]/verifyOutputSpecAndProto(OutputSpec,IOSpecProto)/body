{
  assertEquals(outputSpec.getPhysicalEdgeCount(),outputSpecProto.getPhysicalEdgeCount());
  assertEquals(outputSpec.getDestinationVertexName(),outputSpecProto.getConnectedVertexName());
  assertEquals(outputSpec.getOutputDescriptor().getClassName(),outputSpecProto.getIoDescriptor().getClassName());
  assertEquals(outputSpec.getOutputDescriptor().getUserPayload().getPayload(),outputSpecProto.getIoDescriptor().getUserPayload().getUserPayload().asReadOnlyByteBuffer());
}
