{
  int rc=1;
  SparkClient client=null;
  try {
    configureNumberOfReducers();
    client=SparkClient.getInstance(driverContext.getCtx().getConf());
    rc=client.execute(driverContext,getWork());
  }
 catch (  Exception e) {
    LOG.error("Failed to execute spark task.",e);
    return 1;
  }
 finally {
    if (client != null) {
      rc=close(rc);
    }
  }
  return rc;
}
