{
  Assert.assertEquals(container1.rowCount(),container2.rowCount());
  AbstractRowContainer.RowIterator<List<Object>> iter1=container1.rowIter(), iter2=container2.rowIter();
  for (List<Object> row1=iter1.first(), row2=iter2.first(); row1 != null && row2 != null; row1=iter1.next(), row2=iter2.next()) {
    Assert.assertEquals(row1,row2);
  }
}
