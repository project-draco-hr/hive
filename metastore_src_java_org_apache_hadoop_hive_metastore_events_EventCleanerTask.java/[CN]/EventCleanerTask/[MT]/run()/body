{
  try {
    long deleteCnt=handler.executeWithRetry(new Command<Long>(){
      @Override public Long run(      RawStore ms) throws Exception {
        return ms.cleanupEvents();
      }
    }
);
    if (deleteCnt > 0L) {
      LOG.info("Number of events deleted from event Table: " + deleteCnt);
    }
  }
 catch (  Exception e) {
    LOG.error(e);
  }
}
