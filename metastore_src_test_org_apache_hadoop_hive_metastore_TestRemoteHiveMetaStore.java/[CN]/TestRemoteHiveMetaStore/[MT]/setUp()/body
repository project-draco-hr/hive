{
  super.setUp();
  if (isServerStarted) {
    assertNotNull("Unable to connect to the MetaStore server",client);
    return;
  }
  System.out.println("Starting MetaStore Server on port " + METASTORE_PORT);
  Thread t=new Thread(new RunMS());
  t.start();
  isServerStarted=true;
  Thread.sleep(5000);
  createClient(false);
}
