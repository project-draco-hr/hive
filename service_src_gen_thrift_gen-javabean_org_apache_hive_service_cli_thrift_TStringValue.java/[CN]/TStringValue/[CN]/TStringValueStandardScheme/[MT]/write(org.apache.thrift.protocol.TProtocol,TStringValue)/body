{
  struct.validate();
  oprot.writeStructBegin(STRUCT_DESC);
  if (struct.value != null) {
    if (struct.isSetValue()) {
      oprot.writeFieldBegin(VALUE_FIELD_DESC);
      oprot.writeString(struct.value);
      oprot.writeFieldEnd();
    }
  }
  oprot.writeFieldStop();
  oprot.writeStructEnd();
}
