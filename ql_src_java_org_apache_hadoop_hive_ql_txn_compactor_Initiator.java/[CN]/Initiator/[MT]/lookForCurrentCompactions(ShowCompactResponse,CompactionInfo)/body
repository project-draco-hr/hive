{
  if (compactions.getCompacts() != null) {
    for (    ShowCompactResponseElement e : compactions.getCompacts()) {
      if (!e.getState().equals(TxnHandler.CLEANING_RESPONSE) && e.getDbname().equals(ci.dbname) && e.getTablename().equals(ci.tableName)&& (e.getPartitionname() == null && ci.partName == null || e.getPartitionname().equals(ci.partName))) {
        return true;
      }
    }
  }
  return false;
}
