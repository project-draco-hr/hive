{
  l4j.debug(getSessionName() + " Attempting kill.");
  if (runnable != null) {
    try {
      runnable.join(1000);
      l4j.debug(getSessionName() + " Thread join complete");
    }
 catch (    InterruptedException e) {
      l4j.error(getSessionName() + " killing session caused exception ",e);
    }
  }
}
