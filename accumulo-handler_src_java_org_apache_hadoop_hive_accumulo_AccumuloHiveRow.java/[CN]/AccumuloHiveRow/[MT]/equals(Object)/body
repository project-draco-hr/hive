{
  if (o instanceof AccumuloHiveRow) {
    AccumuloHiveRow other=(AccumuloHiveRow)o;
    if (null == rowId) {
      if (null != other.rowId) {
        return false;
      }
    }
 else     if (!rowId.equals(other.rowId)) {
      return false;
    }
    return tuples.equals(other.tuples);
  }
  return false;
}
