{
  Statement stmt=con.createStatement();
  ResultSet res=stmt.executeQuery("select * from " + tableName);
  assertNotNull("ResultSet is null",res);
  res.close();
  try {
    res.getInt(1);
    fail("Expected SQLException");
  }
 catch (  SQLException e) {
  }
  try {
    res.getMetaData();
    fail("Expected SQLException");
  }
 catch (  SQLException e) {
  }
  try {
    res.setFetchSize(10);
    fail("Expected SQLException");
  }
 catch (  SQLException e) {
  }
  stmt.close();
}
