{
  HiveStatement stmt=(HiveStatement)con.createStatement();
  ResultSet rs;
  int expectedCount=1028;
  int rowCount=0;
  boolean isResulSet=stmt.executeAsync("select t1.value as v11, " + "t2.value as v12 from " + tableName + " t1 join "+ tableName+ " t2 on t1.under_col = t2.under_col");
  assertTrue(isResulSet);
  rs=stmt.getResultSet();
  assertNotNull(rs);
  while (rs.next()) {
    String value=rs.getString(2);
    rowCount++;
    assertNotNull(value);
  }
  assertEquals(rowCount,expectedCount);
  stmt.close();
}
