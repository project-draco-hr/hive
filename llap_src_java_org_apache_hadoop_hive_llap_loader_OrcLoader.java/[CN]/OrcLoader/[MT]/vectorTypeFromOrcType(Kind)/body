{
switch (orcType) {
case BOOLEAN:
case BYTE:
case SHORT:
case INT:
case LONG:
case DATE:
case TIMESTAMP:
    return Vector.Type.LONG;
case FLOAT:
case DOUBLE:
  return Vector.Type.DOUBLE;
case STRING:
return Vector.Type.BINARY;
case DECIMAL:
return Vector.Type.DECIMAL;
default :
throw new UnsupportedOperationException("Unsupported type " + orcType);
}
}
