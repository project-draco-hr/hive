{
  MiniMRCluster.JobTrackerRunner runner=mr.getJobTrackerRunner();
  JobTracker tracker=runner.getJobTracker();
  if (tracker != null) {
    for (    JobInProgress running : tracker.getRunningJobs()) {
      try {
        running.kill();
      }
 catch (      Exception e) {
      }
    }
  }
  runner.shutdown();
}
