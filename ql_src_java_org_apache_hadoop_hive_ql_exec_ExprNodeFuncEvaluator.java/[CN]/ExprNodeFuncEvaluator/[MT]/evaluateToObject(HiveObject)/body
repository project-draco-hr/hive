{
  for (int i=0; i < evaluators.size(); i++) {
    Object o=evaluators.get(i).evaluateToObject(row);
    children[i]=o;
  }
  try {
    return expr.getUDFMethod().invoke(udf,children);
  }
 catch (  Exception e) {
    throw new HiveException("Unable to execute UDF function " + udf.getClass() + " "+ expr.getUDFMethod()+ " on inputs "+ "("+ children.length+ ") "+ Arrays.asList(children)+ ": "+ e.getMessage(),e);
  }
}
