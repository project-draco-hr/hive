{
  if ((bw == null) || (pos == null) || (len == null)) {
    return null;
  }
  if ((len.get() <= 0)) {
    return new BytesWritable();
  }
  int[] index=makeIndex(pos.get(),len.get(),bw.getLength());
  if (index == null) {
    return new BytesWritable();
  }
  return new BytesWritable(Arrays.copyOfRange(bw.getBytes(),index[0],index[1]));
}
