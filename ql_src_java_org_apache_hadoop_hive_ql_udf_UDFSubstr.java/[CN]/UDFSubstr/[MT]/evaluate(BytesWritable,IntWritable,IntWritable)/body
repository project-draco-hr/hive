{
  if ((null == bw) || (null == pos) || (null == len)) {
    return null;
  }
  BytesWritable outgoing=new BytesWritable();
  if ((len.get() <= 0) || (pos.get() < 0) || (pos.get() > bw.getLength())) {
    return outgoing;
  }
  byte[] underlying=bw.getBytes();
  outgoing.set(underlying,pos.get(),len.get());
  return outgoing;
}
