{
  if ((t == null) || (pos == null) || (len == null)) {
    return null;
  }
  r.clear();
  if ((len.get() <= 0)) {
    return r;
  }
  int[] index=makeIndex(pos.get(),len.get(),t.getLength());
  if (index == null) {
    return r;
  }
  String s=t.toString();
  r.set(s.substring(index[0],index[1]));
  return r;
}
