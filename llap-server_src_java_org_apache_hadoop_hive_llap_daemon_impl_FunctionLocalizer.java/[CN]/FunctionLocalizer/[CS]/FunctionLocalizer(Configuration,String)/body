{
  this.conf=conf;
  this.localDir=new File(localDir,DIR_NAME);
  this.executorClassloader=(URLClassLoader)Utilities.createUDFClassLoader((URLClassLoader)Thread.currentThread().getContextClassLoader(),new String[]{});
  this.workThread=new Thread(new Runnable(){
    @Override public void run(){
      runWorkThread();
    }
  }
);
}
