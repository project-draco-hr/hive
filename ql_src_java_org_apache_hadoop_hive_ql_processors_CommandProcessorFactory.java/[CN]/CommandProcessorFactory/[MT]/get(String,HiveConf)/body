{
  CommandProcessor result=getForHiveCommand(cmd,conf);
  if (result != null) {
    return result;
  }
  if (isBlank(cmd)) {
    return null;
  }
 else {
    if (conf == null) {
      return new Driver();
    }
    Driver drv=mapDrivers.get(conf);
    if (drv == null) {
      drv=new Driver();
      mapDrivers.put(conf,drv);
    }
    drv.init();
    return drv;
  }
}
