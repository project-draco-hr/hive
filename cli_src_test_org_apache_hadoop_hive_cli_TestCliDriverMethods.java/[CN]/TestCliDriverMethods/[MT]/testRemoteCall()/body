{
  MyCliSessionState ss=new MyCliSessionState(new HiveConf(),org.apache.hadoop.hive.cli.TestCliDriverMethods.MyCliSessionState.ClientResult.RETURN_OK);
  ss.err=System.err;
  ByteArrayOutputStream data=new ByteArrayOutputStream();
  ss.out=new PrintStream(data);
  MyCliSessionState.start(ss);
  CliDriver cliDriver=new CliDriver();
  cliDriver.processCmd("remote command");
  assertTrue(data.toString().contains("test result"));
}
