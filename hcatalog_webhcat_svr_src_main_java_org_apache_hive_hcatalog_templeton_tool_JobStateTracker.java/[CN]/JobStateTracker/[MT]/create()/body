{
  String[] paths=ZooKeeperStorage.getPaths(job_trackingroot);
  for (  String znode : paths) {
    try {
      zk.create(znode,new byte[0],Ids.OPEN_ACL_UNSAFE,CreateMode.PERSISTENT);
    }
 catch (    KeeperException.NodeExistsException e) {
    }
catch (    Exception e) {
      throw new IOException("Unable to create parent nodes");
    }
  }
  try {
    trackingnode=zk.create(makeTrackingZnode(),jobid.getBytes(),Ids.OPEN_ACL_UNSAFE,CreateMode.PERSISTENT_SEQUENTIAL);
  }
 catch (  Exception e) {
    throw new IOException("Unable to create " + makeTrackingZnode());
  }
}
