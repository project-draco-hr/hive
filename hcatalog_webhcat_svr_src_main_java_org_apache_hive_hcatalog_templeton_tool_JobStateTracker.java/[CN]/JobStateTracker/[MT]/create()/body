{
  try {
    zk.create().creatingParentsIfNeeded().withMode(CreateMode.PERSISTENT).withACL(Ids.OPEN_ACL_UNSAFE).forPath(job_trackingroot);
  }
 catch (  KeeperException.NodeExistsException e) {
  }
catch (  Exception e) {
    throw new IOException("Unable to create parent nodes");
  }
  try {
    trackingnode=zk.create().withMode(CreateMode.PERSISTENT_SEQUENTIAL).withACL(Ids.OPEN_ACL_UNSAFE).forPath(makeTrackingZnode(),jobid.getBytes());
  }
 catch (  Exception e) {
    throw new IOException("Unable to create " + makeTrackingZnode());
  }
}
