{
  isdone=false;
  this.conf=conf;
  SessionState ss=SessionState.get();
  try {
    if (ss == null) {
      db=Hive.get(conf);
    }
 else {
      db=ss.getDb();
    }
  }
 catch (  HiveException e) {
    LOG.error(StringUtils.stringifyException(e));
    throw new RuntimeException(e);
  }
  console=new LogHelper(LOG);
  if (childTasks == null) {
    return;
  }
  for (  Task<? extends Serializable> t : childTasks) {
    t.initialize(conf);
  }
}
