{
  try {
    String dbname="foo";
    String name="pete_ms_test";
    DB db=DB.createDB(dbname,conf_);
    assertTrue(db.tableExists(name) == false);
    try {
      Table.create(db,name,createSchema(dbname,name),conf_);
    }
 catch (    MetaException e) {
      e.printStackTrace();
    }
    assertTrue(db.tableExists(name) == true);
    cleanup();
  }
 catch (  MetaException e) {
    e.printStackTrace();
    throw e;
  }
}
