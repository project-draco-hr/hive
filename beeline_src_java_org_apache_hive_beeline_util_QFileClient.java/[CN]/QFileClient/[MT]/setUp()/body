{
  beeLine.runCommands(new String[]{"USE default;","SHOW TABLES;","DROP DATABASE IF EXISTS `" + testname + "` CASCADE;","CREATE DATABASE `" + testname + "`;","USE `" + testname + "`;","set test.data.dir=" + testDataDirectory + ";","set test.script.dir=" + testScriptDirectory + ";","!run " + testScriptDirectory + "/q_test_init.sql"});
}
