{
  checkIsClosed();
  boolean success=false;
  try {
    if (ugi == null) {
      writeImpl(records);
    }
 else {
      ugi.doAs(new PrivilegedExceptionAction<Void>(){
        @Override public Void run() throws StreamingException {
          writeImpl(records);
          return null;
        }
      }
);
    }
    success=true;
  }
 catch (  SerializationError ex) {
    success=true;
    throw ex;
  }
catch (  IOException e) {
    throw new ImpersonationFailed("Failed writing as user '" + username + "' to endPoint :"+ endPt+ ". Transaction Id: "+ getCurrentTxnId(),e);
  }
 finally {
    markDead(success);
  }
}
