{
  if (ugi == null) {
    return fetchTransactionBatchImpl(numTransactions,recordWriter);
  }
  try {
    return ugi.doAs(new PrivilegedExceptionAction<TransactionBatch>(){
      @Override public TransactionBatch run() throws StreamingException, InterruptedException {
        return fetchTransactionBatchImpl(numTransactions,recordWriter);
      }
    }
);
  }
 catch (  IOException e) {
    throw new ImpersonationFailed("Failed to fetch Txn Batch as user '" + ugi.getShortUserName() + "' when acquiring Transaction Batch on endPoint "+ endPt,e);
  }
}
