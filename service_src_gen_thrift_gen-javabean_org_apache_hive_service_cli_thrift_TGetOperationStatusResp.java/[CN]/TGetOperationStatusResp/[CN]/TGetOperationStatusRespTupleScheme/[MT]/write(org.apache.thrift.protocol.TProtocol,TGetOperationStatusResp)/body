{
  TTupleProtocol oprot=(TTupleProtocol)prot;
  struct.status.write(oprot);
  BitSet optionals=new BitSet();
  if (struct.isSetOperationState()) {
    optionals.set(0);
  }
  oprot.writeBitSet(optionals,1);
  if (struct.isSetOperationState()) {
    oprot.writeI32(struct.operationState.getValue());
  }
}
