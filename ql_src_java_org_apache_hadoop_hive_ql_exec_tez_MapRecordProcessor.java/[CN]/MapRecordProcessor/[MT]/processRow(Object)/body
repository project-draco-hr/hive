{
  execContext.resetRow();
  try {
    if (mapOp.getDone()) {
      return false;
    }
 else {
      mapOp.process((Writable)value);
      if (isLogInfoEnabled) {
        logProgress();
      }
    }
  }
 catch (  Throwable e) {
    abort=true;
    if (e instanceof OutOfMemoryError) {
      throw (OutOfMemoryError)e;
    }
 else {
      l4j.fatal(StringUtils.stringifyException(e));
      throw new RuntimeException(e);
    }
  }
  return true;
}
