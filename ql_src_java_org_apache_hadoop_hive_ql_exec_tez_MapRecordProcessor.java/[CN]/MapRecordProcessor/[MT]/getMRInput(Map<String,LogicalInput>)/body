{
  MRInputLegacy theMRInput=null;
  for (  LogicalInput inp : inputs.values()) {
    if (inp instanceof MRInputLegacy) {
      if (theMRInput != null) {
        throw new IllegalArgumentException("Only one MRInput is expected");
      }
      theMRInput=(MRInputLegacy)inp;
    }
  }
  return theMRInput;
}
