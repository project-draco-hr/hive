{
  if (inputs.size() != 1) {
    throw new IllegalArgumentException("MapRecordProcessor expects single input" + ", inputCount=" + inputs.size());
  }
  MRInput in=(MRInput)inputs.iterator().next();
  KeyValueReader reader=in.getReader();
  while (reader.next()) {
    Object value=reader.getCurrentValue();
    boolean needMore=processRow(value);
    if (!needMore) {
      break;
    }
  }
}
