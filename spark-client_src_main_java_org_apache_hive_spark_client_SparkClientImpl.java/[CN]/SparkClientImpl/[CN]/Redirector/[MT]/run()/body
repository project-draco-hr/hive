{
  try {
    String line=null;
    while ((line=in.readLine()) != null) {
      LOG.info(line);
      if (errLogs != null) {
        if (numErrLogLines++ < MAX_ERR_LOG_LINES_FOR_RPC) {
          errLogs.add(line);
        }
      }
    }
  }
 catch (  Exception e) {
    LOG.warn("Error in redirector thread.",e);
  }
}
