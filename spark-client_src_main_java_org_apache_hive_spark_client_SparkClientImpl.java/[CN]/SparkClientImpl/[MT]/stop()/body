{
  if (remoteRef != null) {
    LOG.info("Sending EndSession to remote actor.");
    remoteRef.tell(new Protocol.EndSession(),clientRef);
  }
  unbind(clientRef);
  try {
    driverThread.join();
  }
 catch (  InterruptedException ie) {
    LOG.debug("Interrupted before driver thread was finished.");
  }
}
