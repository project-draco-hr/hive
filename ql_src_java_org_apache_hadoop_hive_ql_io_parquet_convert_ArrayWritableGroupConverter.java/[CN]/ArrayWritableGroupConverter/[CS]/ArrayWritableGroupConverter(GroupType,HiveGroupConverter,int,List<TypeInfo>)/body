{
  this.parent=parent;
  this.index=index;
  int count=groupType.getFieldCount();
  if (count < 1 || count > 2) {
    throw new IllegalStateException("Field count must be either 1 or 2: " + count);
  }
  isMap=count == 2;
  converters=new Converter[count];
  for (int i=0; i < count; i++) {
    converters[i]=getConverterFromDescription(groupType.getType(i),i,this,hiveSchemaTypeInfos);
  }
}
