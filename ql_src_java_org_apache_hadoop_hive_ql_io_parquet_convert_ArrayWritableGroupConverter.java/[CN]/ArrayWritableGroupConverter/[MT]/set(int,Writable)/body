{
  if (index != 0 && mapPairContainer == null || index > 1) {
    throw new ParquetDecodingException("Repeated group can only have one or two fields for maps." + " Not allowed to set for the index : " + index);
  }
  if (isMap) {
    mapPairContainer[index]=value;
  }
 else {
    currentValue=value;
  }
}
