{
  long value=1000000 * jobId + 1000 * stageId + taskId;
  return new Metrics(value,value,value,value,value,value,value,Optional.fromNullable(new InputMetrics(DataReadMethod.Memory,value)),Optional.fromNullable(new ShuffleReadMetrics((int)value,(int)value,value,value)),Optional.fromNullable(new ShuffleWriteMetrics(value,value)));
}
