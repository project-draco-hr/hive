{
  OutputJobInfo jobInfo=HCatOutputFormat.getJobInfo(context);
  try {
    handleDuplicatePublish(context,jobInfo,HCatOutputFormat.createHiveClient(jobInfo.getServerUri(),context.getConfiguration()),jobInfo.getTableInfo().getTable());
  }
 catch (  MetaException e) {
    throw new IOException(e);
  }
catch (  TException e) {
    throw new IOException(e);
  }
  of.checkOutputSpecs(context);
}
