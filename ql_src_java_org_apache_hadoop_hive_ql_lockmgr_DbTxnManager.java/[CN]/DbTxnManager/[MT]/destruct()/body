{
  try {
    if (txnId > 0)     rollbackTxn();
    if (lockMgr != null)     lockMgr.close();
    if (client != null)     client.close();
  }
 catch (  Exception e) {
    LOG.error("Caught exception " + e.getClass().getName() + " with message <"+ e.getMessage()+ ">, swallowing as there is nothing we can do with it.");
  }
}
