{
  StringBuilder sb=new StringBuilder();
  sb.append("DROP TABLE IF EXISTS ");
  sb.append(dbName);
  sb.append('.');
  sb.append(tableName);
  if (isReplicatedEvent) {
    sb.append(" FOR REPLICATION(\'");
    sb.append(eventId);
    sb.append("\')");
  }
  return Collections.singletonList(sb.toString());
}
