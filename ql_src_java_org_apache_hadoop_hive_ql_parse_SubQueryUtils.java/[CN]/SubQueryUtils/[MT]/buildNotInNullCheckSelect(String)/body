{
  ASTNode select=(ASTNode)ParseDriver.adaptor.create(HiveParser.TOK_SELECT,"TOK_SELECT");
  ASTNode selectExpr=(ASTNode)ParseDriver.adaptor.create(HiveParser.TOK_SELEXPR,"TOK_SELEXPR");
  ASTNode countStar=(ASTNode)ParseDriver.adaptor.create(HiveParser.TOK_FUNCTIONSTAR,"TOK_FUNCTIONSTAR");
  ASTNode alias=(createAliasAST(cntAlias));
  countStar.addChild((ASTNode)ParseDriver.adaptor.create(HiveParser.Identifier,"count"));
  select.addChild(selectExpr);
  selectExpr.addChild(countStar);
  selectExpr.addChild(alias);
  return select;
}
