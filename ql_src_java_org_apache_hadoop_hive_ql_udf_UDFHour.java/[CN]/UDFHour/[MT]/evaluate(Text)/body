{
  if (dateString == null) {
    return null;
  }
  try {
    Date date=null;
    try {
      date=formatter1.parse(dateString.toString());
    }
 catch (    ParseException e) {
      date=formatter2.parse(dateString.toString());
    }
    calendar.setTime(date);
    result.set(calendar.get(Calendar.HOUR_OF_DAY));
    return result;
  }
 catch (  ParseException e) {
    return null;
  }
}
