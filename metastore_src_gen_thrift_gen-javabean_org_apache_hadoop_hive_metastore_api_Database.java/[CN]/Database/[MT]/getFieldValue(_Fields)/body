{
switch (field) {
case NAME:
    return getName();
case DESCRIPTION:
  return getDescription();
case LOCATION_URI:
return getLocationUri();
case PARAMETERS:
return getParameters();
case PRIVILEGES:
return getPrivileges();
case OWNER_NAME:
return getOwnerName();
case OWNER_TYPE:
return getOwnerType();
}
throw new IllegalStateException();
}
