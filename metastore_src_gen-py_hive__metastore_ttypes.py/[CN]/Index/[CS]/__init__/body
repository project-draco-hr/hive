def __init__(self, d=None):
    self.indexName = None
    self.indexType = None
    self.tableName = None
    self.dbName = None
    self.colNames = None
    if isinstance(d, dict):
        if ('indexName' in d):
            self.indexName = d['indexName']
        if ('indexType' in d):
            self.indexType = d['indexType']
        if ('tableName' in d):
            self.tableName = d['tableName']
        if ('dbName' in d):
            self.dbName = d['dbName']
        if ('colNames' in d):
            self.colNames = d['colNames']
