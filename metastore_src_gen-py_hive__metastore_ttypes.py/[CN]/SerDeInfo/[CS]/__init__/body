def __init__(self, d=None):
    self.name = None
    self.serializationLib = None
    self.parameters = None
    if isinstance(d, dict):
        if ('name' in d):
            self.name = d['name']
        if ('serializationLib' in d):
            self.serializationLib = d['serializationLib']
        if ('parameters' in d):
            self.parameters = d['parameters']
