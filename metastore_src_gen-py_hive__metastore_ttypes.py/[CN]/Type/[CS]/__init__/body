def __init__(self, d=None):
    self.name = None
    self.type1 = None
    self.type2 = None
    self.fields = None
    if isinstance(d, dict):
        if ('name' in d):
            self.name = d['name']
        if ('type1' in d):
            self.type1 = d['type1']
        if ('type2' in d):
            self.type2 = d['type2']
        if ('fields' in d):
            self.fields = d['fields']
