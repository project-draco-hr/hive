{
  assertEquals("0-0",HiveIntervalYearMonth.valueOf("0-0").toString());
  assertEquals("1-2",HiveIntervalYearMonth.valueOf("1-2").toString());
  assertEquals("-1-2",HiveIntervalYearMonth.valueOf("-1-2").toString());
  assertEquals("1-0",HiveIntervalYearMonth.valueOf("1-0").toString());
  assertEquals("-1-0",HiveIntervalYearMonth.valueOf("-1-0").toString());
  assertEquals("0-0",HiveIntervalYearMonth.valueOf("-0-0").toString());
}
