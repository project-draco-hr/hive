{
  if (list1 != list2) {
    if (list1 != null && list2 != null) {
      if (list1.size() != list2.size()) {
        return false;
      }
      for (int i=0; i < list1.size(); i++) {
        if (!list1.get(i).isSame(list2.get(i))) {
          return false;
        }
      }
    }
 else {
      return false;
    }
  }
  return true;
}
