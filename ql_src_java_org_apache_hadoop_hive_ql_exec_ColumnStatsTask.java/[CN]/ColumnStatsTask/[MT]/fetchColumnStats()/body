{
  try {
    InspectableObject io=ftOp.getNextRow();
    if (io == null) {
      throw new CommandNeedRetryException();
    }
    return io;
  }
 catch (  CommandNeedRetryException e) {
    throw e;
  }
catch (  IOException e) {
    throw e;
  }
catch (  Exception e) {
    throw new IOException(e);
  }
}
