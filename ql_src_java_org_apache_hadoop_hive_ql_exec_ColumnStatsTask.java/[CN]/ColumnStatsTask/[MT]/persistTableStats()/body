{
  InspectableObject io=null;
  try {
    io=fetchColumnStats();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
catch (  CommandNeedRetryException e) {
    e.printStackTrace();
  }
  ColumnStatistics colStats=constructColumnStatsFromPackedRow(io.oi,io.o);
  try {
    db.updateTableColumnStatistics(colStats);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return 0;
}
