{
  int bin=0;
  for (int l=0, r=nusedbins; l < r; ) {
    bin=(l + r) / 2;
    if (bins.get(bin).x > v) {
      r=bin;
    }
 else {
      if (bins.get(bin).x < v) {
        l=++bin;
      }
 else {
        break;
      }
    }
  }
  if (bin < nusedbins && bins.get(bin).x == v) {
    bins.get(bin).y++;
  }
 else {
    Coord newBin=new Coord();
    newBin.x=v;
    newBin.y=1;
    bins.add(bin,newBin);
    if (++nusedbins > nbins) {
      trim();
    }
  }
}
