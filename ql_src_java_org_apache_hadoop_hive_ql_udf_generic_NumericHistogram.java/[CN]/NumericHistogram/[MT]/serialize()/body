{
  ArrayList<DoubleWritable> result=new ArrayList<DoubleWritable>();
  result.add(new DoubleWritable(nbins));
  if (bins != null) {
    for (int i=0; i < nusedbins; i++) {
      result.add(new DoubleWritable(bins[i].x));
      result.add(new DoubleWritable(bins[i].y));
    }
  }
  return result;
}
