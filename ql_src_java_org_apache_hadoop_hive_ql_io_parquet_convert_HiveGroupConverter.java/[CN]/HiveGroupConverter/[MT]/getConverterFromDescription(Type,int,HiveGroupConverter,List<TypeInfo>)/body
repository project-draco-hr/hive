{
  if (type == null) {
    return null;
  }
  if (type.isPrimitive()) {
    return ETypeConverter.getNewConverter(type.asPrimitiveType(),index,parent,hiveSchemaTypeInfos);
  }
 else {
    if (type.asGroupType().getRepetition() == Repetition.REPEATED) {
      return new ArrayWritableGroupConverter(type.asGroupType(),parent,index,hiveSchemaTypeInfos);
    }
 else {
      return new DataWritableGroupConverter(type.asGroupType(),parent,index,hiveSchemaTypeInfos);
    }
  }
}
