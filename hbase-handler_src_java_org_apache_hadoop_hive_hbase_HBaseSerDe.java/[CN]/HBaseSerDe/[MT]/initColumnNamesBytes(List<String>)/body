{
  List<byte[]> columnBytes=new ArrayList<byte[]>();
  String column=null;
  for (int i=0; i < columnNames.size(); i++) {
    column=columnNames.get(i);
    if (column.endsWith(":")) {
      columnBytes.add(Bytes.toBytes(column.split(":")[0]));
    }
 else {
      columnBytes.add(Bytes.toBytes(column));
    }
  }
  return columnBytes;
}
