{
  if (beeLine.getDatabaseConnection() == null) {
    return false;
  }
  try {
    if (beeLine.getDatabaseConnection().getConnection() != null && !(beeLine.getDatabaseConnection().getConnection().isClosed())) {
      beeLine.info(beeLine.loc("closing",beeLine.getDatabaseConnection().getConnection().getClass().getName()));
      beeLine.getDatabaseConnection().getConnection().close();
    }
 else {
      beeLine.info(beeLine.loc("already-closed"));
    }
  }
 catch (  Exception e) {
    return beeLine.error(e);
  }
  beeLine.getDatabaseConnections().remove();
  return true;
}
