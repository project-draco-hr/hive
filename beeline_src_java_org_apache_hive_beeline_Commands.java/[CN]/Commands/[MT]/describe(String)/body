{
  String[] table=beeLine.split(line,2,"Usage: describe <table name>");
  if (table == null) {
    return false;
  }
  ResultSet rs;
  if (table[1].equals("tables")) {
    rs=beeLine.getTables();
  }
 else {
    rs=beeLine.getColumns(table[1]);
  }
  if (rs == null) {
    return false;
  }
  beeLine.print(rs);
  rs.close();
  return true;
}
