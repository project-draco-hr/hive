{
  String[] parts=beeLine.split(line,2,"Usage: go <connection index>");
  if (parts == null) {
    return false;
  }
  int index=Integer.parseInt(parts[1]);
  if (!(beeLine.getDatabaseConnections().setIndex(index))) {
    beeLine.error(beeLine.loc("invalid-connection","" + index));
    list("");
    return false;
  }
  return true;
}
